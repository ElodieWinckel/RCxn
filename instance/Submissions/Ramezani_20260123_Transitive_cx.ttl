cx:EnglishTransitiveconstruction rcxn:hasConstructionMeaning cx:EnglishTransitiveconstruction_Meaning ;
    rcxn:hasSlots cx:EnglishTransitiveconstruction_slots ;
    cx:hasExample cx:EnglishTransitiveconstruction_Ex_A .

cx:EnglishTransitiveconstruction_slots a rdf:Seq ;
    rdf:_1 cx:EnglishTransitiveconstruction_1 ;
    rdf:_2 cx:EnglishTransitiveconstruction_2 ;
    rdf:_3 cx:EnglishTransitiveconstruction_3 .

cx:EnglishTransitiveconstruction_1 a rcxn:SlotMandatory ;
    rcxn:hasSyntacticFunction "Subject" .

cx:EnglishTransitiveconstruction_2 a rcxn:SlotMandatory .

cx:EnglishTransitiveconstruction_2_Form a rcxn:SlotForm ;
    rcxn:hasSyntacticForm cx:eng_Verb .

cx:EnglishTransitiveconstruction_3 a rcxn:SlotMandatory ;
    rcxn:hasSyntacticFunction "Object" .

cx:EnglishTransitiveconstruction_Meaning a rcxn:ConstructionMeaning ;
    rcxn:hasMeaning """A [subject]1 performs [an action]2 on [a direct object]3.""" .

cx:EnglishTransitiveconstruction_Ex_A a cx:Example ;
    cx:hasText "The baker baked a cake." ;
    cx:hasAnnotatedText "[The baker]1 [baked]2 [a cake]3." .

