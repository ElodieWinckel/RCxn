cx:EnglishDitransitiveconstruction rcxn:hasConstructionMeaning cx:EnglishDitransitiveconstruction_Meaning ;
    rcxn:hasSlots cx:EnglishDitransitiveconstruction_slots ;
    cx:hasExample cx:EnglishDitransitiveconstruction_Ex_A .

cx:EnglishDitransitiveconstruction_slots a rdf:Seq ;
    rdf:_1 cx:EnglishDitransitiveconstruction_1 ;
    rdf:_2 cx:EnglishDitransitiveconstruction_2 ;
    rdf:_3 cx:EnglishDitransitiveconstruction_3 ;
    rdf:_4 cx:EnglishDitransitiveconstruction_4 .

cx:EnglishDitransitiveconstruction_1 a rcxn:SlotMandatory ;
    rcxn:hasSyntacticFunction "Subject" .

cx:EnglishDitransitiveconstruction_2 a rcxn:SlotMandatory .

cx:EnglishDitransitiveconstruction_2_Form a rcxn:SlotForm ;
    rcxn:hasSyntacticForm cx:eng_Verb .

cx:EnglishDitransitiveconstruction_3 a rcxn:SlotMandatory ;
    rcxn:hasSyntacticFunction "Object" .

cx:EnglishDitransitiveconstruction_4 a rcxn:SlotMandatory ;
    rcxn:hasSyntacticFunction "Object" .

cx:EnglishDitransitiveconstruction_Meaning a rcxn:ConstructionMeaning ;
    rcxn:hasMeaning """A [subject]1 performs [an action of transfer]2 involving [a direct object]3 and [an indirect object]4.""" .

cx:EnglishDitransitiveconstruction_Ex_A a cx:Example ;
    cx:hasText "The teacher gave students homework." ;
    cx:hasAnnotatedText "[The teacher]1 [gave]2 [students]3 [homework]4." .

