@prefix RDFS: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix cx: <http://example.org/cx/> .
@prefix lg: <https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/lg#> .
@prefix membr: <http://example.org/users#> .
@prefix rcxn: <https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/rcxn#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

cx:EvaluativeAttributionConstruction a rcxn:Construction ;
    rcxn:hasTitle "Evaluative Attribution Construction" ;
    rcxn:hasConstructionMeaning cx:EvaluativeAttributionConstruction_Meaning ;
    rcxn:hasMetadata cx:EvaluativeAttributionConstruction_MD ;
    rcxn:hasSlots cx:EvaluativeAttributionConstruction_slots ;
    lg:partOfLanguage lg:engUsUkWr .

cx:EvaluativeAttributionConstruction_1 a rcxn:SlotOptional ;
    rcxn:hasSlotForm cx:EvaluativeAttributionConstruction_1_Form ;
    rcxn:hasOtherSemanticContribution "Attributor" .

cx:EvaluativeAttributionConstruction_1_Form a rcxn:SlotForm ;
    rcxn:hasSyntacticForm cx:eng_NounPhrase .

cx:EvaluativeAttributionConstruction_2 a rcxn:SlotMandatory ;
    rcxn:hasSlotForm cx:EvaluativeAttributionConstruction_2_Form .

cx:EvaluativeAttributionConstruction_2_Form a rcxn:SlotForm ;
    rcxn:hasSyntacticForm cx:eng_VerbPhrase .

cx:EvaluativeAttributionConstruction_3 a rcxn:SlotMandatory ;
    rcxn:hasSlotForm cx:EvaluativeAttributionConstruction_3_Form ;
    rcxn:hasOtherSemanticContribution "Attributee" .

cx:EvaluativeAttributionConstruction_3_Form a rcxn:SlotForm ;
    rcxn:hasSyntacticForm cx:eng_NounPhrase, cx:eng_PrepositionalPhrase .

cx:EvaluativeAttributionConstruction_4 a rcxn:SlotOptional ;
    rcxn:hasSlotForm cx:EvaluativeAttributionConstruction_4_Form .

cx:EvaluativeAttributionConstruction_4_Form a rcxn:SlotForm ;
    rcxn:hasStem "to be (lemma)" .

cx:EvaluativeAttributionConstruction_5 a rcxn:SlotMandatory ;
    rcxn:hasSlotForm cx:EvaluativeAttributionConstruction_5_Form ;
    rcxn:hasOtherSemanticContribution "Attribute" .

cx:EvaluativeAttributionConstruction_5_Form a rcxn:SlotForm ;
    rcxn:hasSyntacticForm cx:eng_NounPhrase, cx:eng_AdjectivePhrase, cx:eng_PrepositionalPhrase, cx:eng_VerbPhrase, cx:eng_clause .

cx:EvaluativeAttributionConstruction_MD a rcxn:Metadata ;
    rcxn:annotator membr:Stampfer ;
    rcxn:creationDate "2024-12-02"^^xsd:date .

cx:EvaluativeAttributionConstruction_Meaning a rcxn:ConstructionMeaning ;
    rcxn:hasMeaning "An ATTRIBUTOR mentally assigns ATTRIBUTE to an ATTRIBUTEE" .

cx:EvaluativeAttributionConstruction_slots a rdf:Seq ;
    rdf:_1 cx:EvaluativeAttributionConstruction_1 ;
    rdf:_2 cx:EvaluativeAttributionConstruction_2 ;
    rdf:_3 cx:EvaluativeAttributionConstruction_3 ;
    rdf:_4 cx:EvaluativeAttributionConstruction_4 ;
    rdf:_5 cx:EvaluativeAttributionConstruction_5 .

cx:eng_AdjectivePhrase a rcxn:Construction ;
    rcxn:hasTitle "Adjective Phrase" ;
    lg:partOfLanguage lg:eng .

cx:eng_clause a rcxn:Construction ;
    rcxn:hasTitle "Clause" ;
    lg:partOfLanguage lg:eng .