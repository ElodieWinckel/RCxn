compcon:inf_imperative-hortative a compcon:inf ;
    rdfs:label "imperative--hortative" ;
    cx:hasExample compcon:imperative-hortative-example-0,
        compcon:imperative-hortative-example-1 ;
    dc:description "a <a inf:speech-acts>speech act</a> which requests that the action expressed in the propositional content of the imperative--hortative be carried out, prototypically by the addressee but possibly by other persons. <i>Example</i>: <i>Dance!</i> is an example of the English imperative--hortative construction for the <a cxn:second-person-pronoun>second person</a>, and <i>Let's dance!</i> is an example of the same for the <a cxn:first-person-pronoun>first person</a> <a sem:number>plural</a>. The term <q>hortative</q> is sometimes used for a first person imperative--hortative, and <q>jussive</q> for a third person imperative--hortative. A negative imperative--hortative is a <a inf:prohibitive>prohibitive</a>. (Sections 12.1, 12.4)" ;
    dcterms:isPartOf "12.1",
        "12.4" ;
    skos:altLabel "hortative",
        "imperative" ;
    compcon:functionOf compcon:cxn_imperative-hortative-construction ;
    compcon:subtypeOf compcon:inf_speech-acts .

compcon:inf_information-packaging a compcon:inf ;
    rdfs:label "information packaging" ;
    dc:description "the way that the <a>meaning</a> or semantic content is <dq>packaged</dq> for communication in discourse. <i>Example</i>: the <a>property</a> concept <i>huge</i> can be presented or packaged as a <a>predication</a> asserted of an <a sem:object-concept>object</a> (<i>That tree is huge!</i>), or it can be used to <a inf:modification>modify</a> or add information about an object (<i>that huge tree over there</i>). (Sections 1.1, <b>1.3</b>)" ;
    dcterms:isPartOf "1.1",
        "1.3" ;
    skos:altLabel "IP",
        "discourse function",
        "information structure" ;
    compcon:functionOf compcon:cxn_construction .

