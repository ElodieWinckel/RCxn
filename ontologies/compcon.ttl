compcon:cxn_adjectival-phrase a compcon:cxn ;
    rdfs:label "adjectival phrase" ;
    cx:hasExample compcon:adjectival-phrase-example-0 ;
    dc:description "an <a>attributive phrase</a> whose <a>head</a> denotes a <a>property concept</a>. <i>Example</i>: in <i>a very large balloon, very large</i> is an adjectival phrase; the head <i>large</i> denotes a property concept. An adjectival phrase is the prototypical attributive phrase, and its head is an <a>adjective</a>. (Section 2.2.3)" ;
    dcterms:isPartOf "2.2.3" ;
    skos:altLabel "adjective attributive phrase" ;
    compcon:subtypeOf compcon:cxn_attributive-phrase,
        compcon:cxn_prototypical-construction .

compcon:cxn_age-term a compcon:cxn ;
    rdfs:label "age term" ;
    cx:hasExample compcon:age-term-example-0,
        compcon:age-term-example-1 ;
    dc:description "a <a>modifier</a> expressing a concept of age, maturity, or ripeness. <i>Examples</i>: <i>old</i> and <i>ripe</i> are English age terms. (Section 4.1.2)" ;
    dcterms:isPartOf "4.1.2" ;
    compcon:subtypeOf compcon:cxn_adjective .

compcon:cxn_agentive-change-of-state-verb a compcon:cxn ;
    rdfs:label "agentive change of state verb" ;
    cx:hasExample compcon:agentive-change-of-state-verb-example-0 ;
    dc:description "a <a>verb</a> that expresses an agentive change of state event. <i>Example</i>: the event of a person drying dishes is an agentive change of state event, and <i>dry</i> is an agentive change of state verb. (Section 6.2.1)" ;
    dcterms:isPartOf "6.2.1" ;
    compcon:subtypeOf compcon:cxn_change-of-state-verb .

compcon:cxn_alienable-possession-attributive-phrase a compcon:cxn ;
    rdfs:label "alienable possession attributive phrase" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_possessive-attributive-phrase .

compcon:cxn_alternative-question-construction a compcon:cxn ;
    rdfs:label "alternative question construction" ;
    cx:hasExample compcon:alternative-question-construction-example-0 ;
    dc:description "a construction expressing the <a>alternative question function</a>. <i>Example</i>: <i>Do you prefer beer or wine?</i> is an instance of an alternative question construction, where the alternatives offered are beer and wine. (Section 12.3.1)" ;
    dcterms:isPartOf "12.3.1" ;
    compcon:subtypeOf compcon:cxn_interrogative-construction .

compcon:cxn_amount-term a compcon:cxn ;
    rdfs:label "amount term" ;
    cx:hasExample compcon:amount-term-example-0 ;
    dc:description "a form used to indicate an imprecise quantity for noncountable entities. <i>Example</i>: in <i>some wine</i>, <i>some</i> is an amount term. (Section 4.1.3)" ;
    dcterms:isPartOf "4.1.3" ;
    compcon:subtypeOf compcon:cxn_quantifier-term .

compcon:cxn_anaphoric-article a compcon:cxn ;
    rdfs:label "anaphoric article" ;
    cx:hasExample compcon:anaphoric-article-example-0 ;
    dc:description "an <a>article</a> that is used for a <a>semi-active referent</a>. <i>Example</i>: in the Nguna text passage <i>e tape araa ni nararo...go nararo wanogoe...</i> <q>[he] took the branch of the <i>nararo</i> tree...and the aforementioned <i>nararo</i> tree...</q>, <i>wanogoe</i> <q>the aforementioned</q> is an anaphoric article. An anaphoric article is always used when the referent is semi-active because it has been previously referred to in the discourse. (Section 3.3.1)" ;
    dcterms:isPartOf "3.3.1" ;
    skos:altLabel "anaphoric (definite) article" ;
    compcon:subtypeOf compcon:cxn_attributive-phrase,
        compcon:cxn_definite-article .

compcon:cxn_animal-noun a compcon:cxn ;
    rdfs:label "animal noun" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_common-noun .

compcon:cxn_animal-plant-product-noun a compcon:cxn ;
    rdfs:label "animal--plant product noun" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_common-noun .

compcon:cxn_antonym a compcon:cxn ;
    rdfs:label "antonym" ;
    cx:hasExample compcon:antonym-example-0,
        compcon:antonym-example-1 ;
    dc:description "opposing values on a <a sem:gradient>gradient scale</a>. <i>Example</i>: <i>tall</i> and <i>short</i> are antonyms on the gradient scale of height. (Section 4.1.2)" ;
    dcterms:isPartOf "4.1.2" ;
    compcon:subtypeOf compcon:cxn_adjective .

compcon:cxn_apodosis-clause a compcon:cxn ;
    rdfs:label "apodosis clause" ;
    cx:hasExample compcon:apodosis-clause-example-0 ;
    dc:description "the <a>clause</a> expressing the causally consequent <a>proposition</a> in a <a>causal</a>, <a>conditional</a>, <a>concessive</a>, <a>concessive conditional</a>, or <a>comparative conditional construction</a>. <i>Example</i>: in <i>If you press this button, the door will open</i>, <i>the door will open</i> is the apodosis; <i>If you press this button</i> is the <a cxn:protasis-clause>protasis</a>. (Section 17.3.1)" ;
    dcterms:isPartOf "17.3.1" ;
    compcon:subtypeOf compcon:cxn_construction .

compcon:cxn_application-verb a compcon:cxn ;
    rdfs:label "application verb" ;
    dc:description "the <a>verb</a> expressing an <a>application event</a>. <i>Examples</i>: smearing (2-dimensional) and loading (3-dimensional) are application events. (Section 7.3.2)" ;
    dcterms:isPartOf "7.3.2" ;
    skos:altLabel "putting verb" ;
    compcon:subtypeOf compcon:cxn_verb .

compcon:cxn_apposition a compcon:cxn ;
    rdfs:label "apposition" ;
    cx:hasExample compcon:apposition-example-0 ;
    dc:description "a <a>strategy</a> for <a sem:object-concept>object</a> <a>reference</a> in which two or more separate <a>referring phrases</a> juxtaposed to each other refer to the same <a>referent</a>. <i>Example</i>: in <i>my brother, the geophysicist</i>, two separate referring phrases, <i>my brother</i> and <i>the geophysicist</i>, are juxtaposed and refer to the same individual. (Section 5.4)" ;
    dcterms:isPartOf "5.4" ;
    compcon:subtypeOf compcon:cxn_referring-phrase .

compcon:cxn_apprehensional-construction a compcon:cxn ;
    rdfs:label "apprehensional construction" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_complex-sentence .

compcon:cxn_auxiliary a compcon:cxn ;
    rdfs:label "auxiliary" ;
    cx:hasExample compcon:auxiliary-example-0 ;
    dc:description "the <a>element</a> expressing <a>TAMP</a> <a>meaning</a> in an <a>auxiliary construction</a>. <i>Example</i>: in <i>The cats have eaten, have</i> is the auxiliary in the auxiliary construction <i>have eaten</i>. (Section 13.4)" ;
    dcterms:isPartOf "13.4" ;
    compcon:subtypeOf compcon:cxn_complex-predicate-part .

compcon:cxn_bodily-motion-verb a compcon:cxn ;
    rdfs:label "bodily motion verb" ;
    cx:hasExample compcon:bodily-motion-verb-example-0 ;
    dc:description "the <a>verb</a> expressing a <a>bodily motion event</a>. <i>Example</i>: <i>stretch out (oneself)</i> expresses a bodily motion event. (Section 7.2)" ;
    dcterms:isPartOf "7.2" ;
    compcon:subtypeOf compcon:cxn_verb .

compcon:cxn_bodily-predicate a compcon:cxn ;
    rdfs:label "bodily predicate" ;
    cx:hasExample compcon:bodily-predicate-example-0 ;
    dc:description "the <a>predicates</a> that express events in the <a>bodily action</a> event class. <i>Example</i>: coughing is a bodily action, and <i>cough</i> is a bodily action predicate. (Section 6.3.3)" ;
    dcterms:isPartOf "6.3.3" ;
    compcon:subtypeOf compcon:cxn_experiential-verb .

compcon:cxn_body-care-verb a compcon:cxn ;
    rdfs:label "body care verb" ;
    cx:hasExample compcon:body-care-verb-example-0,
        compcon:body-care-verb-example-1 ;
    dc:description "the <a>verb</a> expressing a <a>body care event</a>. <i>Examples</i>: <i>shave</i> and <i>wash (oneself)</i> express body care events. (Section 7.2)" ;
    dcterms:isPartOf "7.2" ;
    compcon:subtypeOf compcon:cxn_verb .

compcon:cxn_body-part-noun a compcon:cxn ;
    rdfs:label "body part noun" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_common-noun .

compcon:cxn_body-position-predicate a compcon:cxn ;
    rdfs:label "body position predicate" ;
    cx:hasExample compcon:body-position-predicate-example-0 ;
    dc:description "the <a>predicates</a> that express events in the <a>body position event</a> class. <i>Example</i>: standing is a body position event, and <i>stand</i> is a body position predicate. (Section 6.3.3)" ;
    dcterms:isPartOf "6.3.3" ;
    skos:altLabel "locative stative predicate",
        "maintain position predicate",
        "posture predicate" ;
    compcon:subtypeOf compcon:cxn_predicate .

compcon:cxn_causal-construction a compcon:cxn ;
    rdfs:label "causal construction" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_complex-sentence .

compcon:cxn_causee-phrase a compcon:cxn ;
    rdfs:label "causee phrase" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_nonsubject-argument-phrase .

compcon:cxn_causer-phrase a compcon:cxn ;
    rdfs:label "causer phrase" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_subject-phrase .

compcon:cxn_change-in-position-verb a compcon:cxn ;
    rdfs:label "change in position verb" ;
    cx:hasExample compcon:change-in-position-verb-example-0,
        compcon:change-in-position-verb-example-1 ;
    dc:description "the <a>verb</a> expressing a <a>change in body position event</a>. <i>Example</i>: <i>sit</i> and <i>lean</i> express change in body posture events. (Section 7.2)" ;
    dcterms:isPartOf "7.2" ;
    skos:altLabel "assume position verb",
        "change in (body) position verb",
        "change in posture verb" ;
    compcon:subtypeOf compcon:cxn_verb .

compcon:cxn_cognition-verb a compcon:cxn ;
    rdfs:label "cognition verb" ;
    cx:hasExample compcon:cognition-verb-example-0 ;
    dc:description "a <a>verb</a> that expresses a <a>cognition event</a>. <i>Example</i>: <i>Tim thought about the war</i> is an example of a cognition event, and <i>think (about)</i> is the cognition verb. (Section 7.4)" ;
    dcterms:isPartOf "7.4" ;
    compcon:subtypeOf compcon:cxn_experiential-verb .

compcon:cxn_color-term a compcon:cxn ;
    rdfs:label "color term" ;
    cx:hasExample compcon:color-term-example-0,
        compcon:color-term-example-1,
        compcon:color-term-example-2 ;
    dc:description "a <a>modifier</a> expressing a color concept. <i>Examples</i>: <i>red</i>, <i>black</i>, and <i>green</i> are English color terms. (Section 4.1.2)" ;
    dcterms:isPartOf "4.1.2" ;
    compcon:subtypeOf compcon:cxn_adjective .

compcon:cxn_combining-verb a compcon:cxn ;
    rdfs:label "combining verb" ;
    dc:description "the <a>verb</a> expressing a <a>combining event</a>. <i>Example</i>: blending is a combining event. (Section 7.3.2)" ;
    dcterms:isPartOf "7.3.2" ;
    compcon:subtypeOf compcon:cxn_verb .

compcon:cxn_commentative-construction a compcon:cxn ;
    rdfs:label "commentative construction" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_evaluative-construction .

compcon:cxn_comparative-conditional-construction a compcon:cxn ;
    rdfs:label "comparative conditional construction" ;
    cx:hasExample compcon:comparative-conditional-construction-example-0 ;
    dc:description "a construction that expresses a <a>comparative conditional relation</a>. <i>Example</i>: <i>The longer that Bill had to wait, the angrier he got</i> is an instance of the comparative conditional relation and construction: a degree of length of time that Bill had to wait can (in a <a>generic conditional</a>) or does (in an ordinary, specific <a>conditional</a>) cause the occurrence of the corresponding degree of Bill's anger. (Section 17.4.1)" ;
    dcterms:isPartOf "17.4.1" ;
    compcon:subtypeOf compcon:cxn_complex-sentence .

compcon:cxn_comparative-form a compcon:cxn ;
    rdfs:label "comparative form" ;
    cx:hasExample compcon:comparative-form-example-0 ;
    dc:description "a higher value on a <a>property</a> scale than the comparable value for another object. <i>Example</i>: in <i>more intelligent</i>, <i>more</i> indicates a value higher on the intelligence scale than the comparable value for the person to whom the current referent is being compared. (Section 4.1.2)" ;
    dcterms:isPartOf "4.1.2" ;
    compcon:subtypeOf compcon:cxn_admodifier .

compcon:cxn_comparative-pronoun a compcon:cxn ;
    rdfs:label "comparative pronoun" ;
    cx:hasExample compcon:comparative-pronoun-example-0 ;
    dc:description "a <a>pronoun</a> expressing a <a>comparative referent</a>. <i>Example</i>: in <i>The boy runs as fast as anyone in his class</i>, <i>anyone</i> is a comparative pronoun expressing a hypothetical referent selected from the class representing the standard to which the boy's running is being compared. (Section 3.5)" ;
    dcterms:isPartOf "3.5" ;
    compcon:subtypeOf compcon:cxn_semantically-nonspecific-pronoun .

compcon:cxn_complementary a compcon:cxn ;
    rdfs:label "complementary" ;
    cx:hasExample compcon:complementary-example-0,
        compcon:complementary-example-1 ;
    dc:description "opposing values on a scale where there is no gradience, only <a>categorical</a> values (either the object has the value or it doesn't). <i>Example</i>: <i>alive/dead</i> are complementaries in that one is either alive or not. (Section 4.1.2)" ;
    dcterms:isPartOf "4.1.2" ;
    compcon:subtypeOf compcon:cxn_adjective .

compcon:cxn_conditional-pronoun a compcon:cxn ;
    rdfs:label "conditional pronoun" ;
    cx:hasExample compcon:conditional-pronoun-example-0 ;
    dc:description "a conditional pronoun that expresses a <a>conditional referent</a>. <i>Example</i>: in <i>If you hear <b>anything</b>, tell me</i>, <i>anything</i> is a conditional pronoun expressing a referent that is found only in the hypothetical world introduced by the protasis of the conditional construction. (Section 3.5)" ;
    dcterms:isPartOf "3.5" ;
    compcon:subtypeOf compcon:cxn_semantically-nonspecific-pronoun .

compcon:cxn_consecutive-coordination a compcon:cxn ;
    rdfs:label "consecutive coordination" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_conjunctive-coordination .

compcon:cxn_contact-by-impact-verb a compcon:cxn ;
    rdfs:label "contact by impact verb" ;
    cx:hasExample compcon:contact-by-impact-verb-example-0 ;
    dc:description "the <a>verb</a> expressing a <a>contact by impact event</a>. <i>Example</i>: hitting is a contact by impact event, and <i>hit</i> is a contact by impact verb. (Section 7.3.2)" ;
    dcterms:isPartOf "7.3.2" ;
    compcon:subtypeOf compcon:cxn_verb .

compcon:cxn_container-term a compcon:cxn ;
    rdfs:label "container term" ;
    cx:hasExample compcon:container-term-example-0 ;
    dc:description "a <a>mensural term</a> that selects an amount of a <a>referent</a> according to the container it is found in. <i>Example</i>: in <i>a bottle of wine, bottle</i> functions as a container term for <i>wine</i>. (Section 4.1.3)" ;
    dcterms:isPartOf "4.1.3" ;
    compcon:subtypeOf compcon:cxn_mensural-term .

compcon:cxn_damage-verb a compcon:cxn ;
    rdfs:label "damage verb" ;
    cx:hasExample compcon:damage-verb-example-0 ;
    dc:description "the <a>verb</a> expressing a <a>damage event</a>. <i>Example</i>: scratching something is a damage event, and <i>scratch</i> is a damage verb. (Section 7.3.2)" ;
    dcterms:isPartOf "7.3.2" ;
    compcon:subtypeOf compcon:cxn_verb .

compcon:cxn_demonstrative-attributive a compcon:cxn ;
    rdfs:label "demonstrative attributive" ;
    cx:hasExample compcon:demonstrative-attributive-example-0 ;
    dc:description "a <a>deictic</a> <a>contextual</a> expression that combines with a <a>common noun</a> to form a <a>referring phrase</a>. <i>Example</i>: in <i>This machine drives me crazy!</i>, <i>This</i> is a demonstrative attributive combined with the common noun <i>machine</i>. (Sections <b>3.1.3</b>, 3.2)" ;
    dcterms:isPartOf "3.1.3",
        "3.2" ;
    compcon:subtypeOf compcon:cxn_attributive-phrase,
        compcon:cxn_determiner .

compcon:cxn_demonstrative-pronoun a compcon:cxn ;
    rdfs:label "demonstrative pronoun" ;
    cx:hasExample compcon:demonstrative-pronoun-example-0 ;
    dc:description "a <a>deictic</a> <a>contextual</a> expression that stands alone as a <a>referring phrase</a>. <i>Example</i>: in <i>This is a collared lizard</i>, <i>This</i> is a demonstrative pronoun. (Sections 3.1.1, 3.1.3, 3.2)" ;
    dcterms:isPartOf "3.1.1",
        "3.1.3",
        "3.2" ;
    compcon:subtypeOf compcon:cxn_pronoun .

compcon:cxn_desiderative-construction a compcon:cxn ;
    rdfs:label "desiderative construction" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_complement-clause-construction .

compcon:cxn_dimension-term a compcon:cxn ;
    rdfs:label "dimension term" ;
    cx:hasExample compcon:dimension-term-example-0,
        compcon:dimension-term-example-1,
        compcon:dimension-term-example-2 ;
    dc:description "a <a>modifier</a> expressing a concept of extent on a spatial dimension. <i>Examples</i>: <i>tall</i>, <i>small</i>, and <i>thin</i> are English dimension terms. (Section 4.1.2)" ;
    dcterms:isPartOf "4.1.2" ;
    compcon:subtypeOf compcon:cxn_adjective .

compcon:cxn_direct-negation-pronoun a compcon:cxn ;
    rdfs:label "direct negation pronoun" ;
    cx:hasExample compcon:direct-negation-pronoun-example-0 ;
    dc:description "the <a>pronoun</a> that expresses a <a>direct negation referent</a>. <i>Example</i>: in <i>I noticed nothing</i>, <i>nothing</i> is a direct negation pronoun expressing a referent found only in the negative alternative world to the real world. (Section 3.5)" ;
    dcterms:isPartOf "3.5" ;
    compcon:subtypeOf compcon:cxn_semantically-nonspecific-pronoun .

compcon:cxn_distributive-quantifier-term a compcon:cxn ;
    rdfs:label "distributive quantifier term" ;
    dc:description "a form that specifies the members of the set but treats them individually (that is, the <a>predicate</a> applies to the whole set by virtue of applying to the individual members of the set). Example: in <i>Every dog has fleas, every</i> is a distributive quantifier. (Section 4.1.3)" ;
    dcterms:isPartOf "4.1.3" ;
    compcon:subtypeOf compcon:cxn_quantifier-term .

compcon:cxn_ditransitive-a-phrase a compcon:cxn ;
    rdfs:label "ditransitive A phrase" ;
    dc:description "" ;
    skos:altLabel "ditransitive A argument phrase" ;
    compcon:subtypeOf compcon:cxn_subject-phrase .

compcon:cxn_downtoner a compcon:cxn ;
    rdfs:label "downtoner" ;
    cx:hasExample compcon:downtoner-example-0 ;
    dc:description "a lower than normal value on a <a>property</a> scale. <i>Example</i>: in <i>somewhat long, somewhat</i> indicates a value shorter than is normal for length. (Section 4.1.2)" ;
    dcterms:isPartOf "4.1.2" ;
    compcon:subtypeOf compcon:cxn_admodifier .

compcon:cxn_dynamic-predicate-part a compcon:cxn ;
    rdfs:label "dynamic predicate part" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_eventive-predicate-part .

compcon:cxn_emotion-verb a compcon:cxn ;
    rdfs:label "emotion verb" ;
    cx:hasExample compcon:emotion-verb-example-0 ;
    dc:description "a <a>verb</a> that expresses an <a>emotion event</a>. <i>Example</i>: <i>He fears dogs</i> is an example of an emotion event, and <i>fear</i> is the emotion verb. (Section 7.4)" ;
    dcterms:isPartOf "7.4" ;
    compcon:subtypeOf compcon:cxn_experiential-verb .

compcon:cxn_equational-construction a compcon:cxn ;
    rdfs:label "equational construction" ;
    cx:hasExample compcon:equational-construction-example-0 ;
    dc:description "the <a>construction</a> that expresses an <a inf:equational>equational</a> information packaging. <i>Example</i>: in <i>The Morning Star is the Evening Star</i>, it is asserted that two celestial objects that were once thought to be distinct objects (and given distinct names) are one and the same, namely the planet Venus. (Section 10.1.2)" ;
    dcterms:isPartOf "10.1.2" ;
    compcon:subtypeOf compcon:cxn_identificational-construction .

compcon:cxn_event-central-construction a compcon:cxn ;
    rdfs:label "event-central construction" ;
    cx:hasExample compcon:event-central-construction-example-0 ;
    dc:description "a <a>construction</a> that expresses an <a inf:event-central>event-central</a> information packaging. <i>Example</i>: in <i>The PHONE's ringing</i>, the most important new information is the ringing of the phone, not the existence of the phone. (Sections 10.1.2, <b>11.3.1</b>)" ;
    dcterms:isPartOf "10.1.2",
        "11.3.1" ;
    compcon:subtypeOf compcon:cxn_thetic-construction .

compcon:cxn_exclamative-construction a compcon:cxn ;
    rdfs:label "exclamative construction" ;
    cx:hasExample compcon:exclamative-construction-example-0 ;
    dc:description "a <a>construction</a> that expresses an <a inf:exclamative>exclamative</a> speech act. <i>Example</i>: <i>What a beautiful house!</i> is an instance of an English exclamative construction. (Sections 12.1, 12.5)" ;
    dcterms:isPartOf "12.1",
        "12.5" ;
    compcon:subtypeOf compcon:cxn_speech-act-construction .

compcon:cxn_exclusive-disjunctive-coordination a compcon:cxn ;
    rdfs:label "exclusive disjunctive coordination" ;
    cx:hasExample compcon:exclusive-disjunctive-coordination-example-0 ;
    dc:description "a type of <a>disjunctive coordination</a> in which the alternatives expressed cannot be combined. <i>Example</i>: <i>You can have the soup or the salad</i> is an instance of exclusive disjunctive coordination in the situation where you cannot have both. Exclusive disjunctive coordination can be an instance of <a>exhaustive list coordination</a>. (Section 15.2.1)" ;
    dcterms:isPartOf "15.2.1" ;
    compcon:subtypeOf compcon:cxn_disjunctive-coordination,
        compcon:cxn_exhaustive-list-coordination .

compcon:cxn_exclusive-pronoun a compcon:cxn ;
    rdfs:label "exclusive pronoun" ;
    cx:hasExample compcon:exclusive-pronoun-example-0 ;
    dc:description "a <a>first person pronoun</a> that refers to a group including the speaker but excluding the addressee. <i>Example</i>: Kosraean <i>kitɛl</i> is a first person exclusive pronoun referring to a group that includes the speaker but not the addressee. (Section 3.1.1)" ;
    dcterms:isPartOf "3.1.1" ;
    compcon:subtypeOf compcon:cxn_first-person-pronoun .

compcon:cxn_experiencer-phrase a compcon:cxn ;
    rdfs:label "experiencer-phrase" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_core-argument-phrase .

compcon:cxn_fearing-construction a compcon:cxn ;
    rdfs:label "fearing construction" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_evaluative-construction .

compcon:cxn_figure-phrase a compcon:cxn ;
    rdfs:label "figure phrase" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_subject-phrase .

compcon:cxn_form-term a compcon:cxn ;
    rdfs:label "form term" ;
    cx:hasExample compcon:form-term-example-0 ;
    dc:description "a <a>mensural term</a> that selects an amount of a <a>referent</a> according to the shape defined by the amount. <i>Example</i>: in <i>two piles of sand, pile(s)</i> is a form term. (Section 4.1.3)" ;
    dcterms:isPartOf "4.1.3" ;
    compcon:subtypeOf compcon:cxn_mensural-term .

compcon:cxn_free-choice-pronoun a compcon:cxn ;
    rdfs:label "free choice pronoun" ;
    cx:hasExample compcon:free-choice-pronoun-example-0 ;
    dc:description "a <a>pronoun</a> that expresses a <a>free choice referent</a>. <i>Example</i>: in <i>After the fall of the Wall, East Germans were free to travel anywhere</i>, <i>anywhere</i> is a free choice pronoun expressing a referent -- a place -- toward which the agent in the clause, the East Germans, is free to choose to travel. (Section 3.5)" ;
    dcterms:isPartOf "3.5" ;
    compcon:subtypeOf compcon:cxn_semantically-nonspecific-pronoun .

compcon:cxn_gender-term a compcon:cxn ;
    rdfs:label "gender term" ;
    cx:hasExample compcon:gender-term-example-0,
        compcon:gender-term-example-1 ;
    dc:description "a <a>modifier</a> expressing concepts based on sex. <i>Example</i>: <i>male</i> and <i>female</i> are English gender terms. (Section 4.1.2)" ;
    dcterms:isPartOf "4.1.2" ;
    compcon:subtypeOf compcon:cxn_adjective .

compcon:cxn_generic-conditional-construction a compcon:cxn ;
    rdfs:label "generic conditional construction" ;
    cx:hasExample compcon:generic-conditional-construction-example-0 ;
    dc:description "a construction that expresses a <a>generic conditional relation</a>. <i>Example</i>: <i>If/When/Whenever a dog starts barking, I run away</i> is an instance of a generic conditional relation and construction -- it doesn't describe a specific instance of a dog barking causing me to run away; instead, it describes a general or habitual pattern of this causal sequence of events. (Section 17.3.1)" ;
    dcterms:isPartOf "17.3.1" ;
    compcon:subtypeOf compcon:cxn_conditional-construction .

compcon:cxn_ground-phrase a compcon:cxn ;
    rdfs:label "ground phrase" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_nonsubject-argument-phrase .

compcon:cxn_group-term a compcon:cxn ;
    rdfs:label "group term" ;
    cx:hasExample compcon:group-term-example-0 ;
    dc:description "a <a>mensural term</a> that selects an amount of a set of <a>referents</a> according to some delimiting function. <i>Example</i>: in <i>a herd of cattle, herd</i> is a group term. (Section 4.1.3)" ;
    dcterms:isPartOf "4.1.3" ;
    compcon:subtypeOf compcon:cxn_mensural-term .

compcon:cxn_hoping-construction a compcon:cxn ;
    rdfs:label "hoping construction" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_evaluative-construction .

compcon:cxn_human-noun a compcon:cxn ;
    rdfs:label "human noun" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_common-noun .

compcon:cxn_human-propensity-term a compcon:cxn ;
    rdfs:label "human propensity term" ;
    cx:hasExample compcon:human-propensity-term-example-0,
        compcon:human-propensity-term-example-1,
        compcon:human-propensity-term-example-2 ;
    dc:description "a <a>modifier</a> expressing a concept of a type of behavior that a person has a propensity to exhibit. <i>Examples</i>: <i>smart</i>, <i>rude</i>, and <i>nice</i> are English human propensity terms. (Section 4.1.2)" ;
    dcterms:isPartOf "4.1.2" ;
    skos:altLabel "disposition" ;
    compcon:subtypeOf compcon:cxn_adjective .

compcon:cxn_human-proper-noun a compcon:cxn ;
    rdfs:label "human proper noun" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_proper-noun .

compcon:cxn_inclusive-disjunctive-coordination a compcon:cxn ;
    rdfs:label "inclusive disjunctive coordination" ;
    cx:hasExample compcon:inclusive-disjunctive-coordination-example-0 ;
    dc:description "a type of <a>disjunctive coordination</a> in which any <a>entity</a> enumerated or any combination of the entities enumerated is intended. The simplest case is coordination of two entities where one, the other, or both are intended. <i>Example</i>: <i>Applicants must be a college graduate or have fluency in German</i> is an example of inclusive disjunctive coordination under the assumption that being both a college graduate and fluent in German does not disqualify you from applying. Inclusive disjunctive coordination can be an instance of <a>non-exhaustive list coordination</a>. (Section 15.2.1)" ;
    dcterms:isPartOf "15.2.1" ;
    compcon:subtypeOf compcon:cxn_disjunctive-coordination,
        compcon:cxn_non-exhaustive-list-coordination .

compcon:cxn_indirect-negation-pronoun a compcon:cxn ;
    rdfs:label "indirect negation pronoun" ;
    cx:hasExample compcon:indirect-negation-pronoun-example-0 ;
    dc:description "a <a>pronoun</a> that expresses an <a>indirect negation referent</a>. <i>Example</i>: in <i>I don't think that anybody has seen it</i>, <i>anybody</i> is an indirect negation pronoun expressing a referent that is found only in the negated <dq>world</dq> of the speaker's beliefs. (Section 3.5)" ;
    dcterms:isPartOf "3.5" ;
    compcon:subtypeOf compcon:cxn_semantically-nonspecific-pronoun .

compcon:cxn_information-question-construction a compcon:cxn ;
    rdfs:label "information question construction" ;
    cx:hasExample compcon:information-question-construction-example-0 ;
    dc:description "the <a>construction</a> expressing an <a inf:information-question>information question</a> <a>function</a>. <i>Example</i>: <i>Who is coming?</i> is an instance of the English information question construction, expecting an answer identifying the person(s) who is/are coming. Information questions, unlike <a cxn:polarity-question-construction>polarity questions</a>, contain an <a>interrogative pronoun</a>. (Section 12.3.1)" ;
    dcterms:isPartOf "12.3.1" ;
    compcon:subtypeOf compcon:cxn_interrogative-construction .

compcon:cxn_ingestion-verb a compcon:cxn ;
    rdfs:label "ingestion verb" ;
    cx:hasExample compcon:ingestion-verb-example-0 ;
    dc:description "a <a>verb</a> that expresses an <a>ingestion event</a>. <i>Example</i>: <i>Elena ate a lot of veggie chips</i> is an instance of an ingestion event, and <i>eat</i> is an ingestion verb. (Section 7.4)" ;
    dcterms:isPartOf "7.4" ;
    compcon:subtypeOf compcon:cxn_verb .

compcon:cxn_intensifier a compcon:cxn ;
    rdfs:label "intensifier" ;
    cx:hasExample compcon:intensifier-example-0 ;
    dc:description "a higher than normal value on a <a>property</a> scale. <i>Example</i>: in <i>very long, very</i> indicates a value longer than normal. (Section 4.1.2)" ;
    dcterms:isPartOf "4.1.2" ;
    compcon:subtypeOf compcon:cxn_admodifier .

compcon:cxn_interaction-verb a compcon:cxn ;
    rdfs:label "interaction verb" ;
    dc:description "a <a>verb</a> that expresses an <a>interaction event</a>. (Section 7.3)" ;
    dcterms:isPartOf "7.3" ;
    compcon:subtypeOf compcon:cxn_verb .

compcon:cxn_interrogative-pronoun a compcon:cxn ;
    rdfs:label "interrogative pronoun" ;
    cx:hasExample compcon:interrogative-pronoun-example-0,
        compcon:interrogative-pronoun-example-1 ;
    dc:description "<a>pronoun</a> that is used to ask an addressee about the identity of a <a>referent</a> whose identity is unknown to the speaker. <i>Example</i>: in <i>Who ate my cookie?</i>, <i>who</i> is an interrogative pronoun; the identity of the cookie eater is unknown to the speaker, who is asking the hearer to provide the referent's identity. The interrogative form may also be a <a>modifier</a> rather than a pronoun: in <i>Which book is required reading?, which</i> is an interrogative modifier denoting the missing information about the book that is required reading. (Sections 3.4.2, 12.3.1)" ;
    dcterms:isPartOf "12.3.1",
        "3.4.2" ;
    compcon:subtypeOf compcon:cxn_pronoun .

compcon:cxn_irrealis-pronoun a compcon:cxn ;
    rdfs:label "irrealis pronoun" ;
    cx:hasExample compcon:irrealis-pronoun-example-0 ;
    dc:description "a <a>pronoun</a> that expresses an <a>irrealis referent</a>. <i>Example</i>: in <i>Visit me sometime</i>, <i>sometime</i> is an irrealis <a>pronoun</a> expressing an irrealis referent -- a time only found in the hoped-for mental space of the speaker's offer. (Section 3.5)" ;
    dcterms:isPartOf "3.5" ;
    compcon:subtypeOf compcon:cxn_semantically-nonspecific-pronoun .

compcon:cxn_killing-injuring-verb a compcon:cxn ;
    rdfs:label "killing/injuring verb" ;
    cx:hasExample compcon:killing-injuring-verb-example-0 ;
    dc:description "the <a>verb</a> expressing such a <a>killing/injuring event</a>. <i>Example</i>: stabbing is a killing/injuring event, and <i>stab</i> is a killing/injuring verb. (Section 7.3.2)" ;
    dcterms:isPartOf "7.3.2" ;
    skos:altLabel "injuring verb",
        "killing verb" ;
    compcon:subtypeOf compcon:cxn_verb .

compcon:cxn_knowledge-construction a compcon:cxn ;
    rdfs:label "knowledge construction" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_propositional-attitude-construction .

compcon:cxn_let-alone-construction a compcon:cxn ;
    rdfs:label "let alone construction" ;
    cx:hasExample compcon:let-alone-construction-example-0 ;
    dc:description "a <a>negative</a> sentence that expresses two <a>propositions</a> at different degrees of <dq>strength</dq> in a <a>scalar model</a>; the speaker asserts the most informative of the two propositions, although the less informative proposition is sufficient in the communicative context. <i>Example</i>: in response to the question <i>Did the kids get their breakfast on time this morning?</i>, the sentence <i>I barely got up in time to <sc>eat lunch</sc>, let alone <sc>cook breakfast</sc></i> is an instance of the let alone construction: not getting up in time to cook breakfast would answer the question, but not getting up in time to eat lunch is still more informative (indicating just how long the speaker remained in bed). (Section 17.4.2)" ;
    dcterms:isPartOf "17.4.2" ;
    compcon:subtypeOf compcon:cxn_complex-sentence .

compcon:cxn_locative-modification-construction a compcon:cxn ;
    rdfs:label "locative modification construction" ;
    cx:hasExample compcon:locative-modification-construction-example-0 ;
    dc:description "a <a>referring phrase</a> that expresses a <a>figure--ground spatial relation</a> between the ground <a sem:object-concept>object</a> functioning as the <a>modifier</a> and the figure functioning as the modified <a>referent</a>. <i>Example</i>: <i>the bicycle in the garage</i> is a locative modification construction. (Section 4.1.4)" ;
    dcterms:isPartOf "4.1.4" ;
    compcon:subtypeOf compcon:cxn_nominal-modification-construction .

compcon:cxn_locative-phrase a compcon:cxn ;
    rdfs:label "locative phrase" ;
    cx:hasExample compcon:locative-phrase-example-0 ;
    dc:description "an <a>attributive phrase</a> whose <a>head</a> denotes an <a>object concept</a> <a inf:modification>modifying</a> a <a>referent expression</a> via a <a>figure--ground spatial relation</a>. <i>Example</i>: in <i>the bicycle in the garage, in the garage</i> is a locative attributive phrase: its head denotes an object concept, the garage, and the garage is the ground in a figure--ground spatial relation with respect to the bicycle. (Section 4.1.4)" ;
    dcterms:isPartOf "4.1.4" ;
    skos:altLabel "locative (attributive) phrase" ;
    compcon:subtypeOf compcon:cxn_nominal-attributive-phrase .

compcon:cxn_manipulative-construction a compcon:cxn ;
    rdfs:label "manipulative construction" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_complement-clause-construction .

compcon:cxn_manner-of-motion-verb a compcon:cxn ;
    rdfs:label "manner of motion verb" ;
    cx:hasExample compcon:manner-of-motion-verb-example-0 ;
    dc:description "the <a>verb</a> expressing a <a>manner of motion event</a>. <i>Example</i>: in <i>Sam strode into the room, stride</i> is a manner of motion verb expressing a manner of motion event. (Sections 7.3.1, 14.2)" ;
    dcterms:isPartOf "14.2",
        "7.3.1" ;
    compcon:subtypeOf compcon:cxn_manner-verb,
        compcon:cxn_verb .

compcon:cxn_material-term a compcon:cxn ;
    rdfs:label "material term" ;
    cx:hasExample compcon:material-term-example-0,
        compcon:material-term-example-1 ;
    dc:description "a <a>modifier</a> expressing a concept describing the material or substance out of which an object is made. <i>Example</i>: <i>wood(en)</i> and <i>metal</i> are English material terms. (Section 4.1.2)" ;
    dcterms:isPartOf "4.1.2" ;
    skos:altLabel "substance term" ;
    compcon:subtypeOf compcon:cxn_adjective .

compcon:cxn_means-construction a compcon:cxn ;
    rdfs:label "means construction" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_complex-sentence .

compcon:cxn_measure-term a compcon:cxn ;
    rdfs:label "measure term" ;
    cx:hasExample compcon:measure-term-example-0 ;
    dc:description "a <a>mensural term</a> that selects a measured amount of an uncountable <a>referent</a>. <i>Example</i>: in <i>six gallons of wine, gallon(s)</i> is a measure term. (Section 4.1.3)" ;
    dcterms:isPartOf "4.1.3" ;
    compcon:subtypeOf compcon:cxn_mensural-term .

compcon:cxn_measurement a compcon:cxn ;
    rdfs:label "measurement" ;
    cx:hasExample compcon:measurement-example-0 ;
    dc:description "a calibratable value on a <a>property</a> scale. <i>Example</i>: in <i>three feet long, three feet</i> measures the value of length for the object in question. (Section 4.1.2)" ;
    dcterms:isPartOf "4.1.2" ;
    compcon:subtypeOf compcon:cxn_admodifier .

compcon:cxn_mensural-attributive-phrase a compcon:cxn ;
    rdfs:label "mensural attributive phrase" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_attributive-phrase .

compcon:cxn_motion-verb a compcon:cxn ;
    rdfs:label "motion verb" ;
    cx:hasExample compcon:motion-verb-example-0,
        compcon:motion-verb-example-1 ;
    dc:description "the <a>verb</a> expressing a <a>motion event</a>. <i>Examples</i>: <i>fly</i> and <i>go</i> express motion events. (Sections 7.2, 7.3.1, 14.4)" ;
    dcterms:isPartOf "14.4",
        "7.2",
        "7.3.1" ;
    compcon:subtypeOf compcon:cxn_verb .

compcon:cxn_natural-object-noun a compcon:cxn ;
    rdfs:label "natural object noun" ;
    dc:description "" ;
    skos:altLabel "substance noun" ;
    compcon:subtypeOf compcon:cxn_common-noun .

compcon:cxn_negative-circumstantial-construction a compcon:cxn ;
    rdfs:label "negative circumstantial construction" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_complex-sentence .

compcon:cxn_nonanaphoric-definite-article a compcon:cxn ;
    rdfs:label "nonanaphoric definite article" ;
    dc:description "an <a>article</a> that is used for an <a>inactive</a> or <a>inferrable referent</a>. (Section 3.3.1)" ;
    dcterms:isPartOf "3.3.1" ;
    skos:altLabel "nonanaphoric (definite) article" ;
    compcon:subtypeOf compcon:cxn_attributive-phrase,
        compcon:cxn_definite-article .

compcon:cxn_nonclausal-coordination a compcon:cxn ;
    rdfs:label "nonclausal coordination" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_coordinate-construction .

compcon:cxn_noun-complement-referring-phrase a compcon:cxn ;
    rdfs:label "noun complement referring phrase" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_referring-phrase .

compcon:cxn_numeral-attributive-phrase a compcon:cxn ;
    rdfs:label "numeral attributive phrase" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_attributive-phrase .

compcon:cxn_ordinal-numeral-term a compcon:cxn ;
    rdfs:label "ordinal numeral term" ;
    cx:hasExample compcon:ordinal-numeral-term-example-0 ;
    dc:description "a <a>set-member term</a> for a member in an ordered set, based on the precise position of the member in the ordering of the set. <i>Example</i>: in <i>the second tree</i>, <i>second</i> is an ordinal numeral. (Section 4.1.3)" ;
    dcterms:isPartOf "4.1.3" ;
    compcon:subtypeOf compcon:cxn_set-member-term .

compcon:cxn_p-phrase a compcon:cxn ;
    rdfs:label "P phrase" ;
    dc:description "" ;
    skos:altLabel "P argument phrase" ;
    compcon:subtypeOf compcon:cxn_object-phrase .

compcon:cxn_passive-inverse-a-phrase a compcon:cxn ;
    rdfs:label "passive-inverse A phrase" ;
    dc:description "" ;
    skos:altLabel "passive-inverse A argument phrase" ;
    compcon:subtypeOf compcon:cxn_nonsubject-argument-phrase .

compcon:cxn_passive-inverse-p-phrase a compcon:cxn ;
    rdfs:label "passive-inverse P phrase" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_core-argument-phrase .

compcon:cxn_path-verb a compcon:cxn ;
    rdfs:label "path verb" ;
    cx:hasExample compcon:path-verb-example-0 ;
    dc:description "the <a>verb</a> expressing such a <a>path of motion event</a>. <i>Example</i>: in <i>The guests entered the reception hall, enter</i> is a path of motion verb expressing a path of motion event. (Sections 7.3.1, 14.2)" ;
    dcterms:isPartOf "14.2",
        "7.3.1" ;
    skos:altLabel "path (of motion) verb" ;
    compcon:subtypeOf compcon:cxn_motion-verb,
        compcon:cxn_result-verb .

compcon:cxn_perception-complement-clause-construction a compcon:cxn ;
    rdfs:label "perception complement clause construction" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_complement-clause-construction .

compcon:cxn_physical-property-term a compcon:cxn ;
    rdfs:label "physical property term" ;
    cx:hasExample compcon:physical-property-term-example-0,
        compcon:physical-property-term-example-1 ;
    dc:description "a <a>modifier</a> expressing a physical property (apart from shape; see <a>shape term</a>). <i>Examples</i>: <i>soft</i> and <i>smooth</i> are English physical property terms. (Section 4.1.2)" ;
    dcterms:isPartOf "4.1.2" ;
    compcon:subtypeOf compcon:cxn_adjective .

compcon:cxn_place-noun a compcon:cxn ;
    rdfs:label "place noun" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_common-noun .

compcon:cxn_place-proper-noun a compcon:cxn ;
    rdfs:label "place proper noun" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_proper-noun .

compcon:cxn_plant-noun a compcon:cxn ;
    rdfs:label "plant noun" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_common-noun .

compcon:cxn_polarity-focus-construction a compcon:cxn ;
    rdfs:label "polarity focus construction" ;
    cx:hasExample compcon:polarity-focus-construction-example-0,
        compcon:polarity-focus-construction-example-1 ;
    dc:description "an <a>identificational construction</a> whose <a>focus</a> is the polarity of the <a>proposition</a> (that is, whether it is true or false). <i>Example</i>: in English, <i>I DID finish my assignment!</i> is an instance of a polarity focus construction, whose focus is the positive polarity (accented <i>DID</i>) of the proposition <q>I finished my assignment</q>. (Section 11.4.1)" ;
    dcterms:isPartOf "11.4.1" ;
    skos:altLabel "truth-value focus construction" ;
    compcon:subtypeOf compcon:cxn_identificational-construction .

