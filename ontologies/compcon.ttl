compcon:cxn_pursuit-verb a compcon:cxn ;
    rdfs:label "pursuit verb" ;
    cx:hasExample compcon:pursuit-verb-example-0,
        compcon:pursuit-verb-example-1,
        compcon:pursuit-verb-example-2,
        compcon:pursuit-verb-example-3 ;
    dc:description "the <a>verb</a> expressing a <a>pursuit event</a>. <i>Examples</i>: pursuit events include following, chasing, searching for something, and waiting for someone/something; and <i>follow</i>, <i>chase</i>, <i>search (for)</i>, and <i>wait (for)</i> are pursuit verbs. (Section 7.3.3)" ;
    dcterms:isPartOf "7.3.3" ;
    compcon:subtypeOf compcon:cxn_verb .

compcon:cxn_question-pronoun a compcon:cxn ;
