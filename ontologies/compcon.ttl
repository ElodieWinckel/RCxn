compcon:cxn_relative-clause a compcon:cxn ;
    rdfs:label "relative clause" ;
    cx:hasExample compcon:relative-clause-example-0 ;
    dc:description "the <a>dependent clause</a> in a <a>relative clause construction</a>. The relative clause denotes the <a>event</a> that is used to <a inf:modification>modify</a> the <a>relative clause head</a> <a>referent</a>. <i>Example</i>: in <i>I ate the cheesecake</i> [<i>that Carol baked</i>], <i>that Carol baked</i> is the relative clause. (Section 19.1)" ;
    dcterms:isPartOf "19.1" ;
    skos:altLabel "relative dependent clause" ;
    compcon:subtypeOf compcon:cxn_attributive-phrase,
        compcon:cxn_dependent-clause .

compcon:cxn_removal-verb a compcon:cxn ;
    rdfs:label "removal verb" ;
    cx:hasExample compcon:removal-verb-example-0 ;
    dc:description "the <a>verb</a> expressing a <a>removal event</a>. <i>Example</i>: scrubbing is a removal event, and <i>scrub</i> is a removal verb. (Section 7.3.2)" ;
    dcterms:isPartOf "7.3.2" ;
    compcon:subtypeOf compcon:cxn_verb .

compcon:cxn_s-phrase a compcon:cxn ;
