compcon:cxn_second-person-pronoun a compcon:cxn ;
    rdfs:label "second person pronoun" ;
    cx:hasExample compcon:second-person-pronoun-example-0 ;
    dc:description "a <a>personal pronoun</a> used for <a>contextual</a> <a>reference</a> to a person in their role as addressee. The term is conventionally used also to refer to a group of persons, one of whom is the addressee, as long as the group does not also include the speaker; if the speaker is included, then the pronoun is a <a>first person pronoun</a>. <i>Example</i>: <i>you</i> is a second person pronoun, referring to the addressee, or a group including the addressee but not the speaker. (Section 3.1.1)" ;
    dcterms:isPartOf "3.1.1" ;
    compcon:subtypeOf compcon:cxn_personal-pronoun .

compcon:cxn_semantically-nonspecific-article a compcon:cxn ;
    rdfs:label "semantically nonspecific article" ;
    dc:description "an <a>article</a> that is used for a <a>nonspecific referent</a>. (Section 3.5)" ;
    dcterms:isPartOf "3.5" ;
    skos:altLabel "(semantically) nonspecific (indefinite) article" ;
    compcon:subtypeOf compcon:cxn_attributive-phrase,
        compcon:cxn_indefinite-article .

compcon:cxn_sensation-verb a compcon:cxn ;
    rdfs:label "sensation verb" ;
    cx:hasExample compcon:sensation-verb-example-0 ;
    dc:description "a <a>verb</a> that expresses a <a>sensation event</a>. <i>Example</i>: <i>My head aches</i> is an example of a sensation event, and <i>ache</i> is the sensation verb. (Section 7.4)" ;
    dcterms:isPartOf "7.4" ;
    compcon:subtypeOf compcon:cxn_experiential-verb .

compcon:cxn_set-member-attributive-phrase a compcon:cxn ;
