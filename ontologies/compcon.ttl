compcon:cxn_predicate-adjectival-construction a compcon:cxn ;
    rdfs:label "predicate adjectival construction" ;
    cx:hasExample compcon:predicate-adjectival-construction-example-0 ;
    dc:description "a <a>clause construction</a> defined by the function of <a inf:predication>predicating</a> a <a>property concept</a> of a <a>referent</a> -- that is, asserting that a property applies to the referent. <i>Example</i>: <i>Sarah is intelligent</i> is an example of an English predicate adjectival construction: it predicates intelligence of Sarah. (Sections 1.5, <b>2.2.5</b>, <b>10.3</b>)" ;
    dcterms:isPartOf "1.5",
        "10.3",
        "2.2.5" ;
    skos:altLabel "predicate adjectival (construction)",
        "property predication (construction)" ;
    compcon:subtypeOf compcon:cxn_nonprototypical-predication-construction .

compcon:cxn_predicate-nominal-construction a compcon:cxn ;
    rdfs:label "predicate nominal construction" ;
    cx:hasExample compcon:predicate-nominal-construction-example-0 ;
    dc:description "a <a>clause construction</a> defined by the function of <a inf:predication>predicating</a> an <a>object concept</a> of a <a>referent</a> -- that is, asserting what object category the referent belongs to. <i>Example</i>: <i>Ira is a writer</i> is an example of an English predicate nominal construction; it predicates that the referent of <i>Ira</i> belongs to the category of writer. <a>Predicational construction</a> is another term for a predicate nominal construction. Sometimes <q>predicate nominal</q> is used to cover predicational, <a cxn:presentational-construction>presentational</a> and <a cxn:equational-construction>equational</a> constructions; we will use it in the narrow sense only. (Sections 1.4, <b>2.2.5</b>, <b>10.3</b>)" ;
    dcterms:isPartOf "1.4",
        "10.3",
        "2.2.5" ;
    skos:altLabel "object predication construction",
        "predicational construction" ;
    compcon:subtypeOf compcon:cxn_nonprototypical-predication-construction .

compcon:cxn_predicational-location-construction a compcon:cxn ;
    rdfs:label "predicational location construction" ;
    cx:hasExample compcon:predicational-location-construction-example-0 ;
    dc:description "the <a>construction</a> expressing a <a inf:predicational-location>predicational location</a> <a>function</a>. <i>Example</i>: <i>The pot is on the table</i> is an instance of the predicational locative construction. (Section 10.4.1)" ;
    dcterms:isPartOf "10.4.1" ;
    skos:altLabel "locative predication" ;
    compcon:subtypeOf compcon:cxn_locational-construction,
        compcon:cxn_nonprototypical-predication-construction .

compcon:cxn_predicational-possession-construction a compcon:cxn ;
    rdfs:label "predicational possession construction" ;
    cx:hasExample compcon:predicational-possession-construction-example-0,
        compcon:predicational-possession-construction-example-1 ;
    dc:description "a <a>possession clause</a> in which <a>ownership</a> of the <a>possessum</a> by the <a>possessor</a> is <a>predicated</a> of the possessum. <i>Example</i>: in <i>That laptop belongs to Kerry</i>, ownership of the laptop by Kerry is predicated of the laptop. <i>That laptop is Kerry's</i> is also treated as an instance of a predicational possession construction, <a>recruiting</a> the <a>strategy</a> of a <a>nonprototypical predication construction</a>; but it might be better analyzed as having <a>equational information packaging</a>. (Section 10.4.2)" ;
    dcterms:isPartOf "10.4.2" ;
    skos:altLabel "belong possession" ;
    compcon:subtypeOf compcon:cxn_nonprototypical-predication-construction,
        compcon:cxn_possession-construction .

compcon:cxn_pretense-construction a compcon:cxn ;
    rdfs:label "pretense construction" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_propositional-attitude-construction .

compcon:cxn_prohibitive-construction a compcon:cxn ;
    rdfs:label "prohibitive construction" ;
    cx:hasExample compcon:prohibitive-construction-example-0 ;
    dc:description "the <a>construction</a> that expresses a <a inf:prohibitive>prohibitive</a> speech act. <i>Example</i>: English <i>Don't be a fool!</i> is an instance of a prohibitive; the construction uses a special prohibitive morpheme <i>Don't</i> to express prohibitive function. (Section 12.4.1)" ;
    dcterms:isPartOf "12.4.1" ;
    compcon:subtypeOf compcon:cxn_imperative-hortative-construction,
        compcon:cxn_negation-construction .

compcon:cxn_property-referring-phrase a compcon:cxn ;
    rdfs:label "property referring phrase" ;
    cx:hasExample compcon:property-referring-phrase-example-0,
        compcon:property-referring-phrase-example-1,
        compcon:property-referring-phrase-example-2,
        compcon:property-referring-phrase-example-3 ;
    dc:description "a <a>construction</a> that expresses <a>reference</a> to <a>property concepts</a>. <i>Examples</i>: <i>length</i> (< <i>long</i>), <i>happiness</i> (< <i>happy</i>) are examples of the property referring phrase construction. Property reference is very rare in discourse, little described in reference grammars, and little studied in typology, so they are not further discussed here. (Section 2.2.5)" ;
    dcterms:isPartOf "2.2.5" ;
    compcon:subtypeOf compcon:cxn_referring-phrase .

compcon:cxn_proportional-quantifier-term a compcon:cxn ;
    rdfs:label "proportional quantifier term" ;
    cx:hasExample compcon:proportional-quantifier-term-example-0 ;
    dc:description "a <a>form</a> that specifies the set of instances as a proportion of the whole set of individuals/tokens of the type, or at least the contextually relevant whole set. <i>Example</i>: in <i>few children</i>, <i>few</i> is a proportional quantifier indicating a lesser proportion of the contextually relevant set of children. (Section 4.1.3)" ;
    dcterms:isPartOf "4.1.3" ;
    compcon:subtypeOf compcon:cxn_quantifier-term .

compcon:cxn_protasis-clause a compcon:cxn ;
    rdfs:label "protasis clause" ;
    cx:hasExample compcon:protasis-clause-example-0 ;
    dc:description "the <a>clause</a> expressing the causally antecedent <a>proposition</a> in a <a>causal</a>, <a>conditional</a>, <a>concessive</a>, <a>concessive conditional</a>, or <a>comparative conditional construction</a>. <i>Example</i>: in <i>If you press this button, the door will open</i>, <i>If you press this button</i> is the protasis; <i>the door will open</i> is the <a cxn:apodosis-clause>apodosis</a>. (Section 17.3.1)" ;
    dcterms:isPartOf "17.3.1" ;
    skos:altLabel "antecedent" ;
    compcon:subtypeOf compcon:cxn_construction .

compcon:cxn_purpose-construction a compcon:cxn ;
