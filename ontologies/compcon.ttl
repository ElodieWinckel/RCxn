@prefix compcon: <https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/compcon#> .
@prefix cx: <http://example.org/cx/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

compcon:ExpressionOf a owl:ObjectProperty ;
    rdfs:label "Expresses" .

compcon:cxn_action-modification-construction a compcon:cxn ;
    rdfs:label "action modification construction" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_modification-construction .

compcon:cxn_additive-focus-operator a compcon:cxn ;
    rdfs:label "additive focus operator" ;
    cx:hasExample compcon:additive-focus-operator-example-0 ;
    dc:description "a <a>focus operator</a> that indicates that the <a>focus</a> is the added information in an <a inf:expanding-contrast>expanding counterpresuppositional contrast</a> <a>construction</a>. <i>Example</i>: in the exchange <i>John bought apples. He also bought PEACHES, also</i> is an additive operator. (Section 11.4.1)" ;
    dcterms:isPartOf "11.4.1" ;
    skos:altLabel "additive (focus) operator",
        "inclusive focus operator" ;
    compcon:subtypeOf compcon:cxn_focus-operator .

compcon:cxn_adjective-modification-construction a compcon:cxn ;
    rdfs:label "adjective modification construction" ;
    dc:description "" ;
    skos:altLabel "adjective modification (construction)" ;
    compcon:subtypeOf compcon:cxn_modification-construction .

compcon:cxn_adverbial-matrix-clause a compcon:cxn ;
    rdfs:label "adverbial matrix clause" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_matrix-clause .

compcon:cxn_alienable-possession-modification-construction a compcon:cxn ;
    rdfs:label "alienable possession modification construction" ;
    cx:hasExample compcon:alienable-possession-modification-construction-example-0 ;
    dc:description "a <a>possessive modification construction</a> that always includes the <a>ownership relation</a>, and contrasts in the language with an <a>inalienable possession construction</a>. <i>Example</i>: Crow <i>bas-óosshee</i> <q>my food</q> is an instance of alienable possession, using the distinct index bas- (cf. the inalienable <a>index</a> b-). (Section 4.1.4)" ;
    dcterms:isPartOf "4.1.4" ;
    skos:altLabel "alienable possession (modification construction)" ;
    compcon:subtypeOf compcon:cxn_possessive-modification-construction .

compcon:cxn_anaphoric-head-relative-clause-construction a compcon:cxn ;
    rdfs:label "anaphoric-head relative clause construction" ;
    cx:hasExample compcon:anaphoric-head-relative-clause-construction-example-0 ;
    dc:description "an <a>anaphoric-head construction</a> in which the <a>modifier</a> is a <a>clause</a> denoting an <a>event</a>. <i>Example</i>: in K'ichee' <i>utz</i> [<i>lē xubij lē achi</i>] <q>What the man said is good</q>, the clause <i>lē xubij lē achi</i> <q>the man said [it]</q> is an instance of the anaphoric-head relative clause construction. (Section 19.4)" ;
    dcterms:isPartOf "19.4" ;
    compcon:subtypeOf compcon:cxn_anaphoric-head-construction,
        compcon:cxn_relative-clause-construction .

compcon:cxn_applicative-object-phrase a compcon:cxn ;
    rdfs:label "applicative object phrase" ;
    cx:hasExample compcon:applicative-object-phrase-example-0 ;
    dc:description "the <a>participant role</a> which is expressed as the <a inf:object-argument>object</a> in an <a>applicative construction</a>. <i>Example</i>: in Nomatsiguenga <i>na-manantë-ne-ro kayeta</i> <q>I bought crackers for her</q>, the <a>beneficiary</a> referred to by <q>her</q> is expressed as the object, with the <sc>3sg</sc> Object suffix <i>-ro</i>. (Section 9.3)" ;
    dcterms:isPartOf "9.3" ;
    compcon:subtypeOf compcon:cxn_object-phrase .

compcon:cxn_argument-predicate-part a compcon:cxn ;
    rdfs:label "argument predicate part" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_complex-predicate-part .

compcon:cxn_cardinal-numeral-term a compcon:cxn ;
    rdfs:label "cardinal numeral term" ;
    cx:hasExample compcon:cardinal-numeral-term-example-0,
        compcon:cardinal-numeral-term-example-1,
        compcon:cardinal-numeral-term-example-2 ;
    dc:description "a form that expresses the number (cardinality) of a set of <a>individuals</a> of the <a>type</a>. <i>Examples</i>: in <i>one tree, two boys, three books</i>, <i>one/two/three</i> are cardinal numerals. (Section 4.1.3)" ;
    dcterms:isPartOf "4.1.3" ;
    compcon:subtypeOf compcon:cxn_modifier .

compcon:cxn_commentative-predicate a compcon:cxn ;
    rdfs:label "commentative predicate" ;
    cx:hasExample compcon:commentative-predicate-example-0 ;
    dc:description "the <a>predicate</a> expressing a <a>commentative event</a>. <i>Example</i>: in <i>Nancy is glad that Joe won the election</i>, the commentative predicate <i>is glad</i> expresses Nancy's evaluation of Joe's winning the election, and also presupposes that the speaker believes that Joe indeed won the election. (Section 18.2.2)" ;
    dcterms:isPartOf "18.2.2" ;
    compcon:subtypeOf compcon:cxn_evaluative-predicate .

compcon:cxn_complement-matrix-clause a compcon:cxn ;
    rdfs:label "complement matrix clause" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_matrix-clause .

compcon:cxn_complex-construction a compcon:cxn ;
    rdfs:label "complex construction" ;
    cx:hasExample compcon:complex-construction-example-0 ;
    dc:description "a construction made up of more than one <a>element</a>. <i>Example</i>: the referring phrase <i>an ancient watch</i> is complex because it is made up of three elements (<i>an</i>, <i>ancient</i>, and <i>watch</i>). (Section 2.2.1)" ;
    dcterms:isPartOf "2.2.1" ;
    compcon:subtypeOf compcon:cxn_construction .

compcon:cxn_content-causal-construction a compcon:cxn ;
    rdfs:label "content causal construction" ;
    cx:hasExample compcon:content-causal-construction-example-0 ;
    dc:description "the construction expressing a <a>content causal relation</a>. <i>Example</i>: in <i>If you press this button, the door will open</i>, there is a content causal relation between the event of your pressing the button and the event of the door opening. (Section 17.3.1)" ;
    dcterms:isPartOf "17.3.1" .

compcon:cxn_dependent a compcon:cxn ;
    rdfs:label "dependent" ;
    cx:hasExample compcon:dependent-example-0 ;
    dc:description "any <a>element</a> of a <a>construction</a> that is not the <a>head</a>. <i>Example</i>: in the phrase <i>an ancient watch</i>, <i>ancient</i> and <i>an</i> are dependents. (Section 2.2.1)" ;
    dcterms:isPartOf "2.2.1" ;
    compcon:subtypeOf compcon:cxn_element .

compcon:cxn_desiderative-predicate a compcon:cxn ;
    rdfs:label "desiderative predicate" ;
    cx:hasExample compcon:desiderative-predicate-example-0 ;
    dc:description "the <a>predicate</a> that expresses a <a>desiderative event</a>. <i>Example</i>: in <i>Meagan wants to climb Mt. Baldy on Saturday, wants</i> denotes a desiderative event. Noonan (2007:135) includes intending events in the category of desiderative events. (Section 18.2.2)" ;
    dcterms:isPartOf "18.2.2" ;
    compcon:subtypeOf compcon:cxn_complement-taking-predicate .

compcon:cxn_epistemic-causal-construction a compcon:cxn ;
    rdfs:label "epistemic causal construction" ;
    cx:hasExample compcon:epistemic-causal-construction-example-0 ;
    dc:description "the construction expressing an <a>epistemic causal relation</a>. <i>Example</i>: in <i>If Professor Smith's door is closed, then she is not on campus</i>, there is an epistemic causal relation between the fact that Professor Smith's office door is closed and the inference that she is not on campus. (Section 17.3.1)" ;
    dcterms:isPartOf "17.3.1" .

compcon:cxn_fearing-predicate a compcon:cxn ;
    rdfs:label "fearing predicate" ;
    cx:hasExample compcon:fearing-predicate-example-0 ;
    dc:description "the <a>predicate</a> expressing a <a>fearing event</a>. <i>Example</i>: in <i>Jill fears that Donald has won the election</i>, the commentative predicate <i>fears</i> expresses Jill's evaluation of Donald's winning the election, and also presupposes that the speaker does not know whether Donald has won the election. (Section 18.2.2)" ;
    dcterms:isPartOf "18.2.2" ;
    compcon:subtypeOf compcon:cxn_evaluative-predicate .

compcon:cxn_free-relative-clause-construction a compcon:cxn ;
    rdfs:label "free relative clause construction" ;
    cx:hasExample compcon:free-relative-clause-construction-example-0,
        compcon:free-relative-clause-construction-example-1 ;
    dc:description "a <a>relative clause construction</a> in which the head has one of several possible <a cxn:indefinite-pronoun>indefinite</a> functions -- that is, <a inf:specific-known-referent>specific</a>, <a inf:irrealis-referent>irrealis</a>, <a inf:free-choice-referent>free choice</a>, and/or <a cxn:universal-pronoun>universal</a>. <i>Example</i>: in <i>Take what(ever) you like</i>, <i>what(ever) you like</i> is a free relative clause using a <a>headless strategy</a>, and in <i>Take anything you like</i>, <i>anything you like</i> is a free relative clause using an <a>overt head strategy</a>. (Section 19.4)" ;
    dcterms:isPartOf "19.4" ;
    compcon:subtypeOf compcon:cxn_free-modification-construction,
        compcon:cxn_relative-clause-construction .

compcon:cxn_hanging-topic-construction a compcon:cxn ;
    rdfs:label "hanging topic construction" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_topic-comment-construction .

compcon:cxn_hanging-topic-phrase a compcon:cxn ;
    rdfs:label "hanging topic phrase" ;
    cx:hasExample compcon:hanging-topic-phrase-example-0 ;
    dc:description "a phrase that expresses a <a inf:reference>topic</a> that is not a <a>participant</a> in the <a>predicated</a> <a>event</a>. <i>Example</i>: in Mandarin Chinese <i>xiàng bizi cháng</i> [elephant nose long] <q>Elephant's noses are long / Elephants have long noses</q>, <i>xiàng</i> <q>elephant</q> is a topic but not a participant in the predicated event <i>cháng</i> <q>be long</q>. Hanging topics may use a <a>detached topic phrase</a> strategy. (Section 11.2.3)" ;
    dcterms:isPartOf "11.2.3" ;
    compcon:subtypeOf compcon:cxn_topic-phrase .

compcon:cxn_head a compcon:cxn ;
    rdfs:label "head" ;
    cx:hasExample compcon:head-example-0 ;
    dc:description "the most contentful word that most closely denotes the same function as the phrase (or clause) as a whole. <i>Example</i>: the head of the phrase <i>an ancient watch</i> is <i>watch</i>. (Section 2.2.1)" ;
    dcterms:isPartOf "2.2.1" ;
    compcon:subtypeOf compcon:cxn_element .

compcon:cxn_hoping-predicate a compcon:cxn ;
    rdfs:label "hoping predicate" ;
    cx:hasExample compcon:hoping-predicate-example-0 ;
    dc:description "the predicate expressing a <a>hoping event</a>. <i>Example</i>: in <i>Jill hopes that Joe won the election</i>, the commentative predicate <i>hopes</i> expresses Jill's evaluation of Joe's winning the election, and also presupposes that the speaker does not know whether Joe has won the election. (Section 18.2.2)" ;
    dcterms:isPartOf "18.2.2" ;
    compcon:subtypeOf compcon:cxn_evaluative-predicate .

compcon:cxn_inalienable-possession-modification-construction a compcon:cxn ;
    rdfs:label "inalienable possession modification construction" ;
    cx:hasExample compcon:inalienable-possession-modification-construction-example-0 ;
    dc:description "a <a>possessive modification construction</a> that always includes either <a>body part relations</a> <i>or</i> <a>kinship relations</a> (but not necessarily both), and contrasts in the language with an <a>alienable possession construction</a>. <i>Example</i>: Crow <i>ba-apé</i> <q>my nose</q> is an instance of inalienable possession, using the distinct index <i>b-</i> (cf. the alienable <a>index</a> <i>bas-</i>). (Sections 4.1.4, <b>5.2.3</b>)" ;
    dcterms:isPartOf "4.1.4",
        "5.2.3" ;
    skos:altLabel "inalienable possession" ;
    compcon:subtypeOf compcon:cxn_possessive-modification-construction .

compcon:cxn_knowledge-predicate a compcon:cxn ;
    rdfs:label "knowledge predicate" ;
    cx:hasExample compcon:knowledge-predicate-example-0 ;
    dc:description "the <a>predicate</a> expressing a <a>knowledge event</a>. <i>Example</i>: in <i>Sally knows that Donald won the election</i>, Sally's belief with respect to the proposition that Donald won the election is reported by the speaker; and, in addition, a <a>positive epistemic stance</a> is taken by the speaker toward that proposition (i.e., the speaker believes that Donald indeed won the election). (Section 18.2.2)" ;
    dcterms:isPartOf "18.2.2" ;
    compcon:subtypeOf compcon:cxn_propositional-attitude-predicate .

compcon:cxn_manipulative-predicate a compcon:cxn ;
    rdfs:label "manipulative predicate" ;
    cx:hasExample compcon:manipulative-predicate-example-0 ;
    dc:description "the <a>predicate</a> expressing a <a>manipulative event</a>. <i>Example</i>: in <i>Bruce convinced Greg to take him to San Rafael, convinced</i> denotes a manipulative event. Manipulative <a>complement clause constructions</a> overlap with <a>causative constructions</a>. (Section 18.2.2)" ;
    dcterms:isPartOf "18.2.2" ;
    compcon:subtypeOf compcon:cxn_complement-taking-predicate .

compcon:cxn_noun-complement-head a compcon:cxn ;
    rdfs:label "noun complement head" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_referent-expression .

compcon:cxn_noun-complement-matrix-clause a compcon:cxn ;
    rdfs:label "noun complement matrix clause" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_matrix-clause .

compcon:cxn_numeral-modification-construction a compcon:cxn ;
    rdfs:label "numeral modification construction" ;
    dc:description "" ;
    skos:altLabel "numeral construction",
        "numeral modification (construction)" ;
    compcon:subtypeOf compcon:cxn_modification-construction .

compcon:cxn_partitive-phrase a compcon:cxn ;
    rdfs:label "partitive phrase" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_nominal-attributive-phrase .

compcon:cxn_perception-complement-taking-predicate a compcon:cxn ;
    rdfs:label "perception complement-taking predicate" ;
    cx:hasExample compcon:perception-complement-taking-predicate-example-0 ;
    dc:description "A perception complement-taking predicate is a <a>predicate</a> that expresses perceiving an event, which is expressed as the <a>complement</a> event of the predicate. <i>Example</i>: in <i>We watched the elk graze in the caldera</i>, watched denotes a perception event. The complement event has <a>dependent time reference</a>; the complement event must be occurring at the same time as the perceiving event (although modern media allowing watching a prior event via a recording). (Section 18.2.2)" ;
    dcterms:isPartOf "18.2.2" ;
    compcon:subtypeOf compcon:cxn_complement-taking-predicate .

compcon:cxn_perception-verb a compcon:cxn ;
    rdfs:label "perception verb" ;
    cx:hasExample compcon:perception-verb-example-0 ;
    dc:description "a perception verb is a <a>verb</a> that expresses the event of perceiving an object. <i>Example</i>: <i>Tim heard the macaw</i> is an example of a perception event, and <i>hear</i> is the perception verb (Section 7.4)." ;
    compcon:subtypeOf compcon:cxn_verb .

compcon:cxn_phrase a compcon:cxn ;
    rdfs:label "phrase" ;
    cx:hasExample compcon:phrase-example-0 ;
    dc:description "a construction used for <a>reference</a> or <a>modification</a> (of a referent). <i>Example</i>: <i>the furry kitten</i> is an instance of a phrasal construction. (Section 1.3)" ;
    dcterms:isPartOf "1.3" .

compcon:cxn_piece-term a compcon:cxn ;
    rdfs:label "piece term" ;
    cx:hasExample compcon:piece-term-example-0 ;
    dc:description "a <a>mensural term</a> that selects an amount of a <a>referent</a> which is usually a separated, arbitrary part of the object. <i>Example</i>: in <i>a slice of meat, slice</i> is a piece term. (Section 4.1.3)" ;
    dcterms:isPartOf "4.1.3" ;
    compcon:subtypeOf compcon:cxn_mensural-term .

compcon:cxn_predicational-construction a compcon:cxn ;
    rdfs:label "predicational construction" ;
    cx:hasExample compcon:predicational-construction-example-0 ;
    dc:description "the <a>construction</a> that expresses a <a inf:predicational>predicational</a> information packaging. <i>Example</i>: in <i>Bill is a teacher</i>, what is being predicated of Bill is that he is a member of the object category of teacher. (Section 10.1.2)" ;
    dcterms:isPartOf "10.1.2" .

compcon:cxn_pretense-predicate a compcon:cxn ;
    rdfs:label "pretense predicate" ;
    cx:hasExample compcon:pretense-predicate-example-0 ;
    dc:description "a <a>predicate</a> expressing a <a>pretense event</a>. <i>Example</i>: in <i>Ira pretended that the guests had already left</i>, the proposition that the guests had already left is presented as true in an alternative reality from the shared beliefs of the interlocutors (or, for that matter, Ira). There is a strong implicature that the proposition does not hold in reality (that is, the shared beliefs of the interlocutors). (Section 18.2.2)" ;
    dcterms:isPartOf "18.2.2" ;
    skos:altLabel "pretence predicate" ;
    compcon:subtypeOf compcon:cxn_propositional-attitude-predicate .

compcon:cxn_pronominal-argument-complex-predicate a compcon:cxn ;
    rdfs:label "pronominal argument complex predicate" ;
    cx:hasExample compcon:pronominal-argument-complex-predicate-example-0 ;
    dc:description "an <a>argument complex predicate</a> in which the <a inf:reference>argument</a> is a <a>pronoun</a> rather than a <a>common noun</a>. <i>Example</i>: in English <i>I'm losing it</i> (meaning <q>lose one's mind</q>), <i>losing it</i> is a pronominal argument complex predicate, containing the pronoun <i>it</i>. (Section 13.6)" ;
    dcterms:isPartOf "13.6" ;
    compcon:subtypeOf compcon:cxn_argument-complex-predicate .

compcon:cxn_quantifier-modification-construction a compcon:cxn ;
    rdfs:label "quantifier modification construction" ;
    dc:description "" ;
    skos:altLabel "quantifier construction",
        "quantifier modification (construction)" ;
    compcon:subtypeOf compcon:cxn_modification-construction .

compcon:cxn_relative-clause-head a compcon:cxn ;
    rdfs:label "relative clause head" ;
    cx:hasExample compcon:relative-clause-head-example-0 ;
    dc:description "the <a>referring phrase</a> that denotes the <a>necessarily shared participant</a> in a <a>relative clause construction</a> -- that is, the <a>participant</a> that plays a <a>semantic role</a> in both the <a>event</a> denoted by the <a>relative clause</a> and the event denoted by the <a>matrix clause</a>. <i>Example</i>: in <i>I ate the cheesecake</i> [<i>that Carol baked</i>], <i>the cheesecake</i> is the relative clause head. The relative clause head is an <a inf:reference>argument</a> of the matrix clause predicate, and is <a inf:modification>modified</a> by the relative clause. (Section 19.1)" ;
    dcterms:isPartOf "19.1" ;
    compcon:subtypeOf compcon:cxn_referring-phrase .

compcon:cxn_relative-matrix-clause a compcon:cxn ;
    rdfs:label "relative matrix clause" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_matrix-clause .

compcon:cxn_relative-pronoun a compcon:cxn ;
    rdfs:label "relative pronoun" ;
    cx:hasExample compcon:relative-pronoun-example-0 ;
    dc:description "a <a>pronoun</a> form that is unique to the <a>relative clause construction</a>, and is found in the <a>externally headed</a> relative clause construction using the <a>relative pronoun strategy</a>. <i>Example</i>: in <i>He stole the emerald</i> [<i>which he gave to his wife</i>], <i>which</i> is the relative pronoun. (Section 19.2.2)" ;
    dcterms:isPartOf "19.2.2" ;
    compcon:subtypeOf compcon:cxn_anaphoric-pronoun .

compcon:cxn_relativized-referring-phrase a compcon:cxn ;
    rdfs:label "relativized referring phrase" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_referring-phrase .

compcon:cxn_restrictive-focus-operator a compcon:cxn ;
    rdfs:label "restrictive focus operator" ;
    cx:hasExample compcon:restrictive-focus-operator-example-0 ;
    dc:description "a <a>focus operator</a> that indicates that the <a>focus</a> is the restricted information in a <a inf:restricting-contrast>restricting counterpresuppositional contrast</a>. <i>Example</i>: in the exchange <i>John bought apples and peaches. No, he only bought PEACHES</i>, <i>only</i> is a restrictive operator. (Section 11.4.1)" ;
    dcterms:isPartOf "11.4.1" ;
    skos:altLabel "exclusive focus operator",
        "restrictive (focus) operator" ;
    compcon:subtypeOf compcon:cxn_focus-operator .

compcon:cxn_set-member-modification-construction a compcon:cxn ;
    rdfs:label "set-member modification construction" ;
    dc:description "" ;
    skos:altLabel "set-member (modification) (construction)" ;
    compcon:subtypeOf compcon:cxn_modification-construction .

compcon:cxn_speech-act-causal-construction a compcon:cxn ;
    rdfs:label "speech act causal construction" ;
    cx:hasExample compcon:speech-act-causal-construction-example-0 ;
    dc:description "the construction expressing a <a>speech act causal relation</a>. <i>Example</i>: in <i>Since you asked, ten isn't a prime number</i>, there is a speech act causal relation between the request of the speaker whether ten is a prime number, and the performance of the speech act asserting that ten isn't a prime number. (Section 17.3.1)" ;
    dcterms:isPartOf "17.3.1" .

compcon:cxn_spurious-complex-predicate a compcon:cxn ;
    rdfs:label "spurious complex predicate" ;
    dc:description "" ;
    compcon:subtypeOf compcon:cxn_stative-complex-predicate .

compcon:cxn_support-verb a compcon:cxn ;
    rdfs:label "support verb" ;
    cx:hasExample compcon:support-verb-example-0 ;
    dc:description "the <a>element</a> in a <a>support verb construction</a> that has undergone semantic change -- specifically, semantic generalization -- such that it makes a minimal semantic contribution to the <a>meaning</a> of the whole <a>complex predicate</a>. <i>Example</i>: in English <i>They had a drink, had</i> is the support verb in the support verb construction <i>had a drink</i>. <a>Copulas</a> are analyzed as a subtype of a support verb. (Section 13.5)" ;
    dcterms:isPartOf "13.5" ;
    skos:altLabel "coverb",
        "light verb" ;
    compcon:subtypeOf compcon:cxn_complex-predicate-part .

compcon:cxn_support-verb-construction a compcon:cxn ;
    rdfs:label "support verb construction" ;
    cx:hasExample compcon:support-verb-construction-example-0 ;
    dc:description "an <a>eventive complex predicate</a> in which one of the <a>elements</a>, the <a>support verb</a>, has undergone semantic change -- specifically, semantic generalization -- such that it makes a minimal semantic contribution to the <a>meaning</a> of the whole <a>complex predicate</a>. This element no longer denotes a separate subevent of the whole <a>event</a>, unlike in <u>basic</u> <a>eventive complex predicates</a>. The <a>verb</a> <a>forms</a> in a support verb construction are also in a relatively idiosyncratic semantic relationship. <i>Example</i>: in English <i>Frances Patterson underwent an operation at RMH today</i>, <i>underwent an operation</i> is an instance of a support verb construction. The element other than the support verb is often in an <a>action nominal</a> or other form that is unlike the form of a simple (<a>predicated</a>) verb. (Section 13.5)" ;
    dcterms:isPartOf "13.5" ;
    skos:altLabel "coverb construction",
        "light verb construction" ;
    compcon:subtypeOf compcon:cxn_complex-predicate .

compcon:cxn_utterance-predicate a compcon:cxn ;
    rdfs:label "utterance predicate" ;
    cx:hasExample compcon:utterance-predicate-example-0,
        compcon:utterance-predicate-example-1 ;
    dc:description "the <a>predicate</a> expressing an <a>utterance event</a>. <i>Example</i>: in <i>Sandy said, <dq>I'm buying the house</dq>, said</i> denotes the utterance event. Some predicates denoting utterance events include the <u>addressee</u> as an <a inf:reference>argument</a>, as in <i>Sandy told me that she's buying the house</i>. (Section 18.2.2)" ;
    dcterms:isPartOf "18.2.2" ;
    compcon:subtypeOf compcon:cxn_complement-taking-predicate .

compcon:cxn_wishing-predicate a compcon:cxn ;
    rdfs:label "wishing predicate" ;
    cx:hasExample compcon:wishing-predicate-example-0 ;
    dc:description "the <a>predicate</a> expressing a <a>wishing event</a>. <i>Example</i>: in <i>Jill wishes that Joe had won the election</i>, the wishing predicate <i>wishes</i> expresses Jill's evaluation of Joe's winning the election, and also presupposes that the speaker believes that Joe didn't win the election. (Section 18.2.2)" ;
    dcterms:isPartOf "18.2.2" ;
    compcon:subtypeOf compcon:cxn_evaluative-predicate .

compcon:def_accessibility-hierarchy a compcon:def ;
    rdfs:label "Accessibility Hierarchy" ;
    dc:description "an implicational hierarchy that governs universals of the distribution of <a>relative clause constructions</a> and particular <a>strategies</a> of relative clause constructions, depending on the <a>semantic role</a> that the <a>relative clause head</a> plays in the <a>event</a> denoted by the <a>relative clause</a>. The Accessibility Hierarchy is usually formulated as: <a>subject</a> (<a>A</a>/<a>S</a>) < <a inf:object-argument>object</a> (<a>P</a>/<a>T</a>) < <a str:indirect-object-category>indirect object</a> (<a>G</a>), <a cxn:oblique-phrase>oblique</a> < (<a cxn:attributive-phrase>attributive</a>) <a>possessor</a>. All languages have a relative clause construction that can relativize the <a>subject</a>; a specific construction is used for a continuous segment of the hierarchy; <a>deranked</a> relative clauses are used for the top part of the hierarchy downwards; less explicit relative clause strategies are used for the top part of the hierarchy downwards; and more <a>explicit</a> relative clause strategies are used for the bottom part of the hierarchy upwards. (Section 19.3)" ;
    dcterms:isPartOf "19.3" .

compcon:def_accessibility-scale a compcon:def ;
    rdfs:label "Accessibility Scale" ;
    dc:description "an ordering of types of <a>referring phrases</a> by their degree of <a>accessibility</a>. The Accessibility Scale accommodates the fact that referring phrases may provide a more fine-grained set of <a>information status</a> distinctions than the common three-way classification of <a>active</a>, <a>semi-active</a>, and <a>inactive</a>. (Section 3.3.1)" ;
    dcterms:isPartOf "3.3.1" .

compcon:def_adjunct a compcon:def ;
    rdfs:label "adjunct" ;
    dc:description "a term that is sometimes used for an <a>oblique argument phrase</a> denoting certain <a>participants</a> that are more <a sem:peripheral-participant>peripheral</a> than other participants denoted by oblique phrases, and which is therefore syntactically <dq>optional</dq>. Semantically, there is no clear division between peripheral participants that motivates an (oblique) argument / adjunct distinction. Also, the phrases expressing even central participants that are highly <a>salient</a> may be morphosyntactically <dq>optional</dq>, as is found with <a>zero anaphora</a>. There is no clear comparative concept of <q>adjunct</q>, and the term is not used in this textbook." .

compcon:def_agreement-hierarchy a compcon:def ;
    rdfs:label "Agreement Hierarchy" ;
    dc:description "a typological universal that constrains the <dq>mismatches</dq> that occur in the grammatical categories (typically, <a>number</a> and <a>gender--class</a>) of a <a>pronoun</a> or <a>index</a> and the grammatical categories of a prior <a>referring phrase</a> that <a inf:reference>refers</a> to the same <a>referent</a> as the pronoun or index. The Agreement Hierarchy ranks the constructions as follows: <a>modifier</a> index < <a>predicate</a> index < <a str:relative-pronoun-strategy>relative pronoun</a> < <a>personal pronoun</a>. <i>Example</i>: in British English, in <i>this committee</i>, the modifier <i>this</i> must index the committee as a singular, but a following personal pronoun may index the committee as a singular <i>...It...</i> or as a plural <i>...They...</i>, indicating that the committee is a group. The personal pronoun is lower on the Agreement Hierarchy, and therefore is more likely to index a <dq>semantic</dq> value (plural) that is not overtly encoded on the noun. (Section 4.4.4)" ;
    dcterms:isPartOf "4.4.4" .

compcon:def_animacy-hierarchy a compcon:def ;
    rdfs:label "Animacy Hierarchy" ;
    dc:description "a ranking of entities from human to (nonhuman) animate to inanimate, such that humans are <dq>highest</dq> on the hierarchy. The ranking is presumed to represent the <a>salience</a> of the entity to persons, or possibly the empathy we have toward the entity. (Section 3.1.2)" ;
    dcterms:isPartOf "3.1.2" .

compcon:def_behavioral-potential a compcon:def ;
    rdfs:label "behavioral potential" ;
    dc:description "the ability of a form in a construction to take the inflections or other grammatical <a>elements</a> characteristic of a construction. <i>Example</i>: the noun <i>tree</i> in a referring expression <i>the huge trees</i> has the behavioral potential of inflecting for number (singular and plural) and taking an article (<i>a</i> or <i>the</i>). (Sections 2.4, <b>2.5</b>)" ;
    dcterms:isPartOf "2.4",
        "2.5" .

compcon:def_binding-hierarchy a compcon:def ;
    rdfs:label "Binding Hierarchy" ;
    dc:description "an implicational hierarchy of <a>events</a> that have other events as <a>participants</a> (the <a>complement</a> events), which appears to govern a wide range of strategies for <a>complement clause constructions</a>, including <a>balancing</a> vs. <a>deranking</a> (Section 18.3.1), the <a>grammaticalization</a> of <a>purpose</a> <a>adverbial clauses</a> into deranked <a>complements</a> (Section 18.3.2), the expression of the participants of the <a>complement-taking predicate</a> and complement events (Section 18.4.1), and the use of <a>logophoric constructions</a> (Section 18.4.2). The Binding Hierarchy is described in detail in Givón (1980) and Cristofaro (2003); the latter calls it the Complement Deranking -- Argument Hierarchy. The version used here is a slightly revised version of Cristofaro's hierarchy: <a>utterance</a>, <a>propositional attitude</a>, <a>knowledge</a> < <a>evaluative</a>, <a>perception</a> < <a>desiderative</a>, <a>manipulative</a> < <a sem:modality>modal</a>, <a sem:phasal-aspect>phasal</a>. (Section 18.3.1)" ;
    dcterms:isPartOf "18.3.1" ;
    skos:altLabel "Complement Deranking-Argument Hierarchy" .

compcon:def_comparative-concept a compcon:def ;
    rdfs:label "comparative concept" ;
    dc:description "a concept that can be used to compare the <a>morphosyntactic</a> structure of different languages. <i>Example</i>: a good example of a comparative concept that can easily be defined on a crosslinguistically valid basis is one based on a <a>semantic class</a>, such as words referring to humans. Other examples of comparative concepts are those based on an <a>information packaging</a> function; <a>constructions</a> (in the specific sense); and <a>strategies</a>. (Section 1.4)" ;
    dcterms:isPartOf "1.4" ;
    skos:altLabel "crosslinguistic category" .

compcon:def_conceptual-space a compcon:def ;
    rdfs:label "conceptual space" ;
    dc:description "an underlying network of semantic relationships among functions that are <a str:co-expression>co-expressed</a> across the world's languages. The conceptual space represents a universal pattern in the <a>semantic map model</a>. (Section 3.5)" ;
    dcterms:isPartOf "3.5" .

compcon:def_construction-grammar a compcon:def ;
    rdfs:label "construction grammar" ;
    dc:description "a model of morphosyntax in which the basic unit of grammatical analysis is a <a>construction</a>. (Section 1.1)" ;
    dcterms:isPartOf "1.1" .

compcon:def_crosslinguistically-valid a compcon:def ;
    rdfs:label "crosslinguistically valid" ;
    dc:description "a property of a construction (in the general sense) that can be defined across languages independently of any language-specific categories. <i>Example</i>: relative word order of <a>adjective</a> and <a>noun</a> is a crosslinguistically valid property, depending only on the order in which the adjective and noun are uttered. (Section 1.4)" ;
    dcterms:isPartOf "1.4" .

compcon:def_cumulation a compcon:def ;
    rdfs:label "cumulation" ;
    dc:description "the expression of multiple <dq>grammatical</dq> (not lexical) <a>meanings</a> in a single morpheme. <i>Example</i>: English <i>-s</i> in <i>She sing-s</i> cumulates third <a>person</a>, singular <a>number</a>, and present <a>tense</a> in a single morpheme. (Sections 1.6, 4.4.1)" ;
    dcterms:isPartOf "1.6",
        "4.4.1" .

compcon:def_explicitness a compcon:def ;
    rdfs:label "explicitness" ;
    dc:description "the property of <a>relative clause construction</a> <a>strategies</a> that refers to how explicitly the strategy encodes the <a>semantic role</a> of the <u>shared participant</u> in the <a>event</a> denoted by the <a>relative clause</a>. For example, the <a>pronoun-retention strategy</a> is more explicit than the <a>gap strategy</a> because the former strategy overtly encodes the semantic role of the participant in the relative clause event via the retained <a>pronoun</a>, whereas the latter strategy does not encode the semantic role at all. Explicitness plays a role in determining the distribution of relative clause strategies with respect to the <a>Accessibility Hierarchy</a>. (Section 19.3)" ;
    dcterms:isPartOf "19.3" ;
    skos:altLabel "explicit(ness)",
        "recoverability" .

compcon:def_extended-animacy-hierarchy a compcon:def ;
    rdfs:label "Extended Animacy Hierarchy" ;
    dc:description "a ranking of entities including <a>contextually</a> defined and <a>individually</a> defined entities as well as <u>categories</u> of entities. The Extended Animacy Hierarchy is given below, with the construction for each position on the hierarchy given in square brackets: first/second person [<a>pronoun</a>] < third person [<a>pronoun</a>] < <a>individual</a> [<a>proper noun</a>] < <a>human</a> [<a>common noun</a>] < (nonhuman) <a>animate</a> [common noun] < <a>inanimate</a> [common noun]. The Extended Animacy Hierarchy figures in many grammatical phenomena. (Section 3.1.2)" ;
    dcterms:isPartOf "3.1.2" ;
    skos:altLabel "Referentiality Hierarchy" .

compcon:def_false-cumulation a compcon:def ;
    rdfs:label "false cumulation" ;
    dc:description "the translation of an <a>object language</a> morpheme by more than one English word because English lacks a one-word translation. Example: Spanish <i>buscar</i> must be translated into English as <q>look for</q>. In an <a>interlinear morpheme translation</a> the English combination is ideally notated look_for. (Section 1.6)" ;
    dcterms:isPartOf "1.6" .

compcon:def_file-metaphor a compcon:def ;
    rdfs:label "file metaphor" ;
    dc:description "a metaphor used by linguists from different theoretical traditions to describe the <a>propositional act information packaging</a> functions. The metaphor is based on the notion of a file in which information about <a>referents</a> is stored. (Sections 2.1, 10.1.2)" ;
    dcterms:isPartOf "10.1.2",
        "2.1" .

compcon:def_form a compcon:def ;
    rdfs:label "form" ;
    dc:description "the <a>morphosyntactic</a> structure of a <a>construction</a>. (Section 1.1)" ;
    dcterms:isPartOf "1.1" ;
    skos:altLabel "expression" .

compcon:def_function a compcon:def ;
    rdfs:label "function" ;
    dc:description "the combination of <a>meaning</a> and <a>information packaging</a> conveyed by a <a>construction</a>. <i>Example</i>: the numeral modification construction illustrated by <i>three tree-s</i> combines the meanings of an object, or more precisely a group of objects (trees), and the cardinality of the group (three), packaged as referring to the (group of) trees and adding information about the group of trees -- namely, that its cardinality is three. Another use of the term <q>function</q> is to refer to a <a>role</a> in a construction. (Section 1.1)" ;
    dcterms:isPartOf "1.1" .

compcon:def_functionalism a compcon:def ;
    rdfs:label "functionalism" ;
    dc:description "an approach to the study of language that seeks explanations of language structure in the function of language in communicative interaction. This textbook takes a functionalist approach. (Section 1.1)" ;
    dcterms:isPartOf "1.1" .

compcon:def_grammaticalization a compcon:def ;
    rdfs:label "grammaticalization" ;
    dc:description "the process by which new grammatical <a>constructions</a> emerge from novel and specialized uses of other grammatical constructions; once a grammatical construction acquires a novel, specialized <a>function</a>, it eventually undergoes changes in <a>morphosyntactic</a> structure and scope, and often also phonetic form. <i>Example</i>: <i>a kind of</i> originally expressed a type of <a sem:object-concept>object</a>, then shifted <a>meaning</a> to become a hedging phrase for a less-central member of a category, was extended to describe hedging of a <a>property</a> word (<i>kind of cute</i>), and was phonetically reduced to <i>kinda</i>. (Sections 1.1, <b>2.3</b>)" ;
    dcterms:isPartOf "1.1",
        "2.3" ;
    skos:altLabel "grammaticalize" .

compcon:def_identity-statements a compcon:def ;
    rdfs:label "identity statements" ;
    dc:description "a superordinate category sometimes used to cover both <a inf:equational>equational</a> and <a>presentational information packaging</a>. (Section 10.1.2)" ;
    dcterms:isPartOf "10.1.2" .

compcon:def_interlinear-morpheme-translation a compcon:def ;
    rdfs:label "interlinear morpheme translation" ;
    dc:description "a widely used method to describe the <a>morphosyntactic</a> structure of a language by providing a morpheme-by-morpheme translation of the <a>object language</a>, including abbreviations for morphemes expressing <dq>grammatical</dq> functions, and including notation of the morpheme type (affix, clitic, reduplication, etc.). (Section 1.6)" ;
    dcterms:isPartOf "1.6" ;
    skos:altLabel "IMT",
        "gloss" .

compcon:def_lexicalization a compcon:def ;
    rdfs:label "lexicalize, lexicalization" ;
    dc:description "the diachronic process by which a complex <a>morphosyntactic</a> structure develops an idiosyncratic <a>meaning</a>, and so comes to form one unit, in the sense of a pairing of a <a>form</a> and a unitary, unanalyzable meaning. <i>Example</i>: the English phrase <i>jack-in-the-pulpit</i> has lexicalized to denote a particular species of plant. There is usually an earlier stage where the <a>elements</a> of the <a>complex predicate</a> have an identifiable meaning, even if the meaning of the whole is idiosyncratic. For example <i>red-winged blackbird</i> describes a specific species of bird, but the phrase is partially analyzable in that the bird is mostly black but has a patch of red on its wings. A lexicalized structure may come to be fixed in the order of its elements, and altered or reduced in form, for example the farewell <i>goodbye</i> which originated in the phrase <i>God be with ye</i> (and has now been further reduced to <i>bye</i>). (Sections 13.1.2, 13.4)" ;
    dcterms:isPartOf "13.1.2",
        "13.4" .

compcon:def_metalanguage a compcon:def ;
    rdfs:label "metalanguage" ;
    dc:description "the language used for the free translation of an object language example. The free translation is intended to express the <a>meaning</a> of the object language example. However, in the absence of a theoretical language to describe the components of sentence meaning, linguists use another language, the language of the text (in our case, English), as the metalanguage. (Section 1.6)" ;
    dcterms:isPartOf "1.6" .

compcon:def_modification-predication-continuum a compcon:def ;
    rdfs:label "modification--predication continuum" ;
    dc:description "a continuum of <a>information packaging</a> functions from prototypical <a>modification</a> -- that is, <a>restrictive modification</a> -- to prototypical <a>predication</a>. The intermediate functions in this continuum are identified as (roughly, from most modifier-like to most predicate-like) <a inf:appositive-modification>appositive</a>, <a>complementative</a>, <a>depictive</a>, <a>resultative</a>, and <a>manner</a>. (Section 14.3)" ;
    dcterms:isPartOf "14.3" .

compcon:def_modification-reference-continuum a compcon:def ;
    rdfs:label "modification--reference continuum" ;
    dc:description "a continuum of <a>modification</a> from <a>anchoring</a> <a>nominal modifier constructions</a>, to <a cxn:typifying-construction>non-anchoring</a> <a>nominal modifier constructions</a>, to <a>property</a> <a>modification</a> and <a>selecting</a> <a>modification</a>, to a unitary <a>referent expression</a> (such as a <a>binominal lexeme</a>) formed etymologically from distinct <a inf:modification>modifying</a> and <a inf:reference>referring</a> concepts. (Section 5.2.4)" ;
    dcterms:isPartOf "5.2.4" .

compcon:def_morphology a compcon:def ;
    rdfs:label "morphology" ;
    dc:description "the analysis of the internal structure of words. <i>Example</i>: in <i>walk-ed</i>, the word has been analyzed into the verb root <i>walk</i> and the past tense suffix <i>-ed</i>. (Section 1.1)" ;
    dcterms:isPartOf "1.1" .

compcon:def_morphosyntax a compcon:def ;
    rdfs:label "morphosyntax, morphosyntactic" ;
    dc:description "the analysis of the internal structure of utterances, both above the word level and below it. <i>Example</i>: <i>three tree-s</i> is analyzed as the numeral modifier <i>three</i> combined with the head <i>tree-s</i>, which is made of the root <i>tree</i> and the plural suffix <i>-s</i>. (Section 1.1)" ;
    dcterms:isPartOf "1.1" .

compcon:def_object-language a compcon:def ;
    rdfs:label "object language" ;
    dc:description "in an example presented with an <a>interlinear morpheme translation</a>, the language that is the object of grammatical analysis, and hence the language of the example being analyzed. (Section 1.6)" ;
    dcterms:isPartOf "1.6" .

compcon:def_ontological-categories a compcon:def ;
    rdfs:label "ontological categories" ;
    dc:description "very broad semantic categories that play a role in distinguishing different types of pronouns and determiners. <i>Examples</i>: the ontological categories include: person, thing, place, time, quantity, and manner (this is not an exhaustive list). (Section 3.1.3)" ;
    dcterms:isPartOf "3.1.3" .

compcon:def_ontology a compcon:def ;
    rdfs:label "ontology" ;
    dc:description "a classification of concepts into their <a>semantic classes</a> or categories. (Section 2.1)" ;
    dcterms:isPartOf "2.1" .

compcon:def_particularizing a compcon:def ;
    rdfs:label "particularizing" ;
    dc:description "the process in the <a>verbalization of experience</a> in which a <a>common noun</a>, which denotes a semantic <u>category</u>, is used to refer to a specific <a>individual</a>. This process often involves accompanying forms, such as a <a>demonstrative attributive</a> or an <a>article</a>. Other verbalization processes involved in particularizing are <a>selecting</a> and <a>situating</a>. (Section 3.2)" ;
    dcterms:isPartOf "3.2" .

compcon:def_preferred-argument-structure a compcon:def ;
    rdfs:label "preferred argument structure" ;
    dc:description "the universal that the <a>participant</a> in the <a>A role</a> is, on average, higher on the <a>Accessibility Hierarchy</a> than the participant in the <a>P role</a> in a <a sem:bivalent>bivalent event</a>, and, in fact, the participant in the A role is usually <a>active</a>. (Section 8.1)" ;
    dcterms:isPartOf "8.1" .

compcon:def_prenominal-integration a compcon:def ;
    rdfs:label "Prenominal Integration" ;
    dc:description "the typological phenomenon that prenominal modifiers are more tightly integrated into the noun phrase than postnominal modifiers. (Section 5.3)" ;
    dcterms:isPartOf "5.3" .

compcon:def_role a compcon:def ;
    rdfs:label "role" ;
    dc:description "a <a>construction</a> consists of <a>elements</a>, each of which describes a role that expresses a particular <a>function</a> in a construction. <i>Example</i>: in the English Predicate Adjective Construction [<sc>Sbj</sc> <i>be</i> <sc>PredAdj</sc>], illustrated by <i>She is intelligent</i>, the label <sc>Sbj</sc> describes a role <a inf:reference>referring</a> to an <a sem:object-concept>object</a>, and <sc>PredAdj</sc> describes a role occurring after <i>be</i> that <a>predicates</a> a <a>property</a> of the object referred to in the Subject role. (Section 1.1)" ;
    dcterms:isPartOf "1.1" ;
    skos:altLabel "slot" .

compcon:def_semantic-classes a compcon:def ;
    rdfs:label "semantic classes" ;
    dc:description "a category of concepts. <i>Example</i>: humans are a semantic class. (Section 1.2.2)" ;
    dcterms:isPartOf "1.2.2" .

compcon:def_semantic-map a compcon:def ;
    rdfs:label "semantic map" ;
    dc:description "a representation of the set of functions in a <a>conceptual space</a> that are co-expressed by a particular form in a particular language, usually by a line encircling the set of functions in the conceptual space. A semantic map represents a language-specific <a>morphosyntactic</a> category. It is part of the <a>semantic map model</a>. (Section 3.5)" ;
    dcterms:isPartOf "3.5" .

compcon:def_semantic-map-model a compcon:def ;
    rdfs:label "semantic map model" ;
    dc:description "a model for representing semantic relationships among functions that are <a>co-expressed</a> across the world's languages. The semantic relationships form a <a>conceptual space</a> that is represented in a network (in mathematical terms, a graph). The co-expression of functions by a particular language form is represented by a <a>semantic map</a> encircling the nodes of the graph/network that represent the co-expressed functions. (Section 3.5)" ;
    dcterms:isPartOf "3.5" .

compcon:def_speech-act-situation a compcon:def ;
    rdfs:label "speech act situation" ;
    dc:description "the context in which speaking occurs. The speech act situation includes the roles of speaker and hearer; their spatial location; the time of the speech event; the shared knowledge of the speaker and hearer; and the social roles of the speaker and hearer and their social relation to each other. Many linguistic constructions have their <a>meanings</a> defined in whole or part by <a>elements</a> of the speech act situation -- in particular, <a>contextual</a> <a cxn:referent-expression>referring expressions</a>. (Section 3.1.1)" ;
    dcterms:isPartOf "3.1.1" .

compcon:def_structural-coding a compcon:def ;
    rdfs:label "structural coding" ;
    dc:description "the number of morphemes used to express the function of a <a>construction</a>. <i>Example</i>: in <i>Harry's car</i>, the clitic <i>-'s</i> expresses the function of <a>modification</a> by an <a>object concept</a>, and so is an example of structural coding of modification by one morpheme. (Section 2.5)" ;
    dcterms:isPartOf "2.5" .

compcon:def_syntax a compcon:def ;
    rdfs:label "syntax" ;
    dc:description "the analysis of the internal structure of utterances/sentences -- more specifically, how words are put together. <i>Example</i>: <i>the stolen succulents</i> has the syntax of a referring expression consisting of the head <i>succulents</i>, a preposed modifier <i>stolen</i> and the initial definite article <i>the</i>. (Section 1.1)" ;
    dcterms:isPartOf "1.1" .

compcon:def_token-frequency a compcon:def ;
    rdfs:label "token frequency" ;
    dc:description "the frequency of occurrence of specific <a>constructions</a> (morphemes, words, larger units) in language use, usually measured operationally as the frequency of occurrence in a particular corpus. (Section 2.5)" ;
    dcterms:isPartOf "2.5" .

compcon:def_transitivity-hierarchy a compcon:def ;
    rdfs:label "Transitivity Hierarchy" ;
    dc:description "a ranking of <a>events</a> by their likelihood to be expressed by the <a>transitive construction</a> (the <dq>more transitive</dq> end of the hierarchy) or a <a str:subject-oblique-strategy>subject--oblique</a> <a>construction</a> (the <dq>less transitive</dq> end of the hierarchy). (Section 7.3.3)" ;
    dcterms:isPartOf "7.3.3" .

compcon:def_translation a compcon:def ;
    rdfs:label "translation" ;
    dc:description "in an example presented with an <a>interlinear morpheme translation</a>, the <a>meaning</a> of an example from the language being analyzed, expressed in the <a>metalanguage</a> being used in the text. (Section 1.6)" ;
    dcterms:isPartOf "1.6" ;
    skos:altLabel "(free) translation" .

compcon:def_typology a compcon:def ;
    rdfs:label "typology" ;
    dc:description "an approach to the study of language that starts from the diversity of grammatical structures across the languages of the world, and derives general patterns found in that diversity. (Section 1.1)" ;
    dcterms:isPartOf "1.1" ;
    skos:altLabel "(linguistic) typology" .

compcon:def_verbalization a compcon:def ;
    rdfs:label "verbalization" ;
    dc:description "a model of how experiences are expressed in language developed by Chafe (1977 and later publications) and elaborated in Croft (2007a). In this book, one part of this model, <a>particularizing</a>, is described with respect to the structure of <a>referring phrases</a>. (Section 4.1.1)" ;
    dcterms:isPartOf "4.1.1" ;
    skos:altLabel "verbalization (of experience)" .

compcon:def_word-class a compcon:def ;
    rdfs:label "word class" ;
    dc:description "the set of words defined by their occurrence in a particular <a>role</a> in a <a>construction</a>. <i>Example</i>: in the English Predicate Adjective Construction [<sc>Sbj</sc> <i>be</i> <sc>PredAdj</sc>], illustrated by <i>She is intelligent</i>, the PredAdj role defines a word class consisting of all the words that can occur in that role in that construction (<i>happy</i>, <i>tall</i>, <i>asleep</i>, etc.). Word classes are defined by a set of constructions that the words occur in (<i>a.k.a.</i> distribution, behavior, properties). (Section 1.2.1)" ;
    dcterms:isPartOf "1.2.1" ;
    skos:altLabel "behavior",
        "distribution",
        "part(s) of speech",
        "properties",
        "syntactic category" .

compcon:functionOf a owl:ObjectProperty ;
    rdfs:label "Function of" .

compcon:hasCompCon a owl:ObjectProperty ;
    rdfs:label "Comparative Concept" .

compcon:inf_aboutness a compcon:inf ;
    rdfs:label "about, aboutness" ;
    cx:hasExample compcon:aboutness-example-0 ;
    dc:description "the relation between what is expressed in a <a cxn:topic-comment-construction>topic--comment</a> sentence and the <a>referent</a> or <a inf:reference>topic</a> that the comment or <a>predication</a> is predicated of. <i>Example</i>: in <i>The coyote ran across the lawn</i>, the sentence is about the coyote. Aboutness is intended to capture the notion that an utterance is relevant to <q>a matter of standing interest or concern</q> (Strawson 1964:97). (Section 11.2.1)" ;
    dcterms:isPartOf "11.2.1" ;
    compcon:subtypeOf compcon:inf_discourse-structure .

compcon:inf_active-referent a compcon:inf ;
    rdfs:label "active referent" ;
    dc:description "a <a>referent</a> whose <u>discourse file</u> has been opened and which is at the center of the hearer's consciousness. This is the highest <a>accessibility</a> referent at the current point in the discourse. (Section 3.3.1)" ;
    dcterms:isPartOf "3.3.1" ;
    skos:altLabel "active",
        "in focus" ;
    compcon:functionOf compcon:cxn_anaphoric-pronoun,
        compcon:cxn_definite-pronoun ;
    compcon:subtypeOf compcon:inf_reference .

compcon:inf_actor a compcon:inf ;
    rdfs:label "actor" ;
    dc:description "the main players in the actions reported in the discourse. Actors are typically <a>human</a> or <a>animate</a> (especially personified animals), are referred to multiple times in the discourse, and are often introduced by special <a>constructions</a>. The term <q>actor</q> is also used for the <a>active category</a>. (Section 3.4.1)" ;
    dcterms:isPartOf "3.4.1" ;
    compcon:functionOf compcon:cxn_pragmatically-specific-article .

compcon:inf_admodification a compcon:inf ;
    rdfs:label "admodification" ;
    cx:hasExample compcon:admodification-example-0 ;
    dc:description "the <a>information packaging</a> function of qualifying a <a>modifier</a>, usually via semantically expressing <a>degree</a> or hedging of the <a>property</a> denoted by the modifier. <i>Example</i>: in <i>very slow</i>, the degree <a>admodifier</a> <i>very</i> intensifies the value of the speed dimension denoted by <i>slow</i>. (Section 2.2.2)" ;
    dcterms:isPartOf "2.2.2" ;
    compcon:functionOf compcon:cxn_admodifier ;
    compcon:subtypeOf compcon:inf_discourse-structure .

compcon:inf_alternative-proposition a compcon:inf ;
    rdfs:label "alternative proposition" ;
    cx:hasExample compcon:alternative-proposition-example-0 ;
    dc:description "when a <a>contrast</a> situation is construed as an <a>identificational construction</a>, the <a>propositional content</a> is asserted against a background of an alternative <a>proposition</a> (or propositions) expressed or evoked in the discourse context. <i>Example</i>: in the exchange <i>John bought apples. No, he bought PEACHES</i>, the proposition that John bought peaches is asserted against the background of the alternative proposition that John bought apples. The shared part of the proposition and its alternative, that John bought something, is presupposed -- that is, <q>John bought X</q> is a <a>presupposed open proposition</a>. (Section 11.4.1)" ;
    dcterms:isPartOf "11.4.1" ;
    compcon:subtypeOf compcon:inf_discourse-structure .

compcon:inf_alternative-question a compcon:inf ;
    rdfs:label "alternative question" ;
    cx:hasExample compcon:alternative-question-example-0 ;
    dc:description "an interrogative in which the speaker offers a closed list of alternatives to fill in the unknown piece of information in the propositional content. <i>Example</i>: <i>Do you prefer beer or wine?</i> is an instance of an alternative question construction, where the alternatives offered are beer and wine. (Section 12.3.1)" ;
    dcterms:isPartOf "12.3.1" ;
    skos:altLabel "alternative question (function)" ;
    compcon:functionOf compcon:cxn_alternative-question-construction ;
    compcon:subtypeOf compcon:inf_interrogative .

compcon:inf_anchor a compcon:inf ;
    rdfs:label "anchor" ;
    cx:hasExample compcon:anchor-example-0 ;
    dc:description "an <a sem:object-concept>object</a> that, if its identity is known to speaker and hearer, allows for the identity of a related object to be known to the speaker and hearer. <i>Example</i>: <q>knowing who Peter is we can identify Peter's bag, arm, brother</q> (Koptjevskaja-Tamm 2002:147) -- i.e. Peter serves as the anchor for identifying the bag, arm, or brother. The anchoring function requires that the <a inf:modification>modifying</a> <a sem:object-concept>object</a> concept denote an <a>individual</a> and not a <a>type</a>, and preferably a highly <a>accessible</a> individual. Another term used for <q>anchor</q> is <q>reference point</q>, but the latter term has another use in this book. (Section 5.2.1)" ;
    dcterms:isPartOf "5.2.1" .

compcon:inf_anchoring a compcon:inf ;
    rdfs:label "anchoring" ;
    dc:description "a type of <a>situating</a> in which the <a>referent</a> of the <a sem:object-concept>object</a> <a>modifier</a> serves to identify the referent of the <a>head</a> of the <a>modification construction</a>. (Sections 4.1.1, <b>4.1.4</b>)" ;
    dcterms:isPartOf "4.1.1",
        "4.1.4" ;
    compcon:functionOf compcon:cxn_anchoring-construction,
        compcon:cxn_partitive-construction ;
    compcon:subtypeOf compcon:inf_modification .

compcon:inf_announcement a compcon:inf ;
    rdfs:label "announcement" ;
    cx:hasExample compcon:announcement-example-0 ;
    dc:description "a discourse context which tends to favor a <a inf:thetic>thetic</a> construal. An announcement is an <dq>out of the blue</dq>, usually unexpected and salient reporting of a situation. <i>Example</i>: <i>TRUMP was elected!</i> (with accent on <i>Trump</i>), uttered on November 9, 2016, the day after the Presidential election, is an announcement. (Section 11.3.1)" ;
    dcterms:isPartOf "11.3.1" ;
    compcon:subtypeOf compcon:inf_event-central .

compcon:inf_appositive-modification a compcon:inf ;
    rdfs:label "appositive modification" ;
    cx:hasExample compcon:appositive-modification-example-0 ;
    dc:description "an <a>information packaging</a> <a>function</a> in which the modifying <a>stative</a> concept does not narrow the set of possible <a>referents</a> of the <a>object concept</a> it modifies in a <a>referring phrase</a>; it simply adds a further description of the referent. <i>Example</i>: in <i>The angry young men left the party</i>, <i>angry</i> is an appositive modifier in the context in which it is adding a further description to the young men leaving the party, rather than specifying a subset of the young men, as with <a>restrictive modification</a>. (Section 14.3)" ;
    dcterms:isPartOf "14.3" ;
    skos:altLabel "nonrestrictive modification" ;
    compcon:functionOf compcon:cxn_apposition ;
    compcon:subtypeOf compcon:inf_subcategorizing .

compcon:inf_argument a compcon:inf ;
    rdfs:label "argument" ;
    cx:hasExample compcon:argument-example-0 ;
    dc:description "a <a>referent</a> of which something is being <a>predicated</a>. <i>Example</i>: in <i>Masha is nice</i>, being nice is being predicated of the referent Masha, and hence the referent Masha is an argument. Most referents are also arguments, but it is possible for a referent to <dq>stand alone</dq> in discourse, particularly in spoken discourse, independent of any predication. Arguments are divided into <a>core arguments</a> (<a>subject</a> and <a inf:object-argument>object</a>) and <a inf:oblique-argument>oblique arguments</a>. (Sections 2.1, 6.1.1)" ;
    dcterms:isPartOf "2.1",
        "6.1.1" .

compcon:inf_background-description a compcon:inf ;
    rdfs:label "background description" ;
    cx:hasExample compcon:background-description-example-0 ;
    dc:description "scene-setting information for a narrative which functions as the foreground. A background description is a discourse context which (weakly) tends to favor a <a inf:thetic>thetic</a> construal. <i>Example</i>: <i>DOGS were running in the yard</i> can function as a background description for telling a narrative. (Section 11.3.1)" ;
    dcterms:isPartOf "11.3.1" ;
    compcon:subtypeOf compcon:inf_event-central .

compcon:inf_choosing-contrast a compcon:inf ;
    rdfs:label "choosing contrast" ;
    cx:hasExample compcon:choosing-contrast-example-0 ;
    dc:description "a subtype of <a>counterpresuppositional contrast</a> in which some semantic content is chosen from a list of alternatives. <i>Example</i>: in the exchange <i>Would you like coffee or tea? COFFEE, please</i>, the discourse context presents two alternatives (coffee or tea), and the respose chooses one of the alternatives (coffee). (Dik 1997 proposes the term <q>selecting</q>, but this term is used here with a different meaning.) (Section 11.4.1)" ;
    dcterms:isPartOf "11.4.1" ;
    compcon:subtypeOf compcon:inf_counterpresuppositional-contrast .

compcon:inf_comment a compcon:inf ;
    rdfs:label "comment" ;
    cx:hasExample compcon:comment-example-0 ;
    dc:description "in a <a>topic--comment information packaging</a>, the comment is the information that is predicated of the <a inf:high-topicality>topic</a>. <i>Example</i>: in <i>Bill is a teacher</i>, being a teacher is the comment that is predicated of Bill. Comment is basically synonymous with <a>predication</a>. (Section 10.1.2)" ;
    dcterms:isPartOf "10.1.2" .

compcon:inf_comparative-referent a compcon:inf ;
    rdfs:label "comparative referent" ;
    cx:hasExample compcon:comparative-referent-example-0 ;
    dc:description "an unspecified <a>referent</a> occurring in the <a>standard</a> of comparison in a <a>comparative construction</a>. <i>Example</i>: in <i>The boy runs as fast as anyone in his class</i>, <i>anyone</i> is a comparative pronoun expressing a hypothetical referent selected from the class representing the standard to which the boy's running is being compared. (Section 3.5)" ;
    dcterms:isPartOf "3.5" ;
    compcon:functionOf compcon:cxn_comparative-pronoun ;
    compcon:subtypeOf compcon:inf_nonspecific-referent .

compcon:inf_complementative a compcon:inf ;
    rdfs:label "complementative" ;
    cx:hasExample compcon:complementative-example-0 ;
    dc:description "an <a>information packaging</a> <a>function</a> in which specification of the modifying <a>stative</a> concept is required by the <a>predicate</a>. <i>Example</i>: in English <i>I consider John intelligent</i>, the property of intelligence is necessarily specified of the participant John by the main predicate of attributing a property to a participant. (Section 14.3)" ;
    dcterms:isPartOf "14.3" ;
    skos:altLabel "predicative complement" ;
    compcon:subtypeOf compcon:inf_subcategorizing .

compcon:inf_complex-figure a compcon:inf ;
    rdfs:label "complex figure" ;
    cx:hasExample compcon:complex-figure-example-0 ;
    dc:description "a construal or <a>information packaging</a> of two <a>events</a> such that the two events are construed as parts of a complex whole. <i>Example</i>: in <i>Jim was promoted and Cindy quit</i>, the events of Jim being promoted and Cindy quitting are conceived of as parts of a complex whole (e.g. a causal sequence). A complex figure packaging construes the events as equal in status, and also requires a common denominator for the two events. This information packaging is found in <a>coordinate constructions</a>. (Section 15.1.3)" ;
    dcterms:isPartOf "15.1.3" ;
    skos:altLabel "symmetric" ;
    compcon:functionOf compcon:cxn_coordinate-construction ;
    compcon:subtypeOf compcon:inf_discourse-connection .

compcon:inf_conditional-referent a compcon:inf ;
    rdfs:label "conditional referent" ;
    cx:hasExample compcon:conditional-referent-example-0 ;
    dc:description "an unspecified <a>referent</a> in the <a cxn:protasis-clause>protasis</a> in a <a>conditional construction</a>. <i>Example</i>: in <i>If you hear <b>anything</b>, tell me</i>, <i>anything</i> is a conditional pronoun expressing a referent that is found only in the hypothetical world introduced by the protasis of the conditional construction. (Section 3.5)" ;
    dcterms:isPartOf "3.5" ;
    compcon:functionOf compcon:cxn_conditional-pronoun ;
    compcon:subtypeOf compcon:inf_nonspecific-referent .

compcon:inf_core-arguments a compcon:inf ;
    rdfs:label "core arguments" ;
    cx:hasExample compcon:core-arguments-example-0,
        compcon:core-arguments-example-1 ;
    dc:description "the most <a>salient</a> <a>arguments</a> associated with a <a>predication</a>. <i>Example</i>: in <i>Sally threw the letter into the wastebasket</i>, Sally and the letter are construed as the most salient arguments; in <i>The letter was thrown into the wastebasket</i>, only the letter is construed as a salient argument. When there are two core arguments, the more salient argument is the <a>subject</a> and the less salient argument is the <a inf:object-argument>object</a>. Core arguments are expressed by <a>core argument phrases</a>. (Section 6.1.1)" ;
    dcterms:isPartOf "6.1.1" .

compcon:inf_different-subject a compcon:inf ;
    rdfs:label "different-subject" ;
    cx:hasExample compcon:different-subject-example-0 ;
    dc:description "in <a>complex sentence constructions</a> that express <a>reference tracking</a>, the situation where the <a>salient</a> <a>participant</a> in the current <a>clause</a> is indicated as not <a>coreferential</a> with the salient participant in another clause in the construction. The salient participant is typically, but not always, encoded as the <a>subject</a> of the <a>predicate</a>. <i>Example</i>: in <i>Harry having finished preparing the salad, Bill brought it to the table</i>, the overt referring phrase <i>Harry</i> in <i>Harry having finished preparing the salad</i> signals that the subject referent of finishing the salad is not coreferential with the subject referent of the <a>matrix clause</a> -- namely, Bill. (Section 16.1)" ;
    dcterms:isPartOf "16.1" ;
    skos:altLabel "DS",
        "different subject" ;
    compcon:subtypeOf compcon:inf_token-identity .

compcon:inf_direct-negation-referent a compcon:inf ;
    rdfs:label "direct negation referent" ;
    cx:hasExample compcon:direct-negation-referent-example-0 ;
    dc:description "an unspecified <a>referent</a> which is in the scope of <a>negation</a> in the same <a>clause</a>. <i>Example</i>: in <i>I noticed nothing</i>, <i>nothing</i> is a direct negation pronoun expressing a referent found only in the negative alternative world to the real world. (Section 3.5)" ;
    dcterms:isPartOf "3.5" ;
    compcon:functionOf compcon:cxn_direct-negation-pronoun ;
    compcon:subtypeOf compcon:inf_nonspecific-referent .

compcon:inf_discourse-coherence a compcon:inf ;
    rdfs:label "discourse coherence" ;
    dc:description "" ;
    skos:altLabel "discourse relation" ;
    compcon:functionOf compcon:cxn_complex-sentence ;
    compcon:subtypeOf compcon:inf_discourse-structure .

compcon:inf_discourse-deixis a compcon:inf ;
    rdfs:label "discourse deixis" ;
    dc:description "<a>reference</a> to <a>events</a> or <a>propositions</a> already described in the discourse, in contrast to reference to objects. (Section 3.3.3)" ;
    dcterms:isPartOf "3.3.3" ;
    compcon:functionOf compcon:cxn_definite-pronoun ;
    compcon:subtypeOf compcon:inf_reference .

compcon:inf_ditransitive-predication a compcon:inf ;
    rdfs:label "ditransitive predication" ;
    cx:hasExample compcon:ditransitive-predication-example-0 ;
    dc:description "a <a>predication</a> predicated of three <a>salient</a> <a inf:reference>arguments</a>. <i>Example</i>: in <i>Sarah sent Jerry a letter</i>, sending is a ditransitive predication because it is predicated of three arguments, Sarah, Jerry, and the letter. (Section 6.1.2)" ;
    dcterms:isPartOf "6.1.2" ;
    compcon:subtypeOf compcon:inf_predication .

compcon:inf_equational a compcon:inf ;
    rdfs:label "equational" ;
    cx:hasExample compcon:equational-example-0 ;
    dc:description "a type of <a>identificational information packaging</a> in which two <a>referents</a> that the hearer assumed were different individuals are asserted to be, in fact, one and the same individual. <i>Example</i>: in <i>The Morning Star is the Evening Star</i>, it is asserted that two celestial objects that were once thought to be distinct objects (and given distinct names) are one and the same, namely the planet Venus. (Section 10.1.2)" ;
    dcterms:isPartOf "10.1.2" ;
    compcon:functionOf compcon:cxn_equational-construction ;
    compcon:subtypeOf compcon:inf_information-gap .

compcon:inf_exclamative a compcon:inf ;
    rdfs:label "exclamative" ;
    cx:hasExample compcon:exclamative-example-0 ;
    dc:description "a <a inf:speech-acts>speech act</a> which expresses a strong emotional reaction to the <a>propositional content</a> that it conveys. More precisely, the exclamative speech act expresses the speaker's surprise toward the <a>degree</a> of a scalar <a>property</a> contained in the propositional content of the speech act; the rest of the propositional content consists of a <a>presupposed open proposition</a>. <i>Example</i>: <i>What a beautiful house!</i> is an instance of an English exclamative construction. (Sections 12.1, 12.5)" ;
    dcterms:isPartOf "12.1",
        "12.5" ;
    compcon:functionOf compcon:cxn_exclamative-construction ;
    compcon:subtypeOf compcon:inf_speech-acts .

compcon:inf_expanding-contrast a compcon:inf ;
    rdfs:label "expanding contrast" ;
    cx:hasExample compcon:expanding-contrast-example-0 ;
    dc:description "a subtype of <a>counterpresuppositional contrast</a> in which the sentence rejects a component of a prior assertion by adding something else to the rejected component, rather than simply replacing it. <i>Example</i>: in the exchange <i>John bought apples. He also bought PEACHES</i>, the speaker rejects that what John bought was just apples, and adds peaches to what John bought. <i>Also</i> is an additive focus operator. (Section 11.4.1)" ;
    dcterms:isPartOf "11.4.1" ;
    compcon:subtypeOf compcon:inf_counterpresuppositional-contrast .

compcon:inf_explanation a compcon:inf ;
    rdfs:label "explanation" ;
    cx:hasExample compcon:explanation-example-0,
        compcon:explanation-example-1 ;
    dc:description "a discourse context which tends to favor a <a inf:thetic>thetic</a> construal. The fact that something happened is presupposed, and the statement explains or elaborates what happened. <i>Example</i>: <i>My CAR broke down</i> -- as, for instance, a response to the question <q>What happened?</q> -- is an example of an explanation. (Section 11.3.1)" ;
    dcterms:isPartOf "11.3.1" ;
    compcon:subtypeOf compcon:inf_event-central .

compcon:inf_figure-ground a compcon:inf ;
    rdfs:label "figure--ground" ;
    cx:hasExample compcon:figure-ground-example-0 ;
    dc:description "a construal or <a>information packaging</a> of two <a>events</a> such that one event (the <a>ground</a>) serves as the reference point for the other event (the <a>figure</a>). <i>Example</i>: in <i>Cindy quit after Jim was promoted</i>, the two events are in a figure--ground packaging, such that Jim's being promoted serves as the reference point for the time of Cindy's quitting. This is an extension of the figure--ground construal beyond spatial relations. This information packaging is found in <a>adverbial clause constructions</a>. (Section 15.1.3)" ;
    dcterms:isPartOf "15.1.3" ;
    skos:altLabel "asymmetric" ;
    compcon:functionOf compcon:cxn_adverbial-clause-construction ;
    compcon:subtypeOf compcon:inf_discourse-connection .

compcon:inf_focus a compcon:inf ;
    rdfs:label "focus" ;
    cx:hasExample compcon:focus-example-0 ;
    dc:description "in <a>identificational information packaging</a>, the information which is identified as the <dq>filler</dq> for the open part of the <a>presupposed open proposition</a>. <i>Example</i>: in <i>It was Jack who stole my cookies!</i>, the presupposed open proposition is <q>X stole my cookies</q>, and the filler of X is Jack -- i.e. X = Jack. The focus may be any part of the information in the clause, not just an argument. The term <q>focus construction</q> is sometimes used as a synonym for <q>identificational construction</q>, but we avoid that usage here. (Section 11.4.1)" ;
    dcterms:isPartOf "11.4.1" ;
    compcon:subtypeOf compcon:inf_discourse-structure .

compcon:inf_free-choice-referent a compcon:inf ;
    rdfs:label "free choice referent" ;
    cx:hasExample compcon:free-choice-referent-example-0 ;
    dc:description "an unspecified <a>referent</a> in certain contexts, whose identity can be freely chosen without affecting the truth value of the utterance. <i>Example</i>: in <i>After the fall of the Wall, East Germans were free to travel anywhere</i>, <i>anywhere</i> is a free choice pronoun expressing a referent -- a place -- toward which the agent in the clause, the East Germans, is free to choose to travel. (Section 3.5)" ;
    dcterms:isPartOf "3.5" ;
    compcon:functionOf compcon:cxn_free-choice-pronoun,
        compcon:cxn_free-modification-construction ;
    compcon:subtypeOf compcon:inf_nonspecific-referent .

compcon:inf_generic-referent a compcon:inf ;
    rdfs:label "generic referent" ;
    cx:hasExample compcon:generic-referent-example-0 ;
    dc:description "<a>reference</a> to the <a>type</a> itself, and not to a particular <a>referent</a>/<a>token</a> of the type. <i>Example</i>: in the generic use of <i>You can't come to the party without an invitation, you</i> is referring to the general category of people. (Sections 3.1.1, <b>3.6</b>)" ;
    dcterms:isPartOf "3.1.1",
        "3.6" ;
    compcon:functionOf compcon:cxn_generic-pronoun,
        compcon:cxn_indefinite-pronoun ;
    compcon:subtypeOf compcon:inf_reference .

compcon:inf_high-accessibility a compcon:inf ;
    rdfs:label "high accessibility" ;
    dc:description "" ;
    compcon:subtypeOf compcon:inf_accessibility .

compcon:inf_highest-topicality a compcon:inf ;
    rdfs:label "highest topicality" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_topic-phrase ;
    compcon:subtypeOf compcon:inf_high-topicality .

compcon:inf_identity-known a compcon:inf ;
    rdfs:label "identity known" ;
    dc:description "" ;
    compcon:subtypeOf compcon:inf_identifiability .

compcon:inf_identity-unknown a compcon:inf ;
    rdfs:label "identity unknown" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_anaphoric-head-construction,
        compcon:cxn_indefinite-article ;
    compcon:subtypeOf compcon:inf_identifiability .

compcon:inf_inactive-referent a compcon:inf ;
    rdfs:label "inactive referent" ;
    dc:description "a <a>referent</a> for which the speaker and hearer have a <u>discourse file</u> (also described as: the referent is in the speaker's long-term memory) but which generally has not been activated in the discourse, at least not recently. (Section 3.3.1)" ;
    dcterms:isPartOf "3.3.1" ;
    skos:altLabel "inactive",
        "noncontaining inferrable" ;
    compcon:functionOf compcon:cxn_definite-pronoun ;
    compcon:subtypeOf compcon:inf_reference .

compcon:inf_indirect-negation-referent a compcon:inf ;
    rdfs:label "indirect negation referent" ;
    cx:hasExample compcon:indirect-negation-referent-example-0 ;
    dc:description "an unspecified <a>referent</a> which is in a <a>clause</a> embedded in a <b>negated</b> clause. <i>Example</i>: in <i>I don't think that anybody has seen it</i>, <i>anybody</i> is an indirect negation pronoun expressing a referent that is found only in the negated <dq>world</dq> of the speaker's beliefs. (Section 3.5)" ;
    dcterms:isPartOf "3.5" ;
    compcon:functionOf compcon:cxn_indirect-negation-pronoun ;
    compcon:subtypeOf compcon:inf_nonspecific-referent .

compcon:inf_inferrable-referent a compcon:inf ;
    rdfs:label "inferrable referent" ;
    dc:description "a <a>referent</a> whose identity can be inferred via the <a>type</a> described by the <a>referring phrase</a>. Example: in <i>the bowl of noodles on the kitchen counter</i>, the identity of the referent can be inferred from the type description (is a bowl, contains noodles, is on the kitchen counter) even if the referent has not previously been mentioned. Inferrable referents are very similar to <a>inactive</a> referents (which are sometimes called noncontaining inferrables) and may not be clearly distinguishable from them. (Section 3.3.1)" ;
    dcterms:isPartOf "3.3.1" ;
    skos:altLabel "(containing) inferrable" ;
    compcon:functionOf compcon:cxn_definite-pronoun ;
    compcon:subtypeOf compcon:inf_reference .

compcon:inf_information-question a compcon:inf ;
    rdfs:label "information question" ;
    cx:hasExample compcon:information-question-example-0 ;
    dc:description "an <a inf:interrogative>interrogative</a> in which the unknown piece of the <a>propositional content</a> requested of the addressee is a semantic component of the <a>proposition</a> other than its <a>polarity</a>. <i>Example</i>: <i>Who is coming?</i> is an instance of the English information question construction, expecting an answer identifying the person(s) who is/are coming. (Section 12.3.1)" ;
    dcterms:isPartOf "12.3.1" ;
    compcon:functionOf compcon:cxn_information-question-construction ;
    compcon:subtypeOf compcon:inf_interrogative .

compcon:inf_information-question-response a compcon:inf ;
    rdfs:label "information question response" ;
    cx:hasExample compcon:information-question-response-example-0,
        compcon:information-question-response-example-1,
        compcon:information-question-response-example-2,
        compcon:information-question-response-example-3 ;
    dc:description "the answer to an <a inf:information-question>information question</a>. <i>Example</i>: the answer to the English information question <i>Who is coming?</i> could be <i>Sandra is coming, Sandra is</i>, or just <i>Sandra</i>. (Section 12.3.3)" ;
    dcterms:isPartOf "12.3.3" ;
    skos:altLabel "information (question) response" ;
    compcon:functionOf compcon:cxn_information-question-response-construction ;
    compcon:subtypeOf compcon:inf_response .

compcon:inf_interrogative-unit a compcon:inf ;
    rdfs:label "interrogative unit" ;
    dc:description "" ;
    skos:altLabel "interrogative (information) unit" ;
    compcon:functionOf compcon:cxn_interrogative-pronoun ;
    compcon:subtypeOf compcon:inf_reference .

compcon:inf_interruption a compcon:inf ;
    rdfs:label "interruption" ;
    cx:hasExample compcon:interruption-example-0 ;
    dc:description "a discourse context which tends to favor a <a inf:thetic>thetic</a> construal. Something in the ambient environment of the discourse becomes salient enough to interrupt the conversational interaction. <i>Example</i>: <i>The PHONE's ringing!</i> in response to that event in the context is an interruption. (Section 11.3.1)" ;
    dcterms:isPartOf "11.3.1" ;
    compcon:subtypeOf compcon:inf_event-central .

compcon:inf_intransitive-predication a compcon:inf ;
    rdfs:label "intransitive predication" ;
    cx:hasExample compcon:intransitive-predication-example-0 ;
    dc:description "a <a>predication</a> predicated of one <a>salient</a> <a inf:reference>argument</a>. <i>Example</i>: in <i>Sarah is swimming</i>, swimming is an intransitive predication because it is predicated of one argument, Sarah. (Section 6.1.2)" ;
    dcterms:isPartOf "6.1.2" ;
    compcon:subtypeOf compcon:inf_predication .

compcon:inf_irrealis-referent a compcon:inf ;
    rdfs:label "irrealis referent" ;
    cx:hasExample compcon:irrealis-referent-example-0 ;
    dc:description "a <a>referent</a> which is in the <dq>world</dq> or <a>mental space</a> representing a person's desire, wish, command, etc. <i>Example</i>: in <i>Visit me sometime</i>, <i>sometime</i> is an irrealis <a>pronoun</a> expressing an irrealis referent -- a time only found in the hoped-for mental space of the speaker's offer. (Section 3.5)" ;
    dcterms:isPartOf "3.5" ;
    skos:altLabel "irrealis (referent)" ;
    compcon:functionOf compcon:cxn_free-modification-construction,
        compcon:cxn_irrealis-pronoun ;
    compcon:subtypeOf compcon:inf_nonspecific-referent .

compcon:inf_link a compcon:inf ;
    rdfs:label "link" ;
    cx:hasExample compcon:link-example-0 ;
    dc:description "a concept expressed in a sentence that is linked to a <a>trigger</a> concept previously evoked, and forms a <a>poset</a> with the trigger (and other potentially evoked concepts). <i>Example</i>: in the exchange <i>Do you like this album? Yeah, this song I really like</i>, the song mentioned in the second turn serves as the link to the album which is the trigger mentioned in the first turn. (Section 11.2.3)" ;
    dcterms:isPartOf "11.2.3" ;
    compcon:subtypeOf compcon:inf_reference .

compcon:inf_listing-contrast a compcon:inf ;
    rdfs:label "listing contrast" ;
    cx:hasExample compcon:listing-contrast-example-0 ;
    dc:description "a subtype of <a>parallel contrast</a> in which the two propositions are identical except for the contrasting parts, and the contrasting parts are construed as belonging in a set relationship -- that is, they are members of a poset. <i>Example</i>: in <i>He brought back all the goods, and he also brought back his kinsman Lot and his goods</i>, the second clause is identical except for the members of the poset {all the goods, his kinsman Lot and his goods}. (Section 11.4.1)" ;
    dcterms:isPartOf "11.4.1" ;
    compcon:subtypeOf compcon:inf_parallel-contrast .

compcon:inf_logophoric-subject-identity a compcon:inf ;
    rdfs:label "logophoric subject identity" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_logophoric-construction ;
    compcon:subtypeOf compcon:inf_token-identity .

compcon:inf_low-accessibility a compcon:inf ;
    rdfs:label "low accessibility" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_nonanaphoric-definite-article ;
    compcon:subtypeOf compcon:inf_accessibility .

compcon:inf_low-topicality a compcon:inf ;
    rdfs:label "low topicality" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_oblique-phrase ;
    compcon:subtypeOf compcon:inf_non-high-topicality,
        compcon:inf_topicality .

compcon:inf_medium-accessibility a compcon:inf ;
    rdfs:label "medium accessibility" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_anaphoric-article ;
    compcon:subtypeOf compcon:inf_accessibility .

compcon:inf_mid-topicality a compcon:inf ;
    rdfs:label "mid topicality" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_object-phrase ;
    compcon:subtypeOf compcon:inf_non-high-topicality,
        compcon:inf_non-low-topicality,
        compcon:inf_topicality .

compcon:inf_negative-question-polarity-response a compcon:inf ;
    rdfs:label "negative question polarity response" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_negative-question-polarity-response-construction ;
    compcon:subtypeOf compcon:inf_polarity-response .

compcon:inf_nonverbal-contrast a compcon:inf ;
    rdfs:label "nonverbal contrast" ;
    cx:hasExample compcon:nonverbal-contrast-example-0 ;
    dc:description "a subtype of <a>parallel contrast</a> in which the verbs (or more generally, <a>predicates</a>) are identical or virtually synonymous, and there are at least two sets of parallel nonverbal components that are members of <a>posets</a>. <i>Example</i>: in <i>and Joseph called the name of the elder [of his two sons] Manasseh...and he called the name of the second Ephraim</i>, the verbs <q>call</q> are identical, and there are two sets of parallel nonverbal components that differ and form posets: {the name of the elder son, the name of the second son} and {Manasseh, Ephraim}. (Section 11.4.1)" ;
    dcterms:isPartOf "11.4.1" ;
    compcon:subtypeOf compcon:inf_parallel-contrast .

compcon:inf_object-argument a compcon:inf ;
    rdfs:label "object argument" ;
    cx:hasExample compcon:object-argument-example-0 ;
    dc:description "a <a>core argument</a> that is less salient that the <a>subject argument</a> of the same <a>predication</a>. <i>Example</i>: in <i>Sally threw the letter into the wastebasket</i>, the letter is a core argument but it is less salient than Sally. (Sections 6.1.1, <b>6.3.2</b>)" ;
    dcterms:isPartOf "6.1.1",
        "6.3.2" .

compcon:inf_object-identity a compcon:inf ;
    rdfs:label "object identity" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_object-identity-construction ;
    compcon:subtypeOf compcon:inf_token-identity .

compcon:inf_oblique-argument a compcon:inf ;
    rdfs:label "oblique argument" ;
    cx:hasExample compcon:oblique-argument-example-0 ;
    dc:description "an <a>argument</a> that is less salient than the <a>core arguments</a> of the same <a>predication</a>. <i>Example</i>: in <i>Sally threw the letter into the wastebasket</i>, <i>the wastebasket</i> is an oblique argument; it is less salient than Sally or the letter. (Sections 6.1.1, <b>6.2.1</b>)" ;
    dcterms:isPartOf "6.1.1",
        "6.2.1" .

compcon:inf_polarity-question a compcon:inf ;
    rdfs:label "polarity question" ;
    dc:description "an <a inf:interrogative>interrogative</a> in which the unknown piece of the <a>propositional content</a> requested of the addressee is the <a>polarity</a> (<a>positive</a> or <a>negative</a>) of the proposition. (Section 12.3.1)" ;
    dcterms:isPartOf "12.3.1" ;
    skos:altLabel "polarity question (function)" ;
    compcon:functionOf compcon:cxn_polarity-question-construction ;
    compcon:subtypeOf compcon:inf_interrogative .

compcon:inf_poset a compcon:inf ;
    rdfs:label "poset" ;
    cx:hasExample compcon:poset-example-0 ;
    dc:description "a partially ordered set of discourse entities. The partial ordering is defined by some sort of semantic relation, such as part--whole or type--subtype. <i>Example</i>: in the exchange <i>Do you like this album? Yeah, this song I really like</i>, the album mentioned in the first turn and the song mentioned in the second turn are members of a poset which is defined by a <a>part--whole relation</a> (the album is the whole, and the song is a part of the album). (Section 11.2.3)" ;
    dcterms:isPartOf "11.2.3" ;
    compcon:subtypeOf compcon:inf_discourse-structure .

compcon:inf_positive-question-polarity-response a compcon:inf ;
    rdfs:label "positive question polarity response" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_positive-question-polarity-response-construction ;
    compcon:subtypeOf compcon:inf_polarity-response .

compcon:inf_pragmatic-assertion a compcon:inf ;
    rdfs:label "pragmatic assertion" ;
    cx:hasExample compcon:pragmatic-assertion-example-0 ;
    dc:description "the <a>information</a> added to the discourse context when a <u>sentence</u> is uttered, or, more precisely, <q>the proposition expressed by a sentence which the hearer is expected to know or take for granted as a result of hearing the sentence uttered</q> (Lambrecht 1994:52). <i>Example</i>: in <i>Alarms ringing, the burglar fled</i>, the <a>clause</a> <i>the burglar fled</i> is pragmatically asserted. The criterion typically used for pragmatic assertion is that the content of a sentence can be negated or questioned, though other criteria are sometimes used, such as hedging. (Sections 13.2.1, <b>15.1.2</b>)" ;
    dcterms:isPartOf "13.2.1",
        "15.1.2" ;
    skos:altLabel "pragmatically asserted" ;
    compcon:functionOf compcon:cxn_main-clause ;
    compcon:subtypeOf compcon:inf_discourse-structure .

compcon:inf_pragmatic-presupposition a compcon:inf ;
    rdfs:label "pragmatic presupposition" ;
    cx:hasExample compcon:pragmatic-presupposition-example-0 ;
    dc:description "the set of <a>propositions</a> evoked by the <a>constructions</a> in a sentence <q>which the speaker assumes the hearer already knows or is ready to take for granted at the time the sentence is uttered</q> (Lambrecht 1994:52). <i>Example</i>: in <i>I'm grateful that you finished cleaning the house</i>, the proposition that you finished cleaning the house is presupposed, while the proposition that I am grateful about it is <a inf:pragmatic-assertion>asserted</a>. (Section 11.4.1)" ;
    dcterms:isPartOf "11.4.1" ;
    skos:altLabel "pragmatically non-asserted",
        "presuppose",
        "presupposition" ;
    compcon:functionOf compcon:cxn_adverbial-dependent-clause,
        compcon:cxn_subordinate-clause ;
    compcon:subtypeOf compcon:inf_discourse-structure .

compcon:inf_pragmatically-specific-referent a compcon:inf ;
    rdfs:label "pragmatically specific referent" ;
    dc:description "a referent introduced into the discourse by the speaker that is normally referred to again in subsequent discourse. We will use the shorter term <q>pragmatically specific</q>. (Section 3.4.1)" ;
    dcterms:isPartOf "3.4.1" ;
    skos:altLabel "pragmatically specific (indefinite) referent" ;
    compcon:functionOf compcon:cxn_free-modification-construction,
        compcon:cxn_indefinite-pronoun,
        compcon:cxn_pragmatically-specific-pronoun ;
    compcon:subtypeOf compcon:inf_reference .

compcon:inf_predicate-identity a compcon:inf ;
    rdfs:label "predicate identity" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_predicate-identity-construction ;
    compcon:subtypeOf compcon:inf_type-identity .

compcon:inf_predicational a compcon:inf ;
    rdfs:label "predicational" ;
    cx:hasExample compcon:predicational-example-0 ;
    dc:description "the subtype of <a inf:topic-comment>topic--comment</a> or <a>predication information packaging</a> in which membership in an <a sem:object-concept>object</a> category is what is being predicated. <i>Example</i>: in <i>Bill is a teacher</i>, what is being predicated of Bill is that he is a member of the object category of teacher. In other words, <q>predicational</q> is synonymous with <a sem:object-concept>object</a> <a>predication</a>. (Section 10.1.2)" ;
    dcterms:isPartOf "10.1.2" ;
    skos:altLabel "object predication" .

compcon:inf_predicational-location a compcon:inf ;
    rdfs:label "predicational location" ;
    cx:hasExample compcon:predicational-location-example-0 ;
    dc:description "a spatial location situation with a <a inf:predicational>predicational</a> (<a inf:topic-comment>topic--comment</a>) <a>information packaging</a>, such that the <a>figure</a> in the spatial relation is the <a inf:high-topicality>topic</a> and its location (including the <a>ground</a>) is the <a>comment</a>. <i>Example</i>: <i>The pot is on the table</i> is an instance of the predicational locative construction. (Section 10.4.1)" ;
    dcterms:isPartOf "10.4.1" .

compcon:inf_presentational a compcon:inf ;
    rdfs:label "presentational" ;
    cx:hasExample compcon:presentational-example-0,
        compcon:presentational-example-1 ;
    dc:description "a type of <a inf:entity-central>entity-central</a> <a>thetic information packaging</a> that introduces a <a>referent</a> into the discourse, in order to make the identity of the referent known to the hearer. <i>Example</i>: <i>There's my bicycle</i> and <i>In the corner sat a mouse</i> are sentences that express the presentational information packaging function. (Sections 10.1.2, 10.4)" ;
    dcterms:isPartOf "10.1.2",
        "10.4" ;
    compcon:functionOf compcon:cxn_presentational-construction ;
    compcon:subtypeOf compcon:inf_entity-central .

compcon:inf_presentational-location a compcon:inf ;
    rdfs:label "presentational location" ;
    cx:hasExample compcon:presentational-location-example-0 ;
    dc:description "a <a>presentational information packaging</a> of the spatial location relation in which the <a>figure</a> in the locative relation is introduced in the discourse, <a>anchored</a> by the <a>ground</a> <a sem:object-concept>object</a>. <i>Example</i>: in <i>In the room was a request for breakfast</i>, the request for breakfast is being introduced into the discourse, anchored by its spatial relation to the room. (Sections 10.4.1, 10.4.3)" ;
    dcterms:isPartOf "10.4.1",
        "10.4.3" .

compcon:inf_presupposed-open-proposition a compcon:inf ;
    rdfs:label "presupposed open proposition" ;
    cx:hasExample compcon:presupposed-open-proposition-example-0 ;
    dc:description "in <a>identificational information packaging</a>, the <a>proposition</a> with an unknown part (hence <dq>open</dq> proposition) which is already part of the shared knowledge of the speaker and hearer (that is, it is presupposed). <i>Example</i>: in <i>It was Jack who stole my cookies!</i>, the presupposed open proposition is <q>X stole my cookies</q>; in the appropriate discourse context, it is already known that someone stole my cookies. (Section 11.4.1)" ;
    dcterms:isPartOf "11.4.1" ;
    skos:altLabel "POP" ;
    compcon:subtypeOf compcon:inf_discourse-structure .

compcon:inf_prohibitive a compcon:inf ;
    rdfs:label "prohibitive" ;
    cx:hasExample compcon:prohibitive-example-0 ;
    dc:description "a <a>negative</a> <a inf:imperative-hortative>imperative--hortative</a> <a inf:speech-acts>speech act</a>. <i>Example</i>: English <i>Don't be a fool!</i> is an instance of a prohibitive. (Section 12.4.1)" ;
    dcterms:isPartOf "12.4.1" ;
    compcon:subtypeOf compcon:inf_imperative-hortative .

compcon:inf_prop a compcon:inf ;
    rdfs:label "prop" ;
    dc:description "a supporting entity that plays a role in the actions reported in the discourse. Props are almost always nonhuman, usually are referred to only once, and are rarely introduced by special <a>constructions</a>. (Section 3.4.1)" ;
    dcterms:isPartOf "3.4.1" ;
    compcon:functionOf compcon:cxn_pragmatically-nonspecific-article .

compcon:inf_question-referent a compcon:inf ;
    rdfs:label "question referent" ;
    cx:hasExample compcon:question-referent-example-0 ;
    dc:description "an unspecified <a>referent</a> in the scope of <a inf:interrogative>interrogation</a>, especially <a inf:polarity-question>polar</a> <a cxn:interrogative-construction>interrogatives</a>. <i>Example</i>: in <i>Can you hear <b>anything</b>?</i>, <i>anything</i> is a question pronoun expressing a referent that only <dq>exists</dq> in a hypothetical world, the possibility of whose existence is being entertained by the questioner. (Section 3.5)" ;
    dcterms:isPartOf "3.5" ;
    compcon:functionOf compcon:cxn_question-pronoun ;
    compcon:subtypeOf compcon:inf_nonspecific-referent .

compcon:inf_referent-type-identity a compcon:inf ;
    rdfs:label "referent type identity" ;
    dc:description "" ;
    compcon:subtypeOf compcon:inf_type-identity .

compcon:inf_rejecting-contrast a compcon:inf ;
    rdfs:label "rejecting contrast" ;
    cx:hasExample compcon:rejecting-contrast-example-0 ;
    dc:description "a subtype of <a>counterpresuppositional contrast</a> in which the sentence simply rejects a component of a prior assertion without offering a replacement. <i>Example</i>: in the exchange <i>John bought apples. No, he didn't buy APPLES</i>, the speaker rejects the assertion of what John bought, but without asserting what John did buy. (Section 11.4.1)" ;
    dcterms:isPartOf "11.4.1" ;
    compcon:subtypeOf compcon:inf_counterpresuppositional-contrast .

compcon:inf_replacing-contrast a compcon:inf ;
    rdfs:label "replacing contrast" ;
    cx:hasExample compcon:replacing-contrast-example-0 ;
    dc:description "a subtype of <a>counterpresuppositional contrast</a> in which the sentence rejects a component of a prior assertion by offering a replacement for the rejected information. <i>Example</i>: in the exchange <i>John bought apples. No, he bought PEACHES</i>, the speaker rejects the assertion that what John bought was apples and replaces it with peaches. (Section 11.4.1)" ;
    dcterms:isPartOf "11.4.1" ;
    compcon:subtypeOf compcon:inf_counterpresuppositional-contrast .

compcon:inf_restricting-contrast a compcon:inf ;
    rdfs:label "restricting contrast" ;
    cx:hasExample compcon:restricting-contrast-example-0 ;
    dc:description "a subtype of <a>counterpresuppositional contrast</a> in which the sentence rejects a component of a prior assertion by excluding part of the rejected component rather than simply replacing it. <i>Example</i>: in the exchange <i>John bought apples and peaches. No, he only bought PEACHES</i>, the speaker rejects that what John bought was both apples and peaches, and restricts what John bought to just peaches. <i>Only</i> is a restrictive focus operator. (Section 11.4.1)" ;
    dcterms:isPartOf "11.4.1" ;
    compcon:subtypeOf compcon:inf_counterpresuppositional-contrast .

compcon:inf_restrictive-modification a compcon:inf ;
    rdfs:label "restrictive modification" ;
    cx:hasExample compcon:restrictive-modification-example-0 ;
    dc:description "the prototypical type of <a>modification</a> <a>information packaging</a> <a>function</a> in which the modifying <a>stative</a> concept narrows or restricts the set of possible <a>referents</a> of the <a>object concept</a> it modifies in a <a>referring phrase</a>. <i>Example</i>: in <i>The tall trees block the view, tall</i> restrictively modifies <i>trees</i> in the context where there are tall trees and short trees. (Section 14.3)" ;
    dcterms:isPartOf "14.3" ;
    compcon:subtypeOf compcon:inf_subcategorizing .

compcon:inf_same-subject a compcon:inf ;
    rdfs:label "same-subject" ;
    cx:hasExample compcon:same-subject-example-0 ;
    dc:description "in <a>complex sentence constructions</a> that express <a>reference tracking</a>, the situation where the <a>salient</a> <a>participant</a> in the current <a>clause</a> is indicated to be <a>coreferential</a> with the salient participant in another clause in the construction. The salient participant is typically, but not always, encoded as the <a>subject</a> of the <a>predicate</a>. <i>Example</i>: in <i>Having finished the chapter, Bill went to bed</i>, the zero anaphora in <i>Having finished the chapter</i> signals that the subject referent of finishing the chapter is coreferential with the subject referent of the <a>matrix clause</a>, namely Bill. (Section 16.1)" ;
    dcterms:isPartOf "16.1" ;
    skos:altLabel "SS",
        "same subject",
        "subject identity" ;
    compcon:functionOf compcon:cxn_subject-identity-construction ;
    compcon:subtypeOf compcon:inf_token-identity .

compcon:inf_selecting a compcon:inf ;
    rdfs:label "selecting" ;
    dc:description "a subtype of <a>modification</a> that picks out the <a>instance</a>, or set of instances, of the <a>type</a> that the speaker intends to <a inf:reference>refer</a> to. Usually performed by <a>numerals</a> and <a cxn:quantifier-term>quantifiers</a>, as well other related concepts. (Dik 1997 uses <q>selecting</q> for <a inf:choosing-contrast>choosing</a>.) (Sections 4.1.1, <b>4.1.3</b>)" ;
    dcterms:isPartOf "4.1.1",
        "4.1.3" ;
    compcon:subtypeOf compcon:inf_modification .

compcon:inf_semi-active-referent a compcon:inf ;
    rdfs:label "semi-active referent" ;
    dc:description "a <a>referent</a> whose <u>discourse file</u> has been activated (i.e. it is in short-term memory), but is not at the center of the hearer's consciousness at the current point in the discourse. The term <q>recognitional</q> is used for a referent so peripheral to the hearer's consciousness that the speaker is unsure whether the hearer remembers it. (Section 3.3.1)" ;
    dcterms:isPartOf "3.3.1" ;
    skos:altLabel "accessible",
        "recognitional",
        "semi-active",
        "tracking" ;
    compcon:functionOf compcon:cxn_definite-pronoun ;
    compcon:subtypeOf compcon:inf_reference .

compcon:inf_setting a compcon:inf ;
    rdfs:label "setting" ;
    dc:description "the place where the <a>actors</a> and <a>props</a> are located and the <a>predicated</a> <a sem:action-concept>action(s)</a> take place, and the times at which the actions take place. Settings are not typically referred to multiple times, but they play an important role in organizing discourse, especially narratives. (Section 3.4.1)" ;
    dcterms:isPartOf "3.4.1" .

compcon:inf_situating a compcon:inf ;
    rdfs:label "situating" ;
    dc:description "a subtype of <a>modification</a> that picks out a particular referent by situating it in physical space (<a>deixis</a>) or in the (knowledge/belief) <a>mental space</a> of the interlocutors (<a>information status</a>). (Section 4.1.1)" ;
    dcterms:isPartOf "4.1.1" ;
    skos:altLabel "anchoring" ;
    compcon:functionOf compcon:cxn_anchoring-construction,
        compcon:cxn_partitive-construction ;
    compcon:subtypeOf compcon:inf_modification .

compcon:inf_specific-known-referent a compcon:inf ;
    rdfs:label "specific known referent" ;
    cx:hasExample compcon:specific-known-referent-example-0 ;
    dc:description "a real-world <a>referent</a> whose identity is known to the speaker but not the hearer. <i>Example</i>: if <i>Masha met with someone near the university</i> is used in a context where the speaker knows the identity of the person Masha met, then <i>someone</i> is a specific known pronoun expressing a specific known referent. (Section 3.5)" ;
    dcterms:isPartOf "3.5" ;
    compcon:functionOf compcon:cxn_specific-known-pronoun ;
    compcon:subtypeOf compcon:inf_semantically-specific-referent .

compcon:inf_specific-unknown-referent a compcon:inf ;
    rdfs:label "specific unknown referent" ;
    cx:hasExample compcon:specific-unknown-referent-example-0 ;
    dc:description "a real-world <a>referent</a> whose identity is known neither to the speaker nor to the hearer. <i>Example</i>: if <i>Masha met with somebody near the university</i> is used in a context where the speaker does not know the identity of the person Masha met, then <i>somebody</i> is a specific unknown pronoun expressing a specific unknown referent. (Section 3.5)" ;
    dcterms:isPartOf "3.5" ;
    compcon:functionOf compcon:cxn_specific-unknown-pronoun ;
    compcon:subtypeOf compcon:inf_semantically-specific-referent .

compcon:inf_subject-argument a compcon:inf ;
    rdfs:label "subject argument" ;
    cx:hasExample compcon:subject-argument-example-0,
        compcon:subject-argument-example-1 ;
    dc:description "the most salient <a>argument</a> (and hence <a>core argument</a>) of a <a>predication</a>. <i>Example</i>: in <i>Emily slept</i> and <i>Emily read the paper</i>, Emily is the subject (core) argument. (Sections 6.1.1, <b>6.3.2</b>)" ;
    dcterms:isPartOf "6.1.1",
        "6.3.2" .

compcon:inf_theater-metaphor a compcon:inf ;
    rdfs:label "theater metaphor" ;
    dc:description "a metaphor used to describe the <a>information status</a> of <a>referents</a> in discourse, especially narrative discourse. The theater metaphor divides referents into three categories: <a>actors</a>, <a>props</a>, and <a>settings</a>. (Section 3.4.1)" ;
    dcterms:isPartOf "3.4.1" ;
    compcon:subtypeOf compcon:inf_discourse-structure .

compcon:inf_topic a compcon:inf ;
    rdfs:label "topic" ;
    cx:hasExample compcon:topic-example-0 ;
    dc:description "in a <a>topic--comment information packaging</a>, the topic is the most <a inf:topicality>salient</a> <a>referent</a> that the <a>comment</a> is <a inf:predication>predicated about</a>. <i>Example</i>: in <i>Bill is a teacher</i>, being a teacher is predicated about the referent Bill. A topic is a referent; arguably, all referents are topics, although some referents are more topical and others are less topical. (Section 10.1.2)" ;
    dcterms:isPartOf "10.1.2" .

compcon:inf_topic-comment a compcon:inf ;
    rdfs:label "topic--comment" ;
    cx:hasExample compcon:topic-comment-example-0 ;
    dc:description "the <a>information packaging</a> in which one concept (the <a>comment</a>) is <a inf:predication>predicated about</a> another concept which is <a inf:reference>referred</a> to (the <a inf:reference>topic</a>). <i>Example</i>: <i>The bus stopped</i> is an instance of a topic--comment construction in which <i>stopped</i> is the comment and <i>The bus</i> is the topic. Topic--comment information packaging is basically synonymous with <a>predication</a>; the term <q>topic--comment</q> highlights the fact that a predication is a predication about a <a>referent</a>. (Sections 2.2.2, 10.1.2, 11.1, <b>11.2.1</b>)" ;
    dcterms:isPartOf "10.1.2",
        "11.1",
        "11.2.1",
        "2.2.2" ;
    compcon:functionOf compcon:cxn_topic-comment-construction ;
    compcon:subtypeOf compcon:inf_declarative .

compcon:inf_transitive-predication a compcon:inf ;
    rdfs:label "transitive predication" ;
    cx:hasExample compcon:transitive-predication-example-0 ;
    dc:description "a <a>predication</a> predicated of two <a>salient</a> <a inf:reference>arguments</a>. <i>Example</i>: in <i>Sarah wrote the letter</i>, writing is a transitive predication because it is predicated of two arguments, Sarah and the letter. (Section 6.1.2)" ;
    dcterms:isPartOf "6.1.2" ;
    compcon:subtypeOf compcon:inf_predication .

compcon:inf_trigger a compcon:inf ;
    rdfs:label "trigger" ;
    cx:hasExample compcon:trigger-example-0 ;
    dc:description "a concept evoked or inferrable in the prior discourse that is a member of a <a>poset</a> that also includes the <a>link</a>. <i>Example</i>: in the exchange <i>Do you like this album? Yeah, this song I really like</i>, the album mentioned in the first turn is the trigger that generates a poset together with the song mentioned in the second turn; the latter serves as the link to the trigger. (Section 11.2.3)" ;
    dcterms:isPartOf "11.2.3" ;
    compcon:subtypeOf compcon:inf_reference .

compcon:inf_type-identifiable a compcon:inf ;
    rdfs:label "type identifiable" ;
    cx:hasExample compcon:type-identifiable-example-0 ;
    dc:description "a <a>referent</a> in a nonreal <dq>world</dq> or <a>mental space</a> (desire, hypothetical, negative, etc.) whose individual identity is unknown to speaker and hearer; all that is known about the nonreal referent is its <a>type</a>, as provided by the <a>common noun</a> and any <a>modifiers</a> in the <a>referring phrase</a>. <i>Example</i>: in <i>An undergraduate student may take this course</i>, all that is known about the possible referent is that he or she belongs to the type <q>undergraduate student</q>. (Section 3.5)" ;
    dcterms:isPartOf "3.5" ;
    compcon:functionOf compcon:cxn_anaphoric-head-construction,
        compcon:cxn_indefinite-article,
        compcon:cxn_semantically-nonspecific-article ;
    compcon:subtypeOf compcon:inf_identifiability .

compcon:inf_typifying a compcon:inf ;
    rdfs:label "typifying" ;
    cx:hasExample compcon:typifying-example-0 ;
    dc:description "the <a>information packaging</a> <a>function</a> whereby an <a sem:object-concept>object</a> <a>modifier</a> subclassifies the <a>object concept</a> of the <a>head</a> <a>noun</a> that it <a inf:modification>modifies</a>. <i>Example</i>: in <i>women's magazine</i>, the object modifier expressed by <i>women's</i> subclassifies the type of magazine that the <a>referring phrase</a> as a whole refers to. Typifying is the object modifying variant of the <a>subcategorizing</a> function." ;
    compcon:functionOf compcon:cxn_binominal-lexeme,
        compcon:cxn_typifying-construction ;
    compcon:subtypeOf compcon:inf_modification .

compcon:inf_verbal-contrast a compcon:inf ;
    rdfs:label "verbal contrast" ;
    cx:hasExample compcon:verbal-contrast-example-0 ;
    dc:description "a subtype of <a>parallel contrast</a> in which the <a>predicates</a> in the two propositions have opposite <a>meanings</a>, while other parallel components of the proposition are members of <a>posets</a>. <i>Example</i>: in <i>We will give our daughters to you and we will take your daughters for ourselves</i>, the verbs <q>give</q> and <q>take</q> have opposite meanings, and the other parallel parts of the two propositions that differ form the posets {our daughters, your daughters} and {you, ourselves}. (Section 11.4.1)" ;
    dcterms:isPartOf "11.4.1" ;
    compcon:subtypeOf compcon:inf_parallel-contrast .

compcon:language a owl:ObjectProperty ;
    rdfs:label "Language" .

compcon:roleOf a owl:ObjectProperty ;
    rdfs:label "Role of" .

compcon:sem_a-role a compcon:sem ;
    rdfs:label "A role" ;
    cx:hasExample compcon:a-role-example-0,
        compcon:a-role-example-1 ;
    dc:description "the <a>agent</a> or agent-like <a>central participant role</a> in the prototypical <a sem:bivalent>bivalent event</a> (that is, a breaking event) or the prototypical <a sem:trivalent>trivalent event</a> (that is, a giving event). <i>Examples</i>: in <i>Jack broke the window</i>, Jack plays the A role in the breaking event; and in <i>Jill gave Joe the keys</i>, Jill plays the A role in the giving event. (Section 6.3.1)" ;
    dcterms:isPartOf "6.3.1" ;
    skos:altLabel "A (role)" ;
    compcon:functionOf compcon:cxn_ditransitive-a-phrase,
        compcon:cxn_passive-inverse-a-phrase,
        compcon:cxn_transitive-a-phrase ;
    compcon:roleOf compcon:sem_agentive-change-of-state-event ;
    compcon:subtypeOf compcon:sem_agent .

compcon:sem_achievement a compcon:sem ;
    rdfs:label "achievement" ;
    cx:hasExample compcon:achievement-example-0 ;
    dc:description "a type of <a>aspect</a> that indicates the success or failure in carrying out the <a>complement</a> <a>event</a>. Achievement is often, but not always, expressed by a <a>complement-taking predicate</a> in a <a>complement clause construction</a>. <i>Example</i>: in <i>She managed to solve the riddle, managed</i> expresses achievement -- namely, the successful execution of the complement event of solving the riddle. (Section 18.2.2)" ;
    dcterms:isPartOf "18.2.2" .

compcon:sem_action-concept a compcon:sem ;
    rdfs:label "action concept" ;
    dc:description "a concept belonging to a <a>semantic class</a> that is <a>relational</a>, <a>dynamic</a>, and <a>transitory</a>. <i>Example</i>: motion events such as running, or transfer events such as giving, are action concepts -- they involve change but come to an end. (Section 2.1; <b>Chapters 6--7</b> cover a wide range of action concepts.)" ;
    dcterms:isPartOf "2.1",
        "6",
        "7" ;
    skos:altLabel "action (concept)" ;
    compcon:functionOf compcon:cxn_verbal-clause ;
    compcon:subtypeOf compcon:sem_event .

compcon:sem_addition a compcon:sem ;
    rdfs:label "addition" ;
    dc:description "" ;
    compcon:subtypeOf compcon:sem_event-relation .

compcon:sem_additive a compcon:sem ;
    rdfs:label "additive" ;
    dc:description "a relation between two or more <a>entities</a> such that the entities are construed as combined in a sort of totality. The additive relation may be construed as a <a>complex figure</a>, as in <i>The robins drank water and the juncos ate fennel seeds</i>, or in a <a>figure--ground</a> relation, as in <i>Besides missing my bus, I got my feet all wet</i>. The additive relation is considered the prototype for <a>conjunctive coordination</a>. (Section 15.2.1)" ;
    dcterms:isPartOf "15.2.1" ;
    compcon:functionOf compcon:cxn_additive-construction,
        compcon:cxn_additive-coordination ;
    compcon:subtypeOf compcon:sem_event-relation .

compcon:sem_addressee a compcon:sem ;
    rdfs:label "addressee" ;
    dc:description "" ;
    compcon:roleOf compcon:sem_communication-event ;
    compcon:subtypeOf compcon:sem_co-actor .

compcon:sem_affecting a compcon:sem ;
    rdfs:label "affecting" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_affecting-verb ;
    compcon:subtypeOf compcon:sem_causal-directionality .

compcon:sem_affecting-event a compcon:sem ;
    rdfs:label "affecting event" ;
    cx:hasExample compcon:affecting-event-example-0 ;
    dc:description "an <a>experiential event</a> which describes the <a>stimulus</a> causing a change in mental state of the <a>experiencer</a>. <i>Example</i>: <i>The dog surprised me</i> is an instance of an affecting event. (Section 7.4)" ;
    dcterms:isPartOf "7.4" ;
    compcon:functionOf compcon:cxn_affecting-verb ;
    compcon:subtypeOf compcon:sem_event .

compcon:sem_age a compcon:sem ;
    rdfs:label "age" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_age-term ;
    compcon:subtypeOf compcon:sem_property-concept .

compcon:sem_agentive-experience-event a compcon:sem ;
    rdfs:label "agentive experience event" ;
    dc:description "" ;
    skos:altLabel "agentive experience (event)" ;
    compcon:subtypeOf compcon:sem_experiential-event .

compcon:sem_agentive-ingestion-event a compcon:sem ;
    rdfs:label "agentive ingestion event" ;
    dc:description "" ;
    skos:altLabel "agentive ingestion (event)" ;
    compcon:subtypeOf compcon:sem_experiential-event .

compcon:sem_amount a compcon:sem ;
    rdfs:label "amount" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_amount-term ;
    compcon:subtypeOf compcon:sem_quantitative-relation .

compcon:sem_animate a compcon:sem ;
    rdfs:label "animate" ;
    cx:hasExample compcon:animate-example-0 ;
    dc:description "a semantic <u>category</u> of <a sem:object-concept>objects</a> that denotes animate beings. <i>Example</i>: <i>cat</i> denotes an animate entity. The term <q>animate</q> is often used to contrast with <a>human</a>, and therefore often is used to refer only to nonhuman animates. (Section 3.1.2)" ;
    dcterms:isPartOf "3.1.2" ;
    skos:altLabel "(nonhuman) animate" ;
    compcon:subtypeOf compcon:sem_animacy .

compcon:sem_anterior a compcon:sem ;
    rdfs:label "anterior" ;
    cx:hasExample compcon:anterior-example-0 ;
    dc:description "a temporal sequential relation between two <a>events</a> such that the following event serves as the reference point for the preceding event. <i>Example</i>: in <i>He washed the car before driving to the party</i>, washing the car has an anterior temporal relation with respect to driving to the party. Used to describe a sequential temporal relation between events in a <a>figure--ground information packaging</a>. (Section 15.1.3)" ;
    dcterms:isPartOf "15.1.3" ;
    compcon:functionOf compcon:cxn_temporal-construction ;
    compcon:subtypeOf compcon:sem_event-relation .

compcon:sem_apodosis a compcon:sem ;
    rdfs:label "apodosis" ;
    cx:hasExample compcon:apodosis-example-0 ;
    dc:description "the proposition or event denoted by an <a>apodosis clause</a>. <i>Example</i>: in <i>If you press this button, the door will open</i>, <i>the door will open</i> is the apodosis; <i>If you press this button</i> is the <a cxn:protasis-clause>protasis</a>. Since the conditional relations are defined in terms of both logical implication and causal relation, the semantic use of <q>apodosis</q> can be distinguished as <q>apodosis proposition</q> or <q>apodosis event</q>. (Section 17.3.1)" ;
    dcterms:isPartOf "17.3.1" ;
    skos:altLabel "consequent" ;
    compcon:functionOf compcon:cxn_apodosis-clause .

compcon:sem_application-event a compcon:sem ;
    rdfs:label "application event" ;
    dc:description "an <a>event</a> describing placing or applying one <a sem:object-concept>object</a> onto (2-dimensional) or into (3-dimensional) another object. <i>Examples</i>: smearing (2-dimensional) and loading (3-dimensional) are application events. (Section 7.3.2)" ;
    dcterms:isPartOf "7.3.2" ;
    skos:altLabel "application (event)",
        "putting event" ;
    compcon:functionOf compcon:cxn_application-verb ;
    compcon:subtypeOf compcon:sem_spatial-event .

compcon:sem_apprehensional a compcon:sem ;
    rdfs:label "apprehensional" ;
    cx:hasExample compcon:apprehensional-example-0,
        compcon:apprehensional-example-1 ;
    dc:description "the semantic relation between two <a>events</a> where one event serves as the opposite of the intended outcome of bringing about the other event. In this respect, the apprehensional relation is a <dq>negative</dq> version of the <a>purpose</a> relation. <i>Examples</i>: <i>I grabbed a stick lest he attack me</i> is a <a>figure--ground construal</a> of the simultaneous relation in an <a>adverbial clause construction</a>, and <i>Grab a stick or he will attack you</i> is a <a>complex figure construal</a> of the relation in a <a>coordinate clause construction</a>. In the figure--ground construal, the <dq>negatively intended</dq> event is construed as the ground. As with the purpose relation, the apprehensional event is unrealized; the complex figure construal is possible because the <a>matrix clause</a> event is also unrealized. (Section 15.3.1)" ;
    dcterms:isPartOf "15.3.1" ;
    compcon:functionOf compcon:cxn_apprehensional-construction ;
    compcon:subtypeOf compcon:sem_event-relation .

compcon:sem_arrangement a compcon:sem ;
    rdfs:label "arrangement" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_form-term ;
    compcon:subtypeOf compcon:sem_mensural-concept .

compcon:sem_arrival a compcon:sem ;
    rdfs:label "arrival" ;
    cx:hasExample compcon:arrival-example-0 ;
    dc:description "the final phase of the <a>path</a> in a <a>motion event</a>. <i>Example</i>: in <i>He went from the tree to the house</i>, the path oblique phrase <i>to the house</i> denotes the arrival phase of the motion event. (Section 14.4)" ;
    dcterms:isPartOf "14.4" .

compcon:sem_aspect a compcon:sem ;
    rdfs:label "aspect" ;
    dc:description "how an event unfolds over time. The semantics of aspectual structure is not discussed in detail in this textbook. However, certain basic aspectual distinctions are discussed in Section 6.2.2: <a>dynamic</a> and <a>stative</a>, <a>punctual</a> and <a>durative</a>, and <a>telic</a> and <a>atelic</a>. (Sections 6.1.1, 6.2.2)" ;
    dcterms:isPartOf "6.1.1",
        "6.2.2" ;
    skos:altLabel "aspectual structure" ;
    compcon:subtypeOf compcon:sem_tamp .

compcon:sem_atelic a compcon:sem ;
    rdfs:label "atelic" ;
    cx:hasExample compcon:atelic-example-0 ;
    dc:description "an <a>event</a> in which the relevant <a>participant</a> does not end up in a <dq>natural</dq> result <a>state</a>. <i>Example</i>: in <i>I walked in the park</i>, there is not a natural result state for my walking, and so the event is atelic. (Section 6.2.1)" ;
    dcterms:isPartOf "6.2.1" ;
    compcon:subtypeOf compcon:sem_telicity .

compcon:sem_attending a compcon:sem ;
    rdfs:label "attending" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_attending-verb ;
    compcon:subtypeOf compcon:sem_causal-directionality .

compcon:sem_attending-event a compcon:sem ;
    rdfs:label "attending event" ;
    cx:hasExample compcon:attending-event-example-0 ;
    dc:description "an <a>experiential event</a> which describes the <a>experiencer</a> directing her/his attention to the <a>stimulus</a>. <i>Example</i>: <i>I am looking at the sandhill crane</i> is an instance of an attending event. (Section 7.4)" ;
    dcterms:isPartOf "7.4" ;
    compcon:functionOf compcon:cxn_attending-verb ;
    compcon:subtypeOf compcon:sem_event .

compcon:sem_avalent a compcon:sem ;
    rdfs:label "avalent" ;
    dc:description "" ;
    compcon:subtypeOf compcon:sem_valency .

compcon:sem_base-event a compcon:sem ;
    rdfs:label "base event" ;
    cx:hasExample compcon:base-event-example-0 ;
    dc:description "the <a>event</a> that is expressed in the <a>basic voice construction</a>, with its set of <a sem:central-participant>central</a> and <a>peripheral participants</a> in their prototypical level of <a>salience</a>/<a>topicality</a>, whose <a>valency</a> is considered basic for that event semantic class. <i>Example</i>: in <i>Fred baked me a shepherd's pie</i>, the base event is Fred baking the pie (i.e., excluding transferring the pie to me). (Section 9.1)" ;
    dcterms:isPartOf "9.1" .

compcon:sem_beneficiary a compcon:sem ;
    rdfs:label "beneficiary" ;
    cx:hasExample compcon:beneficiary-example-0 ;
    dc:description "a <a>semantic role</a> including <a>participant roles</a> for a <a>participant</a> that is positively affected by the outcome of the <a>event</a>. <i>Example</i>: in <i>Terry made lunch for Sandy</i>, Sandy is positively affected by the outcome of the event. (Section 6.1.2)" ;
    dcterms:isPartOf "6.1.2" ;
    compcon:subtypeOf compcon:sem_affectee .

compcon:sem_bivalent a compcon:sem ;
    rdfs:label "bivalent" ;
    dc:description "an event with a <a>valency</a> of two -- that is, with two <a>central participant roles</a>. <i>Example</i>: drinking is a bivalent event, with the two central roles of drinker and drink. (Section 6.1.2)" ;
    dcterms:isPartOf "6.1.2" ;
    compcon:functionOf compcon:cxn_transitive-construction ;
    compcon:subtypeOf compcon:sem_valency .

compcon:sem_bodily-action a compcon:sem ;
    rdfs:label "bodily action" ;
    dc:description "the <a>event</a> class of normally uncontrolled actions involving one's body. <i>Example</i>: coughing is a bodily action. (Section 6.3.3)" ;
    dcterms:isPartOf "6.3.3" ;
    compcon:functionOf compcon:cxn_bodily-predicate ;
    compcon:subtypeOf compcon:sem_qualitative-event .

compcon:sem_bodily-motion-event a compcon:sem ;
    rdfs:label "bodily motion event" ;
    cx:hasExample compcon:bodily-motion-event-example-0 ;
    dc:description "a <a sem:monovalent>monovalent event</a> involving an internal bodily motion. <i>Example</i>: <i>stretch out (oneself)</i> expresses a bodily motion event. (Section 7.2)" ;
    dcterms:isPartOf "7.2" ;
    compcon:functionOf compcon:cxn_bodily-motion-verb ;
    compcon:subtypeOf compcon:sem_spatial-event .

compcon:sem_body-care-event a compcon:sem ;
    rdfs:label "body care event" ;
    cx:hasExample compcon:body-care-event-example-0,
        compcon:body-care-event-example-1 ;
    dc:description "a <a sem:monovalent>monovalent event</a> involving a person acting on that person's own body, generally for grooming or hygiene. <i>Examples</i>: <i>shave</i> and <i>wash (oneself)</i> express body care events. (Section 7.2)" ;
    dcterms:isPartOf "7.2" ;
    skos:altLabel "body care (event)",
        "grooming event" ;
    compcon:functionOf compcon:cxn_body-care-verb ;
    compcon:subtypeOf compcon:sem_qualitative-event .

compcon:sem_body-part-relation a compcon:sem ;
    rdfs:label "body part relation" ;
    dc:description "a relation between a person and a physical body part of hers or his. Example: <i>the child's arm</i> is a <a>possession construction</a> expressing a body part relation. (Section 4.1.4)" ;
    dcterms:isPartOf "4.1.4" ;
    skos:altLabel "body part (relation)" ;
    compcon:functionOf compcon:cxn_inalienable-possession-attributive-phrase ;
    compcon:subtypeOf compcon:sem_possession .

compcon:sem_body-position-event a compcon:sem ;
    rdfs:label "body position event" ;
    dc:description "the event class of maintaining a particular body posture or position. <i>Example</i>: standing is a body position event. (Section 6.3.3)" ;
    dcterms:isPartOf "6.3.3" ;
    skos:altLabel "locative stative event",
        "maintain position event",
        "posture event" ;
    compcon:functionOf compcon:cxn_body-position-predicate ;
    compcon:subtypeOf compcon:sem_spatial-event .

compcon:sem_cardinal-numeral a compcon:sem ;
    rdfs:label "cardinal numeral" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_numeral-attributive-phrase ;
    compcon:subtypeOf compcon:sem_numeral,
        compcon:sem_quantitative-relation .

compcon:sem_case a compcon:sem ;
    rdfs:label "case" ;
    cx:hasExample compcon:case-example-0 ;
    dc:description "a semantic category expressed by <a>case affixes</a> which encodes the types of relations that occur between <a>predicate</a> and <a sem:participants>arguments</a>, or the relation between an <a sem:object-concept>object</a> <a>modifier</a> and its <a>head</a> in <a>nominal modification constructions</a>. <i>Example</i>: in Finnish <i>kukassa</i> <q>in a/the flower</q>, the Inessive Case Suffix <i>-ssa</i> expresses the <q>in</q> locative relation. (Section 4.4.2)" ;
    dcterms:isPartOf "4.4.2" .

compcon:sem_categorical a compcon:sem ;
    rdfs:label "categorical" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_complementary ;
    compcon:subtypeOf compcon:sem_scale .

compcon:sem_causal a compcon:sem ;
    rdfs:label "causal" ;
    cx:hasExample compcon:causal-example-0,
        compcon:causal-example-1 ;
    dc:description "the semantic relation between two <a>events</a> where one event causes the other. <i>Example</i>: <i>I left the party because I was tired</i> is a <a>figure--ground construal</a> of the simultaneous relation in an <a>adverbial clause construction</a>, and <i>I was tired and (so) I left the party</i> is a <a>complex figure construal</a> of the relation in a <a>coordinate clause construction</a>. In the figure--ground construal, the causing event (the <a sem:protasis>protasis</a>; Section 17.3.2) is construed as the ground, and expressed in the <a>adverbial dependent clause</a>. Causal relations also occur in <a>conditional</a>, <a>concessive</a>, <a>concessive conditional</a>, and <a>comparative conditional constructions</a>. Causal relations are divided into <a sem:content-causal-relation>content</a>, <a sem:epistemic-causal-relation>epistemic</a>, and <a sem:speech-act-causal-relation>speech act relations</a>. (Sections 15.3.1, 17.2.1, 17.3.2)" ;
    dcterms:isPartOf "15.3.1",
        "17.2.1",
        "17.3.2" ;
    compcon:functionOf compcon:cxn_causal-construction ;
    compcon:subtypeOf compcon:sem_event-relation .

compcon:sem_causal-chain a compcon:sem ;
    rdfs:label "causal chain" ;
    cx:hasExample compcon:causal-chain-example-0 ;
    dc:description "a <a sem:causal-structure>causal / force dynamic structure</a> in which one <a>participant</a> acts on a second participant, which acts on a third participant, and so on. <i>Example</i>: in <i>Jack broke the window with a hammer</i>, Jack acts on the hammer (grasping and moving it), and the hammer acts on the window (breaking it). A causal chain is often represented with arrows: Jack --> hammer --> window. (Sections 6.1.1, <b>6.1.2</b>)" ;
    dcterms:isPartOf "6.1.1",
        "6.1.2" ;
    skos:altLabel "transmission of force" ;
    compcon:subtypeOf compcon:sem_causal-structure .

compcon:sem_causal-event a compcon:sem ;
    rdfs:label "causal event" ;
    dc:description "an <a>event</a> that has an external cause <a>participant</a> in addition to a participant that undergoes some sort of change. <i>Example</i>: a person breaking a vase is an example of a causal event. Causal events are contrasted with <a>noncausal events</a>. (Section 6.3.4)" ;
    dcterms:isPartOf "6.3.4" .

compcon:sem_causative-event a compcon:sem ;
    rdfs:label "causative event" ;
    cx:hasExample compcon:causative-event-example-0 ;
    dc:description "an <a>event</a> consisting of a <a>salient</a> external <a>causer</a> <a>participant role</a> added to the <a>causal chain</a> of a <a>base event</a>. <i>Example</i>: <i>Fred made me fill out the questionnaire</i> is a causative event since an external causer (Fred) is added to the base event of filling out the questionnaire. (Section 9.2)" ;
    dcterms:isPartOf "9.2" .

compcon:sem_cause a compcon:sem ;
    rdfs:label "cause" ;
    cx:hasExample compcon:cause-example-0 ;
    dc:description "a <a>semantic role</a> including <a>participant roles</a> for a <a>participant</a>, usually an event, that causes the <a>event</a> expressed by the <a>predicate</a>. <i>Example</i>: in <i>The house collapsed from neglect</i>, the neglect is the cause of the house collapsing. (Section 6.1.2)" ;
    dcterms:isPartOf "6.1.2" ;
    compcon:subtypeOf compcon:sem_external-cause .

compcon:sem_caused-motion a compcon:sem ;
    rdfs:label "caused motion" ;
    dc:description "" ;
    compcon:subtypeOf compcon:sem_spatial-event .

compcon:sem_causee a compcon:sem ;
    rdfs:label "causee" ;
    cx:hasExample compcon:causee-example-0 ;
    dc:description "in a <a>causative event</a>, the <a>participant role</a> that is the initiator of the <a>causal chain</a> of the <a>base event</a>. <i>Example</i>: in <i>Sandra had Phil sweep the patio</i>, Phil fills the participant role of causee because Phil is the initiator of the sweeping event. The term <q>causee</q> is used only when there is a <a>causer</a> in the event as well. (Section 9.2)" ;
    dcterms:isPartOf "9.2" ;
    compcon:functionOf compcon:cxn_causee-phrase ;
    compcon:subtypeOf compcon:sem_co-actor,
        compcon:sem_external-cause .

compcon:sem_causer a compcon:sem ;
    rdfs:label "causer" ;
    cx:hasExample compcon:causer-example-0 ;
    dc:description "a <a>participant role</a> which names an external cause that brings about an event, and is conceptualized as a <a>central participant</a> in the <a>event</a>. <i>Example</i>: in <i>Harry made the antelope jump</i>, Harry is in the causer role. (Sections 7.4, 9.2)" ;
    dcterms:isPartOf "7.4",
        "9.2" ;
    compcon:functionOf compcon:cxn_causer-phrase ;
    compcon:subtypeOf compcon:sem_external-cause .

compcon:sem_chaining a compcon:sem ;
    rdfs:label "chaining" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_chaining-construction ;
    compcon:subtypeOf compcon:sem_causal-cyclicality .

compcon:sem_chaining-event a compcon:sem ;
    rdfs:label "chaining event" ;
    cx:hasExample compcon:chaining-event-example-0 ;
    dc:description "an <a>event</a> type in which one <a>participant</a> acts upon another participant, and the second participant acts on a third participant in the same way, and so on. That is, each participant in the chain is both the <a>initiator</a> and <a>endpoint</a> of <a>transmission of force</a> for the same type of action -- except the first in the chain, who is only an initiator, and the last, who is only an endpoint. <i>Example</i>: in <i>The guests followed one another into the room</i>, each guest is a follower and a <dq>followee</dq>, except the first and last in the chain. It is also possible to have a closed chain, as in people following each other in a circle, in which all participants are both initiator and endpoint. (Section 7.2)" ;
    dcterms:isPartOf "7.2" ;
    compcon:functionOf compcon:cxn_chaining-construction ;
    compcon:subtypeOf compcon:sem_event .

compcon:sem_change-in-position-event a compcon:sem ;
    rdfs:label "change in position event" ;
    cx:hasExample compcon:change-in-position-event-example-0,
        compcon:change-in-position-event-example-1 ;
    dc:description "a <a sem:monovalent>monovalent event</a> involving a person changing their bodily position. <i>Example</i>: <i>sit</i> and <i>lean</i> express change in body posture events. (Section 7.2)" ;
    dcterms:isPartOf "7.2" ;
    skos:altLabel "assume position event",
        "change in (body) position event",
        "change in posture event" ;
    compcon:functionOf compcon:cxn_change-in-position-verb ;
    compcon:subtypeOf compcon:sem_spatial-event .

compcon:sem_cognition-event a compcon:sem ;
    rdfs:label "cognition event" ;
    cx:hasExample compcon:cognition-event-example-0 ;
    dc:description "an <a>experiential event</a> involving an <a>experiencer</a>'s cognition directed toward a <a>stimulus</a>. <i>Example</i>: <i>Tim thought about the war</i> is an example of a cognition event. (Section 7.4)" ;
    dcterms:isPartOf "7.4" ;
    skos:altLabel "cognition (event)" ;
    compcon:functionOf compcon:cxn_cognition-verb ;
    compcon:subtypeOf compcon:sem_experiential-event .

compcon:sem_collective a compcon:sem ;
    rdfs:label "collective" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_collective-construction ;
    compcon:subtypeOf compcon:sem_causal-cyclicality .

compcon:sem_collective-event a compcon:sem ;
    rdfs:label "collective event" ;
    cx:hasExample compcon:collective-event-example-0 ;
    dc:description "an <a>event</a> type in which two <a>participants</a> both play the same role in two related events (that is, they do it <dq>together</dq>). <i>Example</i>: in <i>Mary and Sue left together</i>, Mary leaves and Sue leaves, and the two leaving events are connected. (Section 7.2)" ;
    dcterms:isPartOf "7.2" ;
    compcon:functionOf compcon:cxn_collective-construction ;
    compcon:subtypeOf compcon:sem_event .

compcon:sem_color a compcon:sem ;
    rdfs:label "color" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_color-term ;
    compcon:subtypeOf compcon:sem_property-concept .

compcon:sem_combining-event a compcon:sem ;
    rdfs:label "combining event" ;
    dc:description "an <a>event</a> describing the combining of two <a sem:object-concept>objects</a>. <i>Example</i>: blending is a combining event. (Section 7.3.2)" ;
    dcterms:isPartOf "7.3.2" ;
    skos:altLabel "combining (event)" ;
    compcon:functionOf compcon:cxn_combining-verb ;
    compcon:subtypeOf compcon:sem_spatial-event .

compcon:sem_comitative a compcon:sem ;
    rdfs:label "comitative" ;
    cx:hasExample compcon:comitative-example-0 ;
    dc:description "a <a>semantic role</a> including <a>participant roles</a> for a <a>participant</a> that accompanies another participant, usually the <a>agent</a>. <i>Example</i>: in <i>I went to the concert with Carol</i>, Carol is in the comitative role. (Section 6.1.2)" ;
    dcterms:isPartOf "6.1.2" ;
    compcon:subtypeOf compcon:sem_co-actor,
        compcon:sem_external-cause .

compcon:sem_commentative-event a compcon:sem ;
    rdfs:label "commentative event" ;
    cx:hasExample compcon:commentative-event-example-0 ;
    dc:description "an <a>evaluative event</a> in which an evaluative judgment about a <a>proposition</a> expressed by the <a>complement</a> of the commentative event is made, and there is a <a>positive epistemic stance</a> by the speaker toward the proposition. <i>Example</i>: in <i>Nancy is glad that Joe won the election</i>, the commentative predicate <i>is glad</i> expresses Nancy's evaluation of Joe's winning the election, and also presupposes that the speaker believes that Joe indeed won the election. (Section 18.2.2)" ;
    dcterms:isPartOf "18.2.2" ;
    skos:altLabel "commentative (event)" ;
    compcon:functionOf compcon:cxn_commentative-construction ;
    compcon:subtypeOf compcon:sem_evaluative-event .

compcon:sem_comparative-conditional-relation a compcon:sem ;
    rdfs:label "comparative conditional relation" ;
    cx:hasExample compcon:comparative-conditional-relation-example-0 ;
    dc:description "a relation between two events, each on a <a>gradable predicative scale</a>, such that an event at one degree on the first predicative scale causes an event at the corresponding degree on the second predicative scale. <i>Example</i>: <i>The longer that Bill had to wait, the angrier he got</i> is an instance of the comparative conditional relation and construction: a degree of length of time that Bill had to wait can (in a <a>generic conditional</a>) or does (in an ordinary, specific <a>conditional</a>) cause the occurrence of the corresponding degree of Bill's anger. (Section 17.4.1)" ;
    dcterms:isPartOf "17.4.1" ;
    skos:altLabel "comparative conditional (relation)" ;
    compcon:functionOf compcon:cxn_comparative-conditional-construction ;
    compcon:subtypeOf compcon:sem_event-relation .

compcon:sem_comparative-degree a compcon:sem ;
    rdfs:label "comparative degree" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_comparative-form ;
    compcon:subtypeOf compcon:sem_degree .

compcon:sem_comparee a compcon:sem ;
    rdfs:label "comparee" ;
    cx:hasExample compcon:comparee-example-0 ;
    dc:description "in a <a>comparative construction</a> or <a>equative construction</a>, the <a>referent</a> whose position on the <a>gradable predicative scale</a> is specified relative to the position of the <a>standard</a> on the predicative scale. <i>Example</i>: in the comparative construction <i>Your cat is bigger than my dog</i>, the cat is the comparee -- it is asserted to exceed the dog on the scale of size. (Sections 17.2.1, 17.2.4)" ;
    dcterms:isPartOf "17.2.1",
        "17.2.4" ;
    compcon:roleOf compcon:sem_comparative-relation,
        compcon:sem_equative-relation .

compcon:sem_completive a compcon:sem ;
    rdfs:label "completive" ;
    dc:description "" ;
    compcon:subtypeOf compcon:sem_phasal-aspect .

compcon:sem_concessive-conditional-relation a compcon:sem ;
    rdfs:label "concessive conditional relation" ;
    cx:hasExample compcon:concessive-conditional-relation-example-0 ;
    dc:description "a <a>causal relation</a> between a set of <a>events</a> that are associated with a <a>scalar model</a> on the one hand, and another event, such that the other event would occur under the entire range of conditions described in the scalar model associated with the first set of events. <i>Example</i>: <i>However much advice you give him, he does exactly what he wants to do</i> is an instance of the concessive conditional relation and construction -- the <a cxn:protasis-clause>protasis</a> <i>However much advice you give him</i> introduces a scalar model of your giving him a full range of amounts of advice; and the <a cxn:apodosis-clause>apodosis</a> <i>he does exactly what he wants to do</i> describes the event that occurs or would occur under any of those conditions. The speaker has a neutral epistemic stance toward the range of events associated with the scalar model. The apodosis has an unexpected causal relation with respect to the set of events that make up the protasis. A concessive conditional may express a <a sem:content-causal-relation>content</a>, <a sem:epistemic-causal-relation>epistemic</a>, or <a>speech act causal relation</a>. (Section 17.3.3)" ;
    dcterms:isPartOf "17.3.3" ;
    skos:altLabel "conditional concessive relation" ;
    compcon:functionOf compcon:cxn_concessive-conditional-construction ;
    compcon:subtypeOf compcon:sem_event-relation .

compcon:sem_concessive-relation a compcon:sem ;
    rdfs:label "concessive relation" ;
    cx:hasExample compcon:concessive-relation-example-0 ;
    dc:description "a relation between two <a>events</a> such that there is an expected <a>causal relation</a> between the two events, but the opposite of the second event, expressed in the <a cxn:apodosis-clause>apodosis</a>, unexpectedly occurs. <i>Example</i>: <i>Although it was raining, I went out</i> is an instance of a concessive relation and construction: the expected causal relation is that rain would lead to my staying in; but in fact I went out. The speaker has a <a>positive epistemic stance</a> toward the concessive construction. A concessive may express a <a sem:content-causal-relation>content</a>, <a sem:epistemic-causal-relation>epistemic</a>, or <a>speech act causal relation</a>. (Section 17.3.2)" ;
    dcterms:isPartOf "17.3.2" ;
    skos:altLabel "concessive (relation)" ;
    compcon:functionOf compcon:cxn_concessive-construction ;
    compcon:subtypeOf compcon:sem_event-relation .

compcon:sem_concomitant-role a compcon:sem ;
    rdfs:label "concomitant role" ;
    cx:hasExample compcon:concomitant-role-example-0 ;
    dc:description "a subset of <a>antecedent roles</a> that includes <a>participant roles</a> in between the participant role expressed as <a>subject</a> and the participant role expressed as <a inf:object-argument>object</a>. <i>Example</i>: in <i>Jack broke the window with a hammer</i>, the hammer is antecedent to the window in the breaking causal chain (Jack --> hammer --> window), and the window is expressed as object; and it is also <a sem:subsequent-role>subsequent</a> to Jack in the breaking <a>causal chain</a>, and Jack is expressed as subject. (Section 6.1.2)" ;
    dcterms:isPartOf "6.1.2" ;
    compcon:subtypeOf compcon:sem_antecedent-role .

compcon:sem_consecutive a compcon:sem ;
    rdfs:label "consecutive" ;
    cx:hasExample compcon:consecutive-example-0 ;
    dc:description "a sequential relation between two <a>events</a>, as expressed by a <a>complex figure</a> construal of the relation between the two events. <i>Example</i>: in <i>He washed the car and drove to the party</i>, the washing of the car and the driving to the party are in a consecutive relation. Used to describe the temporal relation between two events in a <a>complex figure information packaging</a>. (Section 15.1.3)" ;
    dcterms:isPartOf "15.1.3" ;
    compcon:functionOf compcon:cxn_consecutive-coordination ;
    compcon:subtypeOf compcon:sem_event-relation .

compcon:sem_contact-by-impact-event a compcon:sem ;
    rdfs:label "contact by impact event" ;
    dc:description "an <a>event</a> describing contact by impact. <i>Example</i>: hitting is a contact by impact event. (Section 7.3.2)" ;
    dcterms:isPartOf "7.3.2" ;
    skos:altLabel "contact by impact (event)" ;
    compcon:functionOf compcon:cxn_contact-by-impact-verb ;
    compcon:subtypeOf compcon:sem_qualitative-event .

compcon:sem_contact-event a compcon:sem ;
    rdfs:label "contact event" ;
    dc:description "" ;
    compcon:subtypeOf compcon:sem_qualitative-event .

compcon:sem_container a compcon:sem ;
    rdfs:label "container" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_container-term ;
    compcon:subtypeOf compcon:sem_mensural-concept .

compcon:sem_content-causal-relation a compcon:sem ;
    rdfs:label "content causal relation" ;
    cx:hasExample compcon:content-causal-relation-example-0 ;
    dc:description "the semantic relation in a <a>conditional</a>, <a>causal</a>, <a>concessive</a>, or <a>conditional concessive construction</a> that expresses a <a>causal relation</a> between events in the world. <i>Example</i>: in <i>If you press this button, the door will open</i>, there is a content causal relation between the event of your pressing the button and the event of the door opening. A content causal relation contrasts with an <a>epistemic causal relation</a> or a <a>speech act causal relation</a>. (Section 17.3.1)" ;
    dcterms:isPartOf "17.3.1" ;
    compcon:subtypeOf compcon:sem_causal-link .

compcon:sem_continuative a compcon:sem ;
    rdfs:label "continuative" ;
    dc:description "" ;
    skos:altLabel "sem:preservative" ;
    compcon:subtypeOf compcon:sem_phasal-aspect .

compcon:sem_controlled a compcon:sem ;
    rdfs:label "controlled" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_controlled-predicate ;
    compcon:subtypeOf compcon:sem_causal-control .

compcon:sem_controlled-event a compcon:sem ;
    rdfs:label "controlled event" ;
    dc:description "the <a>event</a> class of agentive processes. <i>Example</i>: running is a controlled activity event. (Section 6.3.3)" ;
    dcterms:isPartOf "6.3.3" ;
    skos:altLabel "controlled activity" ;
    compcon:functionOf compcon:cxn_controlled-predicate ;
    compcon:subtypeOf compcon:sem_event .

compcon:sem_creation-event a compcon:sem ;
    rdfs:label "creation event" ;
    dc:description "" ;
    compcon:subtypeOf compcon:sem_qualitative-event .

compcon:sem_damage-event a compcon:sem ;
    rdfs:label "damage event" ;
    dc:description "an <a>event</a> describing material damage to an <a sem:object-concept>object</a>. <i>Example</i>: scratching something is a damage event. (Section 7.3.2)" ;
    dcterms:isPartOf "7.3.2" ;
    compcon:functionOf compcon:cxn_damage-verb ;
    compcon:subtypeOf compcon:sem_qualitative-event .

compcon:sem_deontic-modality a compcon:sem ;
    rdfs:label "deontic modality" ;
    cx:hasExample compcon:deontic-modality-example-0 ;
    dc:description "a type of <a>modality</a> which expresses the attitude of a speaker or other conceiver toward performing an <a>action</a> (i.e. making the action come true). <i>Example</i>: <i>Jerry must get his hair cut</i> is an instance of deontic modality: the speaker expresses her attitude that the situation necessarily will come about. Deontic modality is construed broadly in this textbook, to include wishes as well as intentions and commands, attitude toward oneself performing the action as well as toward others performing the action, and to include <a>objective</a> as well as <a>subjective</a> characterization of the attitude. (Sections 12.1, 12.4)" ;
    dcterms:isPartOf "12.1",
        "12.4" ;
    compcon:subtypeOf compcon:sem_modality .

compcon:sem_departure a compcon:sem ;
    rdfs:label "departure" ;
    cx:hasExample compcon:departure-example-0 ;
    dc:description "the initial phase of the <a>path</a> in a <a>motion event</a>. <i>Example</i>: in <i>He went from the tree to the house</i>, the path oblique phrase <i>from the tree</i> denotes the departure phase of the motion event. (Section 14.4)" ;
    dcterms:isPartOf "14.4" .

compcon:sem_dependent-time-reference a compcon:sem ;
    rdfs:label "dependent time reference" ;
    cx:hasExample compcon:dependent-time-reference-example-0 ;
    dc:description "in a <a>complement clause construction</a>, this is the <a>semantic</a> property that the time reference of the <a>complement</a> <a>event</a> is determined by the time reference of the <a>matrix clause</a> <a>event</a>. <i>Example</i>: in <i>Sally persuaded John to make the cake</i>, the time reference of John's making the cake is dependent on the time reference of Sally's persuading him -- it must follow the persuading event. (Section 18.2.2)" ;
    dcterms:isPartOf "18.2.2" ;
    compcon:subtypeOf compcon:sem_time-reference .

compcon:sem_desiderative-event a compcon:sem ;
    rdfs:label "desiderative event" ;
    cx:hasExample compcon:desiderative-event-example-0 ;
    dc:description "an <a>event</a> that expresses a desire toward the realization of a future event that is expressed by the complement. <i>Example</i>: in <i>Meagan wants to climb Mt. Baldy on Saturday, wants</i> denotes a desiderative event. Noonan (2007:135) includes intending events in the category of desiderative events. (Section 18.2.2)" ;
    dcterms:isPartOf "18.2.2" ;
    skos:altLabel "desiderative (event)" ;
    compcon:functionOf compcon:cxn_desiderative-construction ;
    compcon:subtypeOf compcon:sem_eventive-participant-event .

compcon:sem_dimension a compcon:sem ;
    rdfs:label "dimension" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_dimension-term ;
    compcon:subtypeOf compcon:sem_property-concept .

compcon:sem_direct-causation a compcon:sem ;
    rdfs:label "direct causation" ;
    dc:description "" ;
    compcon:subtypeOf compcon:sem_causal-directness .

compcon:sem_direct-causation-event a compcon:sem ;
    rdfs:label "direct causation event" ;
    cx:hasExample compcon:direct-causation-event-example-0 ;
    dc:description "a <a>causative event</a> in which the <a>causee</a> has no control over the action. <i>Example</i>: <i>I laid the child in the crib</i> is an instance of direct causation. (Section 9.2)" ;
    dcterms:isPartOf "9.2" ;
    compcon:subtypeOf compcon:sem_event .

compcon:sem_directed-change a compcon:sem ;
    rdfs:label "directed change" ;
    cx:hasExample compcon:directed-change-example-0 ;
    dc:description "an <a>event</a> in which the change that a <a>participant</a> undergoes in the course of an event is in one <dq>direction</dq>. <i>Example</i>: in <i>The storm expanded</i>, there is a gradual unidirectional increase in size, and so the event is a directed change. (Section 6.2.1)" ;
    dcterms:isPartOf "6.2.1" ;
    compcon:subtypeOf compcon:sem_change-type .

compcon:sem_distal a compcon:sem ;
    rdfs:label "distal" ;
    dc:description "a <a>contextual</a> category of <a>deixis</a> defined as away from the location of the speaker in the speech event. (Section 3.1.1)" ;
    dcterms:isPartOf "3.1.1" ;
    skos:altLabel "distal (deixis)" ;
    compcon:subtypeOf compcon:sem_spatial-deixis .

compcon:sem_distributive-quantifier a compcon:sem ;
    rdfs:label "distributive quantifier" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_distributive-quantifier-term,
        compcon:cxn_quantifier-attributive-phrase ;
    compcon:subtypeOf compcon:sem_quantitative-relation .

compcon:sem_downtoning a compcon:sem ;
    rdfs:label "downtoning" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_downtoner ;
    compcon:subtypeOf compcon:sem_degree .

compcon:sem_durative a compcon:sem ;
    rdfs:label "durative" ;
    cx:hasExample compcon:durative-example-0 ;
    dc:description "an <a>event</a> that is construed as taking place over a period of time. <i>Example</i>: <i>They inflated the balloon</i> is a durative event. (Section 6.2.1)" ;
    dcterms:isPartOf "6.2.1" ;
    compcon:subtypeOf compcon:sem_durativity .

compcon:sem_dynamic a compcon:sem ;
    rdfs:label "dynamic" ;
    dc:description "a concept construed as involving change over the time course of the <a>event</a>. <i>Example</i>: walking involves change over the time course of the walking event. (Sections 2.1, 6.2.1)" ;
    dcterms:isPartOf "2.1",
        "6.2.1" ;
    compcon:functionOf compcon:cxn_dynamic-predicate-part ;
    compcon:subtypeOf compcon:sem_stativity .

compcon:sem_emotion-event a compcon:sem ;
    rdfs:label "emotion event" ;
    cx:hasExample compcon:emotion-event-example-0 ;
    dc:description "an <a>experiential event</a> involving an <a>experiencer</a>'s emotions directed toward a <a>stimulus</a>. <i>Example</i>: <i>He fears dogs</i> is an example of an emotion event. (Section 7.4)" ;
    dcterms:isPartOf "7.4" ;
    compcon:functionOf compcon:cxn_emotion-verb ;
    compcon:subtypeOf compcon:sem_experiential-event .

compcon:sem_endpoint a compcon:sem ;
    rdfs:label "endpoint" ;
    cx:hasExample compcon:endpoint-example-0 ;
    dc:description "a <a>participant role</a> defined in terms of the <a>participant</a> being acted upon by another participant in a <a>causal chain</a>. <i>Example</i>: in <i>The cats scratched the furniture</i>, the furniture is the endpoint of the causal chain [cats --> furniture]. (Sections 6.1.1, 6.1.2)" ;
    dcterms:isPartOf "6.1.1",
        "6.1.2" ;
    compcon:subtypeOf compcon:sem_central-participant .

compcon:sem_epistemic-causal-relation a compcon:sem ;
    rdfs:label "epistemic causal relation" ;
    cx:hasExample compcon:epistemic-causal-relation-example-0 ;
    dc:description "the semantic relation in a <a>conditional</a>, <a>causal</a>, <a>concessive</a>, or <a>conditional concessive construction</a> that expresses an epistemic inferential relation between two propositions. <i>Example</i>: in <i>If Professor Smith's door is closed, then she is not on campus</i>, there is an epistemic causal relation between the fact that Professor Smith's office door is closed and the inference that she is not on campus. An epistemic causal relation contrasts with a <a>content causal relation</a> and a <a>speech act causal relation</a>. (Section 17.3.1)" ;
    dcterms:isPartOf "17.3.1" ;
    compcon:subtypeOf compcon:sem_causal-link .

compcon:sem_event-oriented a compcon:sem ;
    rdfs:label "event-oriented" ;
    cx:hasExample compcon:event-oriented-example-0 ;
    dc:description "a stative <a>element</a> in a <a>stative complex predicate</a> that describes a <a>state</a> of the <a>event</a> denoted by the complex predicate. <i>Example</i>: in English <i>I ate the carrots slowly, slowly</i> is a property of the event of eating. Event-oriented contrasts with <a>participant-oriented</a>. (Section 14.1)" ;
    dcterms:isPartOf "14.1" ;
    compcon:functionOf compcon:cxn_manner-complex-predicate ;
    compcon:subtypeOf compcon:sem_orientation .

compcon:sem_eventive-perception-event a compcon:sem ;
    rdfs:label "eventive perception event" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_perception-complement-clause-construction ;
    compcon:subtypeOf compcon:sem_eventive-participant-event .

compcon:sem_evidentiality a compcon:sem ;
    rdfs:label "evidentiality" ;
    cx:hasExample compcon:evidentiality-example-0 ;
    dc:description "a category related to <a>epistemic modality</a> which indicates the epistemic justification for believing a <a>proposition</a>. <i>Example</i>: English <i>I hear you're going to Stanford this fall</i> is an evidential construction, using the first singular simple present form <i>I hear...</i>, which indicates that my evidence for your going to Stanford in the fall is hearsay. (Section 12.3.4)" ;
    dcterms:isPartOf "12.3.4" ;
    compcon:subtypeOf compcon:sem_modality .

compcon:sem_exclusive-disjunction a compcon:sem ;
    rdfs:label "exclusive disjunction" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_exclusive-disjunctive-coordination ;
    compcon:subtypeOf compcon:sem_event-relation .

compcon:sem_exhaustive-list a compcon:sem ;
    rdfs:label "exhaustive list" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_non-exhaustive-list-coordination ;
    compcon:subtypeOf compcon:sem_event-relation .

compcon:sem_existence-event a compcon:sem ;
    rdfs:label "existence event" ;
    cx:hasExample compcon:existence-event-example-0 ;
    dc:description "a situation in which the existence of some entity is presented. Existential situations favor a <a inf:thetic>thetic</a> construal. <i>Example</i>: <i>There are apples in the kitchen</i> expresses the existence of the relevant set of apples. (Section 11.3.1)" ;
    dcterms:isPartOf "11.3.1" ;
    skos:altLabel "existential event" ;
    compcon:functionOf compcon:cxn_existential-negation-construction ;
    compcon:subtypeOf compcon:sem_qualitative-event .

compcon:sem_experience a compcon:sem ;
    rdfs:label "experience" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_experience-verb ;
    compcon:subtypeOf compcon:sem_causal-directionality .

compcon:sem_experience-event a compcon:sem ;
    rdfs:label "experience event" ;
    cx:hasExample compcon:experience-event-example-0 ;
    dc:description "an <a>experiential event</a> which describes the state holding between an <a>experiencer</a> directing her/his attention to a <a>stimulus</a> and the stimulus altering the mental state of the experiencer (or the inception of such a state). <i>Example</i>: <i>I saw the dog</i> is an instance of an experience event. (Section 7.4)" ;
    dcterms:isPartOf "7.4" ;
    compcon:functionOf compcon:cxn_experience-verb ;
    compcon:subtypeOf compcon:sem_event .

compcon:sem_experiencer a compcon:sem ;
    rdfs:label "experiencer" ;
    cx:hasExample compcon:experiencer-example-0 ;
    dc:description "the person who experiences the internal mental phenomenon or bodily sensation in an <a>experiential event</a>. <i>Example</i>: in <i>Freddy saw the bear</i>, Freddy is the experiencer. The experiencer is almost always human. (Sections 6.1.2, <b>7.4</b>)" ;
    dcterms:isPartOf "6.1.2",
        "7.4" ;
    compcon:functionOf compcon:cxn_experiencer-phrase ;
    compcon:roleOf compcon:sem_experiential-event ;
    compcon:subtypeOf compcon:sem_co-actor,
        compcon:sem_undergoer .

compcon:sem_expertum a compcon:sem ;
    rdfs:label "expertum" ;
    cx:hasExample compcon:expertum-example-0 ;
    dc:description "the experiential event conceptualized as a participant. <i>Example</i>: in Yórùbá <i>ẹ̀rù´ bà mí</i> <q>I felt afraid</q> [lit. <q>fear fell on me</q>] <i>ẹ̀rù´</i> <q>fear</q> is the expertum. (Section 7.4)" ;
    dcterms:isPartOf "7.4" ;
    compcon:roleOf compcon:sem_experiential-event ;
    compcon:subtypeOf compcon:sem_semantic-role .

compcon:sem_extroverted a compcon:sem ;
    rdfs:label "extroverted" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_extroverted-verb ;
    compcon:subtypeOf compcon:sem_causal-cyclicality .

compcon:sem_extroverted-event a compcon:sem ;
    rdfs:label "extroverted event" ;
    cx:hasExample compcon:extroverted-event-example-0,
        compcon:extroverted-event-example-1 ;
    dc:description "an event not normally performed on oneself or on each other. <i>Examples</i>: seeing something vs. oneself, or loving someone vs. oneself (or even each other), are instances of extroverted events, and <i>see</i> and <i>love</i> are extroverted verbs. (Section 7.2)" ;
    dcterms:isPartOf "7.2" ;
    compcon:functionOf compcon:cxn_extroverted-verb ;
    compcon:subtypeOf compcon:sem_event .

compcon:sem_factive-event a compcon:sem ;
    rdfs:label "factive event" ;
    cx:hasExample compcon:factive-event-example-0 ;
    dc:description "an <a>event</a> in which a <a>positive epistemic stance</a> is taken by the speaker toward a <a>proposition</a> denoted by some part of that construction. <i>Example</i>: <i>It is appalling that Donald won the election</i> -- an example of a <a>commentative</a> <a>complement clause construction</a> -- is factive in that the speaker takes a positive epistemic stance toward the <a>complement</a> proposition that Donald won the election." .

compcon:sem_fearing-event a compcon:sem ;
    rdfs:label "fearing event" ;
    cx:hasExample compcon:fearing-event-example-0 ;
    dc:description "an <a>evaluative event</a> in which a negative evaluative judgment about a <a>proposition</a> expressed by the <a>complement</a> of the commentative event is made, and there is a <a>neutral epistemic stance</a> by the speaker toward the proposition. <i>Example</i>: in <i>Jill fears that Donald has won the election</i>, the commentative predicate <i>fears</i> expresses Jill's evaluation of Donald's winning the election, and also presupposes that the speaker does not know whether Donald has won the election. (Section 18.2.2)" ;
    dcterms:isPartOf "18.2.2" ;
    compcon:functionOf compcon:cxn_fearing-construction ;
    compcon:subtypeOf compcon:sem_evaluative-event .

compcon:sem_figure a compcon:sem ;
    rdfs:label "figure" ;
    cx:hasExample compcon:figure-example-0 ;
    dc:description "the object in a spatial scene whose location or path of motion is being described relative to a <a>ground</a>. <i>Example</i>: in <i>Meagan ran into the cave</i>, Meagan is the figure, and her path of motion is described relative to the location of the cave. (Sections 7.3.1, 14.5)" ;
    dcterms:isPartOf "14.5",
        "7.3.1" ;
    compcon:functionOf compcon:cxn_figure-phrase ;
    compcon:roleOf compcon:sem_spatial-event ;
    compcon:subtypeOf compcon:sem_undergoer .

compcon:sem_figure-ground-spatial-relation a compcon:sem ;
    rdfs:label "figure-ground spatial relation" ;
    cx:hasExample compcon:figure-ground-spatial-relation-example-0 ;
    dc:description "a spatial relation that holds between two <a sem:object-concept>objects</a> such that the ground object functions as a reference point for locating the figure. <i>Example</i>: <i>the bicycle in the garage</i> is a <a>locative modification construction</a> that expresses a figure--ground spatial relation between the bicycle and the garage. (Sections 4.1.4, 10.4.1)" ;
    dcterms:isPartOf "10.4.1",
        "4.1.4" ;
    skos:altLabel "figure--ground spatial relation",
        "location relation",
        "spatial figure--ground relation" ;
    compcon:functionOf compcon:cxn_locative-modification-construction,
        compcon:cxn_predicational-location-construction,
        compcon:cxn_presentational-location-construction ;
    compcon:subtypeOf compcon:sem_spatial-event .

compcon:sem_first-person a compcon:sem ;
    rdfs:label "first person" ;
    dc:description "" ;
    skos:altLabel "1st person" ;
    compcon:functionOf compcon:cxn_first-person-pronoun ;
    compcon:subtypeOf compcon:sem_person-deixis .

compcon:sem_force a compcon:sem ;
    rdfs:label "force" ;
    cx:hasExample compcon:force-example-0 ;
    dc:description "a <a>semantic role</a> including <a>participant roles</a> for a <a>participant</a> that initiates an event but is not volitional (usually because it is not human), i.e. is not an <a>agent</a>. <i>Example</i>: in <i>Lightning shattered the old tree</i>, the lightning initiates the shattering event but is not an agent. (Section 6.1.2)" ;
    dcterms:isPartOf "6.1.2" ;
    compcon:subtypeOf compcon:sem_external-cause .

compcon:sem_future-oriented a compcon:sem ;
    rdfs:label "future-oriented" ;
    cx:hasExample compcon:future-oriented-example-0,
        compcon:future-oriented-example-1 ;
    dc:description "the event in question is a future event from the reference time of the <a sem:deontic-modality>deontic modal</a> situation. <i>Examples</i>: in English <i>You may leave now</i>, a <a>subjective</a> deontic modal construction, the event is in the future from the speech act time (that is, simple future). In English <i>Carol intended to close the door</i>, an <a>objective</a> deontic modal construction, the event is in the future from the reported deontic modal event time: that is, closing the door is projected to a future from the past event of Carol's having the intention. (Section 12.4)" ;
    dcterms:isPartOf "12.4" .

compcon:sem_gender-class a compcon:sem ;
    rdfs:label "gender--class" ;
    dc:description "a <a>semantic</a> category that is expressed as an <a>indexation feature</a>, and covers both distinctions based primarily on sex and animacy (<q>gender</q>) and distinctions based on animacy, sex, and various semantic properties of inanimate -- or at least nonhuman -- <a sem:object-concept>objects</a>. (Section 4.4.2)" ;
    dcterms:isPartOf "4.4.2" ;
    skos:altLabel "class",
        "gender" ;
    compcon:functionOf compcon:cxn_gender-term ;
    compcon:subtypeOf compcon:sem_property-concept .

compcon:sem_generic-conditional-relation a compcon:sem ;
    rdfs:label "generic conditional relation" ;
    cx:hasExample compcon:generic-conditional-relation-example-0 ;
    dc:description "a subtype of the <a>conditional relation</a> in which an event causes another event generically or habitually. <i>Example</i>: <i>If/When/Whenever a dog starts barking, I run away</i> is an instance of a generic conditional relation and construction -- it doesn't describe a specific instance of a dog barking causing me to run away; instead, it describes a general or habitual pattern of this causal sequence of events. (Section 17.3.1)" ;
    dcterms:isPartOf "17.3.1" ;
    skos:altLabel "generic conditional (relation)" ;
    compcon:functionOf compcon:cxn_generic-conditional-construction ;
    compcon:subtypeOf compcon:sem_conditional-relation .

compcon:sem_gradable-predicative-scale a compcon:sem ;
    rdfs:label "gradable predicative scale" ;
    cx:hasExample compcon:gradable-predicative-scale-example-0 ;
    dc:description "in a <a>comparative construction</a> or an <a>equative construction</a>, the <a>predicate</a> that defines the <a sem:scalar-model>scale</a> on which the <a>comparee</a> and <a>standard</a> are located. <i>Example</i>: in <i>Your cat is as big as my dog</i>, size is the gradable predicative scale on which it is asserted that the position of the cat is the same as the position of the dog. (Section 17.2.1)" ;
    dcterms:isPartOf "17.2.1" ;
    skos:altLabel "(gradable) predicative scale" .

compcon:sem_gradient a compcon:sem ;
    rdfs:label "gradient" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_antonym ;
    compcon:subtypeOf compcon:sem_scale .

compcon:sem_ground a compcon:sem ;
    rdfs:label "ground" ;
    cx:hasExample compcon:ground-example-0 ;
    dc:description "the reference point for locating a <a>figure</a> in a spatial scene. <i>Example</i>: in <i>Meagan ran into the cave</i>, the cave serves as the ground for locating the path of motion of Meagan. (Sections 7.3.1, 14.5)" ;
    dcterms:isPartOf "14.5",
        "7.3.1" ;
    compcon:functionOf compcon:cxn_ground-phrase,
        compcon:cxn_locative-phrase ;
    compcon:roleOf compcon:sem_spatial-event ;
    compcon:subtypeOf compcon:sem_semantic-role .

compcon:sem_group a compcon:sem ;
    rdfs:label "group" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_group-term ;
    compcon:subtypeOf compcon:sem_mensural-concept .

compcon:sem_hearer a compcon:sem ;
    rdfs:label "hearer" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_inclusive-pronoun,
        compcon:cxn_inclusory-construction ;
    compcon:subtypeOf compcon:sem_object-concept .

compcon:sem_hoping-event a compcon:sem ;
    rdfs:label "hoping event" ;
    cx:hasExample compcon:hoping-event-example-0 ;
    dc:description "an <a>evaluative event</a> in which a positive evaluative judgment about a <a>proposition</a> expressed by the <a>complement</a> of the commentative event is made, and there is a <a>neutral epistemic stance</a> by the speaker toward the proposition. <i>Example</i>: in <i>Jill hopes that Joe won the election</i>, the commentative predicate <i>hopes</i> expresses Jill's evaluation of Joe's winning the election, and also presupposes that the speaker does not know whether Joe has won the election. (Section 18.2.2)" ;
    dcterms:isPartOf "18.2.2" ;
    skos:altLabel "hoping (event)" ;
    compcon:functionOf compcon:cxn_hoping-construction ;
    compcon:subtypeOf compcon:sem_evaluative-event .

compcon:sem_human a compcon:sem ;
    rdfs:label "human" ;
    cx:hasExample compcon:human-example-0 ;
    dc:description "a semantic <u>category</u> of <a sem:object-concept>objects</a> that denote persons. <i>Example</i>: <i>woman</i> denotes a human. (Section 3.1.2)" ;
    dcterms:isPartOf "3.1.2" ;
    compcon:subtypeOf compcon:sem_animacy .

compcon:sem_human-propensity a compcon:sem ;
    rdfs:label "human propensity" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_human-propensity-term ;
    compcon:subtypeOf compcon:sem_property-concept .

compcon:sem_inanimate a compcon:sem ;
    rdfs:label "inanimate" ;
    cx:hasExample compcon:inanimate-example-0 ;
    dc:description "a semantic <u>category</u> of <a sem:object-concept>objects</a> that denotes inanimate entities. <i>Example</i>: <i>rock</i> denotes an inanimate entity. (Section 3.1.2)" ;
    dcterms:isPartOf "3.1.2" ;
    compcon:subtypeOf compcon:sem_animacy .

compcon:sem_inceptive a compcon:sem ;
    rdfs:label "inceptive" ;
    dc:description "" ;
    compcon:subtypeOf compcon:sem_phasal-aspect .

compcon:sem_inclusive-disjunction a compcon:sem ;
    rdfs:label "inclusive disjunction" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_inclusive-disjunctive-coordination ;
    compcon:subtypeOf compcon:sem_event-relation .

compcon:sem_independent-time-reference a compcon:sem ;
    rdfs:label "independent time reference" ;
    cx:hasExample compcon:independent-time-reference-example-0 ;
    dc:description "in a <a>complement clause construction</a>, this is the <a>semantic</a> property that the time reference of the <a>complement</a> <a>event</a> is not determined by the time reference of the <a>matrix clause</a> <a>event</a>. <i>Example</i>: in <i>Sally thinks John made / is making / will make the cake</i>, the time reference of John's making the cake is independent of the time reference of Sally's thought. (Section 18.2.2)" ;
    dcterms:isPartOf "18.2.2" ;
    compcon:subtypeOf compcon:sem_time-reference .

compcon:sem_indirect-causation a compcon:sem ;
    rdfs:label "indirect causation" ;
    dc:description "" ;
    compcon:subtypeOf compcon:sem_causal-directness .

compcon:sem_indirect-causation-event a compcon:sem ;
    rdfs:label "indirect causation-event" ;
    cx:hasExample compcon:indirect-causation-event-example-0 ;
    dc:description "a <a>causative event</a> in which one <a>agent</a> (the <a>causer</a>) gets the other agent (the <a>causee</a>) to do something but doesn't participate in the carrying out of the action. <i>Example</i>: <i>I had the students fill out the questionnaire</i> is an instance of indirect causation. (Section 9.2)" ;
    dcterms:isPartOf "9.2" ;
    compcon:subtypeOf compcon:sem_event .

compcon:sem_ingestion-event a compcon:sem ;
    rdfs:label "ingestion event" ;
    cx:hasExample compcon:ingestion-event-example-0 ;
    dc:description "an <a>event</a> which describes the ingestion of food or drink by a person or animal, causing the food to disappear but also causing a change in the physiological state of the person/animal. <i>Example</i>: <i>Elena ate a lot of veggie chips</i> is an instance of an ingestion event. (Section 7.4)" ;
    dcterms:isPartOf "7.4" ;
    compcon:functionOf compcon:cxn_ingestion-verb ;
    compcon:subtypeOf compcon:sem_experiential-event .

compcon:sem_initiator a compcon:sem ;
    rdfs:label "initiator" ;
    cx:hasExample compcon:initiator-example-0 ;
    dc:description "a <a>participant role</a> defined in terms of the <a>participant</a> acting on another participant in a <a>causal chain</a>. <i>Example</i>: in <i>The cats scratched the furniture</i>, the cats are the initiator of the causal chain cats --> furniture. (Sections 6.1.1, 6.1.2)" ;
    dcterms:isPartOf "6.1.1",
        "6.1.2" ;
    compcon:subtypeOf compcon:sem_central-participant .

compcon:sem_instrument a compcon:sem ;
    rdfs:label "instrument" ;
    cx:hasExample compcon:instrument-example-0 ;
    dc:description "a <a>semantic role</a> including <a>participant roles</a> for a <a>participant</a> that is manipulated by the agent to bring about an <a>event</a>. <i>Example</i>: in <i>Jack broke the window with a rock</i>, the agent (Jack) manipulates the rock to bring about the breaking of the window. (Sections 6.1.2, <b>6.2.1</b>)" ;
    dcterms:isPartOf "6.1.2",
        "6.2.1" ;
    compcon:subtypeOf compcon:sem_external-cause .

compcon:sem_intensifying a compcon:sem ;
    rdfs:label "intensifying" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_intensifier ;
    compcon:subtypeOf compcon:sem_degree .

compcon:sem_interaction-event a compcon:sem ;
    rdfs:label "interaction event" ;
    dc:description "an <a>event</a> in which one <a>participant</a> acts on a second participant, but the change that occurs to the second participant is at least partly independent of the <a>force</a> <u>transmitted</u> by the first participant. <i>Examples</i>: interaction events include <a>pursuit events</a>, events involving two agents such as ordering someone (to do something) or supervising someone, and events involving an agent and an event, state, social institution, and so on, such as managing a budget, avoiding situations, or conforming to institutional standards. (Section 7.3)" ;
    dcterms:isPartOf "7.3" ;
    compcon:functionOf compcon:cxn_interaction-verb ;
    compcon:subtypeOf compcon:sem_social-event .

compcon:sem_introverted a compcon:sem ;
    rdfs:label "introverted" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_introverted-verb ;
    compcon:subtypeOf compcon:sem_causal-cyclicality .

compcon:sem_introverted-event a compcon:sem ;
    rdfs:label "introverted event" ;
    dc:description "an <a>event</a> typically performed on oneself or by oneself, but that could be performed on someone else. <i>Examples</i>: shaving oneself vs. shaving someone else, laying down vs. laying someone else down, or quarreling (with each other) are instances of introverted events. (Section 7.2)" ;
    dcterms:isPartOf "7.2" ;
    compcon:functionOf compcon:cxn_introverted-verb ;
    compcon:subtypeOf compcon:sem_event .

compcon:sem_killing-injuring-event a compcon:sem ;
    rdfs:label "killing/injuring event" ;
    dc:description "an <a>event</a> describing the injuring of an individual, including to the point that the individual dies. <i>Example</i>: stabbing is a killing/injuring event. (Section 7.3.2)" ;
    dcterms:isPartOf "7.3.2" ;
    skos:altLabel "injuring event",
        "killing event" ;
    compcon:functionOf compcon:cxn_killing-injuring-verb ;
    compcon:subtypeOf compcon:sem_qualitative-event .

compcon:sem_kinship-relation a compcon:sem ;
    rdfs:label "kinship relation" ;
    cx:hasExample compcon:kinship-relation-example-0 ;
    dc:description "a relation that holds between a person and certain other persons by biological relations, or social relations such as marriage, and other socially defined kin relations. <i>Example</i>: <i>my mother</i> is an instance of a <a>possession construction</a> expressing a kinship relation. (Section 4.1.4)" ;
    dcterms:isPartOf "4.1.4" ;
    skos:altLabel "kinship (relation)" ;
    compcon:functionOf compcon:cxn_inalienable-possession-attributive-phrase,
        compcon:cxn_kinship-noun ;
    compcon:subtypeOf compcon:sem_possession .

compcon:sem_knowledge-event a compcon:sem ;
    rdfs:label "knowledge event" ;
    cx:hasExample compcon:knowledge-event-example-0 ;
    dc:description "a <a>propositional attitude event</a> in which a <a>positive epistemic stance</a> toward the relevant <a>proposition</a> expressed is presupposed to be taken by the <u>speaker</u>. <i>Example</i>: in <i>Sally knows that Donald won the election</i>, Sally's belief with respect to the proposition that Donald won the election is reported by the speaker; and, in addition, a <a>positive epistemic stance</a> is taken by the speaker toward that proposition (i.e., the speaker believes that Donald indeed won the election). (Section 18.2.2)" ;
    dcterms:isPartOf "18.2.2" ;
    skos:altLabel "knowledge (event)" ;
    compcon:functionOf compcon:cxn_knowledge-construction ;
    compcon:subtypeOf compcon:sem_propositional-attitude-event .

compcon:sem_less-affected-p a compcon:sem ;
    rdfs:label "less affected P" ;
    cx:hasExample compcon:less-affected-p-example-0,
        compcon:less-affected-p-example-1 ;
    dc:description "a <a>function</a> related to the function of the <a>antipassive construction</a>, in which the <a>P</a> <a>participant</a> is less <a>affected</a> than it is in the equivalent event expressed by <a>transitive construction</a>. <i>Example</i>: in <i>The coyote chewed on the deer bone</i>, the deer bone is a less affected P participant than in the transitive <i>The coyote chewed the deer bone</i>. (Section 8.4)" ;
    dcterms:isPartOf "8.4" ;
    skos:altLabel "LAP" ;
    compcon:functionOf compcon:cxn_antipassive-oblique-phrase ;
    compcon:subtypeOf compcon:sem_p-role .

compcon:sem_less-individuated-p a compcon:sem ;
    rdfs:label "less individuated P" ;
    cx:hasExample compcon:less-individuated-p-example-0 ;
    dc:description "the basic <a>function</a> of the <a>antipassive construction</a>, in which the <a>P</a> <a>participant</a> is less individuated (indefinite, nonspecific, generic, or even simply plural), since less individuation is an indicator of lower <a>salience</a>. <i>Example</i>: in West Greenlandic <i>inun-nik tuqut-si-vuq</i> <q>He killed people</q>, the P participant <q>people</q> is a plural and generic referent. (Section 8.4)" ;
    dcterms:isPartOf "8.4" ;
    skos:altLabel "LIP" ;
    compcon:functionOf compcon:cxn_antipassive-oblique-phrase ;
    compcon:subtypeOf compcon:sem_p-role .

compcon:sem_let-alone-relation a compcon:sem ;
    rdfs:label "let alone relation" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_let-alone-construction ;
    compcon:subtypeOf compcon:sem_event-relation .

compcon:sem_location a compcon:sem ;
    rdfs:label "location" ;
    cx:hasExample compcon:location-example-0 ;
    dc:description "the combination of the <a>path</a> and the <a>ground</a> in a <a>spatial figure--ground relation</a>. <i>Example</i>: in <i>The book is on the table</i>, the location is on the table -- that is, the spatial location of the figure (the book) as defined by the path relating the location of the figure to the location of the ground. (Section 10.4.1)" ;
    dcterms:isPartOf "10.4.1" ;
    compcon:subtypeOf compcon:sem_event .

compcon:sem_maleficiary a compcon:sem ;
    rdfs:label "maleficiary" ;
    cx:hasExample compcon:maleficiary-example-0 ;
    dc:description "a <a>semantic role</a> including <a>participant roles</a> for a <a>participant</a> that is negatively affected by the outcome of the <a>event</a>. <i>Example</i>: in <i>My car broke down on me</i>, I am a maleficiary: I am negatively affected by the outcome of the event. (Section 6.1.2)" ;
    dcterms:isPartOf "6.1.2" ;
    compcon:subtypeOf compcon:sem_affectee .

compcon:sem_manipulative-event a compcon:sem ;
    rdfs:label "manipulative event" ;
    cx:hasExample compcon:manipulative-event-example-0 ;
    dc:description "an <a>event</a> where an <a>agent</a> acts to bring about the event expressed by the <a>complement</a>. <i>Example</i>: in <i>Bruce convinced Greg to take him to San Rafael, convinced</i> denotes a manipulative event. Manipulative events include causative and permissive events. The complement event of manipulative events has <a>dependent time reference</a>. (Section 18.2.2)" ;
    dcterms:isPartOf "18.2.2" ;
    skos:altLabel "manipulative (event)" ;
    compcon:functionOf compcon:cxn_manipulative-construction ;
    compcon:subtypeOf compcon:sem_eventive-participant-event .

compcon:sem_manner a compcon:sem ;
    rdfs:label "manner" ;
    dc:description "" .

compcon:sem_manner-event a compcon:sem ;
    rdfs:label "manner event" ;
    cx:hasExample compcon:manner-event-example-0 ;
    dc:description "an event that is described in terms of the manner by which the process progresses (or is brought about by an external cause). <i>Example</i>: in <i>She smeared jam on the toast</i>, the event is described in terms of the manner by which the jam is applied to the toast. (Section 7.3.2)" ;
    dcterms:isPartOf "7.3.2" ;
    compcon:functionOf compcon:cxn_manner-verb ;
    compcon:subtypeOf compcon:sem_event .

compcon:sem_manner-of-motion-event a compcon:sem ;
    rdfs:label "manner of motion event" ;
    cx:hasExample compcon:manner-of-motion-event-example-0 ;
    dc:description "an event that describes motion of a <a>figure</a> in terms of how the figure travels. <i>Example</i>: in <i>Sam strode into the room, stride</i> is a manner of motion verb expressing a manner of motion event. (Sections 7.3.1, 14.2)" ;
    dcterms:isPartOf "14.2",
        "7.3.1" ;
    skos:altLabel "manner of motion (event)" ;
    compcon:functionOf compcon:cxn_manner-of-motion-verb ;
    compcon:subtypeOf compcon:sem_motion-event .

compcon:sem_manner-profile a compcon:sem ;
    rdfs:label "manner profile" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_manner-verb ;
    compcon:subtypeOf compcon:sem_causal-chain-profile .

compcon:sem_material a compcon:sem ;
    rdfs:label "material" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_material-term ;
    compcon:subtypeOf compcon:sem_property-concept .

compcon:sem_means a compcon:sem ;
    rdfs:label "means" ;
    cx:hasExample compcon:means-example-0,
        compcon:means-example-1 ;
    dc:description "the semantic relation between two <a>events</a> where one event accompanies the other event and further characterizes the other event in some way. <i>Example</i>: <i>He got into the army by lying about his age</i> is a <a>figure--ground construal</a> of the means relation in an <a>adverbial clause construction</a>, and <i>He lied about his age and got into the army</i> is a <a>complex figure construal</a> of the relation in a <a>coordinate clause construction</a>. In the figure--ground construal, the accompanying event is construed as the ground. (Section 15.3.1)" ;
    dcterms:isPartOf "15.3.1" ;
    skos:altLabel "positive circumstantial" ;
    compcon:functionOf compcon:cxn_means-construction ;
    compcon:subtypeOf compcon:sem_event-relation .

compcon:sem_measure a compcon:sem ;
    rdfs:label "measure" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_measure-term ;
    compcon:subtypeOf compcon:sem_mensural-concept .

compcon:sem_measuring a compcon:sem ;
    rdfs:label "measuring" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_measurement ;
    compcon:subtypeOf compcon:sem_degree .

compcon:sem_message a compcon:sem ;
    rdfs:label "message" ;
    dc:description "" ;
    compcon:roleOf compcon:sem_communication-event ;
    compcon:subtypeOf compcon:sem_undergoer .

compcon:sem_mirative a compcon:sem ;
    rdfs:label "mirative" ;
    cx:hasExample compcon:mirative-example-0 ;
    dc:description "a speaker attitude of surprise toward the <a>propositional content</a> in the speaker's utterance. <i>Example</i>: Lhasa Tibetan has a distinct <a>construction</a> for expressing the mirative function with the form <i>'dug</i>: <i>nga-rdngul tog=tsam <b>'dug</b></i> <q>I have some money</q> expresses not only that the speaker has money, but that she is surprised to find this out. (Sections 12.1, 12.5.2)" ;
    dcterms:isPartOf "12.1",
        "12.5.2" ;
    compcon:subtypeOf compcon:sem_mental-space .

compcon:sem_monovalent a compcon:sem ;
    rdfs:label "monovalent" ;
    dc:description "an event with a <a>valency</a> of one -- that is, with one <a>central participant role</a>. <i>Example</i>: sleeping is a monovalent event. (Section 6.1.2)" ;
    dcterms:isPartOf "6.1.2" ;
    compcon:functionOf compcon:cxn_intransitive-construction ;
    compcon:subtypeOf compcon:sem_valency .

compcon:sem_necessary-participant-sharing a compcon:sem ;
    rdfs:label "necessary participant sharing" ;
    cx:hasExample compcon:necessary-participant-sharing-example-0 ;
    dc:description "in a <a>complement clause construction</a>, this is the <a>semantic</a> property that the <a>meaning</a> of the <a>complement-taking predicate</a> requires that one or more <a>participants</a> of the <a>complement</a> <a>event</a> is shared with the event denoted by the complement-taking predicate. <i>Example</i>: in <i>I told Fred to bring me a screwdriver</i>, the <a>agent</a> of the complement bringing event is necessarily also the <a>causee</a> of the manipulative event of telling. (Section 18.2.2)" ;
    dcterms:isPartOf "18.2.2" ;
    skos:altLabel "necessarily shared participant" .

compcon:sem_negative-circumstantial a compcon:sem ;
    rdfs:label "negative circumstantial" ;
    cx:hasExample compcon:negative-circumstantial-example-0,
        compcon:negative-circumstantial-example-1 ;
    dc:description "the semantic relation between two <a>events</a> where one event does not accompany the other event and further characterizes the other event in some way. In this respect, the negative circumstantial relation is a <dq>negative</dq> version of the <a>means</a> or positive circumstantial relation. <i>Example</i>: <i>She carried the punch into the living room without spilling a drop</i> is a <a>figure--ground construal</a> of the negative circumstantial relation in an <a>adverbial clause construction</a>, and <i>She carried the punch into the living room, and she didn't spill a drop!</i> is a <a>complex figure construal</a> of the relation in a <a>coordinate clause construction</a>. In the figure--ground construal, the non-accompanying event is construed as the ground. (Section 15.3.1)" ;
    dcterms:isPartOf "15.3.1" ;
    compcon:functionOf compcon:cxn_negative-circumstantial-construction ;
    compcon:subtypeOf compcon:sem_event-relation .

compcon:sem_negative-epistemic-stance a compcon:sem ;
    rdfs:label "negative epistemic stance" ;
    cx:hasExample compcon:negative-epistemic-stance-example-0 ;
    dc:description "a negative commitment on the part of the speaker to the actuality of a <a>proposition</a> expressed in a <a>clause</a>. <i>Example</i>: in <i>If you had pressed this button, the door would have opened</i>, the speaker has expressed a commitment to the proposition that you didn't press the button. (Section 17.3.1)" ;
    dcterms:isPartOf "17.3.1" ;
    compcon:subtypeOf compcon:sem_hypothetical .

compcon:sem_negative-polarity a compcon:sem ;
    rdfs:label "negative polarity" ;
    cx:hasExample compcon:negative-polarity-example-0,
        compcon:negative-polarity-example-1 ;
    dc:description "indicates that the situation expressed in the utterance is false. <i>Example</i>: <i>Kit didn't find his glasses</i> expresses that Kit finding his glasses is false. This is the prototypical use of negative polarity. Negative polarity may be used to reject other aspects of the construal of the situation expressed in the utterance, as in <i>Kit didn't like the movie -- he loved it!</i> (where the degree expressed by <i>like</i> is the information rejected). (Section 12.1)" ;
    dcterms:isPartOf "12.1" ;
    skos:altLabel "negative (polarity)" ;
    compcon:functionOf compcon:cxn_negation-construction ;
    compcon:subtypeOf compcon:sem_polarity .

compcon:sem_neutral-epistemic-stance a compcon:sem ;
    rdfs:label "neutral epistemic stance" ;
    cx:hasExample compcon:neutral-epistemic-stance-example-0 ;
    dc:description "no commitment on the part of the speaker to the actuality of a <a>proposition</a> expressed in a <a>clause</a>. <i>Example</i>: in <i>If you press this button, the door will open</i>, the speaker does not have a commitment to either your pressing the button or your not pressing the button. (Section 17.3.1)" ;
    dcterms:isPartOf "17.3.1" ;
    compcon:subtypeOf compcon:sem_hypothetical .

compcon:sem_non-exhaustive-list a compcon:sem ;
    rdfs:label "non-exhaustive list" ;
    dc:description "" ;
    compcon:subtypeOf compcon:sem_event-relation .

compcon:sem_noncausal-event a compcon:sem ;
    rdfs:label "noncausal event" ;
    dc:description "an <a>event</a> that has a participant that undergoes some sort of change, possibly brought about by an external cause <a>participant</a>; but the external cause participant is not conceptualized as a <a>central participant</a> in the event. <i>Example</i>: a window breaking is an example of a noncausal event. Noncausal events are contrasted with <a>causal events</a>. (Section 6.3.4)" ;
    dcterms:isPartOf "6.3.4" .

compcon:sem_nonrelational a compcon:sem ;
    rdfs:label "nonrelational" ;
    dc:description "a concept that does not inherently make reference to another entity: a property is a property of something, an action is performed by someone or something, but most members of the object class just <dq>are</dq> without making reference to another entity. <i>Example</i>: a brick is a nonrelational entity: its existence is not dependent on another entity in the way that the color of the brick is dependent on the existence of the brick. (Section 2.1)" ;
    dcterms:isPartOf "2.1" ;
    compcon:subtypeOf compcon:sem_relationality .

compcon:sem_number a compcon:sem ;
    rdfs:label "number" ;
    dc:description "a semantic category that is often (though not always) expressed as an inflectional category, that denotes the cardinality of a referent. Typical values for number inflections are singular, plural, and dual, although there are other rarer values. <a>Indexation</a> frequently indicates the number of the referent. (Section 4.4)" ;
    dcterms:isPartOf "4.4" .

compcon:sem_objective a compcon:sem ;
    rdfs:label "objective" ;
    cx:hasExample compcon:objective-example-0,
        compcon:objective-example-1 ;
    dc:description "describing an <a>entity</a> from an <dq>outside</dq>, explicit perspective on the entity, in contrast to a <a>subjective</a> construal. <i>Example</i>: in the objective <a>epistemic modal</a> <a>construction</a> <i>Jim thought that Wendy was in Santa Fe</i>, the attitude about whether Wendy being in Santa Fe is true is that of Jim, not the speaker, and in the past, not at the time of the speech event; both of these pieces of information are explicitly expressed in the sentence (<i>Jim</i> and <i>thought</i>). It is also possible to construe speaker attitude at the speech event time as objective, as in <i>I think that Wendy was in Santa Fe</i>, where speaker (<i>I</i>) and speaker attitude (<i>think</i>) are explicitly expressed. (Section 12.3.4)" ;
    dcterms:isPartOf "12.3.4" ;
    compcon:subtypeOf compcon:sem_epistemic-modality .

compcon:sem_occupant a compcon:sem ;
    rdfs:label "occupant" ;
    dc:description "" ;
    compcon:roleOf compcon:sem_social-role-event ;
    compcon:subtypeOf compcon:sem_co-actor .

compcon:sem_ordinal-numeral a compcon:sem ;
    rdfs:label "ordinal numeral" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_ordinal-numeral-term ;
    compcon:subtypeOf compcon:sem_numeral,
        compcon:sem_set-member .

compcon:sem_other a compcon:sem ;
    rdfs:label "other" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_associative-construction,
        compcon:cxn_inclusory-construction ;
    compcon:subtypeOf compcon:sem_object-concept .

compcon:sem_ownership a compcon:sem ;
    rdfs:label "ownership" ;
    cx:hasExample compcon:ownership-example-0 ;
    dc:description "a culturally sanctioned relation of control between a person and a physical object, such as an artifact, foodstuff, or shelter, or a more abstract object of value such as shares in a company. <i>Example</i>: in a common interpretation of <i>Sally's truck</i>, Sally owns the truck. (Section 4.1.4)" ;
    dcterms:isPartOf "4.1.4" ;
    skos:altLabel "ownership (relation)" ;
    compcon:functionOf compcon:cxn_alienable-possession-attributive-phrase ;
    compcon:subtypeOf compcon:sem_possession .

compcon:sem_part-whole-relation a compcon:sem ;
    rdfs:label "part--whole relation" ;
    cx:hasExample compcon:part-whole-relation-example-0 ;
    dc:description "a relation between an <a sem:object-concept>object</a>, particularly an <a>inanimate</a> object, and a part of that object. <i>Example</i>: <i>a drawer of the desk</i> is a <a>possession construction</a> expressing a part--whole relation. The part--whole relation is a generalization of the <a>body part relation</a>. (Section 4.1.4)" ;
    dcterms:isPartOf "4.1.4" ;
    skos:altLabel "part--whole (relation)" ;
    compcon:subtypeOf compcon:sem_possession .

compcon:sem_participant a compcon:sem ;
    rdfs:label "participant" ;
    cx:hasExample compcon:participant-example-0 ;
    dc:description "<a>entities</a> that play a role in an <a>event</a>. <i>Example</i>: in <i>Janet set the books on the floor</i>, Janet, the books, and the floor are participants in the setting event. (Section 6.1.1)" ;
    dcterms:isPartOf "6.1.1" ;
    compcon:functionOf compcon:cxn_argument-phrase,
        compcon:cxn_noun-complement-clause-construction .

compcon:sem_participant-oriented a compcon:sem ;
    rdfs:label "participant-oriented" ;
    cx:hasExample compcon:participant-oriented-example-0 ;
    dc:description "a stative <a>element</a> in a <a>stative complex predicate</a> that describes a <a>state</a> of one of the <a>participants</a> in the <a>event</a> denoted by the complex predicate. <i>Example</i>: in English <i>I ate the carrots raw</i>, <i>raw</i> is a property of the carrots, not the event of eating. Contrasts with <a>event-oriented</a>. (Section 14.1)" ;
    dcterms:isPartOf "14.1" ;
    compcon:functionOf compcon:cxn_depictive-complex-predicate,
        compcon:cxn_resultative-complex-predicate ;
    compcon:subtypeOf compcon:sem_orientation .

compcon:sem_passing a compcon:sem ;
    rdfs:label "passing" ;
    cx:hasExample compcon:passing-example-0 ;
    dc:description "the intermediate phase of the <a>path</a> in a <a>motion event</a>. <i>Example</i>: in the Lao <a>serial verb</a> <a>construction</a> <i>man<sup>2</sup> lèèn<sup>1</sup> qòòk<sup>5</sup> caak<sup>5</sup> hùan<sup>2</sup> taam<sup>3</sup> thaang<sup>2</sup> hòòt<sup>4</sup> kòòn<sup>4</sup>-hiin<sup>3</sup></i> <q>He ran (exited) from the house, followed the path, reached the rock</q>, <i>taam<sup>3</sup></i> <q>follow</q> denotes the passing phase of the motion event. (Section 14.4)" ;
    dcterms:isPartOf "14.4" .

compcon:sem_path-event a compcon:sem ;
    rdfs:label "path event" ;
    cx:hasExample compcon:path-event-example-0 ;
    dc:description "an event that describes motion of a <a>figure</a> along a spatial path relative to a <a>ground</a>. <i>Example</i>: in <i>The guests entered the reception hall, enter</i> is a path of motion verb expressing a path of motion event. (Sections 7.3.1, 14.2)" ;
    dcterms:isPartOf "14.2",
        "7.3.1" ;
    skos:altLabel "path",
        "path of motion",
        "path of motion event" ;
    compcon:functionOf compcon:cxn_path-verb ;
    compcon:subtypeOf compcon:sem_motion-event .

compcon:sem_perception-event a compcon:sem ;
    rdfs:label "perception event" ;
    cx:hasExample compcon:perception-event-example-0 ;
    dc:description "an <a>experiential event</a> involving one (or more) of the sensory modalities and directed toward a <a>stimulus</a>. The stimulus may be either an <a sem:object-concept>object</a> or an <a>event</a>. <i>Example</i>: <i>Tim heard the macaw</i> is an example of a perception event, and <i>hear</i> is the perception verb (Section 7.4)." ;
    skos:altLabel "perception (event)" ;
    compcon:functionOf compcon:cxn_perception-clause ;
    compcon:subtypeOf compcon:sem_experiential-event .

compcon:sem_persistent a compcon:sem ;
    rdfs:label "persistent" ;
    dc:description "a concept that describes something that persists over time, and is construed to be a relatively inherent characteristic of the object over its lifetime. <i>Example</i>: being smart or being heavy (of a stone slab) are stable states. The stability of states is a matter of construal; a state construed as stable may change -- for example, a tall tree whose top is broken off. (Section 2.1)" ;
    dcterms:isPartOf "2.1" ;
    skos:altLabel "stable" ;
    compcon:subtypeOf compcon:sem_transitoriness .

compcon:sem_person a compcon:sem ;
    rdfs:label "person" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_human-noun,
        compcon:cxn_human-proper-noun,
        compcon:cxn_kinship-noun,
        compcon:cxn_social-noun ;
    compcon:subtypeOf compcon:sem_object-concept .

compcon:sem_physical-property a compcon:sem ;
    rdfs:label "physical property" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_physical-property-term ;
    compcon:subtypeOf compcon:sem_property-concept .

compcon:sem_piece a compcon:sem ;
    rdfs:label "piece" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_partitive-construction ;
    compcon:subtypeOf compcon:sem_mensural-concept .

compcon:sem_place a compcon:sem ;
    rdfs:label "place" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_place-noun,
        compcon:cxn_place-proper-noun ;
    compcon:subtypeOf compcon:sem_object-concept .

compcon:sem_positive-epistemic-stance a compcon:sem ;
    rdfs:label "positive epistemic stance" ;
    cx:hasExample compcon:positive-epistemic-stance-example-0 ;
    dc:description "a positive commitment on the part of the speaker to the actuality of a proposition expressed in a clause. <i>Example</i>: in <i>Since you pressed the button, the door opened</i>, the speaker has expressed a commitment to the proposition that you did press the button. (Section 17.3.1)" ;
    dcterms:isPartOf "17.3.1" ;
    skos:altLabel "factive" ;
    compcon:subtypeOf compcon:sem_epistemic-stance .

compcon:sem_positive-polarity a compcon:sem ;
    rdfs:label "positive polarity" ;
    cx:hasExample compcon:positive-polarity-example-0 ;
    dc:description "indicates that the situation expressed in the utterance is true. <i>Example</i>: <i>Kit found his glasses</i> expresses that Kit finding his glasses is true. In the vast majority of languages, positive polarity is <a>zero coded</a>, as in this example. (Section 12.1)" ;
    dcterms:isPartOf "12.1" ;
    skos:altLabel "positive (polarity)" ;
    compcon:subtypeOf compcon:sem_polarity .

compcon:sem_possessor a compcon:sem ;
    rdfs:label "possessor" ;
    cx:hasExample compcon:possessor-example-0 ;
    dc:description "the person who has control over the <a>possessum</a> in a <a>possession relation</a>. <i>Example</i>: in <i>I have a car</i>, I am the possessor. The possessor may also serve as the <a>modifier</a> in a <a>possessive modification construction</a>. (Sections 4.1.4, 10.4.2)" ;
    dcterms:isPartOf "10.4.2",
        "4.1.4" ;
    skos:altLabel "possessor role" ;
    compcon:functionOf compcon:cxn_possessor-argument-phrase ;
    compcon:roleOf compcon:sem_possession ;
    compcon:subtypeOf compcon:sem_co-actor,
        compcon:sem_undergoer .

compcon:sem_possessum a compcon:sem ;
    rdfs:label "possessum" ;
    cx:hasExample compcon:possessum-example-0 ;
    dc:description "the object that is controlled by the <a>possessor</a> in a <a>possession relation</a>. <i>Example</i>: in <i>I have a car</i>, the car is the possessum. The possessum may also serve as the head in a <a>possessive modification construction</a>. (Sections 4.1.4, 10.4.2)" ;
    dcterms:isPartOf "10.4.2",
        "4.1.4" ;
    skos:altLabel "possessed role",
        "possessee role",
        "possessum role" ;
    compcon:roleOf compcon:sem_possession ;
    compcon:subtypeOf compcon:sem_undergoer .

compcon:sem_posterior a compcon:sem ;
    rdfs:label "posterior" ;
    cx:hasExample compcon:posterior-example-0 ;
    dc:description "a temporal sequential relation between two <a>events</a> such that the preceding event serves as the reference point for the following event. <i>Example</i>: in <i>He drove to the party after washing the car</i>, driving to the party has a posterior temporal relation with respect to washing the car. Used to describe a sequential temporal relation between events in a <a>figure--ground information packaging</a>. (Section 15.1.3)" ;
    dcterms:isPartOf "15.1.3" ;
    skos:altLabel "sequential" ;
    compcon:functionOf compcon:cxn_resultative-complex-predicate,
        compcon:cxn_temporal-construction ;
    compcon:subtypeOf compcon:sem_event-relation .

compcon:sem_pretense-event a compcon:sem ;
    rdfs:label "pretense event" ;
    cx:hasExample compcon:pretense-event-example-0 ;
    dc:description "a <a>propositional attitude event</a> in which the speaker, or the <a>experiencer</a> of the pretense event, presents the <a>proposition</a> expressed by the <a>complement</a> as true in an alternative reality or <a>mental space</a>. <i>Example</i>: in <i>Ira pretended that the guests had already left</i>, the proposition that the guests had already left is presented as true in an alternative reality from the shared beliefs of the interlocutors (or, for that matter, Ira). There is a strong implicature that the proposition does not hold in reality (that is, the shared beliefs of the interlocutors). (Section 18.2.2)" ;
    dcterms:isPartOf "18.2.2" ;
    skos:altLabel "pretence event" ;
    compcon:functionOf compcon:cxn_pretense-construction ;
    compcon:subtypeOf compcon:sem_propositional-attitude-event .

compcon:sem_proportional-quantifier a compcon:sem ;
    rdfs:label "proportional quantifier" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_proportional-quantifier-term,
        compcon:cxn_quantifier-attributive-phrase ;
    compcon:subtypeOf compcon:sem_quantitative-relation .

compcon:sem_proposition a compcon:sem ;
    rdfs:label "proposition" ;
    cx:hasExample compcon:proposition-example-0 ;
    dc:description "a concept that denotes an <a>event</a> (with its attendant <a>participants</a>, <a>tense</a>, <a>aspect</a>, <a>modality</a>, and <a>polarity</a>) that has a truth value in a particular context -- that is, it may be true or false in that context. <i>Example</i>: in <i>John believes that Mary took the car to the repair shop</i>, <i>Mary took the car to the repair shop</i> is a proposition; it has a truth value in the context of John's beliefs. A <a>pragmatic assertion</a> expresses a proposition that is taken to be true in the speaker's beliefs. (Sections 11.1, 18.2.2)" ;
    dcterms:isPartOf "11.1",
        "18.2.2" .

compcon:sem_propositional-content a compcon:sem ;
    rdfs:label "propositional content" ;
    cx:hasExample compcon:propositional-content-example-0,
        compcon:propositional-content-example-1,
        compcon:propositional-content-example-2 ;
    dc:description "the semantic content of a <a>clause</a>, separately from whether it is packaged as <a inf:topic-comment>topic--comment</a>, <a inf:thetic>thetic</a>, or <a inf:identificational>identificational</a>. Propositional content corresponds basically to <q>who did what to whom</q>. <i>Example</i>: In <i>There's a coyote running down the street!, It's a coyote that's running down the street</i>, and <i>The coyote is running down the street</i>, the propositional content shared by the thetic, identificational, and topic--comment constructions, respectively, is the current running <a>event</a> with a coyote as the runner and the street as the location of the running. (Section 11.1)" ;
    dcterms:isPartOf "11.1" ;
    compcon:functionOf compcon:cxn_speech-act-construction .

compcon:sem_protasis a compcon:sem ;
    rdfs:label "protasis" ;
    cx:hasExample compcon:protasis-example-0 ;
    dc:description "the proposition or <a>event</a> denoted by a <a cxn:protasis-clause>protasis</a> clause. <i>Example</i>: in <i>If you press this button, the door will open</i>, <i>If you press this button</i> is the protasis; <i>the door will open</i> is the <a cxn:apodosis-clause>apodosis</a>. Since the conditional relations are defined in terms of both logical implication and causal relation, the semantic use of <q>protasis</q> can be distinguished as <q>protasis proposition</q> or <q>protasis event</q>. (Section 17.3.1)" ;
    dcterms:isPartOf "17.3.1" ;
    compcon:functionOf compcon:cxn_protasis-clause .

compcon:sem_proximal a compcon:sem ;
    rdfs:label "proximal" ;
    dc:description "a <a>contextual</a> category of <a>deixis</a> defined as near the location of the speaker in the speech event. (Section 3.1.1)" ;
    dcterms:isPartOf "3.1.1" ;
    skos:altLabel "proximal (deixis)" ;
    compcon:subtypeOf compcon:sem_spatial-deixis .

compcon:sem_punctual a compcon:sem ;
    rdfs:label "punctual" ;
    cx:hasExample compcon:punctual-example-0 ;
    dc:description "an <a>event</a> that is construed as taking place in an <dq>instant</dq> of time. <i>Example</i>: <i>The balloon popped</i> is a punctual event. (Section 6.2.1)" ;
    dcterms:isPartOf "6.2.1" ;
    compcon:subtypeOf compcon:sem_durativity .

compcon:sem_purpose a compcon:sem ;
    rdfs:label "purpose" ;
    cx:hasExample compcon:purpose-example-0,
        compcon:purpose-example-1 ;
    dc:description "the semantic relation between two <a>events</a> where one event serves as the intended outcome as a result of bringing about the other event. <i>Example</i>: <i>I will grab a stick to defend myself</i> is a <a>figure--ground construal</a> of the simultaneous relation in an <a>adverbial clause construction</a>, and <i>I will grab a stick and defend myself</i> is a <a>complex figure construal</a> of the relation in a <a>coordinate clause construction</a>. In the figure--ground construal, the intended event is construed as the ground. The intended event is unrealized, which disfavors a complex figure construal (this problem is avoided in the example here because both events are future events). (Section 15.3.1)" ;
    dcterms:isPartOf "15.3.1" ;
    compcon:functionOf compcon:cxn_purpose-construction ;
    compcon:subtypeOf compcon:sem_event-relation .

compcon:sem_pursuit-event a compcon:sem ;
    rdfs:label "pursuit event" ;
    cx:hasExample compcon:pursuit-event-example-0,
        compcon:pursuit-event-example-1,
        compcon:pursuit-event-example-2,
        compcon:pursuit-event-example-3 ;
    dc:description "<a>events</a> in which one <a>participant's</a> motion or location is directed toward another participant. <i>Examples</i>: pursuit events include following, chasing, searching for something, and waiting for someone/something; and <i>follow</i>, <i>chase</i>, <i>search (for)</i>, and <i>wait (for)</i> are pursuit verbs. (Section 7.3.3)" ;
    dcterms:isPartOf "7.3.3" ;
    compcon:functionOf compcon:cxn_pursuit-verb ;
    compcon:subtypeOf compcon:sem_spatial-event .

compcon:sem_quantity a compcon:sem ;
    rdfs:label "quantity" ;
    dc:description "" ;
    compcon:subtypeOf compcon:sem_entity .

compcon:sem_r-role a compcon:sem ;
    rdfs:label "R role" ;
    cx:hasExample compcon:r-role-example-0 ;
    dc:description "the recipient <a>central participant role</a> in a <a sem:transfer-of-possession-event>transfer event</a> -- that is, the <a>participant</a> that receives the theme (<a>T</a>) from the agent. <i>Example</i>: in <i>Randy gave the car to his daughter</i>, Randy's daughter is in the R role. (Section 7.5.2)" ;
    dcterms:isPartOf "7.5.2" ;
    skos:altLabel "G (role)",
        "R (role)" ;
    compcon:functionOf compcon:cxn_r-phrase ;
    compcon:roleOf compcon:sem_transfer-of-possession-event ;
    compcon:subtypeOf compcon:sem_recipient .

compcon:sem_reciprocal a compcon:sem ;
    rdfs:label "reciprocal" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_reciprocal-construction ;
    compcon:subtypeOf compcon:sem_causal-cyclicality .

compcon:sem_reciprocal-event a compcon:sem ;
    rdfs:label "reciprocal event" ;
    cx:hasExample compcon:reciprocal-event-example-0 ;
    dc:description "an <a>event</a> type in which one <a>participant</a> acts upon another participant, and the second participant acts on the first in the same way. That is, each participant is both the <a>initiator</a> and <a>endpoint</a> of <a>transmission of force</a> for the same type of action. <i>Example</i>: in <i>Mary and Sue praised each other</i>, Mary praises Sue and Sue praises Mary. (Section 7.2)" ;
    dcterms:isPartOf "7.2" ;
    compcon:functionOf compcon:cxn_reciprocal-construction ;
    compcon:subtypeOf compcon:sem_event .

compcon:sem_reference-point a compcon:sem ;
    rdfs:label "reference point" ;
    dc:description "a point on the scale for a property that indicates a <dq>normal</dq> value for the property concept word. Example: in regard to height of a person, <i>tall/short</i> means <q>taller/shorter than a <dq>normal</dq> height</q>. The reference point is relative: tall for a person is short for a tree. The reference point can be expressed overtly, as in <i>She's kind of short <b>for a basketball player</b></i>. The term <q>reference point</q> is also used as a synonym for <q>anchor</q>; we will avoid that use in this book. (Section 4.1.2)" ;
    dcterms:isPartOf "4.1.2" .

compcon:sem_reflexive a compcon:sem ;
    rdfs:label "reflexive" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_reflexive-construction ;
    compcon:subtypeOf compcon:sem_causal-cyclicality .

compcon:sem_reflexive-event a compcon:sem ;
    rdfs:label "reflexive event" ;
    cx:hasExample compcon:reflexive-event-example-0,
        compcon:reflexive-event-example-1 ;
    dc:description "an <a>event</a> type in which a <a>participant</a> acts upon themself -- that is, the participant is both the <a>initiator</a> and <a>endpoint</a> of <a>transmission of force</a>. <i>Examples</i>: reflexive events may be direct, when there is no other participant, as in <i>I saw myself</i>; or indirect, when there is another participant in an intermediate position in the <a>causal chain</a>, as in <i>Sally baked a cake for herself</i>, whose causal structure is Sally --> cake --> Sally. (Section 7.2)" ;
    dcterms:isPartOf "7.2" ;
    compcon:functionOf compcon:cxn_reflexive-construction ;
    compcon:subtypeOf compcon:sem_event .

compcon:sem_relational a compcon:sem ;
    rdfs:label "relational" ;
    cx:hasExample compcon:relational-example-0,
        compcon:relational-example-1 ;
    dc:description "a concept that inherently makes reference to another entity. <i>Example</i>: a property such as <q>smooth</q> is a property of something, an action such as <q>run</q> is performed by someone or something. (Section 2.1)" ;
    dcterms:isPartOf "2.1" ;
    compcon:functionOf compcon:cxn_discourse-markers ;
    compcon:subtypeOf compcon:sem_relationality .

compcon:sem_removal-event a compcon:sem ;
    rdfs:label "removal event" ;
    dc:description "an <a>event</a> describing removal of an <a sem:object-concept>object</a> from another object. <i>Example</i>: scrubbing is a removal event. (Section 7.3.2)" ;
    dcterms:isPartOf "7.3.2" ;
    skos:altLabel "removal (event)" ;
    compcon:functionOf compcon:cxn_removal-verb ;
    compcon:subtypeOf compcon:sem_spatial-event .

compcon:sem_result-event a compcon:sem ;
    rdfs:label "result event" ;
    cx:hasExample compcon:result-event-example-0 ;
    dc:description "an <a>event</a> that is described in terms of reaching a result state, often by means of a scalar change (<a>directed change</a>). <i>Example</i>: in <i>Peter broke the window</i>, the event is described in terms of the result state that is reached (a broken window). (Section 7.3.2)" ;
    dcterms:isPartOf "7.3.2" ;
    compcon:functionOf compcon:cxn_result-verb ;
    compcon:subtypeOf compcon:sem_event .

compcon:sem_result-profile a compcon:sem ;
    rdfs:label "result profile" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_result-verb ;
    compcon:subtypeOf compcon:sem_causal-chain-profile .

compcon:sem_s-role a compcon:sem ;
    rdfs:label "S role" ;
    cx:hasExample compcon:s-role-example-0,
        compcon:s-role-example-1 ;
    dc:description "the one <a>central participant role</a> in a <a sem:monovalent>monovalent event</a>. <i>Example</i>: in <i>Jack ran</i> or <i>Jack died</i>, Jack plays the S role in the running and dying events, respectively. It is not entirely clear what event best serves as the exemplar monovalent event for defining the S role (Sections 6.3.1, 6.3.3)" ;
    dcterms:isPartOf "6.3.1",
        "6.3.3" ;
    skos:altLabel "S (role)" ;
    compcon:functionOf compcon:cxn_s-phrase ;
    compcon:roleOf compcon:sem_change-of-state-event ;
    compcon:subtypeOf compcon:sem_undergoer .

compcon:sem_scalar-model a compcon:sem ;
    rdfs:label "scalar model" ;
    cx:hasExample compcon:scalar-model-example-0 ;
    dc:description "a range of situations that can be ranked from <dq>weakest</dq> to <dq>strongest</dq> -- that is, least informative to most informative, on some relevant scale for the <a>meaning</a> of a sentence. <i>Example</i>: <i>He won't eat shrimp, let alone squid</i> makes sense by invoking a scalar model of <dq>things that you would not expect someone to be willing to eat</dq>, such that being unwilling to eat shrimp is at the <dq>strong</dq>, more informative, end of the scalar model, and being unwilling to eat squid is at the <dq>weak</dq>, less informative end of the scalar model (in a culture where eating squid is considered more repulsive than eating shrimp). (Section 17.3.3)" ;
    dcterms:isPartOf "17.3.3" .

compcon:sem_second-person a compcon:sem ;
    rdfs:label "second person" ;
    dc:description "" ;
    skos:altLabel "2nd person" ;
    compcon:functionOf compcon:cxn_second-person-pronoun ;
    compcon:subtypeOf compcon:sem_person-deixis .

compcon:sem_sensation-event a compcon:sem ;
    rdfs:label "sensation event" ;
    cx:hasExample compcon:sensation-event-example-0 ;
    dc:description "an <a>experiential event</a> involving an internal bodily or physiological sensation. <i>Example</i>: <i>My head aches</i> is an example of a sensation event. (Section 7.4)" ;
    dcterms:isPartOf "7.4" ;
    skos:altLabel "(bodily) sensation event" ;
    compcon:functionOf compcon:cxn_sensation-verb ;
    compcon:subtypeOf compcon:sem_experiential-event .

compcon:sem_shape a compcon:sem ;
    rdfs:label "shape" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_shape-term ;
    compcon:subtypeOf compcon:sem_property-concept .

compcon:sem_simple-contrast a compcon:sem ;
    rdfs:label "simple contrast" ;
    cx:hasExample compcon:simple-contrast-example-0 ;
    dc:description "a semantic relation between two <a>events</a> in which there is only a simple contrast, or a construal of a simple contrast, between the two. Simple contrast is considered the prototype for <a>adversative coordination</a>. <i>Example</i>: Ponapean <i>i laid ah e meir</i> [I fish <sc>conj</sc> he sleep] <q>I fished, but he slept</q>, is an instance of simple contrast. (Section 15.2.1)" ;
    dcterms:isPartOf "15.2.1" ;
    compcon:functionOf compcon:cxn_simple-contrast-coordination ;
    compcon:subtypeOf compcon:sem_event-relation .

compcon:sem_simultaneous a compcon:sem ;
    rdfs:label "simultaneous" ;
    cx:hasExample compcon:simultaneous-example-0,
        compcon:simultaneous-example-1 ;
    dc:description "the semantic relation between two <a>events</a> where two events temporally coincide or overlap. <i>Example</i>: <i>He sang while she played the banjo</i> is a <a>figure--ground construal</a> of the simultaneous relation in an <a>adverbial clause construction</a>, and <i>He sang and she played the banjo</i> is a <a>complex figure construal</a> of the relation in a <a>coordinate clause construction</a>. (Section 15.3.1)" ;
    dcterms:isPartOf "15.3.1" ;
    skos:altLabel "overlap" ;
    compcon:functionOf compcon:cxn_depictive-complex-predicate,
        compcon:cxn_manner-complex-predicate,
        compcon:cxn_temporal-construction ;
    compcon:subtypeOf compcon:sem_event-relation .

compcon:sem_social-role a compcon:sem ;
    rdfs:label "social role" ;
    dc:description "" ;
    compcon:roleOf compcon:sem_social-role-event ;
    compcon:subtypeOf compcon:sem_semantic-role .

compcon:sem_sociative-causation a compcon:sem ;
    rdfs:label "sociative causation" ;
    dc:description "" ;
    compcon:subtypeOf compcon:sem_causal-directness .

compcon:sem_sociative-causation-event a compcon:sem ;
    rdfs:label "sociative causation-event" ;
    cx:hasExample compcon:sociative-causation-event-example-0 ;
    dc:description "a <a>causative event</a> in which the relationship between the <a>causer</a> and the <a>causee</a> is more symmetrical, so that the two <a>agents</a> are performing the action more jointly. <i>Example</i>: <i>I helped the students fill out the questionnaire</i> is an instance of sociative causation. (Section 9.2)" ;
    dcterms:isPartOf "9.2" ;
    compcon:subtypeOf compcon:sem_event .

compcon:sem_speaker a compcon:sem ;
    rdfs:label "speaker" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_exclusive-pronoun,
        compcon:cxn_inclusive-pronoun,
        compcon:cxn_inclusory-construction ;
    compcon:subtypeOf compcon:sem_object-concept .

compcon:sem_species a compcon:sem ;
    rdfs:label "species" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_species-term ;
    compcon:subtypeOf compcon:sem_mensural-concept .

compcon:sem_speech-act-causal-relation a compcon:sem ;
    rdfs:label "speech act causal relation" ;
    cx:hasExample compcon:speech-act-causal-relation-example-0 ;
    dc:description "the semantic relation in a <a>conditional</a>, <a>causal</a>, <a>concessive</a>, or <a>conditional concessive construction</a> that expresses a relation between a condition on performing a <a inf:speech-acts>speech act</a> and the performance of that speech act; and the construction expressing that relation. <i>Example</i>: in <i>Since you asked, ten isn't a prime number</i>, there is a speech act causal relation between the request of the speaker whether ten is a prime number, and the performance of the speech act asserting that ten isn't a prime number. A speech act causal relation contrasts with a <a>content causal relation</a> and an <a>epistemic causal relation</a>. (Section 17.3.1)" ;
    dcterms:isPartOf "17.3.1" ;
    compcon:subtypeOf compcon:sem_causal-link .

compcon:sem_spontaneous a compcon:sem ;
    rdfs:label "spontaneous" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_spontaneous-verb ;
    compcon:subtypeOf compcon:sem_causal-control .

compcon:sem_spontaneous-event a compcon:sem ;
    rdfs:label "spontaneous event" ;
    dc:description "a <a sem:monovalent>monovalent event</a> involving a <a>participant</a> that undergoes a change without an external cause. <i>Examples</i>: dying and melting are spontaneous events. (Sections 6.3.4, 7.2)" ;
    dcterms:isPartOf "6.3.4",
        "7.2" ;
    compcon:functionOf compcon:cxn_spontaneous-verb ;
    compcon:subtypeOf compcon:sem_event .

compcon:sem_standard a compcon:sem ;
    rdfs:label "standard" ;
    cx:hasExample compcon:standard-example-0 ;
    dc:description "in a <a>comparative construction</a> or <a>equative construction</a>, the <a>referent</a> whose position on the <a>gradable predicative scale</a> serves as the reference point for the position of the <a>comparee</a> on the predicative scale. <i>Example</i>: in <i>Your cat is bigger than my dog</i>, the dog serves as the standard for the position of the cat on the scale of size -- it is asserted that the cat exceeds the dog on that scale. (Section 17.2.1)" ;
    dcterms:isPartOf "17.2.1" ;
    compcon:roleOf compcon:sem_comparative-relation,
        compcon:sem_equative-relation .

compcon:sem_state a compcon:sem ;
    rdfs:label "state" ;
    dc:description "a concept which is <a>relational</a> and stative but transitory. <i>Example</i>: being sick or being happy are examples of states -- they are stative, but the states of the object they apply to come and go. (Section 2.1)" ;
    dcterms:isPartOf "2.1" ;
    skos:altLabel "state (concept)",
        "temporary state",
        "transitory state" ;
    compcon:functionOf compcon:cxn_nonprototypical-predication-construction,
        compcon:cxn_temporary-predicate ;
    compcon:subtypeOf compcon:sem_qualitative-event .

compcon:sem_stative a compcon:sem ;
    rdfs:label "stative" ;
    dc:description "a concept construed as not involving change over the time course of the <a>event</a>. <i>Example</i>: being tall does not involve any change over the time course being described. (Sections 2.1, 6.2.1)" ;
    dcterms:isPartOf "2.1",
        "6.2.1" ;
    compcon:functionOf compcon:cxn_stative-predicate-part ;
    compcon:subtypeOf compcon:sem_stativity .

compcon:sem_stimulus a compcon:sem ;
    rdfs:label "stimulus" ;
    cx:hasExample compcon:stimulus-example-0 ;
    dc:description "the <a>participant</a> in an <a>experiential event</a> which stimulates the internal mental or bodily experience in the <a>experiencer</a>. <i>Example</i>: in <i>Freddy saw the bear</i>, the bear is the stimulus. (Sections 6.1.2, <b>7.4</b>)" ;
    dcterms:isPartOf "6.1.2",
        "7.4" ;
    compcon:functionOf compcon:cxn_stimulus-phrase ;
    compcon:roleOf compcon:sem_experiential-event ;
    compcon:subtypeOf compcon:sem_undergoer .

compcon:sem_subjective a compcon:sem ;
    rdfs:label "subjective" ;
    cx:hasExample compcon:subjective-example-0 ;
    dc:description "describing an entity from the implicit perspective of the speaker and the speech event, e.g. the speech event time. <i>Example</i>: in the subjective <a>epistemic modal</a> <a>construction</a> <i>Wendy might be in Santa Fe</i>, the uncertainty about Wendy being in Santa Fe is that of the speaker at the time of the speech event, although neither of these pieces of information (the speaker or the time of the speech event) are explicitly expressed in the sentence. (Section 12.3.4)" ;
    dcterms:isPartOf "12.3.4" ;
    compcon:subtypeOf compcon:sem_epistemic-modality .

compcon:sem_subsequent-role a compcon:sem ;
    rdfs:label "subsequent role" ;
    cx:hasExample compcon:subsequent-role-example-0 ;
    dc:description "a <a>participant role</a> that is subsequent to the participant role expressed as <a inf:object-argument>object</a> in the <a>causal chain</a> / <a>causal structure</a> of an <a>event</a>. <i>Example</i>: in <i>Terry made lunch for Sandy</i>, Sandy is subsequent to the lunch in the foodmaking causal chain (Terry --> lunch --> Sandy), and the lunch is expressed as object. (Section 6.1.2)" ;
    dcterms:isPartOf "6.1.2" ;
    compcon:subtypeOf compcon:sem_participant-role .

compcon:sem_substitution a compcon:sem ;
    rdfs:label "substitution" ;
    dc:description "" ;
    compcon:subtypeOf compcon:sem_event-relation .

compcon:sem_substitutive a compcon:sem ;
    rdfs:label "substitutive" ;
    cx:hasExample compcon:substitutive-example-0,
        compcon:substitutive-example-1 ;
    dc:description "the semantic relation between two <a>events</a> where one event is characterized by the substitution of a second event that is not normally a part of the first event. <i>Example</i>: <i>We barbecued chicken at home instead of going out to eat</i> is a <a>figure--ground construal</a> of the simultaneous relation in an <a>adverbial clause construction</a>, and <i>We didn't go out to eat, and barbecued chicken at home (instead)</i> is a <a>complex figure construal</a> of the relation in a <a>coordinate clause construction</a>. In the figure--ground construal, the substituted event is construed as the ground. (Section 15.3.1)" ;
    dcterms:isPartOf "15.3.1" ;
    compcon:functionOf compcon:cxn_substitutive-construction ;
    compcon:subtypeOf compcon:sem_event-relation .

compcon:sem_subtraction a compcon:sem ;
    rdfs:label "subtraction" ;
    dc:description "" ;
    compcon:subtypeOf compcon:sem_event-relation .

compcon:sem_subtractive a compcon:sem ;
    rdfs:label "subtractive" ;
    cx:hasExample compcon:subtractive-example-0,
        compcon:subtractive-example-1 ;
    dc:description "the semantic relation between two <a>events</a> where one event is additionally characterized by the absence of a second event that is normally a part of the first event. In this respect, the subtractive relation is somewhat like a <dq>negative</dq> version of the <a>additive</a> relation. <i>Example</i>: <i>He did all the problems correctly except (that) he missed the proof on the last one</i> is a <a>figure--ground construal</a> of the simultaneous relation in an <a>adverbial clause construction</a>, and <i>He did all the problems correctly but he missed the proof on the last one</i> is a <a>complex figure construal</a> of the relation in a <a>coordinate clause construction</a>. In the figure--ground construal, the absent event is construed as the ground. (Section 15.3.1)" ;
    dcterms:isPartOf "15.3.1" ;
    compcon:functionOf compcon:cxn_subtractive-construction ;
    compcon:subtypeOf compcon:sem_event-relation .

compcon:sem_superlative-degree a compcon:sem ;
    rdfs:label "superlative degree" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_superlative-form ;
    compcon:subtypeOf compcon:sem_degree .

compcon:sem_t-role a compcon:sem ;
    rdfs:label "T role" ;
    cx:hasExample compcon:t-role-example-0 ;
    dc:description "the <a>theme</a> <a>central participant role</a> in a <a sem:transfer-of-possession-event>transfer event</a> -- that is, the <a>participant</a> that is transferred from the <a>agent</a> (<a>A</a>) to the <a>recipient</a> (<a>R</a>). <i>Example</i>: in <i>Randy gave the car to his daughter</i>, the car is in the T role. (Section 7.5.2)" ;
    dcterms:isPartOf "7.5.2" ;
    skos:altLabel "T (role)" ;
    compcon:functionOf compcon:cxn_t-phrase ;
    compcon:roleOf compcon:sem_transfer-of-possession-event ;
    compcon:subtypeOf compcon:sem_theme .

compcon:sem_telic a compcon:sem ;
    rdfs:label "telic" ;
    cx:hasExample compcon:telic-example-0 ;
    dc:description "an <a>event</a> in which the relevant <a>participant</a> ends up in a <dq>natural</dq> result <a>state</a>. <i>Example</i>: in <i>I crossed the street</i>, the natural result state is reaching the other side of the street, and the event is telic. (Section 6.2.1)" ;
    dcterms:isPartOf "6.2.1" ;
    skos:altLabel "telic (event)" ;
    compcon:subtypeOf compcon:sem_telicity .

compcon:sem_temporary-state a compcon:sem ;
    rdfs:label "temporary state" ;
    dc:description "the <a>event</a> class of <a>stative</a> properties that are temporary and thus have come about through some process. <i>Example</i>: being sick is a temporary state. (Section 6.3.3)" ;
    dcterms:isPartOf "6.3.3" .

compcon:sem_tense a compcon:sem ;
    rdfs:label "tense" ;
    dc:description "a grammatical category, typically an inflectional category, that expresses the location of an event in time, usually with respect to the time of the speech act situation. In this textbook, tense is discussed primarily with respect to the expression of <a>TAMP</a> categories. (Sections 6.2.1, 13.4)" ;
    dcterms:isPartOf "13.4",
        "6.2.1" ;
    compcon:subtypeOf compcon:sem_mental-space,
        compcon:sem_tamp .

compcon:sem_terminative a compcon:sem ;
    rdfs:label "terminative" ;
    dc:description "" ;
    compcon:subtypeOf compcon:sem_phasal-aspect .

compcon:sem_thing a compcon:sem ;
    rdfs:label "thing" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_animal-noun,
        compcon:cxn_animal-plant-product-noun,
        compcon:cxn_body-part-noun,
        compcon:cxn_natural-object-noun,
        compcon:cxn_plant-noun ;
    compcon:subtypeOf compcon:sem_object-concept .

compcon:sem_third-person a compcon:sem ;
    rdfs:label "third person" ;
    dc:description "" ;
    skos:altLabel "3rd person" ;
    compcon:functionOf compcon:cxn_third-person-pronoun ;
    compcon:subtypeOf compcon:sem_person-deixis .

compcon:sem_time a compcon:sem ;
    rdfs:label "time" ;
    dc:description "" ;
    compcon:subtypeOf compcon:sem_entity .

compcon:sem_token a compcon:sem ;
    rdfs:label "token" ;
    dc:description "a particular entity with its own identity. <i>Examples</i>: a particular person such as Charlie Chaplin, or a specific table, are individuals. (Section 3.1.1)" ;
    dcterms:isPartOf "3.1.1" ;
    skos:altLabel "individual",
        "instance" ;
    compcon:functionOf compcon:cxn_proper-noun ;
    compcon:subtypeOf compcon:sem_identity .

compcon:sem_topic a compcon:sem ;
    rdfs:label "topic" ;
    dc:description "" ;
    compcon:roleOf compcon:sem_communication-event ;
    compcon:subtypeOf compcon:sem_undergoer .

compcon:sem_transitory a compcon:sem ;
    rdfs:label "transitory" ;
    dc:description "a concept that describes something that <dq>comes and goes</dq> over time. <i>Example</i>: being cold is a state that may be true of a person at some point in time, but not true at another point in time. (Section 2.1)" ;
    dcterms:isPartOf "2.1" ;
    compcon:subtypeOf compcon:sem_transitoriness .

compcon:sem_trivalent a compcon:sem ;
    rdfs:label "trivalent" ;
    dc:description "an event with a <a>valency</a> of three -- that is, with three <a>central participant roles</a>. <i>Example</i>: giving is a trivalent event. (Section 6.1.2)" ;
    dcterms:isPartOf "6.1.2" ;
    compcon:functionOf compcon:cxn_ditransitive-construction ;
    compcon:subtypeOf compcon:sem_valency .

compcon:sem_type a compcon:sem ;
    rdfs:label "type" ;
    dc:description "a general concept that generally subsumes multiple instances (<a>individuals</a>). <i>Example</i>: the category of tables is a type. (Section 3.1.1)" ;
    dcterms:isPartOf "3.1.1" ;
    skos:altLabel "category" ;
    compcon:functionOf compcon:cxn_common-noun,
        compcon:cxn_generic-article,
        compcon:cxn_generic-pronoun,
        compcon:cxn_universal-pronoun ;
    compcon:subtypeOf compcon:sem_identity .

compcon:sem_uncontrolled a compcon:sem ;
    rdfs:label "uncontrolled" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_uncontrolled-predicate ;
    compcon:subtypeOf compcon:sem_causal-control .

compcon:sem_uncontrolled-event a compcon:sem ;
    rdfs:label "uncontrolled event" ;
    dc:description "the <a>event</a> class of activities not under the control of an <a>agent</a> (apart from uncontrolled <a>bodily actions</a> and <a>change of state</a>). <i>Example</i>: dying is an uncontrolled activity. (Section 6.3.3)" ;
    dcterms:isPartOf "6.3.3" ;
    skos:altLabel "uncontrolled activity" ;
    compcon:functionOf compcon:cxn_uncontrolled-predicate ;
    compcon:subtypeOf compcon:sem_event .

compcon:sem_undirected-change a compcon:sem ;
    rdfs:label "undirected change" ;
    cx:hasExample compcon:undirected-change-example-0 ;
    dc:description "an <a>event</a> in which the change that a <a>participant</a> undergoes in the course of an event is not in any particular <dq>direction</dq>. <i>Example</i>: in <i>The ball was bouncing in the playpen</i>, the ball does not undergo an incremental or <dq>directed</dq> change in either vertical or horizontal direction: the vertical motion is up and down, and the horizontal motion goes in any direction. (Section 6.2.1)" ;
    dcterms:isPartOf "6.2.1" ;
    compcon:subtypeOf compcon:sem_change-type .

compcon:sem_unexpected-co-occurrence a compcon:sem ;
    rdfs:label "unexpected co-occurrence" ;
    cx:hasExample compcon:unexpected-co-occurrence-example-0 ;
    dc:description "a semantic relation between two <a>events</a> in which two events are juxtposed and the second event is unexpected. Unexpected co-occurrence is often expressed with <a>adversative coordination</a>. <i>Example</i>: Russian <i>Vanja prostudilsja, no poshël v shkolu</i> [Vanja caught_cold <sc>conj</sc> went to school] <q>Vanja caught a cold, but went to school</q> is an instance of the unexpected co-occurrence relation, using a <a>coordinator</a> <i>no</i> which is distinct from the coordinator <i>a</i> which is used for <a>simple contrast</a>. (Section 15.2.1)" ;
    dcterms:isPartOf "15.2.1" ;
    compcon:functionOf compcon:cxn_unexpected-co-occurrence-coordination ;
    compcon:subtypeOf compcon:sem_event-relation .

compcon:sem_utterance-event a compcon:sem ;
    rdfs:label "utterance event" ;
    cx:hasExample compcon:utterance-event-example-0 ;
    dc:description "an <a>event</a> of saying in which one <a>participant</a> is the <u>speaker</u> of the utterance and another participant is the utterance itself. <i>Example</i>: in <i>Sandy said, <dq>I'm buying the house</dq>, said</i> denotes the utterance event. (Section 18.2.2)" ;
    dcterms:isPartOf "18.2.2" ;
    skos:altLabel "utterance (event)" ;
    compcon:functionOf compcon:cxn_utterance-construction ;
    compcon:subtypeOf compcon:sem_eventive-participant-event .

compcon:sem_vague-numeral a compcon:sem ;
    rdfs:label "vague numeral" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_quantifier-attributive-phrase,
        compcon:cxn_vague-numeral-term ;
    compcon:subtypeOf compcon:sem_quantitative-relation .

compcon:sem_value a compcon:sem ;
    rdfs:label "value" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_value-term ;
    compcon:subtypeOf compcon:sem_property-concept .

compcon:sem_weather-event a compcon:sem ;
    rdfs:label "weather event" ;
    cx:hasExample compcon:weather-event-example-0,
        compcon:weather-event-example-1 ;
    dc:description "a situation type describing the weather that is typically construed as <a inf:thetic>thetic</a>. <i>Examples</i>: <i>It's raining</i> and <i>The wind is blowing</i> are expressions of weather. Weather is sometimes described as an <a>event</a> without any (<a>salient</a>) <a>participants</a>. Its tendency to be construed as thetic is attributed to the fact that weather is difficult to divide into a <a inf:reference>topic</a> and a <a>comment</a> and be construed as <a>topic--comment information packaging</a>. (Sections 11.3.1, 11.3.3)" ;
    dcterms:isPartOf "11.3.1",
        "11.3.3" ;
    compcon:subtypeOf compcon:sem_qualitative-event .

compcon:sem_wishing-event a compcon:sem ;
    rdfs:label "wishing event" ;
    cx:hasExample compcon:wishing-event-example-0 ;
    dc:description "an <a>evaluative event</a> in which a positive evaluative judgment about a <a>proposition</a> expressed by the <a>complement</a> of the wishing event is made, and there is a <a>negative epistemic stance</a> by the speaker toward the proposition. <i>Example</i>: in <i>Jill wishes that Joe had won the election</i>, the wishing predicate <i>wishes</i> expresses Jill's evaluation of Joe's winning the election, and also presupposes that the speaker believes that Joe didn't win the election. (Section 18.2.2)" ;
    dcterms:isPartOf "18.2.2" ;
    compcon:functionOf compcon:cxn_wishing-construction ;
    compcon:subtypeOf compcon:sem_evaluative-event .

compcon:str_a-not-a a compcon:str ;
    rdfs:label "A--not-A" ;
    cx:hasExample compcon:a-not-a-example-0 ;
    dc:description "a strategy for <a cxn:polarity-question-construction>polarity question constructions</a> in which both the <a>positive</a> and <a>negative</a> form of the proposition are expressed. <i>Example</i>: Mandarin <i>tā zài jiā bu zài jiā</i> [lit. <q>S/he at home not at home</q>] <q>Is s/he at home?</q> is an instance of the A--not-A strategy for polarity questions. The A--not-A strategy is essentially the recruitment of the <a cxn:alternative-question>alternative question</a> construction for the polarity question function." ;
    compcon:expressionOf compcon:cxn_polarity-question-construction ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_absolutive-category a compcon:str ;
    rdfs:label "absolutive category" ;
    dc:description "the category in the <a>ergative alignment strategy</a> that <a>co-expresses</a> both <a>S</a> and <a>P roles</a>. <i>Example</i>: Yuwaalaraay <a>argument phrases</a> use the same <a str:zero-coding>zero</a> <a>flag</a> to express the S and P roles, and hence this is an absolutive flag. (Section 6.3.1)" ;
    dcterms:isPartOf "6.3.1" .

compcon:str_accusative-alignment a compcon:str ;
    rdfs:label "accusative alignment" ;
    cx:hasExample compcon:accusative-alignment-example-0,
        compcon:accusative-alignment-example-1 ;
    dc:description "a <a>system</a> in which the <a>A</a> and <a>S roles</a> are expressed with the same <a>form</a>, but the <a>P role</a> is expressed with a different form. <i>Example</i>: English <a>argument phrases</a> expressing the A and S roles are <a>indexed</a> on Present Tense verbs (<i>Emily sing-s, Emily play-s the piano</i>), while an argument phrase expressing the P role is not indexed (in <i>Emily play-s the piano</i>, the verb does not index <i>the piano</i>). (Section 6.3.1)" ;
    dcterms:isPartOf "6.3.1" ;
    skos:altLabel "accusative alignment (system)" ;
    compcon:subtypeOf compcon:str_transitive-alignment .

compcon:str_accusative-category a compcon:str ;
    rdfs:label "accusative category" ;
    dc:description "the <a>morphosyntactic</a> category in the <a>accusative alignment system</a> that exclusively expresses the <a>P role</a>. <i>Example</i>: the English accusative <a>pronoun</a> forms <i>me</i>, <i>him</i>, <i>her</i>, <i>us</i>, and <i>them</i> are used only for the P role (the S and A roles use the <a str:nominative-category>nominative</a> forms <i>I</i>, <i>he</i>, <i>she</i>, <i>we</i>, and <i>they</i>), and represent the accusative <a>flag</a> (morphologically manifested in English as base modification). (Section 6.3.1)" ;
    dcterms:isPartOf "6.3.1" .

compcon:str_action-nominal a compcon:str ;
    rdfs:label "action nominal" ;
    cx:hasExample compcon:action-nominal-example-0 ;
    dc:description "a <a>deranked</a> <a>predicate</a> form that is capable of inflecting for <a>case</a> or of taking <a>adpositions</a> in the same way as <a>nouns</a> do, and with reasonable productivity. <i>Example</i>: in <i>He remains under investigation</i>, <i>investigation</i> is an action nominal. Action nominals overwhelmingly lack predicate-like tense--aspect--mood and argument indexation inflections. (Section 15.3.2)" ;
    dcterms:isPartOf "15.3.2" ;
    compcon:expressionOf compcon:cxn_adverbial-dependent-clause,
        compcon:cxn_complement .

compcon:str_active-alignment a compcon:str ;
    rdfs:label "active alignment" ;
    dc:description "a <a>system</a> in which some <a>S roles</a> are co-expressed with the <a>A role</a>, and other S roles are co-expressed with the <a>P role</a>. <i>Example</i>: in Lakhota, the <a>index</a> for the S role of <q>come</q> is the same <a>form</a> as the index for the A role of <q>help</q>, but the index for the S role of <q>be sick</q> is the same form as the index for the P role of <q>help</q>. (Section 6.3.3)" ;
    dcterms:isPartOf "6.3.3" ;
    skos:altLabel "active alignment (system)" ;
    compcon:expressionOf compcon:cxn_intransitive-construction ;
    compcon:subtypeOf compcon:str_alignment-system .

compcon:str_active-category a compcon:str ;
    rdfs:label "active category" ;
    cx:hasExample compcon:active-category-example-0,
        compcon:active-category-example-1 ;
    dc:description "the <a>morphosyntactic</a> category in the <a>active alignment system</a> that <a>co-expresses</a> some <a>S roles</a> -- in particular, the S role of <q>walk</q> -- and the <a>A role</a>. <i>Example</i>: in Lakhota, the index <i>ya-</i> <q>you (sg.)</q> in <i>ó-ma-ya-kiye</i> <q>you help/helped me</q> and <i>ya-ʔu</i> <q>you (sg.) are coming</q> expresses the active category. (Section 6.3.3)" ;
    dcterms:isPartOf "6.3.3" ;
    skos:altLabel "actor category",
        "agentive (category)" .

compcon:str_actual-information-packaging-strategy a compcon:str ;
    rdfs:label "actual information packaging strategy" ;
    cx:hasExample compcon:actual-information-packaging-strategy-example-0 ;
    dc:description "a <a>strategy</a> found with <a>nonprototypical construction</a> types, such as <a>complement constructions</a> which express <a>reference</a> to <a>actions</a>. The actual IP strategy is to recruit the strategy used for the prototypical <a>semantic content</a> function of the <a>information packaging</a> function of the nonprototypical construction. <i>Example</i>: in the English Nominalization Construction exemplified by <i>the corporation's neglect of worker's rights</i>, the <a>action concept</a> <i>neglect</i> that is being <a inf:reference>referred</a> to recruits the construction used for <a sem:object-concept>object</a> <a>reference</a>, the prototypical <a>referring phrase</a> -- it is preceded by the Possessive Phrase <i>the corporation's</i> and followed by the Genitive Oblique <i>of worker's rights</i>; contrast the different strategy found with the prototypical <a>action</a> <a>predication</a> construction <i>The corporation neglected worker's rights</i>. The actual IP strategy contrasts with the <a>semantic IP strategy</a> and the <a>hybrid IP strategy</a>. (Section 2.4)" ;
    dcterms:isPartOf "2.4" ;
    skos:altLabel "actual IP strategy",
        "actual information packaging (strategy)" ;
    compcon:expressionOf compcon:cxn_nonprototypical-construction ;
    compcon:subtypeOf compcon:str_information-packaging-alignment .

compcon:str_adjective-impersonal-strategy a compcon:str ;
    rdfs:label "adjective impersonal strategy" ;
    cx:hasExample compcon:adjective-impersonal-strategy-example-0 ;
    dc:description "a <a>strategy</a> used in the <a>stative complex predicate construction</a> in which the <a cxn:manner-complex-predicate>manner</a> (more generally, <a>stative</a>) <u>component</u> is expressed like an <a>adjective</a> (<a>property</a> <a>modifier</a>) in an <a>argument phrase</a> that does not <a>index</a> an <a inf:participant>argument</a> of the <a>event</a> <a>predicate</a> in the complex predicate construction. <i>Example</i>: in Manchu <i>sargan jui hocikon ucule-he</i> <q>The girl sang beautifully</q>, <i>hocikon</i> <q>beautifully</q> does not index <i>sargan jui</i> <q>girl</q>. (Section 14.2)" ;
    dcterms:isPartOf "14.2" ;
    compcon:expressionOf compcon:cxn_stative-complex-predicate ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_adjective-personal-strategy a compcon:str ;
    rdfs:label "adjective personal strategy" ;
    cx:hasExample compcon:adjective-personal-strategy-example-0 ;
    dc:description "a <a>strategy</a> used in the <a>stative complex predicate construction</a> in which the <a cxn:manner-complex-predicate>manner</a> (more generally, <a>stative</a>) <u>component</u> is expressed like an <a>adjective</a> (<a>property</a> <a>modifier</a>) in an <a>argument phrase</a> that <a>indexes</a> an <a sem:participant>argument</a> of the <a>event</a> <a>predicate</a> in the complex predicate construction. <i>Example</i>: in Latin <i>mendicus a me tristis stipem petivit</i> <q>The beggar asked me sadly for a gift</q>, the form <i>tristis</i> <q>sadly</q> indexes the subject <i>mendicus</i> <q>beggar</q> in <a>case</a>, <a>number</a> and <a>gender--class</a>. (Section 14.2)" ;
    dcterms:isPartOf "14.2" ;
    compcon:expressionOf compcon:cxn_stative-complex-predicate ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_adjoined-strategy a compcon:str ;
    rdfs:label "adjoined strategy" ;
    cx:hasExample compcon:adjoined-strategy-example-0 ;
    dc:description "a <a>strategy</a> for the <a>relative clause construction</a> in which the <a>relative clause</a> is juxtaposed to the <a>matrix clause</a>. <i>Example</i>: Warlpiri <i>ŋatʲuluɭu ɳa yankiri pantuɳu</i> [<i>kutʲalpa ŋapa ŋaɳu</i>] <q>I speared the emu which was drinking water</q> illustrates the adjoined strategy -- the relative clause <i>kutʲalpa ŋapa ŋaɳu</i> <q>which was drinking water</q> is juxtaposed to the matrix clause <i>ŋatʲuluɭu ɳa yankiri pantuɳu</i> <q>I speared the emu</q>, and not adjacent to the relative clause head <i>yankiri</i> <q>emu</q>. The adjoined strategy is quite rare and largely found in Australian languages, where it is identical to the <u>temporal</u> <a>adverbial clause construction</a> and presumably recruited from it. (Section 19.2.3)" ;
    dcterms:isPartOf "19.2.3" ;
    skos:altLabel "adjoined (strategy)" ;
    compcon:subtypeOf compcon:str_non-externally-headed .

compcon:str_adnominal-possessive-strategy a compcon:str ;
    rdfs:label "adnominal possessive strategy" ;
    cx:hasExample compcon:adnominal-possessive-strategy-example-0 ;
    dc:description "a <a>strategy</a> for the <a>presentational possession construction</a> in which the <a>possessor</a> is expressed not by an <a>argument phrase</a> but with a <a>possessive modification construction</a>. <i>Example</i>: Mokilese <i>mine woaroa-n woal-o war</i> [exist <sc>clf-3sg</sc> man-that canoe] <q>That man has a canoe</q> [lit. <q>That man's canoe exists</q>] is an instance of the adnominal possessive strategy. The adnominal possessive strategy is an instance of the <a>internal possessor strategy</a>. (Section 10.4.2)" ;
    dcterms:isPartOf "10.4.2" ;
    skos:altLabel "adnominal possessive (strategy)",
        "genitive strategy" ;
    compcon:expressionOf compcon:cxn_presentational-possession-construction ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_adpositional-personal-strategy a compcon:str ;
    rdfs:label "adpositional personal strategy" ;
    cx:hasExample compcon:adpositional-personal-strategy-example-0 ;
    dc:description "a <a>strategy</a> used in the <a>stative complex predicate construction</a> in which the <a sem:stative>stative component</a> is expressed with a <a>flag</a> just like an <a>argument phrase</a>, and in addition it <a>indexes</a> a <a>participant</a> in the <a>event</a>. <i>Example</i>: in Russian <i>on umer molodym</i> <q>He died young</q>, <i>molodym</i> <q>young</q> is in the Masculine Singular Instrumental form, indexing Masculine Singular <i>on</i> <q>he</q> as well as taking an <a cxn:oblique-phrase>oblique</a> Instrumental flag. (Section 14.2)" ;
    dcterms:isPartOf "14.2" ;
    compcon:expressionOf compcon:cxn_stative-complex-predicate ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_adpositional-strategy a compcon:str ;
    rdfs:label "adpositional strategy" ;
    cx:hasExample compcon:adpositional-strategy-example-0 ;
    dc:description "a <a>strategy</a> used in the <a>stative complex predicate construction</a> in which the <a cxn:manner-complex-predicate>manner</a> (more generally, <a>stative</a>) <u>component</u> is expressed with a <a>case marker</a> just like an <a>argument phrase</a>, either in the basic lexical form or in a nominalized form of the stative concept word. <i>Example</i>: in Mordvin <i>t'ejt'er-es mor-i mazi-ste</i> <q>The girls sings beautifully</q>, <i>mazi</i> <q>beautiful</q> takes the Elative <a cxn:oblique-phrase>oblique</a> flag <i>-ste</i>. (Section 14.2)" ;
    dcterms:isPartOf "14.2" ;
    compcon:expressionOf compcon:cxn_stative-complex-predicate ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_adverbial-clause-strategy a compcon:str ;
    rdfs:label "adverbial clause strategy" ;
    dc:description "" ;
    compcon:expressionOf compcon:cxn_concessive-conditional-construction,
        compcon:cxn_concessive-construction,
        compcon:cxn_conditional-construction ;
    compcon:subtypeOf compcon:str_recruitment-strategy .

compcon:str_adverbial-strategy a compcon:str ;
    rdfs:label "adverbial strategy" ;
    cx:hasExample compcon:adverbial-strategy-example-0 ;
    dc:description "a <a>strategy</a> used in the <a>stative complex predicate construction</a> in which the <a cxn:manner-complex-predicate>manner</a> (more generally, <a>stative</a>) <u>component</u> is expressed using a distinct and unique <a>morphosyntactic</a> form. <i>Example</i>: in English <i>The girl sang beautiful-ly</i>, <i>beautiful</i> uses the unique suffix <i>-ly</i> to combine with the <a>event</a> <a>predicate</a> <i>sang</i>. The adverbial strategy is probably a more <a>grammaticalized</a> version of other strategies for stative complex predicates. (Section 14.2)" ;
    dcterms:isPartOf "14.2" ;
    compcon:expressionOf compcon:cxn_stative-complex-predicate ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_adverbializer a compcon:str ;
    rdfs:label "adverbializer" ;
    cx:hasExample compcon:adverbializer-example-0 ;
    dc:description "a morpheme that overtly expresses the semantic relation in an <a>adverbial clause construction</a>. <i>Example</i>: in <i>I left the party because I was tired, because</i> is the adverbializer. An adverbial clause construction with an adverbializer is an example of <a>syndetic</a> <a cxn:subordinate-clause>subordination</a>. If the morpheme is affixed to a <a>predicate</a>, it is not an adverbializer but an overt marker of <a>deranked</a> status. (Section 15.3.2)" ;
    dcterms:isPartOf "15.3.2" ;
    compcon:expressionOf compcon:cxn_adverbial-clause-construction ;
    compcon:subtypeOf compcon:str_conjunction .

compcon:str_affixal-negation a compcon:str ;
    rdfs:label "affixal negation" ;
    dc:description "" ;
    compcon:expressionOf compcon:cxn_declarative-negation-construction ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_agree-disagree-alignment a compcon:str ;
    rdfs:label "agree/disagree alignment" ;
    cx:hasExample compcon:agree-disagree-alignment-example-0,
        compcon:agree-disagree-alignment-example-1,
        compcon:agree-disagree-alignment-example-2 ;
    dc:description "an <a>alignment strategy</a> for the <a>polarity response construction</a> in which the answer agrees/disagrees with the polarity of the question. <i>Example</i>: in Gulf Arabic, the answers to the negative polarity question <i>maa ʕindik fluus, muu chidhi?</i> <q>You don't have any money, right?</q> are <i>naʕam</i> <q>yes, I have no money</q> (agreeing with the speaker) or <i>bala</i> <q>no, I do have money</q> (disagreeing with the speaker). (Section 12.3.3)" ;
    dcterms:isPartOf "12.3.3" ;
    skos:altLabel "agree/disagree alignment (strategy)" ;
    compcon:subtypeOf compcon:str_polarity-response-alignment .

compcon:str_allative-comparative a compcon:str ;
    rdfs:label "allative comparative" ;
    cx:hasExample compcon:allative-comparative-example-0 ;
    dc:description "a <a>fixed-case strategy</a> in <a>comparative constructions</a> in which there is a <a>clause</a> which attributes a <a>gradable predicative scale</a> to the <a>comparee</a>, and the <a>standard</a> is expressed as an <a>oblique argument phrase</a> using a spatial <a>flag</a> with an allative (<q>to</q>) <a>meaning</a>. <i>Example</i>: Nuer <i>diid ne gän ke ji</i> <q>I am bigger than you</q> is an instance of the allative comparative -- <i>diid ne gän</i> asserts that I am big, and <i>ke ji</i> expresses the standard, you, with a flag <i>ke</i> meaning <q>to</q>. (Section 17.2.2)" ;
    dcterms:isPartOf "17.2.2" ;
    compcon:expressionOf compcon:cxn_comparative-construction ;
    compcon:subtypeOf compcon:str_fixed-case,
        compcon:str_ordered-strategy .

compcon:str_alternative-concessive-conditional-strategy a compcon:str ;
    rdfs:label "alternative concessive conditional strategy" ;
    cx:hasExample compcon:alternative-concessive-conditional-strategy-example-0 ;
    dc:description "a strategy for expressing a <a>concessive conditional construction</a> where the <a cxn:protasis-clause>protasis</a>, which specifies the set of conditions for the concessive conditional, invokes the two polar alternatives possible in the <a>scalar model</a>; the <a cxn:apodosis-clause>apodosis</a> expresses the unexpected opposite of the expected causal relation between either alternative and the outcome, and so implies the same outcome no matter what. <i>Example</i>: <i>Whether he is right or not, we must support him</i> uses the alternative concessive conditional strategy -- either he is right or he is wrong, and under either of those conditions that make up the scalar model, we must support him. (Section 17.3.3)" ;
    dcterms:isPartOf "17.3.3" ;
    compcon:expressionOf compcon:cxn_concessive-conditional-construction ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_anchoring-nominal-strategy a compcon:str ;
    rdfs:label "anchoring nominal strategy" ;
    dc:description "" ;
    compcon:expressionOf compcon:cxn_typifying-construction ;
    compcon:subtypeOf compcon:str_recruitment-strategy .

compcon:str_animacy-based-split-ergativity a compcon:str ;
    rdfs:label "animacy-based split ergativity" ;
    cx:hasExample compcon:animacy-based-split-ergativity-example-0,
        compcon:animacy-based-split-ergativity-example-1 ;
    dc:description "a <a>strategy</a> for the <a>passive--inverse voice construction</a> in which there is a distinct, <a str:overt-coding>overt</a> <a>flag</a> of higher-<a>salience</a> <a>P</a> <a>participants</a> and also for lower-salience <a>A</a> <a>participants</a>. <i>Example</i>: in Dyirbal, the Accusative Case suffix <i>-na</i> is used for <a cxn:first-person-pronoun>first</a> and <a cxn:second-person-pronoun>second pronominal</a> P participants, and the Ergative Case suffix <i>-ŋu</i> is used for <a cxn:third-person-pronoun>third person</a> pronominal and <a>common noun</a> A participants. (Section 8.3)" ;
    dcterms:isPartOf "8.3" ;
    compcon:subtypeOf compcon:str_passive-inverse-alignment .

compcon:str_anterior-deranking a compcon:str ;
    rdfs:label "anterior deranking" ;
    cx:hasExample compcon:anterior-deranking-example-0 ;
    dc:description "the variant of a <a>deranking strategy</a> in which the deranked <a>clauses</a> precede the clause that is expressed like a simple <a>main clause</a>. <i>Example</i>: Japanese <i>ojiisanga yamade hataraite obaasanga miseno bano shita</i> <q>The old man worked at the mountain, and the old woman tended the store</q> is an instance of anterior deranking -- the suffix <i>-te</i> on <i>hataraite</i> <q>worked</q> indicates that the anterior predicate is deranked. (Section 15.2.3)" ;
    dcterms:isPartOf "15.2.3" ;
    compcon:expressionOf compcon:cxn_coordinate-clause-construction ;
    compcon:subtypeOf compcon:str_deranking,
        compcon:str_word-order .

compcon:str_anterior-zero a compcon:str ;
    rdfs:label "anterior zero" ;
    dc:description "" ;
    compcon:expressionOf compcon:cxn_object-identity-construction,
        compcon:cxn_predicate-identity-construction,
        compcon:cxn_subject-identity-construction ;
    compcon:subtypeOf compcon:str_zero-coding .

compcon:str_anticausative a compcon:str ;
    rdfs:label "anticausative" ;
    cx:hasExample compcon:anticausative-example-0 ;
    dc:description "a <a>system</a> in which a <a>noncausal event</a> and its counterpart <a>causal event</a> are expressed such that the noncausal event <a>predicate</a> adds <a>overt coding</a> to the causal event predicate. <i>Example</i>: in Yagua, the noncausal event <i>supatá-y</i> <q>come out</q> is formed by the causal event predicate <i>supatá</i> <q>pull out</q> plus the overt anticausative suffix <i>-y</i>. (Section 6.3.4)" ;
    dcterms:isPartOf "6.3.4" ;
    compcon:expressionOf compcon:cxn_passive-inverse-voice .

compcon:str_associative-equative a compcon:str ;
    rdfs:label "associative equative" ;
    cx:hasExample compcon:associative-equative-example-0 ;
    dc:description "a <a>fixed-case strategy</a> in <a>equative constructions</a> in which there is a <a>clause</a> which attributes a <a>gradable predicative scale</a> to the <a>comparee</a>, and the <a>standard</a> is expressed as an <a>oblique argument phrase</a> with a spatial <a>flag</a> with an associative (<q>with</q>) <a>meaning</a>. <i>Example</i>: Mandarin Chinese <i>Tā gēn nǐ yíyàng gāo</i> <q>She is as tall as you</q> is an instance of an associative equative -- the predicate is <i>yíyàng gāo</i> <q>[is] one.manner tall</q> and the standard <i>nǐ</i> <q>you</q> uses the flag <i>gēn</i> <q>with</q>. The associative marker may also be a <a cxn:coordinate-construction>coordinating</a> <a>conjunction</a>. (Section 17.2.4)" ;
    dcterms:isPartOf "17.2.4" ;
    compcon:expressionOf compcon:cxn_equative-construction ;
    compcon:subtypeOf compcon:str_fixed-case,
        compcon:str_ordered-strategy .

compcon:str_associative-strategy a compcon:str ;
    rdfs:label "associative strategy" ;
    dc:description "" ;
    compcon:expressionOf compcon:cxn_associative-construction ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_asyndetic a compcon:str ;
    rdfs:label "asyndetic" ;
    cx:hasExample compcon:asyndetic-example-0 ;
    dc:description "the combination of <a>clauses</a> in <a>complex sentence constructions</a>, and of <a>coordinands</a> in <a>coordinate constructions</a> (whether the coordinands are clauses or not), by simple <a>juxtaposition</a>. <i>Example</i>: in Lavukaleve <i>ngabakala ngauia tula</i> [my:paddle my:knife small] <q>my paddle and my small knife</q>, the coordinate construction is asyndetic. (Section 15.2.2)" ;
    dcterms:isPartOf "15.2.2" ;
    compcon:expressionOf compcon:cxn_complex-sentence ;
    compcon:subtypeOf compcon:str_zero-coding .

compcon:str_balancing a compcon:str ;
    rdfs:label "balanced, balancing" ;
    cx:hasExample compcon:balancing-example-0 ;
    dc:description "a <a>strategy</a> in which the <a>predicate</a> in a <a>complex sentence construction</a> or a <a>complex predicate construction</a> <a>recruits</a> the predicate construction in a simple predication, inflections and all. <i>Example</i>: <i>The robins drank water and the juncos ate fennel seeds</i> is an instance of a balanced <a>coordinate clause construction</a>: <i>drank</i> and <i>ate</i> are in the same <a>form</a> as in the simple predications <i>The robins drank water</i> and <i>The juncos ate fennel seeds</i>. (Sections 14.2, <b>15.2.3</b>)" ;
    dcterms:isPartOf "14.2",
        "15.2.3" ;
    compcon:subtypeOf compcon:str_clause-alignment .

compcon:str_bare-verb-stem a compcon:str ;
    rdfs:label "bare verb stem" ;
    cx:hasExample compcon:bare-verb-stem-example-0 ;
    dc:description "a <a>strategy</a> for the <a>imperative--hortative construction</a>, particularly the <a sem:second-person>second person</a> variant, in which the <a>verb</a> stem without any inflection is used. <i>Example</i>: English <i>Dance!</i> is an instance of the bare verb stem strategy for the second person imperative--hortative. (Section 12.4.1)" ;
    dcterms:isPartOf "12.4.1" ;
    compcon:expressionOf compcon:cxn_imperative-hortative-construction ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_base-object-inertia a compcon:str ;
    rdfs:label "base object inertia" ;
    dc:description "" ;
    compcon:subtypeOf compcon:str_applicative-alignment .

compcon:str_basic-word-order a compcon:str ;
    rdfs:label "basic word order" ;
    cx:hasExample compcon:basic-word-order-example-0 ;
    dc:description "a <a>strategy</a> for expressing the categories of <a>subject</a> (S), <a>verb</a> (V), and <a inf:object-argument>object</a> (O) in a <a>transitive construction</a>, and <a>subject</a> in an <a>intransitive construction</a>. Some linguists use <a>A</a> and <a>P</a> instead of S and O, respectively, in describing the basic word order of transitive constructions. Basic word order is distinguished from nonbasic word orders in terms of <a>information packaging</a> (basic word order represents <a inf:topic-comment>topic--comment</a> information packaging); less <a>structural coding</a>; and higher <a>token frequency</a>. <i>Example</i>: the basic word order of English is SVO, as in <i>Jerry saw the bluebird on Sunday</i>. Some linguists analyze basic word order in terms of the pair of binary orders subject--verb and object--verb. Sometimes the order of oblique phrases is included; if so, the oblique phrase is abbreviated X; English is SVOX. (Section 6.2.2)" ;
    dcterms:isPartOf "6.2.2" ;
    compcon:expressionOf compcon:cxn_clause ;
    compcon:subtypeOf compcon:str_word-order .

compcon:str_biclausal-reciprocal a compcon:str ;
    rdfs:label "biclausal reciprocal" ;
    cx:hasExample compcon:biclausal-reciprocal-example-0 ;
    dc:description "the <a>strategy</a> of <a>recruiting</a> a <a>construction</a> with two <a>clauses</a> to use as a <a>reciprocal construction</a>. <i>Example</i>: Colloquial Cantonese <i>léih hóyíh bōng ngóh ngóh hóyíh bōng léih</i> <q>We can help each other</q> is literally <q>You can help me, I can help you</q>. (Section 7.2)" ;
    dcterms:isPartOf "7.2" ;
    compcon:expressionOf compcon:cxn_reciprocal-construction ;
    compcon:subtypeOf compcon:str_recruitment-strategy .

compcon:str_bisyndetic a compcon:str ;
    rdfs:label "bisyndetic" ;
    cx:hasExample compcon:bisyndetic-example-0,
        compcon:bisyndetic-example-1 ;
    dc:description "a <a>strategy</a> used in <a>syndetic</a> <a>coordination</a> where there are as many <a>coordinators</a> as <a>coordinands</a>. <i>Example</i>: in Upper Kuskokwim <i>dineje ʔɨɬ midzish ʔɨɬ</i> <q>moose and caribou</q>, the coordinator <i>ʔɨɬ</i> occurs with each coordinand. In some cases, the two coordinators are different, as in English <i>Either...or...</i>, as in <i>Either you leave or I leave</i>. (Section 15.2.2)" ;
    dcterms:isPartOf "15.2.2" ;
    compcon:expressionOf compcon:cxn_coordinate-construction ;
    compcon:subtypeOf compcon:str_syndetic .

compcon:str_case-affix a compcon:str ;
    rdfs:label "case affix" ;
    cx:hasExample compcon:case-affix-example-0 ;
    dc:description "a <a>flag</a> which occurs as an affix. <i>Example</i>: in Russian <i>kniga Ivan-a</i> <q>John's book</q>, <i>-a</i> is a Genitive case marking suffixed to <i>Ivan</i> <q>John</q>. (Section 4.3)" ;
    dcterms:isPartOf "4.3" ;
    compcon:subtypeOf compcon:str_flagging .

compcon:str_causative a compcon:str ;
    rdfs:label "causative" ;
    cx:hasExample compcon:causative-example-0 ;
    dc:description "a <a>system</a> of <a>strategies</a> in which a <a>noncausal event</a> and its counterpart <a>causal event</a> are expressed such that the causal event <a>predicate</a> adds <a>overt coding</a> to the noncausal event predicate. <i>Example</i>: in Turkish, the causal event predicate <i>öl-dür-</i> <q>kill</q> is formed by the noncausal event predicate <i>öl</i> <q>die</q> plus the overt causative suffix <i>-dür</i>. (Section 6.3.4)" ;
    dcterms:isPartOf "6.3.4" ;
    compcon:expressionOf compcon:cxn_causative-construction .

compcon:str_causative-applicative-co-expression a compcon:str ;
    rdfs:label "causative--applicative co-expression" ;
    cx:hasExample compcon:causative-applicative-co-expression-example-0,
        compcon:causative-applicative-co-expression-example-1 ;
    dc:description "a <a>system</a> in which the <a>causative construction</a> and an <a>applicative construction</a> are identical. <i>Example</i>: Kinyarwanda <i>Umugabo a-ra-andik-iiš-a umugabo ibárúwa</i> <q>The man is making the man write a letter</q> is a causative construction with the overt suffix <i>-iiš</i> on the verb, and <i>Umugabo a-ra-andik-iiš-a ikárámu ibárúwa</i> <q>The man is writing a letter with a pen</q> is an applicative construction with the <a>instrument role</a> expressed as <a cxn:applicative-object-phrase>applicative object</a>, with the same suffix <i>-iiš</i> on the verb. (Section 9.3)" ;
    dcterms:isPartOf "9.3" ;
    compcon:expressionOf compcon:cxn_applicative-construction,
        compcon:cxn_causative-construction ;
    compcon:subtypeOf compcon:str_co-expression .

compcon:str_causative-coordinate-strategy a compcon:str ;
    rdfs:label "causative coordinate strategy" ;
    dc:description "" ;
    compcon:expressionOf compcon:cxn_stative-complex-predicate ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_causative-predicational-strategy a compcon:str ;
    rdfs:label "causative predicational strategy" ;
    dc:description "" ;
    compcon:expressionOf compcon:cxn_stative-complex-predicate ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_circumposition a compcon:str ;
    rdfs:label "circumposition" ;
    cx:hasExample compcon:circumposition-example-0 ;
    dc:description "an <a>adposition</a> which occurs as two morphemes, one before and one after the <a>modifier</a> or <a inf:reference>argument</a> <a>head</a>. <i>Example</i>: in Pashto <i>ter maktaba poori</i> <q>as far as the school</q>, the <a>meaning</a> <q>as far as</q> is expressed by the combination of <i>ter</i> preceding <i>maktaba</i> <q>school</q> and <i>poori</i> following it. (Section 4.3)" ;
    dcterms:isPartOf "4.3" ;
    compcon:subtypeOf compcon:str_adposition .

compcon:str_cleft-strategy a compcon:str ;
    rdfs:label "cleft strategy" ;
    cx:hasExample compcon:cleft-strategy-example-0 ;
    dc:description "a <a>strategy</a> for <a>identificational constructions</a> that uses an <a cxn:equational-construction>equational</a> <a>copula</a> to link the <a>focus</a> and the <a>presupposed open proposition</a> that makes up the remainder of the <a>clause</a>. <i>Example</i>: the English Pseudocleft Construction that is the second sentence in <i>Nikki Caine, 19, doesn't want to be a movie star. What she hopes to do is be a star on the horse-show circuit</i>, uses the cleft strategy: the equational copula <i>is</i> links the presupposed open proposition <i>what she hopes to do</i> and the focus <i>be a star on the horse-show circuit</i>. (Section 11.4.2)" ;
    dcterms:isPartOf "11.4.2" ;
    compcon:expressionOf compcon:cxn_identificational-construction ;
    compcon:subtypeOf compcon:str_recruitment-strategy .

compcon:str_cognate-head-dependent-strategy a compcon:str ;
    rdfs:label "cognate head-dependent strategy" ;
    cx:hasExample compcon:cognate-head-dependent-strategy-example-0 ;
    dc:description "a <a>strategy</a> for an <a>argument complex predicate</a> in which the <a>verb</a> and the <a>noun</a> (<a>argument phrase</a>) are phonologically related (and sometimes even identical). <i>Example</i>: in Maale <i>ʔízí ʃeeʃi ʃéék'k'-á-ne</i> <q>He is urinating</q>, <i>ʃeeʃi</i> <q>urine</q> and <i>ʃéék'k'</i> <q>urinate</q> are morphologically related as well as semantically extremely close. (Section 13.6)" ;
    dcterms:isPartOf "13.6" ;
    skos:altLabel "cognate object (strategy)" ;
    compcon:expressionOf compcon:cxn_argument-complex-predicate ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_complex-predicate-applicative-strategy a compcon:str ;
    rdfs:label "complex predicate applicative strategy" ;
    cx:hasExample compcon:complex-predicate-applicative-strategy-example-0 ;
    dc:description "a <a>strategy</a> for the <a>applicative construction</a> in which there is a second <a>verb</a> accompanying the main verb that encodes the fact that there is an <a>applicative object</a>. <i>Example</i>: Yoruba <i>ó ra iṣu fún mi</i> <q>He bought a yam for me</q> is an instance of a complex predicate applicative strategy with the verbs <i>ra</i> <q>buy</q> and <i>fún</i> <q>give</q>. (Section 9.3)" ;
    dcterms:isPartOf "9.3" .

compcon:str_complex-predicate-asc-strategy a compcon:str ;
    rdfs:label "complex predicate ASC strategy" ;
    dc:description "" ;
    compcon:expressionOf compcon:cxn_clause ;
    compcon:subtypeOf compcon:str_overt-coding .

compcon:str_complex-predicate-causative-strategy a compcon:str ;
    rdfs:label "complex predicate causative strategy" ;
    cx:hasExample compcon:complex-predicate-causative-strategy-example-0 ;
    dc:description "a <a>strategy</a> for the <a>causative construction</a> in which there is a second <a>verb</a> accompanying the main verb that encodes the fact that this is a <a>causative event</a> with a <a>causer</a> <a>participant role</a> added to the <a>causal chain</a> of the <a>base event</a>. <i>Example</i>: the English Periphrastic Causative Construction, illustrated by <i>Sandra had Phil sweep the patio</i>, is an example of the complex predicate causative strategy; there is a second verb <i>had</i> accompanying the verb <i>sweep</i> that indicates that Sandra is the causer of the causative event. (Section 9.2)" ;
    dcterms:isPartOf "9.2" ;
    skos:altLabel "periphrastic causative strategy" .

compcon:str_complex-predicate-passive-inverse-voice-strategy a compcon:str ;
    rdfs:label "complex predicate passive--inverse strategy" ;
    cx:hasExample compcon:complex-predicate-passive-inverse-voice-strategy-example-0 ;
    dc:description "a <a>strategy</a> for the <a>passive--inverse voice construction</a> in which there is a second <a>verb</a> accompanying the main verb that encodes the fact that the <a>subject</a> <a>referring phrase</a> expresses the <a>P</a> <a>participant</a>. <i>Example</i>: in Vietnamese <i>Nam bị Nga đánh</i> <q>Nam was beaten by Nga</q>, the verb <i>bi</i> <q>suffer</q> accompanying the main verb <i>đánh</i> <q>beat</q> is an instance of the complex predicate passive--inverse voice strategy. (Section 8.3)" ;
    dcterms:isPartOf "8.3" ;
    skos:altLabel "complex predicate passive--inverse (voice) strategy" .

compcon:str_compounding a compcon:str ;
    rdfs:label "compounding" ;
    cx:hasExample compcon:compounding-example-0 ;
    dc:description "a strategy for encoding the relation in <a>major propositional acts</a> (<a>modifier</a>--<a>referent</a>, <a>predicate</a>--<a inf:reference>argument</a>), in which the two <a>elements</a> are combined in a single word. <i>Example</i>: in <i>doorknob</i>, <i>door</i> and <i>knob</i> form an instance of compounding. The term <q>compounding</q> or <q>compound</q> is also used for the <a>typifying construction</a> and the <a>binominal lexeme construction</a>. (Section 4.2)" ;
    dcterms:isPartOf "4.2" ;
    compcon:expressionOf compcon:cxn_modification-construction ;
    compcon:subtypeOf compcon:str_simple-strategy .

compcon:str_conditional-deranking a compcon:str ;
    rdfs:label "conditional deranking" ;
    cx:hasExample compcon:conditional-deranking-example-0,
        compcon:conditional-deranking-example-1 ;
    dc:description "a <a>system</a> where a <a>same-subject</a> <a>reference tracking construction</a> uses a <a>deranking strategy</a>, whereas the <a>different-subject</a> reference tracking construction uses a <a>balanced</a> strategy. <i>Example</i>: Wolof <i>dem na ma <b>à o</b> ko</i> <q>I went and called him</q> [same-subject] uses a Serial Marker <i>à</i> and a deranked Subjunctive verb form <i>o</i>; <i>nyeu <b>on na</b> te wakh <b>on na</b> ma ko</i> <q>He came and I told (it to) him</q> uses the balanced construction with the simple verb form, the Past Tense <i>on</i> and the Indicative Marker <i>na</i>. (Section 16.3)" ;
    dcterms:isPartOf "16.3" ;
    skos:altLabel "conditional deranking (system)",
        "conditionally deranked" ;
    compcon:subtypeOf compcon:str_reference-tracking-system .

compcon:str_conditional-discourse-reference-system a compcon:str ;
    rdfs:label "conditional discourse reference system" ;
    cx:hasExample compcon:conditional-discourse-reference-system-example-0 ;
    dc:description "a <a>system</a> where a <a>same-subject</a> <a>reference tracking construction</a> uses a different <a>strategy</a> from the standard discourse reference strategies found in connected discourse. This is essentially the use of <a>zero anaphora</a> in the same-subject reference tracking construction that is conditional on the construction rather than on the discourse context. <i>Example</i>: the coordination clause construction illustrated in <i>Bilbo<sub>i</sub> found a ring and Ø<sub>i</sub> put it in his pocket</i> uses zero anaphora in the second clause; zero anaphora is not generally allowed in discourse reference (*<i>Put it in his pocket</i>). (Section 16.2)" ;
    dcterms:isPartOf "16.2" ;
    compcon:subtypeOf compcon:str_reference-tracking-system .

compcon:str_conjoined-comparative a compcon:str ;
    rdfs:label "conjoined comparative" ;
    cx:hasExample compcon:conjoined-comparative-example-0 ;
    dc:description "a <a>derived-case</a> <a>comparative</a> <a>strategy</a> which consists of a <a>coordinate clause construction</a> where the two <a>clauses</a> assert that the <a>gradable predicative scale</a> applies to the <a>comparee</a> and the <a>standard</a>. <i>Example</i>: Sika <i>dzarang tica gahar, dzarang rei kesik</i> <q>That horse is bigger than this horse</q> is an instance of the conjoined comparative -- it conjoins <i>dzarang tica gahar</i> <q>That horse is big</q> and <i>dzarang rei kesik</i> <q>This horse is small</q>. (Section 17.2.2)" ;
    dcterms:isPartOf "17.2.2" ;
    compcon:expressionOf compcon:cxn_comparative-construction ;
    compcon:subtypeOf compcon:str_derived-case,
        compcon:str_independent-strategy .

compcon:str_conjoined-exceed-comparative a compcon:str ;
    rdfs:label "conjoined exceed comparative" ;
    cx:hasExample compcon:conjoined-exceed-comparative-example-0 ;
    dc:description "a rare <a>strategy</a> for the <a>comparative construction</a> which recruits a <a>coordinate clause construction</a> (making it similar to the <a>conjoined comparative</a>), but where one <a>clause</a> expresses the <a>proposition</a> that the <a>comparee</a> exceeds the <a>standard</a> (making it similar to the <a>exceed comparative</a>), while the other clause expresses that the <a>gradable predicative scale</a> applies to the comparee, or, sometimes, to the standard instead. <i>Example</i>: Acholi <i>gwok mera dit ki kato meri</i> <q>My dog is bigger than yours</q> is an instance of the conjoined exceed comparative strategy -- the first clause <i>gwok mera dit</i> <q>my dog [is] big</q> attributes size to the comparee, the dog; the second clause <i>kato meri</i> <q>[it] exceeds yours</q> expresses the relation of the comparee to the standard, your dog; and the clauses are conjoined by <i>ki</i> <q>and</q>. (Section 17.2.3)" ;
    dcterms:isPartOf "17.2.3" ;
    skos:altLabel "conjoined exceed (comparative)" ;
    compcon:expressionOf compcon:cxn_comparative-construction ;
    compcon:subtypeOf compcon:str_derived-case,
        compcon:str_relative-strategy .

compcon:str_contiguity-of-serial-verbs a compcon:str ;
    rdfs:label "contiguity of serial verbs" ;
    cx:hasExample compcon:contiguity-of-serial-verbs-example-0 ;
    dc:description "alternative <a>strategies</a> found with <a>serial verb</a> and <a>auxiliary constructions</a>, whether they are contiguous or not. <i>Example</i>: in Jeh <i>ĕn loh chièu reng rŭp bùh cha chŏ̀'wan</i> <q>He went out and got somebody's pig and roasted and ate it</q>, the serial verbs <i>loh chièu reng rŭp bùh cha</i> [exit go search catch roast eat] are all contiguous. Another alternative contiguity strategy in auxiliary constructions is to position the <a>auxiliary</a> in a fixed position in the clause, typically second position, rather than relative to the position of the <a>verb</a> in the auxiliary construction. (Sections 13.2, 13.3.2)" ;
    dcterms:isPartOf "13.2",
        "13.3.2" ;
    compcon:expressionOf compcon:cxn_auxiliary-construction,
        compcon:cxn_eventive-complex-predicate ;
    compcon:subtypeOf compcon:str_serial-verb-strategy .

compcon:str_control-based-strategy a compcon:str ;
    rdfs:label "control-based strategy" ;
    cx:hasExample compcon:control-based-strategy-example-0,
        compcon:control-based-strategy-example-1 ;
    dc:description "<u>monoclausal</u> <a>strategies</a> for the <a>causative construction</a> in which the <a>causee</a> is coded differently depending on how much control the causee has in the outcome of the event. <i>Example</i>: in Hungarian, there is a contrast between encoding the causee in the Accusative Case when the causee has no control (<i>Köhögtettem a gyerek<b>-et</b></i> <q>I made the boy [<sc>acc</sc>] cough</q>) and encoding the causee in the Instrumental Case when the causee has some control over the outcome (<i>Köhögtettem a gyerek-<b>kel</b></i> <q>I had the boy [<sc>inst</sc>] cough</q>). (Section 9.2)" ;
    dcterms:isPartOf "9.2" ;
    compcon:subtypeOf compcon:str_causative-alignment .

compcon:str_controller a compcon:str ;
    rdfs:label "controller" ;
    cx:hasExample compcon:controller-example-0 ;
    dc:description "in a <a>construction</a> using the <a>indexation strategy</a>, the <a>referring phrase</a> that denotes the same <a>referent</a> that is denoted by the <a>index</a>. <i>Example</i>: in Spanish <i>los libros rojos</i> <q>the red books</q>, <i>libros</i> is the controller; it denotes the same referent that the masculine singular nonperson index <i>-os</i> in <i>rojos</i> does. The controller is almost always a referring phrase. (Section 4.4)" ;
    dcterms:isPartOf "4.4" .

compcon:str_converb a compcon:str ;
    rdfs:label "converb" ;
    cx:hasExample compcon:converb-example-0 ;
    dc:description "a <a>deranked</a> predicate form in an <a>asyndetic</a> <a>deranked</a> <a>adverbial clause construction</a> that lacks <a>subject</a> <a>person indexation</a>. <i>Example</i>: in <i>Sitting in front of his desk, he noticed a bright light out the window, sitting</i> is a converb. Haspelmath (1995:5) excludes from the converb category forms that are deranked with subject person indexation that is different from that found with <a inf:declarative>declarative</a> <a>main clause</a> <a>predication</a>." ;
    compcon:expressionOf compcon:cxn_adverbial-dependent-clause .

compcon:str_converb-strategy a compcon:str ;
    rdfs:label "converb strategy" ;
    cx:hasExample compcon:converb-strategy-example-0 ;
    dc:description "a <a>strategy</a> used in the <a>stative complex predicate construction</a> in which the <a cxn:manner-complex-predicate>manner</a> (more generally, <a>stative</a>) <u>component</u> is packaged as a separate primary <a>predication</a> <a cxn:coordinate-construction>coordinated</a> with the <a>event</a> predication using a <a>deranked</a> <a>complex sentence</a> <a>strategy</a>. However, the stative predicate does not index (any of) its argument(s). <i>Example</i>: in Turkana <i>è-pès-e-tè nɪ-a-ron-o-nì̥</i> <q>They kick him badly</q> [lit. <q>They kick him, it being bad</q>] the form of <i>nɪ-a-ron-o-nì̥</i> <q>be bad</q> is a Neuter deranked form, not indexing the third person plural subject of <q>kick</q>. (Section 14.2)" ;
    dcterms:isPartOf "14.2" ;
    compcon:expressionOf compcon:cxn_stative-complex-predicate ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_coordinate-clause-strategy a compcon:str ;
    rdfs:label "coordinate clause strategy" ;
    dc:description "" ;
    compcon:expressionOf compcon:cxn_concessive-conditional-construction,
        compcon:cxn_concessive-construction,
        compcon:cxn_conditional-construction ;
    compcon:subtypeOf compcon:str_recruitment-strategy .

compcon:str_coordinate-impersonal-strategy a compcon:str ;
    rdfs:label "coordinate impersonal strategy" ;
    cx:hasExample compcon:coordinate-impersonal-strategy-example-0 ;
    dc:description "a <a>strategy</a> used in the <a>stative complex predicate construction</a> in which the <a cxn:manner-complex-predicate>manner</a> (more generally, <a>stative</a>) <u>component</u> is packaged as a separate primary <a>predication</a> <a cxn:coordinate-construction>coordinated</a> with the <a>event</a> predication using a <a>balanced</a> <a>complex sentence</a> <a>strategy</a>. However, the stative predicate does not index (any of) its argument(s). <i>Example</i>: in Koasati <i>wayóhka-k ho-palkálki-palámni-n</i> <q>They fly all very fast</q> [lit. <q>They fly (and) it is fast</q>], the second clause <i>ho-palkálki-palámni-n</i> recruits the form of a main clause predicate, but does not index the fliers. (Section 14.2)" ;
    dcterms:isPartOf "14.2" ;
    compcon:expressionOf compcon:cxn_stative-complex-predicate ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_coordinate-personal-strategy a compcon:str ;
    rdfs:label "coordinate personal strategy" ;
    cx:hasExample compcon:coordinate-personal-strategy-example-0 ;
    dc:description "a <a>strategy</a> used in the <a>stative complex predicate construction</a> in which the <a cxn:manner-complex-predicate>manner</a> (more generally, <a>stative</a>) <u>component</u> is packaged as a separate primary <a>predication</a> <a cxn:coordinate-construction>coordinated</a> with the <a>event</a> predication using a <a>balanced</a> <a>complex sentence</a> <a>strategy</a>. In addition, the stative predicate <a>indexes</a> (one of) its argument(s). <i>Example</i>: in Muna <i>ne-rimba no-tende</i> <q>he runs fast</q> [lit. <q>He is fast (and) he runs</q>], <i>ne-rimba</i> [<sc>3sg.rl</sc>-be_fast] recruits the form of a simple predicate and indexes its subject argument. (Section 14.2)" ;
    dcterms:isPartOf "14.2" ;
    compcon:expressionOf compcon:cxn_stative-complex-predicate ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_coordinator a compcon:str ;
    rdfs:label "coordinator" ;
    cx:hasExample compcon:coordinator-example-0 ;
    dc:description "a morpheme that overtly expresses the coordination relation in a <a>coordinate construction</a>. <i>Example</i>: in <i>She picked up the pieces and dumped them in the wastebasket</i>, <i>and</i> is the coordinator. A coordinate construction with a coordinator is an example of <a>syndetic</a> coordination. If the morpheme is affixed to a predicate, it is not a coordinator but an overt marker of <a>deranked</a> status. (Section 15.2.2)" ;
    dcterms:isPartOf "15.2.2" ;
    compcon:expressionOf compcon:cxn_coordinate-construction ;
    compcon:subtypeOf compcon:str_conjunction .

compcon:str_copular-participle-strategy a compcon:str ;
    rdfs:label "copular participle strategy" ;
    cx:hasExample compcon:copular-participle-strategy-example-0 ;
    dc:description "a <a>strategy</a> used in the <a>stative complex predicate construction</a> in which the <a cxn:manner-complex-predicate>manner</a> (more generally, <a>stative</a>) <u>component</u> recruits a <a>nonprototypical predication construction</a> that employs a <a>copula</a>, and in addition the copula is in a <a>deranked</a> form. <i>Example</i>: in Malayalam <i>aval bhamgiy-aayi prasamgiccu</i> <q>She spoke beautifully</q>, <i>bhamgiy</i> <q>beauty</q> is suffixed with a deranked form of the copula <i>-aayi</i>. (Section 14.2)" ;
    dcterms:isPartOf "14.2" ;
    compcon:expressionOf compcon:cxn_stative-complex-predicate ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_correlative-strategy a compcon:str ;
    rdfs:label "correlative strategy" ;
    cx:hasExample compcon:correlative-strategy-example-0 ;
    dc:description "a <a>strategy</a> for the <a>relative clause construction</a> in which the <a>relative clause</a> is juxtaposed to the <a>matrix clause</a> (not unlike the <a>adjoined strategy</a>), the <a>necessarily shared participant</a> is expressed in the relative clause (not unlike the <a>internally headed strategy</a>) accompanied by a relative clause marking morpheme (which may be a <a str:relative-pronoun-strategy>relative pronoun</a> form), and the shared participant is also expressed in the matrix clause, as either a <a>noun</a> or a <a>pronoun</a>. <i>Example</i>: Hindi [<i>ādmī ne jis cākū se murgī ko mārāthā</i>] <i>us cākū ko rām ne dekhā</i> <q>Ram saw the knife with which the man killed the chicken</q> is an instance of the correlative strategy: the relative clause <i>ādmī ne jis cākū se murgī ko mārāthā</i> <q>The man killed the chicken with which knife</q> contains the relative clause head <i>cākū</i> <q>knife</q>, modified by the relative pronoun <i>jis</i> <q>which</q>, and is preposed to the matrix clause <i>us cākū ko rām ne dekhā</i> <q>that knife Ram saw</q>, which contains a second expression of the relative clause head <i>us cākū</i> <q>that knife</q>. (Section 19.2.3)" ;
    dcterms:isPartOf "19.2.3" ;
    skos:altLabel "correlative (strategy)" ;
    compcon:subtypeOf compcon:str_non-externally-headed .

compcon:str_declarative-marker a compcon:str ;
    rdfs:label "declarative marker" ;
    dc:description "" ;
    compcon:expressionOf compcon:cxn_declarative-construction ;
    compcon:subtypeOf compcon:str_overt-coding .

compcon:str_definite-article-strategy a compcon:str ;
    rdfs:label "definite article strategy" ;
    dc:description "" ;
    compcon:expressionOf compcon:cxn_generic-article ;
    compcon:subtypeOf compcon:str_recruitment-strategy .

compcon:str_degree-affix a compcon:str ;
    rdfs:label "degree affix" ;
    cx:hasExample compcon:degree-affix-example-0 ;
    dc:description "a scalar <a>admodifier</a> expressed by an affix. <i>Example</i>: <i>-er</i> in <i>sillier</i> is a degree affix. (Section 4.1.2)" ;
    dcterms:isPartOf "4.1.2" ;
    compcon:expressionOf compcon:cxn_modification-construction ;
    compcon:subtypeOf compcon:str_affixation .

compcon:str_degree-equative a compcon:str ;
    rdfs:label "degree equative" ;
    cx:hasExample compcon:degree-equative-example-0 ;
    dc:description "a <a>fixed-case strategy</a> in <a>equative constructions</a> in which there is a <a>clause</a> which attributes a <a>gradable predicative scale</a> to the <a>comparee</a>, and the <a>standard</a> is expressed as a <a cxn:nominal-attributive-phrase>nominal modifier</a> of a word denoting <q>degree</q>, <q>manner</q>, or <q>quantity</q>. <i>Example</i>: Harar Oromo <i>isíin akká isaaní d'eertuu</i> <q>She is as tall as them</q> is an instance of the degree equative strategy, with the predicate <i>d'eertuu</i> <q>is tall</q>, and the standard, <i>isaaní</i> <q>they [<sc>gen</sc>]</q>, modifies <i>akká</i> <q>manner</q>. (Section 17.2.4)" ;
    dcterms:isPartOf "17.2.4" ;
    compcon:expressionOf compcon:cxn_equative-construction ;
    compcon:subtypeOf compcon:str_fixed-case,
        compcon:str_relative-strategy .

compcon:str_degree-marker a compcon:str ;
    rdfs:label "degree marker" ;
    cx:hasExample compcon:degree-marker-example-0,
        compcon:degree-marker-example-1 ;
    dc:description "a free morpheme or affix that expresses the relative degree of the gradable predicative scale applied to the comparee, in comparative and equative constructions. <i>Example</i>: in <i>taller</i>, the suffix <i>-er</i> is a degree marker. In addition to comparative and equative degree markers, some languages have a distinct <a cxn:superlative-form>superlative degree marker</a>, such as <i>-est</i> in English <i>tallest</i>. (Section 17.2.2)" ;
    dcterms:isPartOf "17.2.2" ;
    compcon:expressionOf compcon:cxn_admodifier,
        compcon:cxn_comparative-construction ;
    compcon:subtypeOf compcon:str_affixation .

compcon:str_degree-modifier a compcon:str ;
    rdfs:label "degree modifier" ;
    cx:hasExample compcon:degree-modifier-example-0 ;
    dc:description "a scalar <a>admodifier</a> expressed by an independent word. <i>Example</i>: <i>very</i> in <i>very happy</i> is a degree modifier. (Section 4.1.2)" ;
    dcterms:isPartOf "4.1.2" ;
    skos:altLabel "degree adverb" ;
    compcon:expressionOf compcon:cxn_admodifier ;
    compcon:subtypeOf compcon:str_affixation .

compcon:str_demonstrative-strategy a compcon:str ;
    rdfs:label "demonstrative strategy" ;
    dc:description "" ;
    compcon:expressionOf compcon:cxn_anaphoric-pronoun ;
    compcon:subtypeOf compcon:str_recruitment-strategy .

compcon:str_detached-complement-strategy a compcon:str ;
    rdfs:label "detached complement strategy" ;
    dc:description "" ;
    compcon:subtypeOf compcon:str_complement-disalignment .

compcon:str_detached-topic-phrase a compcon:str ;
    rdfs:label "detached topic phrase" ;
    cx:hasExample compcon:detached-topic-phrase-example-0 ;
    dc:description "a strategy for expressing a <a inf:reference>topic</a> in which the topic <a>referring phrase</a> differs from the expression of a prototypical <a>subject</a> phrase. <i>Example</i>: in German <i>Peter, ich habe ihn heute nicht getroffen</i> <q>Peter, I have not met him today</q>, the topic phrase <i>Peter</i> is in intial position, prosodically separated from the rest of the clause, and there is a <a>pronoun</a> <i>ihn</i> <q>him</q> referring to Peter in the clause. Not all detached topics have all three of the <a>morphosyntactic</a> properties of the German example; there is a continuum from the prototypical subject/topic phrase and a <dq>fully</dq> detached topic phrase. (Section 11.2.2)" ;
    dcterms:isPartOf "11.2.2" ;
    skos:altLabel "detached (topic phrase)" ;
    compcon:expressionOf compcon:cxn_topic-comment-construction ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_differential-object-marking a compcon:str ;
    rdfs:label "differential object marking" ;
    cx:hasExample compcon:differential-object-marking-example-0 ;
    dc:description "a strategy that is marginal for the <a>passive--inverse voice construction</a> in which there is a distinct, <a str:overt-coding>overt</a> <a>flag</a> of higher <a def:animacy-hierarchy>animacy</a> / higher <a>information status</a> -- that is, higher-<a>salience</a> -- Ps. <i>Example</i>: in Spanish, <i>El director busca a un empleado</i> <q>The director is looking for a [specific] clerk</q>, the phrase <i>un empleado</i> <q>a clerk</q>, referring to the P participant, uses the overt flag <i>a</i>. Differential object marking is not usually considered a strategy of the passive--inverse construction because the <a>A</a> <a>participant</a> is not reduced in salience, although the P participant is higher in salience. (Section 8.3)" ;
    dcterms:isPartOf "8.3" ;
    compcon:subtypeOf compcon:str_passive-inverse-alignment .

compcon:str_direct-object-category a compcon:str ;
    rdfs:label "direct object category" ;
    cx:hasExample compcon:direct-object-category-example-0,
        compcon:direct-object-category-example-1 ;
    dc:description "the <a>morphosyntactic</a> category in the <a>indirective alignment system</a> that <a>co-expresses</a> both <a>T</a> and <a>P roles</a>. <i>Example</i>: in <i>Randy gave the car to his daughter</i>, the T role (<i>the car</i>) is expressed in the same way as the P role in <i>Randy started the car</i>. (Section 7.5.2)" ;
    dcterms:isPartOf "7.5.2" .

compcon:str_direct-question-strategy a compcon:str ;
    rdfs:label "direct question strategy" ;
    dc:description "" ;
    compcon:expressionOf compcon:cxn_interrogative-complement ;
    compcon:subtypeOf compcon:str_recruitment-strategy .

compcon:str_direct-report a compcon:str ;
    rdfs:label "direct report" ;
    cx:hasExample compcon:direct-report-example-0 ;
    dc:description "a <a>strategy</a> for the <a>complement</a> of an <a>utterance event</a> in which the form as well as the content of an utterance is expressed. <i>Example</i>: in <i>Sandy said, <q>I'm buying the house</q></i>, the complement <i>I'm buying the house</i> is presented in its original form -- the speaker is not asserting that s/he is buying the house. A direct report strategy may be accompanied by a <a>quotative marker</a>. Although the direct report strategy originates with utterance <a>complement clause constructions</a>, it is also used with other complement clause constructions lower in the <a>Binding Hierarchy</a>. (Sections <b>18.2.2</b>, 18.3.2)" ;
    dcterms:isPartOf "18.2.2",
        "18.3.2" ;
    compcon:expressionOf compcon:cxn_utterance-construction ;
    compcon:subtypeOf compcon:str_utterance-clause-alignment .

compcon:str_discourse-reference-system a compcon:str ;
    rdfs:label "discourse reference system" ;
    cx:hasExample compcon:discourse-reference-system-example-0 ;
    dc:description "a <a>system</a> where both <a>same-subject</a> and <a>different-subject</a> <a>reference tracking constructions</a> use the standard discourse reference strategies found in connected discourse. Discourse reference systems are found when both same-subject and different-subject constructions are <a>balanced</a>. <i>Example</i>: in Spanish, <a>syndetic</a> <a>coordinate constructions</a> such as <i>El Gobierno Nacional volvió a prorrogar los actuales mandatos en los sindicatos y Ø<sub>i</sub> suspend<b>ió</b> los preocesos electorales hasta 2021</i> express the same-subject relation in the same way as in connected discourse -- namely, with zero anaphora and subject person indexation on the verb. (Section 16.2)" ;
    dcterms:isPartOf "16.2" ;
    compcon:subtypeOf compcon:str_reference-tracking-system .

compcon:str_domain a compcon:str ;
    rdfs:label "domain" ;
    cx:hasExample compcon:domain-example-0 ;
    dc:description "in a <a>construction</a> using the <a>indexation strategy</a>, the construction as a whole that the <a>controller</a>, <a>target</a>, and <a>index</a> occur in. <i>Example</i>: in Spanish <i>los libros rojos</i> <q>the red books</q>, the domain is the <a>modification construction</a> including the <a>modifier</a> <i>rojos</i> <q>red</q> (the target), the modifier suffix <i>-os</i> <q>[Masculine Plural]</q>, and the <a>head</a> <a>noun</a> <i>libros</i> <q>books</q> (the controller). (Section 4.4)" ;
    dcterms:isPartOf "4.4" .

compcon:str_double-coding-strategy a compcon:str ;
    rdfs:label "double-coding strategy" ;
    cx:hasExample compcon:double-coding-strategy-example-0 ;
    dc:description "a <a>strategy</a> used for <a>experiential constructions</a> in which both <a>experiencer</a> and <a>stimulus</a> are expressed with the same <a>core argument phrase</a> (either <a>subject</a> or <a inf:object-argument>object</a>). <i>Example</i>: in the Japanese sentence <i>Dare ga eiga ga suki desu ka</i> <q>Who likes movies?</q>, both the experiencer <i>dare</i> <q>who</q> and the stimulus <i>eiga</i> <q>movie</q> are expressed with the Subject <a>flag</a> <i>ga</i>. (Section 7.4)" ;
    dcterms:isPartOf "7.4" ;
    compcon:subtypeOf compcon:str_experiential-alignment .

compcon:str_double-expression a compcon:str ;
    rdfs:label "double expression" ;
    cx:hasExample compcon:double-expression-example-0 ;
    dc:description "a <a>strategy</a> for the expression of <a>motion events</a> in which the <a>path of motion</a> is expressed as (at least) part of the <a>predicate</a> and also as a <a>satellite</a>. <i>Example</i>: in Russian <i>Ja vy-bežal iz doma</i> <q>I ran out of the house</q>, the path of going out of the house is expressed both as part of the <a>verb</a> (the prefix <i>vy-</i> in <i>vy-bežal</i>) and as the <a>flag</a> <i>iz</i>. (Section 14.5)" ;
    dcterms:isPartOf "14.5" ;
    skos:altLabel "double framing",
        "double marking" ;
    compcon:expressionOf compcon:cxn_motion-construction ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_double-negation-strategy a compcon:str ;
    rdfs:label "double negation strategy" ;
    cx:hasExample compcon:double-negation-strategy-example-0 ;
    dc:description "a <a>strategy</a> found in <a>negation constructions</a>, particularly <a>declarative negation constructions</a>, in which there are two morphemes that express <a>negative polarity</a>. <i>Example</i>: in the French sentence <i>Je <b>ne</b> vois <b>pas</b> la lune</i> <q>I do not see the moon</q>, negative polarity is expressed by both <i>ne</i> and <i>pas</i>. (Section 12.2)" ;
    dcterms:isPartOf "12.2" ;
    compcon:expressionOf compcon:cxn_declarative-negation-construction ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_double-oblique-strategy a compcon:str ;
    rdfs:label "double-oblique strategy" ;
    cx:hasExample compcon:double-oblique-strategy-example-0 ;
    dc:description "a <a>strategy</a> used for <a>experiential constructions</a> in which both <a>experiencer</a> and <a>stimulus</a> are expressed in an <a>oblique argument phrase</a> (the two participant roles are usually expressed with different oblique <a>flags</a>). <i>Example</i>: in Ancient Greek <i>mélei moi toúto:n hô:n ero:táis</i> <q>I care about what you ask</q>, the experiencer argument phrase <i>moi</i> <q>I</q> uses the oblique (Dative) flag, and the stimulus argument phrase <i>toúto:n hô:n ero:táis</i> <q>what you ask</q> uses the oblique (Genitive) flag. (Section 7.4)" ;
    dcterms:isPartOf "7.4" ;
    compcon:subtypeOf compcon:str_experiential-alignment .

compcon:str_doubling a compcon:str ;
    rdfs:label "doubling" ;
    dc:description "expression of a <a>referent</a> twice in a <a>clause</a>, by a <a>referring phrase</a> and simultaneously by an <a>index</a>. (Section 3.3.3)" ;
    dcterms:isPartOf "3.3.3" ;
    compcon:expressionOf compcon:cxn_clause ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_echo-strategy a compcon:str ;
    rdfs:label "echo strategy" ;
    cx:hasExample compcon:echo-strategy-example-0,
        compcon:echo-strategy-example-1,
        compcon:echo-strategy-example-2 ;
    dc:description "a <a>strategy</a> for the <a>polarity response construction</a> in which the answer to a <a cxn:polarity-question-construction>polarity question</a> repeats part of the question. <i>Example</i>: in Welsh, the answers to the polarity question <i>A welwch chwi hwy?</i> [<sc>q</sc> see you them] <q>Do you see them?</q> are either <i>Gwelaf</i> <q>I see (them)</q> or <i>Na gwelaf</i> <q>I don't see (them)</q>. (Section 12.3.3)" ;
    dcterms:isPartOf "12.3.3" ;
    compcon:expressionOf compcon:cxn_polarity-response-construction ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_ellipsis a compcon:str ;
    rdfs:label "ellipsis" ;
    cx:hasExample compcon:ellipsis-example-0,
        compcon:ellipsis-example-1,
        compcon:ellipsis-example-2,
        compcon:ellipsis-example-3 ;
    dc:description "a <a>strategy</a> for <a>identificational constructions</a> in which the <a>presupposed open proposition</a> is deleted, or sometimes expressed only by a <q>pro-verb</q>. <i>Example</i>: in English, elliptical responses to the question <i>Who took the cookies?</i> are <i>It was JIM, JIM</i>, or <i>JIM did</i> (with the pro-verb form <i>did</i>), in contrast to the full <a str:prosody>prosodic strategy</a> <i>JIM took the cookies</i> or the <a inf:equational>equational</a> <a>strategy</a> <i>It was JIM who took the cookies</i>. (Section 11.4.2)" ;
    dcterms:isPartOf "11.4.2" ;
    compcon:expressionOf compcon:cxn_identificational-construction,
        compcon:cxn_information-question-response-construction,
        compcon:cxn_thetic-construction ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_equal-equative a compcon:str ;
    rdfs:label "equal equative" ;
    cx:hasExample compcon:equal-equative-example-0 ;
    dc:description "a <a>fixed-case strategy</a> in <a>equative constructions</a> in which there are two <a>predicates</a>, one which attributes a <a>gradable predicative scale</a> to the <a>comparee</a>, and another which asserts that the comparee is identical to the <a>standard</a> on that scale. <i>Example</i>: Nkore-Kiga <i>noingana Mugasho oburaingwa</i> <q>You are as tall as Mugasho</q> [lit. <q>you-are-equal-to Mugasho in-height</q>] is an instance of the equal equative strategy. (Section 17.2.4)" ;
    dcterms:isPartOf "17.2.4" ;
    compcon:expressionOf compcon:cxn_equative-construction ;
    compcon:subtypeOf compcon:str_fixed-case,
        compcon:str_relative-strategy .

compcon:str_equipollent a compcon:str ;
    rdfs:label "equipollent" ;
    cx:hasExample compcon:equipollent-example-0,
        compcon:equipollent-example-1,
        compcon:equipollent-example-2,
        compcon:equipollent-example-3 ;
    dc:description "a <a>strategy</a> in which a <a>noncausal event</a> and its counterpart <a>causal event</a> are expressed by <a>forms</a> of equal morphological structure. <i>Example</i>: in German, the causal event <a>predicate</a> <i>aufwechen</i> <q>wake (someone) up</q> and the noncausal event predicate <i>aufwachen</i> <q>wake up</q> are expressed by morphologically related forms, but neither form is morphologically more complex than the other. In English, the suppletive causal event predicate <i>kill</i> vs. noncausal event predicate <i>die</i> are also analyzed as instances of the equipollent strategy. The term <q>equipollent</q> is used for the same strategy in other constructions as well. (Section 6.3.4)" ;
    dcterms:isPartOf "6.3.4" ;
    compcon:subtypeOf compcon:str_system .

compcon:str_ergative-alignment a compcon:str ;
    rdfs:label "ergative alignment" ;
    cx:hasExample compcon:ergative-alignment-example-0 ;
    dc:description "a <a>system</a> in which the <a>S</a> and <a>P roles</a> are expressed with the same <a>form</a>, but the <a>A role</a> is expressed with a different form. <i>Example</i>: in Yuwaalaraay, <a>argument phrases</a> expressing the A role use the Ergative <a>flag</a> <i>-gu</i>, but argument phrases expressing both the S and P roles use the zero-coded Absolutive flag. (Section 6.3.1)" ;
    dcterms:isPartOf "6.3.1" ;
    skos:altLabel "ergative alignment (system)" ;
    compcon:subtypeOf compcon:str_transitive-alignment .

compcon:str_ergative-category a compcon:str ;
    rdfs:label "ergative category" ;
    cx:hasExample compcon:ergative-category-example-0 ;
    dc:description "the <a>morphosyntactic</a> category in the <a>ergative alignment system</a> that exclusively expresses the <a>A role</a>. <i>Example</i>: The Yuwaalaraay <a>flag</a> <i>-gu</i> exclusively expresses the A role, and hence is an ergative flag. (Section 6.3.1)" ;
    dcterms:isPartOf "6.3.1" .

compcon:str_exceed-comparative a compcon:str ;
    rdfs:label "exceed comparative" ;
    cx:hasExample compcon:exceed-comparative-example-0 ;
    dc:description "a <a>fixed-case strategy</a> in <a>comparative constructions</a> in which there are two <a>predicates</a>, one which attributes a <a>gradable predicative scale</a> to the <a>comparee</a>, and another which asserts that the comparee exceeds the <a>standard</a> on that scale. <i>Example</i>: Swahili <i>mti huu ni mrefu ku-shinda ule</i> <q>This tree is taller than that (tree)</q> is an instance of the exceed comparative: the first part <i>mti huu ni mrefu</i> <q>this tree is big</q> attributes a degree of height to this tree, and the second part <i>ku-shinda ule</i> <q>exceed [<sc>inf</sc>] that (tree)</q> asserts that this tree exceeds that tree in height. (Section 17.2.2)" ;
    dcterms:isPartOf "17.2.2" ;
    compcon:expressionOf compcon:cxn_comparative-construction ;
    compcon:subtypeOf compcon:str_fixed-case,
        compcon:str_relative-strategy .

compcon:str_existential-negative-strategy a compcon:str ;
    rdfs:label "existential negative strategy" ;
    dc:description "" ;
    compcon:expressionOf compcon:cxn_declarative-negation-construction ;
    compcon:subtypeOf compcon:str_recruitment-strategy .

compcon:str_experiencer-oriented-strategy a compcon:str ;
    rdfs:label "experiencer-oriented strategy" ;
    cx:hasExample compcon:experiencer-oriented-strategy-example-0 ;
    dc:description "a <a>strategy</a> for an <a>experiential construction</a> in which the <a>experiencer</a> <a>argument phrase</a> is <a>co-expressed</a> with the <a>subject</a> argument phrase of a <a>transitive</a> or <a>intransitive construction</a>. <i>Example</i>: the <a>argument structure construction</a> found in <i>I fear dogs</i>, with the experiencer argument phrase <i>I</i> co-expressed with the prototypical subject in English, is an instance of the experiencer-oriented strategy. (Section 7.4)" ;
    dcterms:isPartOf "7.4" ;
    skos:altLabel "experiencer-subject strategy" ;
    compcon:subtypeOf compcon:str_experiential-alignment .

compcon:str_external-possessor-strategy a compcon:str ;
    rdfs:label "external possessor strategy" ;
    cx:hasExample compcon:external-possessor-strategy-example-0 ;
    dc:description "a <a>strategy</a> in which a <a>referent</a> in a <a>possessive relation</a> to a <a>participant</a> in an <a>event</a> (normally in the <a>P role</a> or sometimes the <a>S role</a> of the event) is expressed as an <a inf:reference>argument</a> of the <a>predicate</a> instead of in a <a>possessive modification construction</a>. <i>Example</i>: in Maasai <i>áa-bʊak-ıtá ɔldía</i> <q>My dog is barking</q>, the portmanteau index <i>áa-</i> <a>indexes</a> the owner of the dog as the <a inf:mid-topicality>object</> <a inf:reference>argument</a> of the predicate. (Section 7.5.3)" ;
    dcterms:isPartOf "7.5.3" ;
    compcon:expressionOf compcon:cxn_possessed-argument-construction ;
    compcon:subtypeOf compcon:str_recruitment-strategy .

compcon:str_extraposed-strategy a compcon:str ;
    rdfs:label "extraposed strategy" ;
    cx:hasExample compcon:extraposed-strategy-example-0 ;
    dc:description "a <a>word order strategy</a> for <a>externally headed</a> <a>relative clause constructions</a> in which the <a>relative clause</a>, rather than being adjacent to the external <a>relative clause head</a> (either <a>prenominal</a> or <a>postnominal</a>), instead follows the entire <a>matrix clause</a>. <i>Example</i>: <i>I found a linguistics book in a used bookstore last week</i> [<i>that I have been looking for for years</i>] is an instance of the extraposed strategy: the relative clause <i>that I have been looking for for years</i> does not immediately follow the external relative clause head <i>a linguistics book</i>, but instead follows the entire matrix clause <i>I found a linguistics book in a used bookstore last week</i>. (Section 19.2.4)" ;
    dcterms:isPartOf "19.2.4" ;
    skos:altLabel "extraposed (strategy)" ;
    compcon:subtypeOf compcon:str_externally-headed,
        compcon:str_word-order .

compcon:str_figure-incorporating a compcon:str ;
    rdfs:label "figure-incorporating" ;
    cx:hasExample compcon:figure-incorporating-example-0 ;
    dc:description "a <a>strategy</a> for the expression of <a>motion events</a> in which the <a>predication</a> incorporates the semantic type of the <a>figure</a> of motion. <i>Example</i>: in Atsugewi <i>w'ost'aq'ík: a</i> <q>Runny icky material [e.g. guts] is lying on the ground</q>, the verb <i>st'aq'</i> <q>lie [of runny, icky material]</q> incorporates the semantic type of the figure (the guts) as runny, icky material. (Section 14.5)" ;
    dcterms:isPartOf "14.5" ;
    compcon:expressionOf compcon:cxn_motion-construction ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_first-person-plural-strategy a compcon:str ;
    rdfs:label "first person plural strategy" ;
    dc:description "" ;
    compcon:expressionOf compcon:cxn_generic-pronoun ;
    compcon:subtypeOf compcon:str_recruitment-strategy .

compcon:str_focus-marker a compcon:str ;
    rdfs:label "focus marker" ;
    cx:hasExample compcon:focus-marker-example-0 ;
    dc:description "expression by a separate morpheme of the information in an <a>identificational construction</a> that is the <a>focus</a>. <i>Example</i>: in Rendille <i>ínam-é y-imi</i> <q>The BOY came</q>, the suffix <i>-é</i> marks the Noun <i>ínam-</i> <q>(the) boy</q> as the focus. (Section 11.4.2)" ;
    dcterms:isPartOf "11.4.2" ;
    compcon:expressionOf compcon:cxn_identificational-construction ;
    compcon:subtypeOf compcon:str_overt-coding .

compcon:str_gap-strategy a compcon:str ;
    rdfs:label "gap strategy" ;
    cx:hasExample compcon:gap-strategy-example-0 ;
    dc:description "a <a>strategy</a> for the expression of the <a>necessarily shared participant</a> in the <a>relative clause</a> of an <a>externally headed</a> <a>relative clause construction</a>, in which the participant is not expressed at all in the relative clause. <i>Example</i>: <i>I ate the cheesecake</i> [<i>that Carol baked</i>] uses the gap strategy because the shared participant, the cheesecake, is expressed only as the external <a>relative clause head</a>, and not at all in the relative clause <i>that Carol baked</i>. (Section 19.2.2)" ;
    dcterms:isPartOf "19.2.2" ;
    skos:altLabel "gap (strategy)" ;
    compcon:subtypeOf compcon:str_externally-headed .

compcon:str_general-extender a compcon:str ;
    rdfs:label "general extender" ;
    cx:hasExample compcon:general-extender-example-0 ;
    dc:description "a form used as part of a <a>non-exhaustive coordination construction</a> that indicates the non-exhaustiveness of the list. <i>Example</i>: in the non-exhaustive coordination construction in <i>She sold baskets, pots, and stuff / and everything</i>, <i>and stuff</i> or <i>and everything</i> are general extenders. (Section 15.2.2)" ;
    dcterms:isPartOf "15.2.2" ;
    compcon:expressionOf compcon:cxn_non-exhaustive-list-coordination ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_have-possessive-strategy a compcon:str ;
    rdfs:label "have-possessive strategy" ;
    cx:hasExample compcon:have-possessive-strategy-example-0 ;
    dc:description "a <a>strategy</a> for the <a>presentational possession construction</a> in which the <a>possessor</a> is expressed in a <a>subject phrase</a>, and the <a>possessum</a> in an <a>object phrase</a>. <i>Example</i>: English <i>Kerry has a laptop</i> is an instance of the have-possessive strategy. (Section 10.4.2)" ;
    dcterms:isPartOf "10.4.2" ;
    skos:altLabel "action strategy" ;
    compcon:expressionOf compcon:cxn_presentational-possession-construction ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_headless a compcon:str ;
    rdfs:label "headless" ;
    cx:hasExample compcon:headless-example-0 ;
    dc:description "a <a>strategy</a> for the <a>anaphoric-head construction</a> in which there is no <a str:overt-coding>overt</a> morpheme that functions as the <a>head</a>. <i>Example</i>: in <i>My bicycle is older than Greg's</i>, the anaphoric-head construction <i>Greg's</i> has no overt morpheme functioning as the head. (Section 5.4)" ;
    dcterms:isPartOf "5.4" ;
    skos:altLabel "null anaphoric head" ;
    compcon:expressionOf compcon:cxn_anaphoric-head-construction,
        compcon:cxn_free-modification-construction ;
    compcon:subtypeOf compcon:str_zero-coding .

compcon:str_human-noun-strategy a compcon:str ;
    rdfs:label "human noun strategy" ;
    dc:description "" ;
    compcon:expressionOf compcon:cxn_generic-pronoun ;
    compcon:subtypeOf compcon:str_recruitment-strategy .

compcon:str_hybrid-information-packaging-strategy a compcon:str ;
    rdfs:label "hybrid information packaging strategy" ;
    cx:hasExample compcon:hybrid-information-packaging-strategy-example-0 ;
    dc:description "a <a>strategy</a> found with <a>nonprototypical construction</a> types, such as <a>complement constructions</a> which express <a>reference</a> to <a>actions</a>. The hybrid IP strategy uses a mixture of the <a>semantic IP strategy</a> and the <a>actual IP strategy</a>. <i>Example</i>: in <i>Her drinking coffee (surprises me)</i>, the English Gerund Construction combines the Possessive Pronoun <i>her</i>, characteristic of the prototypical <a sem:object-concept>object</a> <a>reference</a> construction and hence an instance of the actual information packaging of her drinking coffee (reference), with the Object form <i>coffee</i>, characteristic of the prototypical <a>action</a> <a>predication</a> construction and hence an instance of the prototypical information packaging function of the semantics of the action being referred to, namely predication. (Section 2.4)" ;
    dcterms:isPartOf "2.4" ;
    skos:altLabel "hybrid IP strategy" ;
    compcon:expressionOf compcon:cxn_nonprototypical-construction ;
    compcon:subtypeOf compcon:str_information-packaging-alignment .

compcon:str_identificational-strategy a compcon:str ;
    rdfs:label "identificational strategy" ;
    dc:description "" ;
    compcon:expressionOf compcon:cxn_declarative-negation-construction,
        compcon:cxn_interrogative-construction ;
    compcon:subtypeOf compcon:str_recruitment-strategy .

compcon:str_inactive-category a compcon:str ;
    rdfs:label "inactive category" ;
    cx:hasExample compcon:inactive-category-example-0,
        compcon:inactive-category-example-1 ;
    dc:description "the category in the <a>active alignment system</a> that <a>co-expresses</a> some <a>S roles</a> -- in particular, the S role of <q>die</q> -- and the <a>P role</a>. <i>Example</i>: in Lakhota, the index <i>ma-</i> <q>I/me</q> in <i>ó-ma-ya-kiye</i> <q>you help / helped me</q> and <i>ma-khuže</i> <q>I am sick</q> expresses the inactive category. (Section 6.3.3)" ;
    dcterms:isPartOf "6.3.3" ;
    skos:altLabel "patientive category",
        "stative category" .

compcon:str_inclusory-strategy a compcon:str ;
    rdfs:label "inclusory strategy" ;
    dc:description "" ;
    compcon:expressionOf compcon:cxn_inclusory-construction ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_incorporation-of-serial-verbs a compcon:str ;
    rdfs:label "incorporation of serial verbs" ;
    cx:hasExample compcon:incorporation-of-serial-verbs-example-0 ;
    dc:description "alternative <a>strategies</a> found with <a>serial verb</a> and <a>auxiliary constructions</a>, whether they form a single word or not. <i>Example</i>: in Alamblak <i>yĕnt mi-ak-tita-r-t</i> <q>He carried the girl down there on his shoulders</q>, the verbs <i>ak</i> <q>get</q> and <i>tita</i> <q>carry on shoulders</q> form a single word. <a>Noun incorporation</a> refers to a strategy found in other constructions. (Sections 13.2, 13.3.2)" ;
    dcterms:isPartOf "13.2",
        "13.3.2" ;
    skos:altLabel "morphological boundness of serial verbs" ;
    compcon:expressionOf compcon:cxn_auxiliary-construction,
        compcon:cxn_eventive-complex-predicate ;
    compcon:subtypeOf compcon:str_serial-verb-strategy .

compcon:str_independent-referring-phrase-strategy a compcon:str ;
    rdfs:label "independent referring phrase strategy" ;
    cx:hasExample compcon:independent-referring-phrase-strategy-example-0 ;
    dc:description "a <a>strategy</a> for the <a>presentational construction</a> in which the <a>referring phrase</a> introducing the <a>referent</a> in the discourse is the only <a>element</a> in the construction. <i>Example</i>: in spoken English, the independent referring strategy is used, for example, in the Pear Stories narrative example <i>and then a little boy, /about/ a bic a red bicycle, that was too big for him, he stopped, ...</i>, <i>a little boy</i> is an independent referring phrase that is used to present a new referent in the discourse. (Section 10.4.3)" ;
    dcterms:isPartOf "10.4.3" ;
    compcon:expressionOf compcon:cxn_presentational-construction ;
    compcon:subtypeOf compcon:str_recruitment-strategy .

compcon:str_index a compcon:str ;
    rdfs:label "index" ;
    cx:hasExample compcon:index-example-0 ;
    dc:description "the morpheme in an <a>indexical strategy</a> which <a inf:reference>refers</a> to (indexes) the referent. <i>Example</i>: in Spanish <i>las chicas cantaban</i> <q>the girls were singing</q>, the 3rd person plural suffix <i>-an</i> on the imperfective form of <q>sing</q> is the index, referring to the same referent as <i>las chicas</i> <q>the girls</q>. (Section 4.4)" ;
    dcterms:isPartOf "4.4" .

compcon:str_indexation-feature a compcon:str ;
    rdfs:label "indexation feature" ;
    cx:hasExample compcon:indexation-feature-example-0 ;
    dc:description "in a <a>construction</a> using the <a>indexation strategy</a>, the categories of the <a>referent</a> that the <a>index</a> expresses. Typical categories are <a>person</a>, <a>gender--class</a>, and <a>number</a>. <i>Example</i>: in Spanish <i>los libros roj-os</i> <q>the red books</q>, the suffix <i>-os</i> on the modifier <i>rojos</i> <q>red</q> indexes the plural number and masculine gender of <i>libros</i> <q>books</q>. (Section 4.4)" ;
    dcterms:isPartOf "4.4" ;
    skos:altLabel "agreement feature" .

compcon:str_indexed a compcon:str ;
    rdfs:label "indexed" ;
    dc:description "a <a>strategy</a> in which the <a>stative</a> <a>predicate</a> in a <a>stative complex predicate construction</a> <a>indexes</a> an <a inf:reference>argument</a> of the other (<a>dynamic</a>) predicate in the construction. (Section 14.2)" ;
    dcterms:isPartOf "14.2" ;
    compcon:subtypeOf compcon:str_overt-coding .

compcon:str_indirect-object-category a compcon:str ;
    rdfs:label "indirect object category" ;
    cx:hasExample compcon:indirect-object-category-example-0 ;
    dc:description "the <a>morphosyntactic</a> category in the <a>indirective alignment system</a> that exclusively expresses the <a>R role</a>. <i>Example</i>: in <i>Randy gave the car to his daughter</i>, the <a>flag</a> <i>to</i> exclusively expresses the R role. (Section 7.5.2)" ;
    dcterms:isPartOf "7.5.2" .

compcon:str_indirect-report a compcon:str ;
    rdfs:label "indirect report" ;
    cx:hasExample compcon:indirect-report-example-0 ;
    dc:description "a strategy for the <a>complement</a> of an <a>utterance event</a> in which only the content of an utterance is expressed. <i>Example</i>: in <i>Sandy said that she was buying the house</i>, only the content of Sandy's utterance is reported (contrast with the direct report strategy in <i>Sandy said, <q>I'm buying the house</q></i>). (Section 18.2.2)" ;
    dcterms:isPartOf "18.2.2" ;
    compcon:expressionOf compcon:cxn_utterance-construction ;
    compcon:subtypeOf compcon:str_utterance-clause-alignment .

compcon:str_indirective-alignment a compcon:str ;
    rdfs:label "indirective alignment" ;
    cx:hasExample compcon:indirective-alignment-example-0,
        compcon:indirective-alignment-example-1 ;
    dc:description "a <a>system</a> in which the <a>P</a> and <a>T roles</a> are expressed with the same <a>form</a>, but the <a>R role</a> is expressed with a different form. <i>Example</i>: in <i>Randy gave the car to his daughter</i>, the T role (<i>the car</i>) is expressed in the same way as the P role in <i>Randy started the car</i>, and the R role is expressed distinctly, with the <a>flag</a> <i>to</i>. (Section 7.5.2)" ;
    dcterms:isPartOf "7.5.2" ;
    skos:altLabel "indirective alignment (system)" ;
    compcon:subtypeOf compcon:str_ditransitive-alignment .

compcon:str_insubordination a compcon:str ;
    rdfs:label "insubordination" ;
    cx:hasExample compcon:insubordination-example-0 ;
    dc:description "the <a>recruitment</a> of <a>deranked</a> <a>clause constructions</a> to express a <b>nondeclarative main clause function</b>. <i>Example</i>: <i>No smoking</i> recruits the English Gerund Verb form ending in <i>-ing</i> in order to express an <a cxn:imperative-hortative-construction>imperative</a>. (Section 15.2.3)" ;
    dcterms:isPartOf "15.2.3" ;
    compcon:expressionOf compcon:cxn_imperative-hortative-construction ;
    compcon:subtypeOf compcon:str_recruitment-strategy .

compcon:str_internal-recipient-strategy a compcon:str ;
    rdfs:label "internal recipient strategy" ;
    cx:hasExample compcon:internal-recipient-strategy-example-0 ;
    dc:description "a <a>strategy</a> for the <a>ditransitive construction</a> in which the <a>T role</a> and <a>R role</a> are co-expressed with the <a>possessive modification construction</a>, so that the <a>noun</a> denoting the <a>participant</a> in the T role is the <a>head</a> and the noun denoting the participant in the R role is the possessive <a>modifier</a>. <i>Example</i>: in Mokilese <i>ngoah insigeh-di kijinlikkoan-oaw nih-mw</i> <q>I wrote a letter to/for you</q>, <i>nih-mw</i> [<sc>clf-2sg.poss</sc>] <q>your</q> is a possessive modifier of <i>kijinlikkoan-oaw</i> <q>a letter</q>. The internal recipient strategy is an instance of the <a>internal possessor strategy</a>. (Section 7.5.3)" ;
    dcterms:isPartOf "7.5.3" ;
    compcon:expressionOf compcon:cxn_ditransitive-construction ;
    compcon:subtypeOf compcon:str_internal-possessor-strategy .

compcon:str_internally-headed-strategy a compcon:str ;
    rdfs:label "internally headed strategy" ;
    cx:hasExample compcon:internally-headed-strategy-example-0 ;
    dc:description "a <a>strategy</a> for the expression of the <a>necessarily shared participant</a> in the <a>relative clause construction</a>, in which the <a>relative clause head</a> denoting the shared participant is expressed only inside the <a>relative clause</a>. <i>Example</i>: in Imbabura Quechua [<i>kan kwitsaman kwintuta villashka</i>]<i>-ka ali kwitsami</i> <q>The girl to whom you told the story is a good girl</q>, the relative clause <i>kan kwitsaman kwintuta villashka</i> <q>you told the story to the girl</q> contains the relative clause head <i>kwitsaman</i> <q>(to) the girl</q>, and the girl is not expressed in the <a>matrix clause</a> ... <i>ali kwitsami</i> <q>... is a good girl</q>. The internally headed relative clause as a whole functions as an <a inf:reference>argument</a> of the matrix clause <a>predicate</a> and may be recruited from the <a>complement clause construction</a>. The internally headed strategy is rare but fairly widely dispersed among the languages of the world. (Section 19.2.3)" ;
    dcterms:isPartOf "19.2.3" ;
    skos:altLabel "internally headed (strategy)" ;
    compcon:subtypeOf compcon:str_non-externally-headed .

compcon:str_interrogative-complementizer a compcon:str ;
    rdfs:label "interrogative complementizer" ;
    dc:description "" ;
    compcon:expressionOf compcon:cxn_interrogative-complement ;
    compcon:subtypeOf compcon:str_complementizer .

compcon:str_interrogative-pronoun-strategy a compcon:str ;
    rdfs:label "interrogative pronoun strategy" ;
    dc:description "" ;
    compcon:expressionOf compcon:cxn_pragmatically-specific-pronoun ;
    compcon:subtypeOf compcon:str_recruitment-strategy .

compcon:str_juxtaposition a compcon:str ;
    rdfs:label "juxtaposition" ;
    cx:hasExample compcon:juxtaposition-example-0 ;
    dc:description "a strategy for encoding the relation in <a>major propositional acts</a> (<a>modifier</a>--<a>referent</a>, <a>predicate</a>--<a inf:reference>argument</a>), in which the two <a>elements</a> are simply syntactically juxtaposed without any additional morpheme expressing the propositional act relation. <i>Example</i>: in <i>red ball</i>, the adjective <i>red</i> is simply juxtaposed to the noun <i>ball</i> to indicate that the referent of <i>ball</i> is modified by the property denoted by <i>red</i>. (Section 4.2)" ;
    dcterms:isPartOf "4.2" ;
    compcon:expressionOf compcon:cxn_modification-construction ;
    compcon:subtypeOf compcon:str_simple-strategy .

compcon:str_labile a compcon:str ;
    rdfs:label "labile" ;
    cx:hasExample compcon:labile-example-0,
        compcon:labile-example-1 ;
    dc:description "a <a>strategy</a> in which the <a>verb</a> expressing a <a>noncausal event</a> and the verb expressing its counterpart <a>causal event</a> are identical. <i>Example</i>: in English, the same verb <i>break</i> is used for the causal event <i>I broke the vase</i> and the noncausal event <i>The vase broke</i>. (Section 6.3.4)" ;
    dcterms:isPartOf "6.3.4" ;
    skos:altLabel "ambitransitive",
        "lexical causative" ;
    compcon:expressionOf compcon:cxn_causative-construction,
        compcon:cxn_passive-inverse-voice .

compcon:str_linker a compcon:str ;
    rdfs:label "linker" ;
    cx:hasExample compcon:linker-example-0 ;
    dc:description "a more highly <a>grammaticalized</a> strategy for encoding the relation in <a>major propositional acts</a> (<a>modifier</a>--<a>referent</a>, <a>predicate</a>--<a inf:reference>argument</a>), in which there is an invariant third morpheme that signals the relationship between two <a>elements</a>. <i>Example</i>: in English <i>Jerry's bicycle</i>, the invariant genitive form <i>-'s</i> is a linker. (Section 4.5)" ;
    dcterms:isPartOf "4.5" ;
    compcon:expressionOf compcon:cxn_modification-construction ;
    compcon:subtypeOf compcon:str_overt-coding .

compcon:str_locational-possessive-strategy a compcon:str ;
    rdfs:label "locational possessive strategy" ;
    cx:hasExample compcon:locational-possessive-strategy-example-0 ;
    dc:description "a strategy for the <a>presentational possession construction</a> in which the <a>possessum</a> is expressed in a <a>subject phrase</a> and the <a>possessor</a> in an <a>oblique phrase</a> which is locative, or probably locative, in origin. <i>Example</i>: Russian <i>u menja mašina</i> [at me car] <q>I have a car</q> is an instance of the locational possessive strategy. The locational possessive strategy essentially <a>recruits</a> a <a>locative clause</a>, in particular a <a>presentational locative</a>, to express possession. (Section 10.4.2)" ;
    dcterms:isPartOf "10.4.2" ;
    skos:altLabel "goal strategy",
        "location strategy",
        "locational possessive (strategy)" ;
    compcon:expressionOf compcon:cxn_presentational-possession-construction ;
    compcon:subtypeOf compcon:str_recruitment-strategy .

compcon:str_locational-strategy a compcon:str ;
    rdfs:label "locational strategy" ;
    cx:hasExample compcon:locational-strategy-example-0 ;
    dc:description "the <a>strategy</a> of <a>recruiting</a> what was originally a <a>locative predication construction</a> for other <a>predication</a> <a>constructions</a>, both <a>prototypical</a> and <a>nonprototypical predication constructions</a>. An originally locative predication construction employs a <a cxn:locative-phrase>location predicate</a>, typically a <a cxn:body-position-predicate>body position verb</a>. <i>Example</i>: Amele <i>uqa me bil-i-a</i> [he good sit-<sc>3sg-prs</sc>] <q>He is good</q> recruits the locative predication construction with the posture verb <q>sit</q> for property predication. (Section 10.2)" ;
    dcterms:isPartOf "10.2" ;
    skos:altLabel "locational (strategy)" ;
    compcon:expressionOf compcon:cxn_nonprototypical-predication-construction ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_locative-comparative a compcon:str ;
    rdfs:label "locative comparative" ;
    cx:hasExample compcon:locative-comparative-example-0 ;
    dc:description "a <a>fixed-case strategy</a> in <a>comparative constructions</a> in which there is a <a>clause</a> which attributes a <a>gradable predicative scale</a> to the <a>comparee</a>, and the <a>standard</a> is expressed as an oblique <a>argument phrase</a> using a spatial flag with a locative (<q>on, at</q>) <a>meaning</a>. <i>Example</i>: Ubykh <i>yi-gune wo-gune-n ca-qasaqa-j</i> <q>This tree is taller than that tree</q> is an instance of the locative comparative: <i>yi-gune ca-qasaqa-j</i> asserts that this tree is bigger, and <i>wo-gune-n</i> expresses the standard, that tree, with a suffix <i>-n</i> meaning <q>on</q>. (Section 17.2.2)" ;
    dcterms:isPartOf "17.2.2" ;
    compcon:expressionOf compcon:cxn_comparative-construction ;
    compcon:subtypeOf compcon:str_fixed-case,
        compcon:str_ordered-strategy .

compcon:str_locus-of-inflection-of-serial-verbs a compcon:str ;
    rdfs:label "locus of inflection of serial verbs" ;
    dc:description "alternative <a>strategies</a> found with <a>serial verb</a> and <a>auxiliary constructions</a>, based on where the verbal inflections are located. There are several possibilities, including: inflection on the first <a>verb</a> (or inflection on the <a>auxiliary</a>); inflection on the last verb (or inflection on the main verb in an auxiliary construction); same inflection on both/all verbs/auxiliaries; inflection split across the two verbs (or verb and auxiliary); or separate inflection (the last relevant to <u>basic</u> <a>eventive complex predicates</a> only). (Sections 13.2, 13.3.2)" ;
    dcterms:isPartOf "13.2",
        "13.3.2" ;
    compcon:expressionOf compcon:cxn_auxiliary-construction,
        compcon:cxn_eventive-complex-predicate ;
    compcon:subtypeOf compcon:str_serial-verb-strategy .

compcon:str_logophoric-system a compcon:str ;
    rdfs:label "logophoric system" ;
    cx:hasExample compcon:logophoric-system-example-0,
        compcon:logophoric-system-example-1 ;
    dc:description "a <a>system</a> found with certain <a>complement clause constructions</a> where one <a>strategy</a> is used when a <a>participant</a> in the <a>complement</a> <a>event</a> is <a>coreferential</a> with the <u>speaker</u>, <u>addressee</u>, or <a>experiencer</a> of an <a>utterance</a>, <a>propositional attitude</a>, <a>knowledge</a>, or <a>commentative event</a> (the <a>logophoric construction</a>), and a different strategy is used when there is no such coreference relation. <i>Example</i>: in Donno Sɔ <i>Oumar Anta inyemɛñ waa be gi</i> <q>Oumar<sub>i</sub> said that Anta had seen him<sub>i</sub></q>, the reference to Oumar in the complement clause uses a special <a>pronoun</a> form <i>inyemɛñ</i>, but in <i>Oumar Anta woñ waa be gi</i> <q>Oumar<sub>i</sub> said that Anta had seen him<sub>k</sub></q>, the referent in the complement is not Oumar, and so the ordinary <a>third person</a> <a>anaphoric pronoun</a> <i>woñ</i> is used. (Section 18.4.2)" ;
    dcterms:isPartOf "18.4.2" ;
    compcon:expressionOf compcon:cxn_logophoric-construction ;
    compcon:subtypeOf compcon:str_system .

compcon:str_long-distance-reflexive a compcon:str ;
    rdfs:label "long-distance reflexive" ;
    cx:hasExample compcon:long-distance-reflexive-example-0 ;
    dc:description "a <a>strategy</a> in which a <a>reflexive</a> <a>pronoun</a> is used where the <a>referring phrase</a> denoting the referent with which the reflexive pronoun is <a>coreferential</a> does not occur in the same <a>clause</a> (more or less; the precise definition of <q>local</q> uses of the reflexive pronoun varies). In particular, in the context of this textbook, a reflexive pronoun is used in a <a>logophoric construction</a>. <i>Example</i>: in Japanese <i>Takasi wa Taroo ni Yosiko ga zibun o nikundeiru koto o hanasita</i> <q>Takasi<sub>i</sub> told Taroo that Yosiko hated him<sub>i</sub></q>, the reflexive pronoun <i>zibun</i> in the <a>utterance</a> <a>complement</a> <i>Yosiko ga zibun o nikundeiru koto o</i> is coreferential with the <u>speaker participant</u> <i>Takasi</i> in the <a>matrix clause</a>. It appears that use of a long-distance reflexive in a logophoric construction is part of a larger range of uses of long-distance reflexives, and may not represent a true <a>logophoric system</a>. (Section 18.4.2)" ;
    dcterms:isPartOf "18.4.2" ;
    compcon:expressionOf compcon:cxn_adverbial-clause-construction,
        compcon:cxn_complement-clause-construction ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_mensural-classifier a compcon:str ;
    rdfs:label "mensural classifier" ;
    cx:hasExample compcon:mensural-classifier-example-0 ;
    dc:description "a <a>strategy</a> used for the <a>mensural construction</a> that is morphosyntactically similar to the <a str:sortal-classifier>(sortal) classifier strategy</a> used in the language. <i>Example</i>: in Cantonese <i>léuhng wún faahn</i> <q>two bowls of rice</q>, <i>wún</i> <q>bowl</q> is a mensural classifier that recruits the same construction as is used for sortal classifiers in the language. (Section 5.2.2)" ;
    dcterms:isPartOf "5.2.2" ;
    compcon:expressionOf compcon:cxn_mensural-modification-construction ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_merged-argument-structure-strategy a compcon:str ;
    rdfs:label "merged argument structure strategy" ;
    cx:hasExample compcon:merged-argument-structure-strategy-example-0 ;
    dc:description "a <a>strategy</a> found in <a>complement clause constructions</a> in which there is a single <a>argument structure construction</a> associated with the combination of the <a>complement-taking predicate</a> and the <a>complement</a> <a>predicate</a>. <i>Example</i>: French <i>J'ai fait manger le pain par le chat</i> <q>I made the cat eat the bread</q> has a single argument structure for the participants of the combined event of <q>making eat</q>: the <a>causer</a> <i>J'</i> <q>I</q> is the Subject, the <a>causee</a>/<a>agent</a> <i>par le chat</i> <q>(by) the cat</q> is an Oblique, and the <a>patient</a> of the eating event <i>le pain</i> <q>the bread</q> is the Object. A merged argument structure strategy is essentially a simple argument structure construction that is found with a biclausal complement clause construction, <a>balanced</a> or <a>deranked</a>. (Section 18.4.1)" ;
    dcterms:isPartOf "18.4.1" ;
    compcon:expressionOf compcon:cxn_complement-clause-construction ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_middle-voice a compcon:str ;
    rdfs:label "middle voice" ;
    cx:hasExample compcon:middle-voice-example-0 ;
    dc:description "a <a>strategy</a> for the <a>intransitive construction</a> that <a>recruits</a> a <a>reflexive</a> or <a>reciprocal construction</a>, for the expression of a subset of <a sem:monovalent>monovalent events</a>. <i>Example</i>: Ancient Greek <i>pete-sthai</i> <q>fly</q> is an example of a middle voice form, using the suffix <i>-sthai</i>. The middle voice construction may be a more <a>grammaticalized</a> form of the original construction. (Section 7.2)" ;
    dcterms:isPartOf "7.2" ;
    compcon:expressionOf compcon:cxn_intransitive-construction ;
    compcon:subtypeOf compcon:str_recruitment-strategy .

compcon:str_monoclausal-transitive-reciprocal-strategy a compcon:str ;
    rdfs:label "monoclausal transitive reciprocal strategy" ;
    cx:hasExample compcon:monoclausal-transitive-reciprocal-strategy-example-0 ;
    dc:description "the <a>strategy</a> of <a>recruiting</a> the <a>transitive construction</a> to use as a <a>reciprocal construction</a> but with only one <q>direction</q> of the reciprocal <a>event</a> directly expressed. <i>Example</i>: the Tonga sentence <i>Joni ba-la-yand-ana amukaintu wakwe</i> <q>John and his wife love each other</q> is literally <q>John mutually-loves his wife</q>, with <i>amukaintu wakwe</i> <q>his wife</q> as Direct Object and also an <a str:overt-coding>overt</a> Reciprocal suffix <i>-ana</i> on the <a>verb</a>. (Section 7.2)" ;
    dcterms:isPartOf "7.2" ;
    compcon:expressionOf compcon:cxn_reciprocal-construction ;
    compcon:subtypeOf compcon:str_recruitment-strategy .

compcon:str_monosyndetic a compcon:str ;
    rdfs:label "monosyndetic" ;
    cx:hasExample compcon:monosyndetic-example-0,
        compcon:monosyndetic-example-1,
        compcon:monosyndetic-example-2 ;
    dc:description "a <a>strategy</a> used in <a>syndetic</a> <a>coordination</a> where there are fewer <a>coordinators</a> than <a>coordinands</a>. <i>Example</i>: the simplest example, which gives rise to the term, is one coordinator in a construction with two or more coordinands, as in <i>Iraqw and English</i> or <i>Jerry, Bobby, and Phil</i>. The term has been extended to coordination with multiple coordinands and one less coordinator than coordinand, as in Iraqw <i>Kwermuhl nee Tlawi nee Dongobesh nee Haydom nee Daudi</i> <q>Kwermuhl, Tlawi, Dongobesh, Haydom, and Daudi [place names]</q>. (Section 15.2.2)" ;
    dcterms:isPartOf "15.2.2" ;
    compcon:expressionOf compcon:cxn_coordinate-construction ;
    compcon:subtypeOf compcon:str_syndetic .

compcon:str_negative-auxiliary-strategy a compcon:str ;
    rdfs:label "negative auxiliary strategy" ;
    dc:description "" ;
    compcon:expressionOf compcon:cxn_declarative-negation-construction ;
    compcon:subtypeOf compcon:str_recruitment-strategy .

compcon:str_negative-fusion a compcon:str ;
    rdfs:label "negative fusion" ;
    dc:description "" ;
    compcon:expressionOf compcon:cxn_existential-negation-construction,
        compcon:cxn_prohibitive-construction ;
    compcon:subtypeOf compcon:str_fusion .

compcon:str_negative-particle a compcon:str ;
    rdfs:label "negative particle" ;
    dc:description "" ;
    compcon:expressionOf compcon:cxn_declarative-negation-construction ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_neutral-alignment a compcon:str ;
    rdfs:label "neutral alignment" ;
    cx:hasExample compcon:neutral-alignment-example-0,
        compcon:neutral-alignment-example-1 ;
    dc:description "a <a>system</a> in which all three of the <a>A</a>, <a>P</a>, and <a>S roles</a> are expressed with the same <a>form</a>. <i>Example</i>: English <a>argument phrases</a> use the same (zero) <a>flag</a> for A, P, and S roles: <i>Jack</i> [A] <i>broke the window</i> [P] and <i>Jack</i> [S] <i>died</i>. (Section 6.3.1)" ;
    dcterms:isPartOf "6.3.1" ;
    skos:altLabel "neutral (transitive) alignment (system)" ;
    compcon:subtypeOf compcon:str_transitive-alignment .

compcon:str_neutral-ditransitive-alignment a compcon:str ;
    rdfs:label "neutral ditransitive alignment" ;
    cx:hasExample compcon:neutral-ditransitive-alignment-example-0,
        compcon:neutral-ditransitive-alignment-example-1 ;
    dc:description "a <a>system</a> in which all three of the <a>P</a>, <a>T</a>, and <a>R roles</a> are expressed with the same <a>form</a>. <i>Example</i>: the English Double Object Construction, as in <i>Carol sent the landlord the check</i>, expresses the T and R roles (<i>the check</i> and <i>the landlord</i>, respectively) in the same way as the P role in <i>Carol wrote the check</i> -- namely, as postverbal <a>argument phrases</a> without a <a>flag</a>. (Section 7.5.2)" ;
    dcterms:isPartOf "7.5.2" ;
    skos:altLabel "double object alignment (system)",
        "neutral ditransitive alignment (system)" ;
    compcon:subtypeOf compcon:str_ditransitive-alignment .

compcon:str_nominal-strategy a compcon:str ;
    rdfs:label "nominal strategy" ;
    dc:description "either the <a>nonverbal copula strategy</a> or the <a>zero strategy</a> for <a>predication</a> <a>constructions</a>. These two strategies are grouped together because both strategies appear to originate in <a>equational constructions</a>, are first recruited by object predication constructions, and then are recruited by other predication constructions. (Section 10.2)" ;
    dcterms:isPartOf "10.2" .

compcon:str_nominalizer a compcon:str ;
    rdfs:label "nominalizer" ;
    cx:hasExample compcon:nominalizer-example-0,
        compcon:nominalizer-example-1 ;
    dc:description "a form that signals that a word is being used to <a inf:reference>refer</a>, typically pertaining to a referent from a nonprototypical <a>semantic class</a> for <a>reference</a>. <i>Example</i>: the suffix <i>-ness</i> in <i>oddness</i> and the suffix <i>-ment</i> in <i>movement</i> are used when a <a>property</a> concept like <q>odd</q> or an <a>action</a> concept like <q>move</q> are being referred to. (Section 3.4.2)" ;
    dcterms:isPartOf "3.4.2" ;
    compcon:expressionOf compcon:cxn_referring-phrase ;
    compcon:subtypeOf compcon:str_overt-coding .

compcon:str_nominative-category a compcon:str ;
    rdfs:label "nominative category" ;
    cx:hasExample compcon:nominative-category-example-0,
        compcon:nominative-category-example-1 ;
    dc:description "the <a>morphosyntactic</a> category in the <a>accusative alignment system</a> that <a>co-expresses</a> both <a>A</a> and <a>S roles</a>. <i>Example</i>: English verbs use the same <a>index</a> in the Present Tense (3rd Person Singular <i>-s</i>, otherwise zero) for both A and S roles (<i>Emily sing-s, Emily play-s the piano</i>), hence the <i>-s/-Ø</i> index is a nominative index. (Section 6.3.1)" ;
    dcterms:isPartOf "6.3.1" .

compcon:str_non-indexed a compcon:str ;
    rdfs:label "non-indexed" ;
    dc:description "a <a>strategy</a> in which the <a>stative</a> <a>predicate</a> in a <a>stative complex predicate construction</a> does not <a>index</a> an <a inf:reference>argument</a> of the other (<a>dynamic</a>) predicate in the construction. (Section 14.2)" ;
    dcterms:isPartOf "14.2" ;
    compcon:subtypeOf compcon:str_zero-coding .

compcon:str_non-nominative-flag-index a compcon:str ;
    rdfs:label "non-nominative flag--index" ;
    dc:description "" ;
    compcon:subtypeOf compcon:str_subject-predicate-alignment .

compcon:str_nonperson-indexation a compcon:str ;
    rdfs:label "nonperson indexation" ;
    cx:hasExample compcon:nonperson-indexation-example-0 ;
    dc:description "an <a>indexical strategy</a> in which certain categories, typically <a>number</a> and <a>gender--class</a>, but not the category of <a>person</a>, are encoded in the <a>index</a>. <i>Example</i>: in Russian <i>molod-aja sosna</i> <q>young pine</q>, the suffix <i>-aja</i> on <i>molod-</i> <q>young</q> indexes its <a>referent</a>, also referred to by <i>sosna</i> <q>pine</q>, by <a>number</a> and <a>gender--class</a> (Feminine Singular), and <a>case</a> (Nominative). (Section 4.4.2)" ;
    dcterms:isPartOf "4.4.2" ;
    compcon:subtypeOf compcon:str_indexation .

compcon:str_nonspecific-article-strategy a compcon:str ;
    rdfs:label "nonspecific article strategy" ;
    dc:description "" ;
    compcon:expressionOf compcon:cxn_generic-article ;
    compcon:subtypeOf compcon:str_recruitment-strategy .

compcon:str_nonspecific-pronoun-strategy a compcon:str ;
    rdfs:label "nonspecific pronoun strategy" ;
    dc:description "" ;
    compcon:expressionOf compcon:cxn_generic-pronoun ;
    compcon:subtypeOf compcon:str_recruitment-strategy .

compcon:str_nonverbal-copula-strategy a compcon:str ;
    rdfs:label "nonverbal copula strategy" ;
    cx:hasExample compcon:nonverbal-copula-strategy-example-0 ;
    dc:description "the <a>strategy</a> for <a cxn:nonprototypical-predication-construction>nonprototypical predication</a> that employs an uninflecting <a>copula</a>, typically derived from a <a cxn:personal-pronoun>personal</a> or <a>demonstrative pronoun</a>, <a  inf:reference>topic</a> <u>marker</u>, or <a>focus marker</a>. <i>Example</i>: Nakanai <i>eia <b>la</b> taua sesele</i> <q>he is truly a spirit</q> uses the demonstrative form <i>la</i> for object predication. (Section 10.2)" ;
    dcterms:isPartOf "10.2" ;
    skos:altLabel "nonverbal copula (strategy)" ;
    compcon:expressionOf compcon:cxn_nonprototypical-predication-construction ;
    compcon:subtypeOf compcon:str_copula .

compcon:str_noun-incorporation a compcon:str ;
    rdfs:label "noun incorporation" ;
    cx:hasExample compcon:noun-incorporation-example-0 ;
    dc:description "a <a>strategy</a> used for a range of <a>functions</a> but mostly for the <a>antipassive construction</a> function, in which the <a>noun</a> expressing the <a>P</a> <a>participant</a> is morphologically reduced and compounded with the <a>verb</a>. <i>Example</i>: in <i>He is off mountain-climbing</i>, the P participant, the mountain, is expressed by the noun <i>mountain</i> compounded with the verb form <i>climbing</i>. (Section 8.4)" ;
    dcterms:isPartOf "8.4" ;
    compcon:subtypeOf compcon:str_antipassive-alignment,
        compcon:str_applicative-alignment,
        compcon:str_subject-predicate-alignment .

compcon:str_noun-modifying-clause-strategy a compcon:str ;
    rdfs:label "noun-modifying clause strategy" ;
    cx:hasExample compcon:noun-modifying-clause-strategy-example-0,
        compcon:noun-modifying-clause-strategy-example-1 ;
    dc:description "the <a>strategy</a> of employing the same <a>morphosyntactic</a> structure for both the <a>noun complement clause construction</a> and the <a>relative clause construction</a>. <i>Example</i>: Japanese uses the noun-modifying clause strategy -- the same <a>externally headed</a> strategy is used for both the relative clause construction ([<i>gakusei ga katta</i>] <i>hon</i> <q>the book that the student bought</q>) and the noun complement clause construction ([<i>gakusei ga hon o katta</i>] <i>zizitu</i> <q>the fact that the student bought the book</q>). (Section 19.2.4)" ;
    dcterms:isPartOf "19.2.4" ;
    skos:altLabel "noun-modifying clause (strategy)" ;
    compcon:expressionOf compcon:cxn_noun-complement-clause-construction ;
    compcon:subtypeOf compcon:str_recruitment-strategy .

compcon:str_numeral-strategy a compcon:str ;
    rdfs:label "numeral strategy" ;
    dc:description "" ;
    compcon:expressionOf compcon:cxn_pragmatically-specific-pronoun ;
    compcon:subtypeOf compcon:str_recruitment-strategy .

compcon:str_object-equative a compcon:str ;
    rdfs:label "object equative" ;
    dc:description "" ;
    compcon:expressionOf compcon:cxn_equative-construction ;
    compcon:subtypeOf compcon:str_fixed-case .

compcon:str_oblique-p-strategy a compcon:str ;
    rdfs:label "oblique P strategy" ;
    dc:description "" ;
    compcon:subtypeOf compcon:str_antipassive-alignment,
        compcon:str_applicative-alignment .

compcon:str_omitted-a-strategy a compcon:str ;
    rdfs:label "omitted A strategy" ;
    dc:description "" ;
    compcon:subtypeOf compcon:str_passive-inverse-alignment .

compcon:str_omitted-p-strategy a compcon:str ;
    rdfs:label "omitted P strategy" ;
    cx:hasExample compcon:omitted-p-strategy-example-0 ;
    dc:description "a <a>strategy</a> of the <a>antipassive construction</a> in which the <a>P</a> <a>participant</a> is unexpressed. <i>Example</i>: in <i>She ate</i>, the P participant (the food eaten) is left unexpressed. (Section 8.4)" ;
    dcterms:isPartOf "8.4" ;
    compcon:subtypeOf compcon:str_antipassive-alignment .

compcon:str_overt-article a compcon:str ;
    rdfs:label "overt article" ;
    dc:description "" ;
    compcon:expressionOf compcon:cxn_article ;
    compcon:subtypeOf compcon:str_overt-coding .

compcon:str_overt-pronoun a compcon:str ;
    rdfs:label "overt pronoun" ;
    dc:description "" ;
    skos:altLabel "nonnull pronoun" ;
    compcon:expressionOf compcon:cxn_anaphoric-pronoun ;
    compcon:subtypeOf compcon:str_overt-coding .

compcon:str_overtly-coded-single-role-strategy a compcon:str ;
    rdfs:label "overtly coded single role strategy" ;
    cx:hasExample compcon:overtly-coded-single-role-strategy-example-0,
        compcon:overtly-coded-single-role-strategy-example-1 ;
    dc:description "the <a>strategy</a> of <a>recruiting</a> the <a>intransitive construction</a> for use in a <a>reflexive construction</a> or a <a>reciprocal construction</a>, but with <a>overt coding</a> of the predicate indicating the <a sem:reflexive>reflexive event</a> or <a reciprocal>reciprocal event</a> type. <i>Examples</i>: Abkhaz <i>l-çə-l-k°abe-yt'</i> <q>she washed herself</q> is an Intransitive Verb form with the Reflexive prefix <i>çə-</i>, and Swahili <i>wa-na-pend-an-a</i> <q>they love each other</q> is an Intransitive Verb form with the Reciprocal suffix <i>-an</i>. (Section 7.2)" ;
    dcterms:isPartOf "7.2" ;
    skos:altLabel "derived intransitive" ;
    compcon:expressionOf compcon:cxn_reciprocal-construction ;
    compcon:subtypeOf compcon:str_overt-coding,
        compcon:str_single-role-strategy .

compcon:str_overtly-coded-verb a compcon:str ;
    rdfs:label "overtly coded verb" ;
    cx:hasExample compcon:overtly-coded-verb-example-0 ;
    dc:description "a <a>strategy</a> with any of the different kinds of <a>voice constructions</a> in which there is <a>overt coding</a> of the <a>function</a> of the voice construction on the <a>verb</a>. <i>Example</i>: Hungarian <i>János <b>be</b>-ültette a kerte-t fák-kal</i> <q>John planted the garden with trees</q> is an applicative construction with the overt applicative prefix <i>be-</i> on the verb <i>ültette</i> <q>planted</q>. (Section 9.4)" ;
    dcterms:isPartOf "9.4" ;
    skos:altLabel "overtly verb-coded voice strategy" ;
    compcon:expressionOf compcon:cxn_clause ;
    compcon:subtypeOf compcon:str_overt-coding .

compcon:str_overtly-headed-strategy a compcon:str ;
    rdfs:label "overtly headed strategy" ;
    cx:hasExample compcon:overtly-headed-strategy-example-0 ;
    dc:description "a <a>strategy</a> for the <a>anaphoric-head construction</a> in which there is an <a str:overt-coding>overt</a> morpheme that functions as the <a>head</a>. <i>Example</i>: in <i>I took a red candy and Greg took a green one, a green one</i> is an example of the overtly headed strategy for the anaphoric-head construction, because <i>one</i> serves as an anaphoric head (with respect to <a>type identity</a>) in the construction. (Section 5.4)" ;
    dcterms:isPartOf "5.4" ;
    skos:altLabel "overt anaphoric head strategy",
        "overt head strategy" ;
    compcon:expressionOf compcon:cxn_anaphoric-head-construction,
        compcon:cxn_free-modification-construction ;
    compcon:subtypeOf compcon:str_overt-coding .

compcon:str_p-alignment a compcon:str ;
    rdfs:label "P alignment" ;
    cx:hasExample compcon:p-alignment-example-0 ;
    dc:description "a <a>strategy</a> of the <a>antipassive construction</a> in which the <a>P</a> <a>participant</a> is expressed with an <a cxn:oblique-phrase>oblique</a> <a>flag</a>. <i>Example</i>: in West Greenlandic <i>inun-nik tuqut-si-vuq</i> <q>He killed people</q>, the P participant is encoded with the oblique Instrumental Case suffix <i>-nik</i>. (Section 8.4)" ;
    dcterms:isPartOf "8.4" ;
    skos:altLabel "oblique P strategy" ;
    compcon:expressionOf compcon:cxn_antipassive-construction ;
    compcon:subtypeOf compcon:str_alignment-system .

compcon:str_partially-merged-argument-structure-strategy a compcon:str ;
    rdfs:label "partially merged argument structure strategy" ;
    cx:hasExample compcon:partially-merged-argument-structure-strategy-example-0 ;
    dc:description "a <a>strategy</a> found in <a>complement clause constructions</a> in which the <a>argument structure construction</a> associated with the <a>complement-taking predicate</a> (CTP) is only partially distinct from the argument structure construction associated with the <a>complement</a> <a>predicate</a>. <i>Example</i>: in <i>I made him cook dinner</i>, the CTP <i>made</i> and the complement predicate <i>cook</i> each has its own Object (<i>him</i> and <i>dinner</i>, respectively), indicating that the argument structure constructions are at least partially distinct; but the <a>necessarily shared participant</a> <i>him</i> is expressed only once, as the Object of the CTP. A partially merged argument structure strategy is typically, but not always, associated with a <a>deranked</a> complement clause construction. A partially merged argument structure strategy always involves the expression of a <a>participant</a> in the <a>complement</a> as an <a>argument phrase</a> dependent on the <a>matrix clause</a> predicate -- or, eventually, the <a>complex predicate</a> made up of the former matrix clause and complement clause predicates. (Section 18.4.1)" ;
    dcterms:isPartOf "18.4.1" ;
    compcon:expressionOf compcon:cxn_complement-clause-construction ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_participial-strategy a compcon:str ;
    rdfs:label "participial strategy" ;
    cx:hasExample compcon:participial-strategy-example-0 ;
    dc:description "a <a>strategy</a> used in the <a>stative complex predicate construction</a> in which the <a cxn:manner-complex-predicate>manner</a> (more generally, <a>stative</a>) <u>component</u> is packaged as a separate primary <a>predication</a> <a cxn:coordinate-construction>coordinated</a> with the <a>event</a> predication using a <a>deranked</a> <a>complex sentence</a> <a>strategy</a>. In addition, the stative predicate is predicated of, and ideally <a>indexes</a>, (one of) its argument(s). <i>Example</i>: in Sanuma <i>opi-i a kali-palo-ma</i> <q>He worked slowly</q> [lit. <q>being slow, he worked</q>] <i>opi-i</i> <q>be slow</q> is in a deranked form with suffix <i>-i</i>; the suffix, however, indicates that <i>opi</i> <q>slow</q> has the same subject as <i>kali-palo-ma</i> <q>worked</q> -- namely, <q>he</q>. (Section 14.2)" ;
    dcterms:isPartOf "14.2" ;
    compcon:expressionOf compcon:cxn_stative-complex-predicate ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_participle a compcon:str ;
    rdfs:label "participle" ;
    cx:hasExample compcon:participle-example-0 ;
    dc:description "a <a>deranked</a> <a>relative clause</a> <a>predicate</a>. <i>Example</i>: in <i>The car almost hit the roadrunner</i> [<i>eat<b>ing</b> a grasshopper</i>], <i>eating</i> is a participle in the relative clause <i>eating a grasshopper</i>. (Section 19.2.1)" ;
    dcterms:isPartOf "19.2.1" ;
    compcon:expressionOf compcon:cxn_relative-clause .

compcon:str_particle-comparative a compcon:str ;
    rdfs:label "particle comparative" ;
    cx:hasExample compcon:particle-comparative-example-0 ;
    dc:description "a <a>derived-case</a> <a>comparative</a> <a>strategy</a> that consists of two <a>clauses</a> which assert that the <a>gradable predicative scale</a> applies to the <a>comparee</a> and the <a>standard</a>, but the second clause uses the <a str:zero-copula>zero copula strategy</a> for <a cxn:predicate-identity-construction>predicate identity</a>, and the <a>conjunction</a> is a particle with diverse etymological origins. <i>Example</i>: <i>Randy is older than Tom (is)</i> is an instance of the particle strategy: the clause <i>Randy is older</i> is followed by the particle <i>than</i> which introduces the standard phrase <i>Tom</i> without its own <a>predicate</a> (only an optional auxiliary <i>is</i>). (Section 17.2.2)" ;
    dcterms:isPartOf "17.2.2" ;
    compcon:expressionOf compcon:cxn_comparative-construction ;
    compcon:subtypeOf compcon:str_derived-case,
        compcon:str_independent-strategy .

compcon:str_particle-equative a compcon:str ;
    rdfs:label "particle equative" ;
    cx:hasExample compcon:particle-equative-example-0 ;
    dc:description "a <a>derived-case strategy</a> for <a>equative constructions</a> which consists of a <a>matrix clause</a> which expresses that the <a>gradable predicative scale</a> applies to the <a>comparee</a>, and a <a>dependent clause</a> expressing that the same scale applies to the <a>standard</a>. The dependent clause is in the form of a particle acting as a <a>conjunction</a>, and the <a>argument phrase</a> expressing the standard; the <a str:zero-copula>zero copula strategy</a> is used for <a cxn:predicate-identity-construction>predicate identity</a>. <i>Example</i>: Chechen <i>aħa döšu as sanna</i> <q>You read like I [do]</q> is an instance of the particle equative strategy: the matrix clause <i>aħa döšu</i> <q>You read</q> predicates the scale of the comparee; the particle <i>sanna</i> <q>like</q> introduces the standand <i>as</i> <q>I</q>, which is in the same <a>case</a> as the comparee (the Ergative). (Section 17.2.4)" ;
    dcterms:isPartOf "17.2.4" ;
    compcon:expressionOf compcon:cxn_equative-construction ;
    compcon:subtypeOf compcon:str_derived-case,
        compcon:str_independent-strategy .

compcon:str_perception-verb-strategy a compcon:str ;
    rdfs:label "perception verb strategy" ;
    cx:hasExample compcon:perception-verb-strategy-example-0,
        compcon:perception-verb-strategy-example-1 ;
    dc:description "a <a>strategy</a> for the <a>presentational construction</a> in which the <a>referring phrase</a> introducing the <a>referent</a> is expressed as the stimulus of a <a>perception event</a>. <i>Example</i>: an English example from the Pear Stories narrative is <i>and um then you see this little girl. Coming on a bicycle in the opposite direction, ...</i> More grammaticalized versions of this strategy include French <i><b>Voici</b> un coffre ... <b>Voilà</b> un autre coffre</i> <q>Here is a treasure chest ... There is another treasure chest</q>. (Section 10.4.3)" ;
    dcterms:isPartOf "10.4.3" ;
    compcon:expressionOf compcon:cxn_presentational-construction ;
    compcon:subtypeOf compcon:str_recruitment-strategy .

compcon:str_person-indexation a compcon:str ;
    rdfs:label "person indexation" ;
    cx:hasExample compcon:person-indexation-example-0 ;
    dc:description "an <a>indexical strategy</a> in which the category of <a>person</a> is encoded in the <a>index</a>. Other categories, typically <a>number</a> and <a>gender--class</a>, may also be encoded in the index. <i>Example</i>: in Mam <i>t-kamb' meeb'a</i> <q>the orphan's prize</q>, the third person prefix <i>t-</i> on <i>kamb'</i> <q>prize</q> indexes the possessor, who is also referred to by <i>meeb'a</i> <q>orphan</q>. (Section 4.4.1)" ;
    dcterms:isPartOf "4.4.1" ;
    compcon:subtypeOf compcon:str_indexation .

compcon:str_phenomime a compcon:str ;
    rdfs:label "phenomime" ;
    cx:hasExample compcon:phenomime-example-0 ;
    dc:description "the <a>ideophone strategy</a> used to express a <a>property</a> of movement, visual appearance or light emission, texture, or taste. <i>Example</i>: Siwu <i>wùrùfùù</i> <q>fluffy</q> is a phenomime; it describes the texture of an object using an ideophone. (Section 14.4)" ;
    dcterms:isPartOf "14.4" ;
    compcon:expressionOf compcon:cxn_stative-complex-predicate ;
    compcon:subtypeOf compcon:str_ideophones .

compcon:str_phonomime a compcon:str ;
    rdfs:label "phonomime" ;
    cx:hasExample compcon:phonomime-example-0 ;
    dc:description "the <a>ideophone strategy</a> used to express a <a>property</a> of sound emission. <i>Example</i>: Japanese <i>potapota potapota</i> <q>dripping</q> is a phonomime; it describes the sound of dripping using an ideophone. (Section 14.4)" ;
    dcterms:isPartOf "14.4" ;
    compcon:expressionOf compcon:cxn_stative-complex-predicate ;
    compcon:subtypeOf compcon:str_ideophones .

compcon:str_possessive-locative-strategy a compcon:str ;
    rdfs:label "possessive locative strategy" ;
    cx:hasExample compcon:possessive-locative-strategy-example-0 ;
    dc:description "a <a>strategy</a> for the <a>presentational construction</a> in which the <a>referring phrase</a> introducing the <a>referent</a> is expressed as the <a>possessum</a> in a <a>presentational possession construction</a>. <i>Example</i>: Swahili <i>ku na mgeni nyumba-ni</i> [<sc>loc.clf</sc> with stranger home-at] <q>There is a stranger at home</q>, uses the <a>with-possessive strategy</a> of the presentational possessive construction to express the stranger as the possessum; the location at home is expressed by a locative phrase. (Section 10.4.3)" ;
    dcterms:isPartOf "10.4.3" ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_posterior-deranking a compcon:str ;
    rdfs:label "posterior deranking" ;
    cx:hasExample compcon:posterior-deranking-example-0 ;
    dc:description "the variant of a <a>deranking strategy</a> in which the deranked <a>clauses</a> follow the clause that is expressed like a simple <a>main clause</a>. <i>Example</i>: Big <i>Nambas a-əln talei ka-vruh ka-vmi'i arna pitha</i> <q>So they left their knives and ran away and climbed over the hill...</q> is an instance of posterior deranking: the prefix <i>ka-</i> on <i>ka-vruh</i> <q>run away</q> and <i>ka-vmi'i</i> <q>go over</q> indicates that the posterior predicates are deranked. (Section 15.2.3)" ;
    dcterms:isPartOf "15.2.3" ;
    compcon:expressionOf compcon:cxn_coordinate-clause-construction ;
    compcon:subtypeOf compcon:str_deranking,
        compcon:str_word-order .

compcon:str_posterior-zero a compcon:str ;
    rdfs:label "posterior zero" ;
    dc:description "" ;
    compcon:expressionOf compcon:cxn_object-identity-construction,
        compcon:cxn_predicate-identity-construction,
        compcon:cxn_subject-identity-construction ;
    compcon:subtypeOf compcon:str_zero-coding .

compcon:str_postnominal-strategy a compcon:str ;
    rdfs:label "postnominal strategy" ;
    cx:hasExample compcon:postnominal-strategy-example-0 ;
    dc:description "a <a>word order</a> strategy for <a>externally headed</a> <a>relative clause constructions</a> in which the <a>relative clause</a> follows the <a>relative clause head</a>. <i>Example</i>: in <i>I ate the cheesecake</i> [<i>that Carol baked</i>], <i>that Carol baked</i> is postnominal because it follows <i>the cheesecake</i>. The postnominal strategy is much more common than the <a>prenominal</a> strategy. (Section 19.2.2)" ;
    dcterms:isPartOf "19.2.2" ;
    skos:altLabel "postnominal (strategy)" ;
    compcon:subtypeOf compcon:str_externally-headed,
        compcon:str_word-order .

compcon:str_postposition a compcon:str ;
    rdfs:label "postposition" ;
    cx:hasExample compcon:postposition-example-0 ;
    dc:description "an <a>adposition</a> which occurs after the <a>head</a> of the <a>referring phrase</a>. <i>Example</i>: in Urarina <i>nii banaao asae</i> <q>under that leaf shelter</q>, <i>asae</i> <q>under</q> follows <i>nii banaao</i> <q>that leaf shelter</q>. (Section 4.3)" ;
    dcterms:isPartOf "4.3" ;
    compcon:subtypeOf compcon:str_adposition .

compcon:str_predicational-locative-strategy a compcon:str ;
    rdfs:label "predicational locative strategy" ;
    cx:hasExample compcon:predicational-locative-strategy-example-0,
        compcon:predicational-locative-strategy-example-1,
        compcon:predicational-locative-strategy-example-2,
        compcon:predicational-locative-strategy-example-3,
        compcon:predicational-locative-strategy-example-4 ;
    dc:description "a <a>strategy</a> for the <a>presentational location construction</a> (and <a cxn:presentational-construction>presentational</a> constructions in general) which recruits the <a>predicational location construction</a>. The predicational locative strategy has a fixed-order subtype, where the word order of figure phrase and locative phrase remains the same. <i>Example</i>: Welsh <i>mae car yma</i> <q>There is a car here</q> has the same word order as the predicational locative <i>Mae'r car yma</i> <q>The car is here</q>. The predicational locative strategy also has a reverse-order subtype, where the word order is reversed so that the locative phrase precedes the figure phrase. <i>Example</i>: <i>On the table was a large bowl</i> has the same structure as <i>The large bowl was on the table</i>, except that the order of <i>on the table</i> and <i>a/the large bowl</i> is reversed. More <a>grammaticalized</a> versions of this strategy include English <i><b>There's</b> a snake in the back yard</i>. (Section 10.4.3)" ;
    dcterms:isPartOf "10.4.3" ;
    compcon:expressionOf compcon:cxn_presentational-location-construction ;
    compcon:subtypeOf compcon:str_recruitment-strategy .

compcon:str_predicational-strategy a compcon:str ;
    rdfs:label "predicational strategy" ;
    cx:hasExample compcon:predicational-strategy-example-0 ;
    dc:description "a <a>strategy</a> used in the <a>stative complex predicate construction</a> in which the <a cxn:manner-complex-predicate>manner</a> or other <a sem:stative>stative component</a> is <a inf:information-packaging>packaged</a> as the sole <a>predication</a>, and the <a>event</a> is packaged as the <a inf:high-topicality>subject</a> <a inf:reference>argument</a> of the manner predication. <i>Example</i>: in Mokilese <i>ah kijou dahr</i> <q>He runs fast</q> [lit. <q>his running is fast</q>], the speed is predicated of the event. (Section 14.2)" ;
    dcterms:isPartOf "14.2" ;
    compcon:expressionOf compcon:cxn_stative-complex-predicate ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_predicativization-possessive-strategy a compcon:str ;
    rdfs:label "predicativization possessive strategy" ;
    cx:hasExample compcon:predicativization-possessive-strategy-example-0,
        compcon:predicativization-possessive-strategy-example-1 ;
    dc:description "a <a>strategy</a> for the <a>presentational possession construction</a> in which the <a>possessum</a> is <u>incorporated</u> as part of the possessive <a>predicate</a>. <i>Examples</i>: Tundra Yukaghir <i>mārqa-n lāme-n'-ŋi</i> [one dog-with-<sc>3pl.intr</sc>] <q>They had one dog</q> is an instance of the flexional (inflected) subtype of the predicativization possessive strategy, and Pitjantjatjara <i>ngankulukula-tjara</i> [I spear-<sc>proprietive</sc>] <q>I have a spear</q> [lit. <q>I am spear-having</q>] is an instance of the copular/zero (uninflected) subtype of the strategy. (Section 10.4.2)" ;
    dcterms:isPartOf "10.4.2" ;
    compcon:expressionOf compcon:cxn_presentational-possession-construction ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_prenominal-strategy a compcon:str ;
    rdfs:label "prenominal strategy" ;
    cx:hasExample compcon:prenominal-strategy-example-0 ;
    dc:description "a <a>word order</a> strategy for <a>externally headed</a> <a>relative clause constructions</a> in which the <a>relative clause</a> precedes the <a>relative clause head</a>. <i>Example</i>: in Turkish [<i>Hasanın Sinana ver-diğ-i</i>] <i>patatesi yedim</i> <q>I ate the potato that Hasan gave to Sinan</q>, the <i>Hasanın Sinana ver-diğ-i</i> <q>[that] Hasan gave to Sinan</q> is prenominal because it precedes the relative clause head <i>patatesi</i> <q>potato</q>. The prenominal strategy is much less common than the <a>postnominal</a> strategy, and is strongly correlated with <a inf:object-argument>object</a>--<a>verb</a> word order. (Section 19.2.2)" ;
    dcterms:isPartOf "19.2.2" ;
    skos:altLabel "prenominal (strategy)" ;
    compcon:subtypeOf compcon:str_externally-headed,
        compcon:str_word-order .

compcon:str_preposition a compcon:str ;
    rdfs:label "preposition" ;
    cx:hasExample compcon:preposition-example-0 ;
    dc:description "an <a>adposition</a> which occurs before the <a>head</a> of the <a>referring phrase</a>. <i>Example</i>: in <i>on the table, on</i> is an adposition that precedes <i>the table</i>. (Section 4.3)" ;
    dcterms:isPartOf "4.3" ;
    compcon:subtypeOf compcon:str_adposition .

compcon:str_primary-object-category a compcon:str ;
    rdfs:label "primary object category" ;
    cx:hasExample compcon:primary-object-category-example-0 ;
    dc:description "the <a>morphosyntactic</a> category in the <a>secundative alignment system</a> that <a>co-expresses</a> both <a>R</a> and <a>P roles</a>. <i>Example</i>: in Huichol <i>nee uuki uukari ne-wa-puuzeiyastɨa</i> <q>I showed the man to the girls</q>, the <a>index</a> <i>wa-</i> <q><sc>3pl</sc></q> is the same used to index <i>tɨɨri</i> <q>children</q> in <i>uukaraawiciizɨ tɨɨri me-wa-zeiya</i> <q>The women see the children</q>, and hence represents the primary object category. (Section 7.5.2)" ;
    dcterms:isPartOf "7.5.2" .

compcon:str_pronoun-retention-strategy a compcon:str ;
    rdfs:label "pronoun-retention strategy" ;
    cx:hasExample compcon:pronoun-retention-strategy-example-0 ;
    dc:description "a <a>strategy</a> for the expression of the <a>necessarily shared participant</a> in the <a>relative clause</a> of an <a>externally headed</a> <a>relative clause construction</a>, in which the participant is expressed in the relative clause by an <a>anaphoric pronoun</a>. <i>Example</i>: the Modern Hebrew construction illustrated by <i>hasarim</i> [<i>she-ha-nasi shalax otam la mitsraim</i>] <q>the ministers that the President sent to Egypt</q>, uses the pronoun-retention strategy: the anaphoric pronoun <i>otam</i> <q>them</q> occurs in the relative clause <i>she-ha-nasi shalax otam la mitsraim</i> <q>that the President sent them to Egypt</q>, and refers to the external <a>relative clause head</a> <i>hasarim</i> <q>the ministers</q>. (Section 19.2.2)" ;
    dcterms:isPartOf "19.2.2" ;
    skos:altLabel "pronoun-retention (strategy)" ;
    compcon:subtypeOf compcon:str_externally-headed .

compcon:str_prosody a compcon:str ;
    rdfs:label "prosody" ;
    cx:hasExample compcon:prosody-example-0 ;
    dc:description "a phonological <a>strategy</a> typically used for expressing certain <a cxn:nonpredicational-construction>nonpredicational clause constructions</a> and <a cxn:speech-act-construction>speech act</a> constructions. <i>Example</i>: prosody can be used in English to express a <a cxn:thetic-construction>thetic</a> construction: <i>(What's the matter?) My NECK hurts</i>. (Section 11.3.2)" ;
    dcterms:isPartOf "11.3.2" ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_pseudo-partitive a compcon:str ;
    rdfs:label "pseudo-partitive" ;
    dc:description "a <a>strategy</a> for <a>mensural constructions</a> in which a <a>possessive modification construction</a> is <a>recruited</a> (or was recruited, as the pseudo-partitive often differs from the counterpart modification construction) to express the mensural relation. (Section 5.2.2)" ;
    dcterms:isPartOf "5.2.2" ;
    compcon:expressionOf compcon:cxn_mensural-modification-construction ;
    compcon:subtypeOf compcon:str_recruitment-strategy .

compcon:str_psychomime a compcon:str ;
    rdfs:label "psychomime" ;
    cx:hasExample compcon:psychomime-example-0 ;
    dc:description "the <a>ideophone strategy</a> used to express an experiential property such as bodily sensation or emotion. <i>Example</i>: Japanese <i>kurakura</i> <q>dizzy</q> is a psychomime; it describes a physiological sensation (dizziness) using an ideophone. (Section 14.4)" ;
    dcterms:isPartOf "14.4" ;
    compcon:expressionOf compcon:cxn_stative-complex-predicate ;
    compcon:subtypeOf compcon:str_ideophones .

compcon:str_question-marker a compcon:str ;
    rdfs:label "question marker" ;
    dc:description "" ;
    compcon:expressionOf compcon:cxn_interrogative-construction ;
    compcon:subtypeOf compcon:str_overt-coding .

compcon:str_quotative-marker a compcon:str ;
    rdfs:label "quotative marker" ;
    cx:hasExample compcon:quotative-marker-example-0 ;
    dc:description "a <a>complementizer</a> used with <a>direct reports</a>. <i>Example</i>: in Kobon <i>ban nöp hagöp</i> [<i>yad ram arabin</i>] <i>a göp</i> <q>Who said to you, <dq>I am going home</dq>?</q>, <i>a</i> is a quotative marker used with the direct report <a>utterance</a> <a>complement</a> <i>yad ram arabin</i> <q>I am going home</q>. Like the direct report <a>strategy</a> it accompanies, a quotative marker originates with utterance <a>complement clause constructions</a> but is extended to other complement clause constructions lower in the <a>Binding Hierarchy</a>. (Section 18.3.2)" ;
    dcterms:isPartOf "18.3.2" ;
    compcon:expressionOf compcon:cxn_utterance-construction ;
    compcon:subtypeOf compcon:str_complementizer .

compcon:str_reciprocal-strategy a compcon:str ;
    rdfs:label "reciprocal strategy" ;
    dc:description "" ;
    compcon:expressionOf compcon:cxn_chaining-construction,
        compcon:cxn_collective-construction,
        compcon:cxn_reflexive-construction ;
    compcon:subtypeOf compcon:str_recruitment-strategy .

compcon:str_reflexive-strategy a compcon:str ;
    rdfs:label "reflexive strategy" ;
    dc:description "" ;
    compcon:expressionOf compcon:cxn_chaining-construction,
        compcon:cxn_collective-construction,
        compcon:cxn_reciprocal-construction ;
    compcon:subtypeOf compcon:str_recruitment-strategy .

compcon:str_relative-based-equative a compcon:str ;
    rdfs:label "relative-based equative" ;
    cx:hasExample compcon:relative-based-equative-example-0 ;
    dc:description "a <a>derived-case strategy</a> for <a>equative constructions</a> which consists of a <a>relative clause</a>-like <a>construction</a> where the <a>matrix clause</a> expresses that the <a>gradable predicative scale</a> applies to the <a>comparee</a>, and a relative clause expresses that the same scale applies to the <a>standard</a>. The relative clause is reduced to a <a>relativizer</a> and an <a>argument phrase</a> expressing the standard; the <a str:zero-copula>zero copula strategy</a> is used for <a cxn:predicate-identity-construction>predicate identity</a>. <i>Example</i>: Lithuanian <i>Šiandien taip šalta kaip vakar</i> <q>Today it is as cold as yesterday</q> is an instance of the relative-based equative strategy: <i>Šiandien taip šalta</i> <q>today [is] so cold</q> is the matrix clause, with the degree marker <i>taip</i> <q>so</q>, and <i>kaip vakar</i> <q>how yesterday</q> is the relative-based clause, with the pronoun <i>kaip</i> <q>how</q>, the standard, <q>yesterday</q>, and a zero predicate. Typically the relative-based equative recruits a correlative relative clause, with a <a inf:free-choice-referent>free</a> (<a cxn:indefinite-pronoun>indefinite head</a>) <a cxn:relative-clause>relative</a> or interrogative-based <a str:relative-pronoun-strategy>relative pronoun</a>. (Section 17.2.4)" ;
    dcterms:isPartOf "17.2.4" ;
    compcon:expressionOf compcon:cxn_equative-construction ;
    compcon:subtypeOf compcon:str_derived-case,
        compcon:str_independent-strategy .

compcon:str_relative-equal-equative a compcon:str ;
    rdfs:label "relative equal equative" ;
    cx:hasExample compcon:relative-equal-equative-example-0 ;
    dc:description "a <a>strategy</a> for <a>equative constructions</a> which combines two predicates, one expressing equality of the comparee on the <a>gradable predicative scale</a>, similar to the <a>equal equative</a>, and the other expressing the standard in a structure similar to the <a>relative-based equative</a>. Like the relative-based equative, but unlike the equal equative, the relative equal equative is a <a>derived-case</a> strategy. <i>Example</i>: Spanish <i>Douglas y Pedro son igual de altos que María</i> <q>Douglas and Pedro are as tall as María</q> is an instance of the relative equal equative; the matrix predicate is <i>son igual</i> <q>[they] are equal</q> and the deranked <i>de altos</i> describes the scale. (Section 17.2.4)" ;
    dcterms:isPartOf "17.2.4" ;
    compcon:expressionOf compcon:cxn_equative-construction ;
    compcon:subtypeOf compcon:str_derived-case,
        compcon:str_relative-strategy .

compcon:str_relative-pronoun-strategy a compcon:str ;
    rdfs:label "relative pronoun strategy" ;
    cx:hasExample compcon:relative-pronoun-strategy-example-0 ;
    dc:description "a <a>strategy</a> for the expression of the <a>necessarily shared participant</a> in the <a>relative clause</a> of an <a>externally headed</a> <a>relative clause construction</a>, in which the participant in the relative clause is expressed by a <a cxn:pronoun>relative pronoun</a> unique to the relative clause construction. <i>Example</i>: in <i>He stole the emerald</i> [<i>which he gave to his wife</i>], the relative clause is introduced by the relative pronoun <i>which</i>, which refers to the emerald, which is also expressed by the external <a>relative clause head</a> <i>the emerald</i>. The relative pronoun strategy is rare crosslinguistically, and largely restricted to the European area. (Section 19.2.2)" ;
    dcterms:isPartOf "19.2.2" ;
    compcon:subtypeOf compcon:str_externally-headed .

compcon:str_relativizer a compcon:str ;
    rdfs:label "relativizer" ;
    cx:hasExample compcon:relativizer-example-0 ;
    dc:description "a form that signals that an <a>action</a> is being used as a <a>modifier</a> -- that is, it signals a <a>relative clause</a>. <i>Example</i>: in <i>I ate the cheesecake</i> [<i>that Carol baked</i>], <i>that</i> is the relativizer. A relativizer makes a <a>relative clause construction</a> <a>syndetic</a>. (Section 19.2.1)" ;
    dcterms:isPartOf "19.2.1" ;
    compcon:expressionOf compcon:cxn_relative-clause-construction ;
    compcon:subtypeOf compcon:str_conjunction .

compcon:str_repeater a compcon:str ;
    rdfs:label "repeater" ;
    cx:hasExample compcon:repeater-example-0 ;
    dc:description "a <a>sortal classifier</a> that is identical in <a>form</a> to the <a>noun</a> it <a inf:modification>modifies</a>. <i>Example</i>: in Lao <i>hang2 sip2 hang2</i> [nest two <sc>clf</sc>] <q>two nests</q>, the sortal classifier <i>hang2</i> is identical to the noun <i>hang2</i> <q>nest</q>. (Section 4.4.3)" ;
    dcterms:isPartOf "4.4.3" ;
    compcon:expressionOf compcon:cxn_modification-construction ;
    compcon:subtypeOf compcon:str_classifier .

compcon:str_satellite a compcon:str ;
    rdfs:label "satellite" ;
    cx:hasExample compcon:satellite-example-0 ;
    dc:description "a <a>strategy</a> for expressing a semantic component of an <a>event</a> (such as a <a>motion event</a>) in an <a>element</a> of the <a>clause</a> other than the <a>verb</a>. <i>Example</i>: in <i>Sam strode into the room</i>, the path of motion is expressed not by the verb (<i>stride</i>), but by the <a inf:oblique-argument>oblique</a> <a>flag</a> <i>into (the room)</i>. (Sections 7.3.1, 14.2)" ;
    dcterms:isPartOf "14.2",
        "7.3.1" .

compcon:str_satellite-framing-strategy a compcon:str ;
    rdfs:label "satellite-framing strategy" ;
    cx:hasExample compcon:satellite-framing-strategy-example-0 ;
    dc:description "a <a>strategy</a> for expressing <a>events</a> such that the manner of how the event comes about is expressed by a <a>manner verb</a>, and the <a sem:result-event>result state</a> (including also paths of motion) is expressed in a <a>satellite</a>. <i>Example</i>: <i>Sam strode into the room</i> uses the satellite-framing strategy -- <i>stride</i> is a manner of motion verb, and the path of motion is expressed by the oblique phrase <i>into the room</i>. (Section 7.3.1)" ;
    dcterms:isPartOf "7.3.1" ;
    compcon:expressionOf compcon:cxn_motion-construction ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_scalar-concessive-conditional-strategy a compcon:str ;
    rdfs:label "scalar concessive conditional strategy" ;
    cx:hasExample compcon:scalar-concessive-conditional-strategy-example-0 ;
    dc:description "a strategy for expressing a <a>concessive conditional construction</a> where the <a cxn:protasis-clause>protasis</a>, which specifies the set of conditions for the concessive conditional, invokes the most informative or <dq>strongest</dq> condition in the <a>scalar model</a>; the <a cxn:apodosis-clause>apodosis</a> expresses the unexpected opposite of the expected causal relation between the most extreme condition and the outcome, and so implies the same outcome under the less extreme conditions. <i>Example</i>: <i>Even if you drink (only) a little, your boss will fire you</i> uses the scalar concessive conditional strategy: drinking only a little is the least likely condition (in the scalar model of drinking to various degrees) to lead to your boss firing you, and yet it will lead to your boss firing you. (Section 17.3.3)" ;
    dcterms:isPartOf "17.3.3" ;
    compcon:expressionOf compcon:cxn_concessive-conditional-construction ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_second-position a compcon:str ;
    rdfs:label "second position" ;
    cx:hasExample compcon:second-position-example-0 ;
    dc:description "a <a>strategy</a> in which the <a>element</a> in question is positioned after the first element in the phrase or <a>clause</a>. <i>Example</i>: in Tohono O'odham <i>ban 'o g cu: wĭ huhu'id</i> [coyote <sc>aux</sc> <sc>def</sc> jackrabbit chase] <q>The coyote is chasing the jackrabbit</q>, the auxiliary <i>o'</i> is in second position in the clause. (Section 13.4)" ;
    dcterms:isPartOf "13.4" ;
    compcon:expressionOf compcon:cxn_auxiliary-construction ;
    compcon:subtypeOf compcon:str_serial-verb-strategy .

compcon:str_secondary-object-category a compcon:str ;
    rdfs:label "secondary object category" ;
    cx:hasExample compcon:secondary-object-category-example-0 ;
    dc:description "the <a>morphosyntactic</a> category in the <a>secundative alignment system</a> that exclusively expresses the <a>T role</a>. <i>Example</i>: in Huichol <i>nee uuki uukari ne-wa-puuzeiyastɨa</i> <q>I showed the man to the girls</q>, <i>uuki</i> <q>man</q> is exclusively expressed by the absence of <a>indexation</a> on the <a>verb</a> form <i>ne-wa-puuzeiyastɨa</i> <q>I showed them</q>. (Section 7.5.2)" ;
    dcterms:isPartOf "7.5.2" .

compcon:str_secundative-alignment a compcon:str ;
    rdfs:label "secundative alignment" ;
    cx:hasExample compcon:secundative-alignment-example-0,
        compcon:secundative-alignment-example-1 ;
    dc:description "a <a>system</a> in which the <a>P</a> and <a>R roles</a> are expressed with the same <a>form</a>, but the <a>T role</a> is expressed with a different form. <i>Example</i>: in Huichol <i>nee uuki uukari ne-wa-puuzeiyastɨa</i> <q>I showed the man to the girls</q>, the <a>index</a> <i>wa-</i> <q><sc>3pl</sc></q> is the same used to index <i>tɨɨri</i> <q>children</q> in <i>uukaraawiciizɨ tɨɨri me-wa-zeiya</i> <q>The women see the children</q>. (Section 7.5.2)" ;
    dcterms:isPartOf "7.5.2" ;
    skos:altLabel "secundative (ditransitive) alignment (system)" ;
    compcon:subtypeOf compcon:str_ditransitive-alignment .

compcon:str_semantic-information-packaging-strategy a compcon:str ;
    rdfs:label "semantic information packaging strategy" ;
    cx:hasExample compcon:semantic-information-packaging-strategy-example-0 ;
    dc:description "a <a>strategy</a> found with <a>nonprototypical construction</a> types, such as <a>predicate nominal constructions</a> which express <a>predication</a> of an <a>object concept</a>. The semantic IP strategy is to <a>recruit</a> the strategy used for the prototypical <a>information packaging</a> function of the (actual) information packaging in the nonprototypical construction. <i>Example</i>: in the English Predicate Nominal Construction exemplified by <i>Rich is an excellent violinist</i>, the semantic <a>object concept</a> <i>violinist</i> that is being predicated recruits the construction used for object <a>reference</a>, the prototypical object construction: it is preceded by the article <i>an</i> and the adjective <i>excellent</i>. The semantic IP strategy contrasts with the <a>actual IP strategy</a> and the <a>hybrid IP strategy</a>. (Section 2.4)" ;
    dcterms:isPartOf "2.4" ;
    skos:altLabel "semantic IP strategy" ;
    compcon:expressionOf compcon:cxn_nonprototypical-construction ;
    compcon:subtypeOf compcon:str_information-packaging-alignment .

compcon:str_separative-comparative a compcon:str ;
    rdfs:label "separative comparative" ;
    cx:hasExample compcon:separative-comparative-example-0 ;
    dc:description "a <a>fixed-case strategy</a> in <a>comparative constructions</a> in which there is a <a>clause</a> which attributes a <a>gradable predicative scale</a> to the <a>comparee</a>, and the <a>standard</a> is expressed as an <a>oblique argument phrase</a> with a spatial <a>flag</a> with an ablative (<q>from</q>) <a>meaning</a>. <i>Example</i>: Mundari <i>sadom-ete hati mananga-i</i> <q>The elephant is bigger than the horse</q> is an instance of the separative comparative -- <i>hati mananga-i</i> asserts that the elephant is big, and <i>sadom-ete</i> expresses the standard, the horse, with a suffix <i>-ete</i> meaning <q>from</q>. (Section 17.2.2)" ;
    dcterms:isPartOf "17.2.2" ;
    compcon:expressionOf compcon:cxn_comparative-construction ;
    compcon:subtypeOf compcon:str_fixed-case,
        compcon:str_ordered-strategy .

compcon:str_simple-predicate-applicative-strategy a compcon:str ;
    rdfs:label "simple predicate applicative strategy" ;
    cx:hasExample compcon:simple-predicate-applicative-strategy-example-0 ;
    dc:description "a <a>strategy</a> for the <a>applicative construction</a> in which the <a>event</a> including the <a>participant</a> expressed as the <a>applicative object</a> is expressed in a single <a>clause</a> with a simple <a>predicate</a> (not a <a>complex predicate</a>). <i>Example</i>: Nomatsiguenga <i>na-manantë-ne-ro kayeta</i> [I-buy-<sc>ben.appl</sc>-her cracker] <q>I bought crackers for her</q> is an example of the monoclausal/simple predicate applicative strategy, with a single <a>verb</a> <i>na-manantë-ne-ro</i>. (Section 9.3)" ;
    dcterms:isPartOf "9.3" .

compcon:str_simple-predicate-causative-strategy a compcon:str ;
    rdfs:label "simple predicate causative strategy" ;
    cx:hasExample compcon:simple-predicate-causative-strategy-example-0 ;
    dc:description "a <a>strategy</a> for the <a>causative construction</a> in which the <a>causative event</a> is expressed in a single <a>clause</a> with a simple <a>predicate</a> (not a <a>complex predicate</a>). <i>Example</i>: in Hungarian, <i>Köhögtettem a gyerek-et</i> <q>I made the boy cough</q> is an instance of the monoclausal causative strategy; there is only a single verb <i>köhögtettem</i> [cough: <sc>caus:1sg.pst</sc>] <q>(I) made cough</q>. (Section 9.2)" ;
    dcterms:isPartOf "9.2" ;
    skos:altLabel "monoclausal causative strategy",
        "morphological causative" .

compcon:str_sortal-classifier a compcon:str ;
    rdfs:label "sortal classifier" ;
    cx:hasExample compcon:sortal-classifier-example-0 ;
    dc:description "the third morpheme in a <a>classifier</a> strategy. <i>Example</i>: in Chrau <i>du tong aq</i> <q>one crossbow</q>, <i>tong</i> is a sortal classifier for long objects that refers to the crossbow (<i>aq</i>) and combines with the <a>modifier</a> <i>du</i> <q>one</q>. A sortal classifer contrasts with a <a>mensural classifier</a> in that it does not measure out an amount of the object (crossbow), it simply classifies it by its <a>type</a> or sort. A <a>repeater</a> is a special type of sortal classifier. (Section 5.2.2)" ;
    dcterms:isPartOf "5.2.2" ;
    compcon:expressionOf compcon:cxn_modification-construction ;
    compcon:subtypeOf compcon:str_classifier .

compcon:str_special-form a compcon:str ;
    rdfs:label "special form" ;
    cx:hasExample compcon:special-form-example-0 ;
    dc:description "a highly grammaticalized strategy for encoding the relation in <a>major propositional acts</a> (<a>modifier</a>--<a>referent</a>, <a>predicate</a>--<a inf:reference>argument</a>), in which one of the two <a>elements</a> is in a morphologically distinct form. <i>Example</i>: in Syrian Arabic <i>ʔəṣṣeṭ haz-zalame</i> <q>that fellow's story</q>, the Construct Form <i>ʔəṣṣeṭ</i> <q>story</q> is an example of a special form used in this <a>nominal modifier construction</a>. (Section 4.5)" ;
    dcterms:isPartOf "4.5" ;
    skos:altLabel "base modification" ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_special-p-strategy a compcon:str ;
    rdfs:label "special P strategy" ;
    cx:hasExample compcon:special-p-strategy-example-0 ;
    dc:description "a <a>strategy</a> of the <a>passive--inverse voice construction</a> in which the <a>P</a> <a>participant</a> is encoded by a form that is not found for either the <a>A</a> <a>participant</a> or the P participant in the <a>basic voice construction</a>. <i>Example</i>: in Upriver Halkomelem <i>tə́s-l-əm θúƛ'à tə swíyəqə</i> <q>She was bumped into by the man</q>, the P participant (the woman referred to by <q>she</q>) is <a>indexed</a> by the suffix <i>-əm</i>, which is distinct from the expression of A or P participants in the basic voice construction. (Section 8.3)" ;
    dcterms:isPartOf "8.3" ;
    compcon:expressionOf compcon:cxn_passive-inverse-voice ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_specialized-dual-role-strategy a compcon:str ;
    rdfs:label "specialized dual role strategy" ;
    cx:hasExample compcon:specialized-dual-role-strategy-example-0,
        compcon:specialized-dual-role-strategy-example-1 ;
    dc:description "the <a>strategy</a> of construing the <u>affected</u> <a>subject</a> <a>participant</a> as playing two distinct <a>roles</a> in a <a>reflexive</a> or <a sem:reciprocal>reciprocal event</a>; hence, it is expressed by two distinct <a>argument phrases</a> in a <a>reflexive construction</a> or a <a>reciprocal construction</a>, but with a special form functioning as the nonsubject argument phrase. <i>Examples</i>: English <i>I saw myself</i> and <i>Mary and Sue praised each other</i> construe the participants -- I, and Mary and Sue, respectively -- as playing two roles in the seeing and praising event, but with special object forms <i>myself</i> and <i>each other</i> to express that the events are a reflexive event and a reciprocal event, respectively. (Section 7.2)" ;
    dcterms:isPartOf "7.2" ;
    compcon:subtypeOf compcon:str_dual-role-strategy,
        compcon:str_overt-coding .

compcon:str_split-argument-structure-strategy a compcon:str ;
    rdfs:label "split argument structure strategy" ;
    cx:hasExample compcon:split-argument-structure-strategy-example-0 ;
    dc:description "a <a>strategy</a> found in <a>complement clause constructions</a> in which the <a>argument structure construction</a> associated with the <a>complement-taking predicate</a> is completely distinct from the argument structure construction associated with the <a>complement</a> <a>predicate</a>. <i>Example</i>: the sentence <i>Terry believes that the company will give her a raise next month</i> uses a split argument structure strategy: the CTP <i>believes</i> has its own Subject (<i>Terry</i>) and Object (the complement clause), and the complement predicate <i>will give</i> has its own Subject (<i>the company</i>), Objects (<i>her</i> and <i>a raise</i>), and Oblique (<i>next month</i>). (Section 18.4.1)" ;
    dcterms:isPartOf "18.4.1" ;
    compcon:expressionOf compcon:cxn_complement-clause-construction ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_split-structure a compcon:str ;
    rdfs:label "split structure" ;
    cx:hasExample compcon:split-structure-example-0 ;
    dc:description "a strategy found for the expression of <a>thetic constructions</a>. In a split structure, the information normally expressed as the comment or <a>predicate</a> in a <a>topic--comment construction</a> may be expressed in a different <a>form</a>, usually some sort of <a>subordinate clause</a>-like form. The information normally expressed as the <a  inf:reference>topic</a> in the topic--comment construction may be expressed in a different form, usually with a <a>presentational construction</a>; or both types of information are expressed differently. <i>Example</i>: in French <i>Voilà la sirène | qui hurle</i> [There's a siren | that's wailing] <q>The SIREN is wailing</q>, the <a>interruption</a> thetic construction expresses the participant (the siren) in a presentational construction, and expresses the event in a <a>relative clause construction</a>. (Section 11.3.2)" ;
    dcterms:isPartOf "11.3.2" ;
    compcon:subtypeOf compcon:str_subject-predicate-alignment .

compcon:str_stamp-strategy a compcon:str ;
    rdfs:label "STAMP strategy" ;
    cx:hasExample compcon:stamp-strategy-example-0 ;
    dc:description "a <a>strategy</a> found with <a>auxiliary constructions</a> in which the <a>auxiliary</a> <a>element</a> expressing <a>TAMP</a> is morphologically combined with the <a>subject phrase</a>, usually a subject <a>pronoun</a>, rather than with the <a>verb</a> of the auxiliary construction. <i>Example</i>: English <i>I'll think about it</i>, the future auxiliary <i>will</i> is reduced and combined morphologically with the subject pronoun <i>I</i>. (Section 13.4)" ;
    dcterms:isPartOf "13.4" ;
    compcon:expressionOf compcon:cxn_auxiliary-construction ;
    compcon:subtypeOf compcon:str_serial-verb-strategy .

compcon:str_stimulus-oriented-strategy a compcon:str ;
    rdfs:label "stimulus-oriented strategy" ;
    cx:hasExample compcon:stimulus-oriented-strategy-example-0 ;
    dc:description "a <a>strategy</a> for an <a>experiential construction</a> in which the <a>stimulus</a> <a>argument phrase</a> is <a>co-expressed</a> with the <a>subject</a> argument phrase of a <a>transitive</a> or <a>intransitive construction</a>. <i>Example</i>: the <a>argument structure construction</a> found in <i>Dogs frighten me</i>, with the stimulus argument phrase <i>Dogs</i> co-expressed with the prototypical subject <a>participant</a> in English, is an instance of the stimulus-oriented strategy. (Section 7.4)" ;
    dcterms:isPartOf "7.4" ;
    skos:altLabel "experiencer-object strategy" ;
    compcon:subtypeOf compcon:str_experiential-alignment .

compcon:str_subject-oblique-strategy a compcon:str ;
    rdfs:label "subject--oblique strategy" ;
    cx:hasExample compcon:subject-oblique-strategy-example-0 ;
    dc:description "an <a cxn:clause>argument structure</a> <a>strategy</a> that <a>co-expresses</a> the first <a>participant role</a> of the <a>event</a> with the <a>subject</a> of the <a>transitive construction</a>, but does not co-express the second participant role with the <a inf:object-argument>object</a> of the transitive construction; that is, the second participant role is expressed by an <a>oblique phrase</a>. <i>Example</i>: <i>She walked toward the tree</i> is an instance of the the subject--oblique strategy for the <a>motion event</a> expressed by <i>walked</i> -- the <a>figure</a> participant <i>She</i> is expressed as a <a>subject phrase</a>, and the <a>ground</a> participant <i>the tree</i> is expressed as an oblique phrase. (Section 7.3.1)" ;
    dcterms:isPartOf "7.3.1" ;
    skos:altLabel "extended intransitive" ;
    compcon:expressionOf compcon:cxn_experiential-construction,
        compcon:cxn_motion-construction,
        compcon:cxn_perception-clause,
        compcon:cxn_possessed-argument-construction ;
    compcon:subtypeOf compcon:str_system .

compcon:str_summarizer a compcon:str ;
    rdfs:label "summarizer" ;
    cx:hasExample compcon:summarizer-example-0 ;
    dc:description "a <a>form</a> used as part of an <a>exhaustive coordination construction</a> that indicates that the list is exhaustive. <i>Example</i>: in Classical Tibetan <i>lus ŋag yid gsum</i> [body speech mind three] <q>body, speech and mind</q>, the numeral functions as a summarizer for the exhaustive list. (Section 15.2.2)" ;
    dcterms:isPartOf "15.2.2" ;
    compcon:expressionOf compcon:cxn_exhaustive-list-coordination ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_switch-reference-system a compcon:str ;
    rdfs:label "switch-reference system" ;
    cx:hasExample compcon:switch-reference-system-example-0 ;
    dc:description "an <a>absolute deranking system</a> where the <a>same-subject</a> and <a>different-subject</a> <a>reference tracking constructions</a> systematically use different <a>deranked</a> constructions. <i>Example</i>: in Tauya <i>nono imai-te-pa ai mene-a-te pai aʔate-pa</i>... <q>She carried the child and came and stayed, they hit [= killed] the pigs and...</q> <i>imai-te-pa</i> [(<sc>3sg</sc>)-carry-get-SS] is a deranked predicate <a>form</a> with the same-subject deranking morpheme <i>-pa</i> since the following subject is also <q>she</q>, and <i>mene-a-te</i> [stay-3sg-DS] is a deranked predicate form with the different-subject deranking morpheme <i>-te</i> since the following subject switches to <q>they</q>. (Section 16.4)" ;
    dcterms:isPartOf "16.4" ;
    compcon:expressionOf compcon:cxn_reference-tracking-construction ;
    compcon:subtypeOf compcon:str_absolute-deranking-system .

compcon:str_tag a compcon:str ;
    rdfs:label "tag" ;
    cx:hasExample compcon:tag-example-0 ;
    dc:description "a strategy for <a cxn:polarity-question-construction>polarity question constructions</a> which consists of a word or phrase added to the <a>clause</a>, functioning to signal that the clause expresses a polarity question. <i>Example</i>: in the Russian polarity question <i>Ty ego slyšal, pravda?</i> [lit. <q>You heard him, true?</q>], <i>pravda</i> <q>true?</q> is the tag. Tags typically make the polarity question into a biased question, unlike a simple interrogative marker. (Section 12.3.2)" ;
    dcterms:isPartOf "12.3.2" ;
    compcon:expressionOf compcon:cxn_interrogative-construction ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_target a compcon:str ;
    rdfs:label "target" ;
    cx:hasExample compcon:target-example-0 ;
    dc:description "in a <a>construction</a> using the <a>indexation strategy</a>, the <a>element</a> of the construction with which the <a>index</a> is combined, usually as an affix. <i>Example</i>: in Spanish <i>los libros rojos</i> <q>the red books</q>, the target is the modifier <i>rojos</i> <q>red</q>, which has the Masculine Plural index <i>-os</i> suffixed to it. (Section 4.4)" ;
    dcterms:isPartOf "4.4" .

compcon:str_tense-iconicity a compcon:str ;
    rdfs:label "tense iconicity" ;
    cx:hasExample compcon:tense-iconicity-example-0 ;
    dc:description "a <a>strategy</a> in which the temporal sequence of the <a>clauses</a> in a <u>sentence</u> necessarily mirrors the <a>consecutive</a> temporal relation of the <a>events</a> denoted by the clauses. <i>Example</i>: in <i>He washed the car and drove to the party</i>, the temporal order of the clauses <i>He washed the car</i> and <i>drove to the party</i> mirrors the consecutive temporal relation of the events. (Section 15.1.3)" ;
    dcterms:isPartOf "15.1.3" ;
    compcon:expressionOf compcon:cxn_coordinate-construction ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_thetic-marker a compcon:str ;
    rdfs:label "thetic marker" ;
    dc:description "" ;
    compcon:expressionOf compcon:cxn_thetic-construction ;
    compcon:subtypeOf compcon:str_overt-coding .

compcon:str_topic-locational-hybrid-possessive-strategy a compcon:str ;
    rdfs:label "topic-locational hybrid possessive strategy" ;
    cx:hasExample compcon:topic-locational-hybrid-possessive-strategy-example-0 ;
    dc:description "a <a>strategy</a> for the <a>presentational possession construction</a> in which the <a>possessum</a> is expressed in a <a>subject phrase</a>, usually the subject of a <a>verb</a> glossed as <q>exist</q>, and the <a>possessor</a> is expressed both as a <a>topic phrase</a> and as a <a cxn:locative-phrase>locative</a> <a>argument phrase</a> (like the <a>locational possessive strategy</a>) <a>coreferential</a> with the topic phrase. <i>Example</i>: Eastern Tarafit <i>lɣula ttuɣa ɣr-es idž n wəzɛuq</i> [ogress was at-her one of little_donkey] <q>The ogress had a little donkey</q> is an instance of the topic-locational hybrid possessive strategy: the ogress is expressed as both a topic phrase <i>lɣula</i> and as a coreferential locative argument phrase <i>ɣr-es</i> <q>at her</q>. (Section 10.4.2)" ;
    dcterms:isPartOf "10.4.2" ;
    compcon:expressionOf compcon:cxn_presentational-possession-construction ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_topic-marker a compcon:str ;
    rdfs:label "topic marker" ;
    dc:description "" ;
    compcon:expressionOf compcon:cxn_topic-comment-construction ;
    compcon:subtypeOf compcon:str_overt-coding .

compcon:str_topic-possessive-strategy a compcon:str ;
    rdfs:label "topic possessive strategy" ;
    cx:hasExample compcon:topic-possessive-strategy-example-0 ;
    dc:description "a <a>strategy</a> for the <a>presentational possession construction</a> in which the <a>possessor</a> is expressed as a <a>topic phrase</a>, and the <a>possessum</a> is expressed in a <a>subject phrase</a>, usually the subject of a <a>verb</a> glossed as <q>exist</q>. <i>Example</i>: Cupeño <i>neʔ ne-mixen ʔiket miyexwe</i> [I my net is] <q>I have a net</q> is an instance of the topic possessive strategy, with <q>I</q> expressed in a topic phrase and <q>my net</q> in a subject phrase. (Section 10.4.2)" ;
    dcterms:isPartOf "10.4.2" ;
    skos:altLabel "distributed subject possessive strategy",
        "double subject possessive strategy" ;
    compcon:expressionOf compcon:cxn_presentational-possession-construction ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_transitivity-based-strategy a compcon:str ;
    rdfs:label "transitivity-based strategy" ;
    cx:hasExample compcon:transitivity-based-strategy-example-0,
        compcon:transitivity-based-strategy-example-1 ;
    dc:description "<u>monoclausal</u> <a>strategies</a> for the <a>causative construction</a> in which the <a>causee</a> in the <a>causative event</a> is coded differently depending on the <a>valency</a> of the corresponding <a>base event</a>. <i>Example</i>: in Turkish, the causee in a causative event is encoded in the Accusative Case if the base event is <a>monovalent</a> (<i>Ali Hasan<b>-t</b> öl-dür-dü</i> <q>Ali killed Hasan [<sc>acc</sc>]</q>), and in the Dative Case if the base event is <a>bivalent</a> (<i>Dişçi mektub-u müdür<b>-e</b> imzala-t-tí</i> <q>The dentist got the director [<sc>dat</sc>] to sign the letter</q>). (Section 9.2)" ;
    dcterms:isPartOf "9.2" ;
    compcon:subtypeOf compcon:str_causative-alignment .

compcon:str_tripartite-alignment a compcon:str ;
    rdfs:label "tripartite alignment" ;
    dc:description "a <a>co-expression strategy</a> in which all three of the <a>A</a>, <a>P</a>, and <a>S roles</a> are expressed with different <a>forms</a>. Tripartite alignment is extremely rare. (Section 6.3.1)" ;
    dcterms:isPartOf "6.3.1" ;
    compcon:subtypeOf compcon:str_transitive-alignment .

compcon:str_unique-equative a compcon:str ;
    rdfs:label "unique equative" ;
    dc:description "" ;
    compcon:expressionOf compcon:cxn_equative-construction ;
    compcon:subtypeOf compcon:str_fixed-case .

compcon:str_universal-concessive-conditional-strategy a compcon:str ;
    rdfs:label "universal concessive conditional strategy" ;
    cx:hasExample compcon:universal-concessive-conditional-strategy-example-0 ;
    dc:description "a strategy for expressing a <a>concessive conditional construction</a> where the <a cxn:protasis-clause>protasis</a>, which specifies the set of conditions for the concessive conditional, quantifies over all the alternatives possible in the <a>scalar model</a>; the <a cxn:apodosis-clause>apodosis</a> expresses that the outcome is the same under all conditions. <i>Example</i>: <i>However much advice you give him, he does exactly what he wants to do</i> uses the universal concessive conditional strategy: the protasis quantifies over all amounts of advice that you could give him, and the apodosis asserts that he does exactly what he wants to do under any of those conditions. (Section 17.3.3)" ;
    dcterms:isPartOf "17.3.3" ;
    compcon:expressionOf compcon:cxn_concessive-conditional-construction ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_universal-pronoun-strategy a compcon:str ;
    rdfs:label "universal pronoun strategy" ;
    dc:description "" ;
    compcon:expressionOf compcon:cxn_semantically-nonspecific-pronoun ;
    compcon:subtypeOf compcon:str_recruitment-strategy .

compcon:str_verb-coding-strategy a compcon:str ;
    rdfs:label "verb-coding strategy" ;
    cx:hasExample compcon:verb-coding-strategy-example-0 ;
    dc:description "a <a>strategy</a> for the expression of the <a>semantic role</a> of the <a>relative clause head</a> in the event denoted by the <a>relative clause</a> of an <a>externally headed</a> <a>relative clause construction</a>, in which the <a>predicate</a> of the relative clause uses different <a cxn:clause>voice</a> <a>forms</a> in order to specify the semantic role of the shared participant in the relative clause event. <i>Example</i>: in Luganda <i>ekiso</i> [<i>John kyeyattisa enkoko</i>] <q>the knife with which John killed the chicken</q>, the predicate <i>kyeyattisa</i> <q>killed</q> contains the <a sem:instrument>instrumental</a> <a cxn:applicative-object-phrase>applicative</a> suffix <i>-is</i> that indicates that the <a>relative clause head</a> <i>ekiso</i> <q>knife</q> denotes the <a>instrument</a> participant in the killing event. Comrie (2003b) restricts verb-coding strategies to languages which use voice forms that are exclusively found in relative clause constructions (and thus would exclude the Luganda example); we follow the broader definition introduced in Keenan (1972). (Section 19.3)" ;
    dcterms:isPartOf "19.3" ;
    skos:altLabel "verb-coding" ;
    compcon:expressionOf compcon:cxn_relative-clause-construction ;
    compcon:subtypeOf compcon:str_overt-coding .

compcon:str_verb-framing-strategy a compcon:str ;
    rdfs:label "verb-framing strategy" ;
    cx:hasExample compcon:verb-framing-strategy-example-0 ;
    dc:description "a <a>strategy</a> for expressing <a>motion events</a> such that the path of motion is expressed by a <a>path verb</a>. <i>Example</i>: <i>The guests entered the reception hall</i> uses the verb-framing strategy: the path verb <i>enter</i> describes a <a>path of motion event</a>. (Section 7.3.1)" ;
    dcterms:isPartOf "7.3.1" ;
    compcon:expressionOf compcon:cxn_motion-construction ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_verbal-copula a compcon:str ;
    rdfs:label "verbal copula" ;
    dc:description "a <a>strategy</a> used for <a cxn:nonprototypical-predication-construction>nonprototypical predication</a>, which uses a morpheme different from the <a sem:object-concept>object</a> concept word, the <a>copula</a>, which is inflected for at least some of the categories that prototypical <a>predication</a> <a>constructions</a> in the language -- that is, predication constructions headed by a <a>verb</a> -- also inflect for. Example: in the English Predicate Nominal Construction, illustrated by <i>Sam is a bloodhound</i>, <i>is</i> is a copula that inflects for <a>person</a>, <a>number</a>, and <a>tense</a> like English Verbs do. (Sections 1.4, <b>10.2</b>)" ;
    dcterms:isPartOf "1.4",
        "10.2" ;
    compcon:expressionOf compcon:cxn_nonprototypical-predication-construction ;
    compcon:subtypeOf compcon:str_copula .

compcon:str_verbal-strategy a compcon:str ;
    rdfs:label "verbal strategy" ;
    cx:hasExample compcon:verbal-strategy-example-0,
        compcon:verbal-strategy-example-1 ;
    dc:description "the <a>strategy</a> of <a>recruiting</a> what was originally the <a>action</a> <a>predication</a> <a>construction</a> for <a cxn:nonprototypical-predication-construction>nonprototypical predication</a>. It is presumed that an originally action predication construction lacks an <a>auxiliary</a>, and it expresses certain grammatical categories, in particular <a>person indexation</a> and <a>negation</a>. <i>Example</i>: Nahuatl <i>ni-ti:citl</i> [<sc>1sg</sc>-doctor] <q>I am a doctor</q> recruits the action predication construction for <a sem:object-concept>object</a> <a>predication</a>; cf. <i>ni-cho:ca</i> [<sc>1sg</sc>-cry] <q>I am crying</q>. (Section 10.2)" ;
    dcterms:isPartOf "10.2" ;
    compcon:expressionOf compcon:cxn_nonprototypical-predication-construction ;
    compcon:subtypeOf compcon:str_zero-coding .

compcon:str_with-possessive-strategy a compcon:str ;
    rdfs:label "with-possessive strategy" ;
    cx:hasExample compcon:with-possessive-strategy-example-0 ;
    dc:description "a <a>strategy</a> for the <a>presentational possession construction</a> in which the <a>possessor</a> is expressed in a <a>subject phrase</a>, and the <a>possessum</a> is expressed in a <a>comitative</a> <a cxn:referring-phrase>phrase</a>. <i>Example</i>: Amele <i>ija sigin <b>ca</b></i> [I knife <b>with</b>] <q>I have a knife</q> [lit. <q>I am with a knife</q>] is an instance of the with-possessive strategy. (Section 10.4.2)" ;
    dcterms:isPartOf "10.4.2" ;
    skos:altLabel "companion strategy" ;
    compcon:expressionOf compcon:cxn_presentational-possession-construction ;
    compcon:subtypeOf compcon:str_encoding-strategy .

compcon:str_yes-no-alignment a compcon:str ;
    rdfs:label "yes/no alignment" ;
    cx:hasExample compcon:yes-no-alignment-example-0 ;
    dc:description "an <a>alignment strategy</a> for the <a>polarity response construction</a> in which the answer to a negative <a cxn:polarity-question-construction>polarity question</a> aligns the polarity of the answer, not the polarity of the speaker's question. <i>Example</i>: in English, the answer to the negative polarity question <i>Do you not have any money?</i> where the addressee does not have any money is <i>No</i>; the polarity of the response matches the polarity of the answer (that the addressee does not have any money). (Section 12.3.3)" ;
    dcterms:isPartOf "12.3.3" ;
    skos:altLabel "yes/no alignment (strategy)" ;
    compcon:subtypeOf compcon:str_polarity-response-alignment .

compcon:str_yes-no-disagree-alignment a compcon:str ;
    rdfs:label "yes/no/disagree alignment" ;
    cx:hasExample compcon:yes-no-disagree-alignment-example-0,
        compcon:yes-no-disagree-alignment-example-1,
        compcon:yes-no-disagree-alignment-example-2,
        compcon:yes-no-disagree-alignment-example-3 ;
    dc:description "an <a>alignment strategy</a> for the <a>polarity response construction</a> in which the answer to a negative <a cxn:polarity-question-construction>polarity question</a> aligns the polarity of the answer when the answer is negative, but uses a special disagreeing form when the answer is positive. <i>Example</i>: in French, when the question is <q>Do you not have any money?</q>, the answer <i>non</i> <q>no</q> indicates that the respondent has no money, but the answer <i>si</i> (different from the positive answer <i>oui</i> <q>yes</q>) indicates that the respondent disagrees with the speaker and indeed does have money. (Section 12.3.3)" ;
    dcterms:isPartOf "12.3.3" ;
    skos:altLabel "yes/no/disagree alignment (strategy)" ;
    compcon:subtypeOf compcon:str_polarity-response-alignment .

compcon:str_zero-anaphora a compcon:str ;
    rdfs:label "zero anaphora" ;
    dc:description "the absence of a <a>referring phrase</a> for a <a>referent</a> in a construction. Zero anaphora is frequently used when the referent is <a>active</a>. (Section 3.3.1)" ;
    dcterms:isPartOf "3.3.1" ;
    skos:altLabel "DNI",
        "definite null instantiation",
        "null anaphora" ;
    compcon:expressionOf compcon:cxn_anaphoric-pronoun ;
    compcon:subtypeOf compcon:str_zero-coding .

compcon:str_zero-article a compcon:str ;
    rdfs:label "zero article" ;
    dc:description "" ;
    compcon:expressionOf compcon:cxn_article ;
    compcon:subtypeOf compcon:str_zero-coding .

compcon:str_zero-coded-verb a compcon:str ;
    rdfs:label "zero coded verb" ;
    cx:hasExample compcon:zero-coded-verb-example-0 ;
    dc:description "a <a>strategy</a> with any of the different kinds of <a>voice constructions</a> in which there is <a>zero coding</a> of the <a>function</a> of the voice construction on the <a>verb</a>. <i>Example</i>: <i>John planted the garden with trees</i> is an applicative construction without any overt coding of the applicative function on the verb <i>planted</i>. (Section 9.4)" ;
    dcterms:isPartOf "9.4" ;
    skos:altLabel "zero verb-coded voice strategy" ;
    compcon:expressionOf compcon:cxn_clause ;
    compcon:subtypeOf compcon:str_zero-coding .

compcon:str_zero-copula a compcon:str ;
    rdfs:label "zero copula" ;
    cx:hasExample compcon:zero-copula-example-0 ;
    dc:description "the <a>strategy</a> of <a>recruiting</a> a <a>construction</a> without any overt coding of <a>predication</a> (i.e. no <a>copula</a>), and without any inflection, for different types of predication. <i>Example</i>: Tiwi <i>purukupaɹli maɹtina</i> <q>Purukuparli is boss</q> simply juxtaposes the referring phrases for <q>Purukuparli</q> and <q>boss</q>. (Section 10.2)" ;
    dcterms:isPartOf "10.2" ;
    compcon:expressionOf compcon:cxn_nonprototypical-predication-construction ;
    compcon:subtypeOf compcon:str_zero-coding .

