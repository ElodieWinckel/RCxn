compcon:cxn_set-member-term a compcon:cxn ;
    rdfs:label "set-member term" ;
    cx:hasExample compcon:set-member-term-example-0,
        compcon:set-member-term-example-1,
        compcon:set-member-term-example-2,
        compcon:set-member-term-example-3 ;
    dc:description "a <a>form</a> that specifies a member or members of a designated set, usually previously presented in the discourse or inferrable from it, which has some sort of ordering. <i>Example</i>: in <i>the next question</i>, <i>next</i> is a set-member term. Other set-member terms include <i>last</i>, <i>another, (the) other</i>, and the <a cxn:ordinal-numeral>ordinal numerals</a>. (Section 4.1.3)" ;
    dcterms:isPartOf "4.1.3" ;
    compcon:subtypeOf compcon:cxn_modifier .

compcon:cxn_shape-term a compcon:cxn ;
    rdfs:label "shape term" ;
    cx:hasExample compcon:shape-term-example-0 ;
    dc:description "a <a>modifier</a> expressing a concept of physical shape or form. <i>Example</i>: <i>round</i> is an English shape term. (Section 4.1.2)" ;
    dcterms:isPartOf "4.1.2" ;
