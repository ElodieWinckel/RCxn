compcon:cxn_superlative-form a compcon:cxn ;
    rdfs:label "superlative form" ;
    cx:hasExample compcon:superlative-form-example-0 ;
    dc:description "the most extreme value on a <a>property</a> scale for the relevant referents. <i>Example</i>: in <i>most expensive</i>, <i>most</i> indicates the highest value on the expensiveness scale for the relevant set of objects. (Section 4.1.2)" ;
    dcterms:isPartOf "4.1.2" ;
    compcon:subtypeOf compcon:cxn_admodifier .

compcon:cxn_temporary-predicate a compcon:cxn ;
    rdfs:label "temporary predicate" ;
    cx:hasExample compcon:temporary-predicate-example-0 ;
    dc:description "the <a>predicates</a> that express the class of <a sem:state>temporary state</a> events. <i>Example</i>: being sick is a temporary state, and <i>(be) sick</i> is a temporary state predicate. (Section 6.3.3)" ;
    dcterms:isPartOf "6.3.3" ;
    compcon:subtypeOf compcon:cxn_predicate .

compcon:cxn_third-person-pronoun a compcon:cxn ;
    rdfs:label "third person pronoun" ;
    cx:hasExample compcon:third-person-pronoun-example-0 ;
