    rdfs:label "avaru kadite eɽudiiʈʈu naaval moɽipeyarttaaru" ;
    compcon:language "Tamil" .

compcon:absolute-deranking-system-example-1 a cx:Example ;
    rdfs:label "naan paɳam kuɖuttu avan sinimaavukku poonaan" ;
    compcon:language "Tamil" .

compcon:accusative-alignment-example-0 a cx:Example ;
