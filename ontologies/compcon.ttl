compcon:cxn_universal-pronoun a compcon:cxn ;
    rdfs:label "universal pronoun" ;
    cx:hasExample compcon:universal-pronoun-example-0 ;
    dc:description "universal pronouns express when the <a>predication</a> applies to all <a>referents</a> in a set determined by the type description provided by the pronoun. <i>Example</i>: <i>Everyone left the room</i> predicates of all members of a contextually determined set of people (indicated by <i>-one</i>) that they left the room. (Section 3.5)" ;
    dcterms:isPartOf "3.5" ;
    compcon:subtypeOf compcon:cxn_pronoun .

compcon:cxn_vague-numeral-term a compcon:cxn ;
