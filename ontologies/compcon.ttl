compcon:sem_agent a compcon:sem ;
    rdfs:label "agent" ;
    cx:hasExample compcon:agent-example-0 ;
    dc:description "a <a>semantic role</a> that includes <a>participant roles</a> for a <a>participant</a> that volitionally initiates an event. <i>Example</i>: in <i>Jack broke the window</i>, Jack volitionally initiates the breaking event. (Section 6.1.2)" ;
    dcterms:isPartOf "6.1.2" ;
    compcon:subtypeOf compcon:sem_external-cause .

compcon:sem_antecedent-role a compcon:sem ;
    rdfs:label "antecedent role" ;
    cx:hasExample compcon:antecedent-role-example-0 ;
    dc:description "a <a>participant role</a> that is antecedent to the participant role expressed as <a inf:object-argument>object</a> in the <a>causal chain</a> / <a>causal structure</a> of an <a>event</a>. <i>Example</i>: in <i>Jack broke the window with a hammer</i>, the hammer is antecedent to the window in the breaking causal chain (Jack --> hammer --> window), and the window is expressed as object. (Section 6.1.2)" ;
    dcterms:isPartOf "6.1.2" ;
    compcon:subtypeOf compcon:sem_peripheral-participant .

compcon:sem_causal-structure a compcon:sem ;
    rdfs:label "causal structure" ;
    cx:hasExample compcon:causal-structure-example-0 ;
    dc:description "the interactions among participants in an event, specifically causal interactions (although there are other types of interactions among participants in an event which are subsumed under the term). <i>Example</i>: in <i>The cats scratched the furniture</i>, the cats act on the furniture, and the furniture undergoes a change of state. The causal structure of events that is expressed by a single <a>argument structure construction</a> in a single <a>clause</a> is generally in the form of a <a>causal chain</a>. (Sections 6.1.1, <b>6.1.2</b>)" ;
    dcterms:isPartOf "6.1.1",
        "6.1.2" ;
    skos:altLabel "force dynamics" .

compcon:sem_change-of-state-event a compcon:sem ;
    rdfs:label "change of state event" ;
    dc:description "an <a>event</a> in which a <a>participant</a>, the <a>patient</a>, undergoes a change such that the patient enters a resulting <a>state</a>, usually a change of physical state. <i>Example</i>: the event of dishes becoming dry is a change of state event. (Sections 6.1.2, <b>6.2.1</b>)" ;
    dcterms:isPartOf "6.1.2",
        "6.2.1" ;
    skos:altLabel "COS (event)",
        "change of state (event)" ;
    compcon:functionOf compcon:cxn_change-of-state-verb ;
    compcon:subtypeOf compcon:sem_qualitative-event .

compcon:sem_conditional-relation a compcon:sem ;
    rdfs:label "conditional relation" ;
    cx:hasExample compcon:conditional-relation-example-0 ;
    dc:description "a semantic relation between two <a>events</a> that involves a logical material implication relation between their corresponding <a>propositions</a>, some type of <a>causal relation</a> between the corresponding <a>events</a>, and non-positive <a>epistemic stance</a>. <i>Example</i>: <i>If you press this button, the door will open</i> is an instance of the conditional relation and construction. The causally antecedent proposition is the <a sem:protasis>protasis</a>, and the causally consequent proposition is the <a sem:apodosis>apodosis</a>. A conditional may express a <a sem:content-causal-relation>content</a>, <a sem:epistemic-causal-relation>epistemic</a> or <a>speech act causal relation</a>. (Section 17.3.1)" ;
    dcterms:isPartOf "17.3.1" ;
    skos:altLabel "conditional (relation)" ;
    compcon:functionOf compcon:cxn_conditional-construction ;
    compcon:subtypeOf compcon:sem_event-relation .

compcon:sem_meaning a compcon:sem ;
    rdfs:label "meaning" ;
    dc:description "the information content that is conventionally conveyed by a <a>construction</a>. <i>Example</i>: a word such as <i>square</i> has the meaning of a particular shape. (Section 1.1)" ;
    dcterms:isPartOf "1.1" ;
    skos:altLabel "information (content)",
        "semantic (content)" ;
    compcon:functionOf compcon:cxn_construction .

compcon:sem_patient a compcon:sem ;
    rdfs:label "patient" ;
    cx:hasExample compcon:patient-example-0 ;
    dc:description "a <a>semantic role</a> including <a>participant roles</a> for a <a>participant</a> that undergoes a significant change as a result of the event occurring. <i>Example</i>: in <i>Jack broke the window</i>, the window is broken as a result of the breaking event. (Section 6.1.2)" ;
    dcterms:isPartOf "6.1.2" ;
    compcon:roleOf compcon:sem_qualitative-event ;
    compcon:subtypeOf compcon:sem_undergoer .

compcon:sem_peripheral-participant a compcon:sem ;
    rdfs:label "peripheral participant" ;
    dc:description "certain <a>participants</a> are considered to be less central to the <a>event</a>, in particular those that do not initiate the event and those that are not strongly affected by the action. <i>Example</i>: in an eating event, the utensils used by the eater and the plate on which the food was located are peripheral participants in the event. Peripheral participants are quite diverse. (Section 6.1.1)" ;
    dcterms:isPartOf "6.1.1" ;
    compcon:functionOf compcon:cxn_noun-complement-referring-phrase,
        compcon:cxn_oblique-phrase ;
    compcon:subtypeOf compcon:sem_participant-role .

compcon:sem_recipient a compcon:sem ;
    rdfs:label "recipient" ;
    cx:hasExample compcon:recipient-example-0 ;
    dc:description "a <a>semantic role</a> including <a>participant roles</a> for a <a>participant</a> that receives an <a>entity</a> from another participant. <i>Example</i>: in <i>I sent the forms to the accountant</i>, the accountant is the recipient of the forms sent. (Section 6.1.2)" ;
    dcterms:isPartOf "6.1.2" ;
    skos:altLabel "G",
        "G role",
        "R",
        "R role" ;
    compcon:functionOf compcon:cxn_r-phrase ;
    compcon:roleOf compcon:sem_transfer-of-possession-event ;
    compcon:subtypeOf compcon:sem_co-actor .

compcon:sem_set-member a compcon:sem ;
    rdfs:label "set member" ;
    dc:description "" ;
    compcon:functionOf compcon:cxn_set-member-attributive-phrase ;
    compcon:subtypeOf compcon:sem_quantitative-relation .

compcon:sem_theme a compcon:sem ;
    rdfs:label "theme" ;
    cx:hasExample compcon:theme-example-0 ;
    dc:description "a <a>semantic role</a> including <a>participant roles</a> for a <a>participant</a> that is the transferred <a>entity</a> in a <a sem:transfer-of-possession-event>transfer event</a>. <i>Example</i>: in <i>I sent the forms to the accountant</i>, the entity transferred is the forms. The term <q>theme</q> is also used for the participant functioning as the <a>figure</a> in <a>motion</a>, <a>application</a>, <a>removal</a>, and other events involving the movement of the participant. (Section 6.1.2)" ;
    dcterms:isPartOf "6.1.2" ;
    skos:altLabel "T",
        "T role" ;
    compcon:functionOf compcon:cxn_t-phrase ;
    compcon:roleOf compcon:sem_transfer-of-possession-event ;
    compcon:subtypeOf compcon:sem_undergoer .

