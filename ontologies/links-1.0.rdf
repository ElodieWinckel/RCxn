        <rdfs:label xml:lang="en">cross-linguistic different form and same function</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#CL_differentFormSimilarFunction -->

