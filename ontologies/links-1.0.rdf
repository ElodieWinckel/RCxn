        <dc:description xml:lang="en">connects two constructions from different languages with equal forms and equal functions</dc:description>
        <rdfs:label xml:lang="en">cross-linguistic same form and same function</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#CL_sameFormSimilarFunction -->

    <owl:ObjectProperty rdf:about="https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#CL_sameFormSimilarFunction">
        <rdfs:subPropertyOf rdf:resource="https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#CL_sameForm"/>
        <rdfs:subPropertyOf rdf:resource="https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#CL_similarFunction"/>
        <dc:description xml:lang="en">connects two constructions from different languages with equal forms and whose functions are similar (i.e., their main features are alignable)</dc:description>
        <rdfs:label xml:lang="en">cross-linguistic same form and similar function</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#CL_sameFunction -->

