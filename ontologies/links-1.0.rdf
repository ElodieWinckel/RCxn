        <dc:description xml:lang="en">connects two constructions from different languages with the same function</dc:description>
        <rdfs:label xml:lang="en">cross-linguistic same function</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#CL_similarForm -->

    <owl:ObjectProperty rdf:about="https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#CL_similarForm">
