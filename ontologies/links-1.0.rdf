        <rdfs:label xml:lang="en">cross-linguistic same form</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#CL_sameFormDifferentFunction -->

