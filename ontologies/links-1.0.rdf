        <dc:description xml:lang="en">connects two constructions from different languages whose forms differ</dc:description>
        <rdfs:label xml:lang="en">cross-linguistic different form</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#CL_differentFormSameFunction -->

