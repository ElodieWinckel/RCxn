        <rdfs:subPropertyOf rdf:resource="https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#CL_similarFunction"/>
        <dc:description xml:lang="en">connects two constructions from different languages whose forms and functions are similar (i.e., their main features are alignable)</dc:description>
        <rdfs:label xml:lang="en">cross-linguistic similar form and similar function</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#CL_similarFunction -->

    <owl:ObjectProperty rdf:about="https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#CL_similarFunction">
