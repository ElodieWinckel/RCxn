        <dc:description xml:lang="en">connects two constructions from different languages with equal forms but whose functions differ</dc:description>
        <rdfs:label xml:lang="en">cross-linguistic same form and different function</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#CL_sameFormSameFunction -->

