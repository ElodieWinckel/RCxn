        <dc:description xml:lang="en">connects two constructions from different languages whose functions are similar (i.e., their main features are alignable)</dc:description>
        <rdfs:label xml:lang="en">cross-linguistic similar function</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#CrossLinguisticLink -->

    <owl:ObjectProperty rdf:about="https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#CrossLinguisticLink">
        <rdfs:subPropertyOf rdf:resource="https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#Link"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <dc:description xml:lang="en">connects a construction in a language A with a construction in a language B, where A and B are distinct languages</dc:description>
        <rdfs:label xml:lang="en">cross-linguistic link</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#LanguageInternalLink -->

    <owl:ObjectProperty rdf:about="https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#LanguageInternalLink">
        <rdfs:subPropertyOf rdf:resource="https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#Link"/>
        <dc:description xml:lang="en">connects a construction with another construction; both constructions belong to the same language</dc:description>
        <rdfs:label xml:lang="en">language-internal link</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#Link -->

    <owl:ObjectProperty rdf:about="https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#Link">
        <rdfs:domain rdf:resource="https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/rcxn#Construction"/>
        <rdfs:range rdf:resource="https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/rcxn#Construction"/>
        <dc:description xml:lang="en">connects two constructions in a &quot;network of signs&quot;</dc:description>
        <rdfs:label xml:lang="en">link</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#ParagRel -->

    <owl:ObjectProperty rdf:about="https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#ParagRel">
        <rdfs:subPropertyOf rdf:resource="https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#LanguageInternalLink"/>
        <rdfs:label xml:lang="en">paradigmatic relation</rdfs:label>
        <skos:altLabel xml:lang="en">elaboration link</skos:altLabel>
    </owl:ObjectProperty>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#ParagRel"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2004/02/skos/core#altLabel"/>
        <owl:annotatedTarget xml:lang="en">elaboration link</owl:annotatedTarget>
        <rdfs:isDefinedBy>Langacker, Ronald W. 1988. An overview of cognitive grammar. In Brygida Rudzka-Ostyn (ed.), Topics in Cognitive Linguistics (Current Issues in Linguistic Theory), 3. John Benjamins Publishing Company. https://doi.org/10.1075/cilt.50.03lan.</rdfs:isDefinedBy>
    </owl:Axiom>
    


    <!-- https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#ParagRelHorizontal -->

    <owl:ObjectProperty rdf:about="https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#ParagRelHorizontal">
        <rdfs:subPropertyOf rdf:resource="https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#ParagRel"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <dc:description xml:lang="en">link between &quot;similar or contrastive constructions, even when these constructions are not (immediately) subsumed under a schema&quot; (Diessel 2019:200)</dc:description>
        <rdfs:label xml:lang="en">horizontal paradigmatic link</rdfs:label>
        <skos:altLabel xml:lang="en">horizontal relation</skos:altLabel>
    </owl:ObjectProperty>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#ParagRelHorizontal"/>
        <owl:annotatedProperty rdf:resource="http://purl.org/dc/elements/1.1/description"/>
        <owl:annotatedTarget xml:lang="en">link between &quot;similar or contrastive constructions, even when these constructions are not (immediately) subsumed under a schema&quot; (Diessel 2019:200)</owl:annotatedTarget>
        <rdfs:isDefinedBy>Diessel, Holger. 2019. The Grammar Network: How Linguistic Structure Is Shaped by Language Use. Cambridge: Cambridge University Press. https://doi.org/10.1017/9781108671040.</rdfs:isDefinedBy>
    </owl:Axiom>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#ParagRelHorizontal"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2004/02/skos/core#altLabel"/>
        <owl:annotatedTarget xml:lang="en">horizontal relation</owl:annotatedTarget>
        <rdfs:isDefinedBy>Diessel, Holger. 2019. The Grammar Network: How Linguistic Structure Is Shaped by Language Use. Cambridge: Cambridge University Press. https://doi.org/10.1017/9781108671040. (p. 200)</rdfs:isDefinedBy>
        <rdfs:isDefinedBy>Diessel, Holger. 2023. The Constructicon: Taxonomies and Networks. Elements in Construction Grammar. Cambridge University Press. https://doi.org/10.1017/9781009327848. (p. 16)</rdfs:isDefinedBy>
    </owl:Axiom>
    


    <!-- https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#ParagRelVertical -->

    <owl:ObjectProperty rdf:about="https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#ParagRelVertical">
        <rdfs:subPropertyOf rdf:resource="https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#ParagRel"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <dc:description xml:lang="en">Based on the mechanism of inheritance, by which a child-construction receives properties from its parent-construction</dc:description>
        <rdfs:label xml:lang="en">vertical paradigmatic link</rdfs:label>
        <skos:altLabel xml:lang="en">taxonomic relation</skos:altLabel>
    </owl:ObjectProperty>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#ParagRelVertical"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2004/02/skos/core#altLabel"/>
        <owl:annotatedTarget xml:lang="en">taxonomic relation</owl:annotatedTarget>
        <dc:description>A link &quot;connecting linguistic representations at different levels of abstraction&quot; (Diessel 2019:43).</dc:description>
        <rdfs:isDefinedBy>Diessel, Holger. 2019. The Grammar Network: How Linguistic Structure Is Shaped by Language Use. Cambridge: Cambridge University Press. https://doi.org/10.1017/9781108671040. (p. 43-62)</rdfs:isDefinedBy>
    </owl:Axiom>
    


    <!-- https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#allostruction -->

    <owl:ObjectProperty rdf:about="https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#allostruction">
        <rdfs:subPropertyOf rdf:resource="https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#functionSimilarity"/>
        <dc:description xml:lang="en">Connects two constructions whose functions are very similar. The exact definition of what makes two constructions considered allostructions varies from author to author.</dc:description>
        <rdfs:label xml:lang="en">allostruction</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#constructemeOf -->

    <owl:ObjectProperty rdf:about="https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#constructemeOf">
        <rdfs:subPropertyOf rdf:resource="https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#inheritedBy"/>
        <owl:inverseOf rdf:resource="https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#hasConstructeme"/>
        <dc:description xml:lang="en">Takes the constructeme as its subject and one of its allostructions as its object. Allostructions are constructions with very similar meaning and/or functions, and their constructeme is the schema that encompasses their shared properties.</dc:description>
        <rdfs:label xml:lang="en">constructeme of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#differentForm -->

