        <dc:description xml:lang="en">connects two constructions from different languages whose forms are similar (i.e., their main features are alignable) and whose functions are the same</dc:description>
        <rdfs:label xml:lang="en">cross-linguistic similar form and same function</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0#CL_similarFormSimilarFunction -->

