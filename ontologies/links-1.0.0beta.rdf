        <owl:annotatedSource rdf:resource="https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0.0beta#inheritedBy"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2000/01/rdf-schema#comment"/>
        <owl:annotatedTarget xml:lang="en">In usage-based approaches, &quot;the downward direction is motivated by categorization&quot; (Diessel 2023:17)</owl:annotatedTarget>
        <rdfs:isDefinedBy>Diessel, Holger. 2023. The Constructicon: Taxonomies and Networks. Elements in Construction Grammar. Cambridge University Press. https://doi.org/10.1017/9781009327848.</rdfs:isDefinedBy>
    </owl:Axiom>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links-1.0.0beta#inheritedBy"/>
