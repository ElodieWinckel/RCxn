    <!-- https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links#ParagRelVertical -->

    <owl:ObjectProperty rdf:about="https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links#ParagRelVertical">
        <rdfs:subPropertyOf rdf:resource="https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links#ParagRel"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <dc:description xml:lang="en">Based on the mechanism of inheritance, by which a child-construction receives properties from its parent-construction</dc:description>
        <rdfs:label xml:lang="en">vertical paradigmatic link</rdfs:label>
        <skos:altLabel xml:lang="en">instance link</skos:altLabel>
        <skos:altLabel xml:lang="en">taxonomic relation</skos:altLabel>
    </owl:ObjectProperty>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links#ParagRelVertical"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2004/02/skos/core#altLabel"/>
        <owl:annotatedTarget xml:lang="en">instance link</owl:annotatedTarget>
        <dc:description xml:lang="en">&quot;Instance links are posited when a particular construction is a special case of another construction; that is, an instance link exists between constructions iff one construction is a more fully specified version of the other.&quot; (Goldberg 1995:79)</dc:description>
        <rdfs:isDefinedBy xml:lang="en">Goldberg, Adele E. 1995. Constructions: A Construction Grammar Approach to Argument Structure. University of Chicago Press. (p. 79-81)</rdfs:isDefinedBy>
    </owl:Axiom>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="https://bdlweb.phil.uni-erlangen.de/RCxn/ontologies/links#ParagRelVertical"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2004/02/skos/core#altLabel"/>
        <owl:annotatedTarget xml:lang="en">taxonomic relation</owl:annotatedTarget>
        <dc:description>A link &quot;connecting linguistic representations at different levels of abstraction&quot; (Diessel 2019:43).</dc:description>
